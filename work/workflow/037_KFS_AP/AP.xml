<data xmlns="ns:workflow" xsi:schemaLocation="ns:workflow resource:WorkflowData" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <documentTypes xmlns="ns:workflow/DocumentType" xsi:schemaLocation="ns:workflow/DocumentType resource:DocumentType">
    <documentType>
      <name>
        EIRT
      </name>
      <parent>
        AP
      </parent>
      <label>
        Electronic Invoice Reject Document
      </label>
      <docHandler>
        ${application.url}/purapElectronicInvoiceReject.do?methodToCall=docHandler
      </docHandler>
      <docSearchHelpURL>
        default.htm?turl=WordDocuments%2Fcustomdocumentsearches.htm
      </docSearchHelpURL>
      <active>
        true
      </active>
      <routingVersion>
        2
      </routingVersion>
      <routePaths>
        <routePath>
          <start name="AdHoc" nextNode="Management"/>
          <role name="Management"/>
        </routePath>
      </routePaths>
      <routeNodes>
        <start name="AdHoc"/>
        <role name="Management">
          <qualifierResolverClass>
            org.kuali.rice.kns.workflow.attribute.DataDictionaryQualifierResolver
          </qualifierResolverClass>
        </role>
      </routeNodes>
    </documentType>
    <documentType>
      <name>
        CM
      </name>
      <parent>
        AP
      </parent>
      <label>
        Vendor Credit Memo
      </label>
      <docHandler>
        ${application.url}/purapVendorCreditMemo.do?methodToCall=docHandler
      </docHandler>
      <helpDefinitionURL>
        default.htm?turl=WordDocuments%2Fvendorcreditmemo.htm
      </helpDefinitionURL>
      <docSearchHelpURL>
        default.htm?turl=WordDocuments%2Fcustomdocumentsearches.htm
      </docSearchHelpURL>
      <active>
        true
      </active>
      <routingVersion>
        2
      </routingVersion>
      <routePaths>
        <routePath>
          <start name="AdHoc" nextNode="RequiresImageAttachment"/>
          <split name="RequiresImageAttachment" nextNode="Account">
            <branch name="True">
              <role name="ImageAttachment" nextNode="JoinRequiresImageAttachment"/>
            </branch>
            <branch name="False">
              <simple name="NoOpRequiresImageAttachment" nextNode="JoinRequiresImageAttachment"/>
            </branch>
            <join name="JoinRequiresImageAttachment"/>
          </split>
          <role name="Account"/>
        </routePath>
      </routePaths>
      <routeNodes>
        <start name="AdHoc"/>
        <split name="RequiresImageAttachment">
          <type>
            org.kuali.kfs.sys.document.workflow.SimpleBooleanSplitNode
          </type>
        </split>
        <role name="ImageAttachment">
          <qualifierResolverClass>
            org.kuali.rice.kns.workflow.attribute.DataDictionaryQualifierResolver
          </qualifierResolverClass>
        </role>
        <join name="JoinRequiresImageAttachment"/>
        <simple name="NoOpRequiresImageAttachment">
          <type>
            org.kuali.rice.kew.engine.node.NoOpNode
          </type>
        </simple>
        <role name="Account">
          <qualifierResolverClass>
            org.kuali.rice.kns.workflow.attribute.DataDictionaryQualifierResolver
          </qualifierResolverClass>
        </role>
      </routeNodes>
    </documentType>
    <documentType>
      <name>
        PREQ
      </name>
      <parent>
        AP
      </parent>
      <label>
        Payment Request
      </label>
      <docHandler>
        ${application.url}/purapPaymentRequest.do?methodToCall=docHandler
      </docHandler>
      <helpDefinitionURL>
        default.htm?turl=WordDocuments%2Fpaymentrequest.htm
      </helpDefinitionURL>
      <docSearchHelpURL>
        default.htm?turl=WordDocuments%2Fcustomdocumentsearches.htm
      </docSearchHelpURL>
      <active>
        true
      </active>
      <routingVersion>
        2
      </routingVersion>
      <routePaths>
        <routePath>
          <start name="AdHoc" nextNode="RequiresImageAttachment"/>
          <split name="RequiresImageAttachment" nextNode="PurchaseWasReceived">
            <branch name="True">
              <role name="ImageAttachment" nextNode="JoinRequiresImageAttachment"/>
            </branch>
            <branch name="False">
              <simple name="NoOpRequiresImageAttachment" nextNode="JoinRequiresImageAttachment"/>
            </branch>
            <join name="JoinRequiresImageAttachment"/>
          </split>
          <split name="PurchaseWasReceived" nextNode="SubAccount">
            <branch name="True">
              <role name="Receiving" nextNode="JoinReceiving"/>
            </branch>
            <branch name="False">
              <simple name="NoOpReceiving" nextNode="JoinReceiving"/>
            </branch>
            <join name="JoinReceiving"/>
          </split>
          <role name="SubAccount" nextNode="Account"/>
          <role name="Account" nextNode="AccountingOrganizationHierarchy"/>
          <role name="AccountingOrganizationHierarchy" nextNode="VendorIsEmployeeOrNonResidentAlien"/>
          <split name="VendorIsEmployeeOrNonResidentAlien">
            <branch name="True">
              <role name="Tax" nextNode="JoinVendorIsEmployeeOrNonResidentAlien"/>
            </branch>
            <branch name="False">
              <simple name="NoOpVendorIsEmployeeOrNonResidentAlien" nextNode="JoinVendorIsEmployeeOrNonResidentAlien"/>
            </branch>
            <join name="JoinVendorIsEmployeeOrNonResidentAlien"/>
          </split>
        </routePath>
      </routePaths>
      <routeNodes>
        <start name="AdHoc"/>
        <split name="RequiresImageAttachment">
          <type>
            org.kuali.kfs.sys.document.workflow.SimpleBooleanSplitNode
          </type>
        </split>
        <role name="ImageAttachment">
          <qualifierResolverClass>
            org.kuali.rice.kns.workflow.attribute.DataDictionaryQualifierResolver
          </qualifierResolverClass>
        </role>
        <join name="JoinRequiresImageAttachment"/>
        <simple name="NoOpRequiresImageAttachment">
          <type>
            org.kuali.rice.kew.engine.node.NoOpNode
          </type>
        </simple>
        <split name="PurchaseWasReceived">
          <type>
            org.kuali.kfs.sys.document.workflow.SimpleBooleanSplitNode
          </type>
        </split>
        <role name="Receiving">
          <qualifierResolverClass>
            org.kuali.rice.kns.workflow.attribute.DataDictionaryQualifierResolver
          </qualifierResolverClass>
        </role>
        <join name="JoinReceiving"/>
        <simple name="NoOpReceiving">
          <type>
            org.kuali.rice.kew.engine.node.NoOpNode
          </type>
        </simple>
        <role name="SubAccount">
          <qualifierResolverClass>
            org.kuali.rice.kns.workflow.attribute.DataDictionaryQualifierResolver
          </qualifierResolverClass>
        </role>
        <role name="Account">
          <qualifierResolverClass>
            org.kuali.rice.kns.workflow.attribute.DataDictionaryQualifierResolver
          </qualifierResolverClass>
          <mandatoryRoute>
            true
          </mandatoryRoute>
        </role>
        <role name="AccountingOrganizationHierarchy">
          <qualifierResolverClass>
            org.kuali.rice.kns.workflow.attribute.DataDictionaryQualifierResolver
          </qualifierResolverClass>
        </role>
        <split name="VendorIsEmployeeOrNonResidentAlien">
          <type>
            org.kuali.kfs.sys.document.workflow.SimpleBooleanSplitNode
          </type>
        </split>
        <role name="Tax">
          <qualifierResolverClass>
            org.kuali.rice.kns.workflow.attribute.DataDictionaryQualifierResolver
          </qualifierResolverClass>
        </role>
        <join name="JoinVendorIsEmployeeOrNonResidentAlien"/>
        <simple name="NoOpVendorIsEmployeeOrNonResidentAlien">
          <type>
            org.kuali.rice.kew.engine.node.NoOpNode
          </type>
        </simple>
      </routeNodes>
    </documentType>
  </documentTypes>
</data>
