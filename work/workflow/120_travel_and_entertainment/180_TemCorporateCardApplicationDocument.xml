<data xmlns="ns:workflow" xsi:schemaLocation="ns:workflow resource:WorkflowData" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<documentTypes xmlns="ns:workflow/DocumentType" xsi:schemaLocation="ns:workflow/DocumentType resource:DocumentType">
		<documentType>
			<name>CCAP</name>
			<parent>KFST</parent>
			<label>Corporate Card Application</label>
			<docHandler>${application.url}/temCorporateCardApplication.do?methodToCall=docHandler</docHandler>
			<active>true</active>
			<routingVersion>1</routingVersion>
			<validApplicationStatuses>
				<status>Application</status>
				<status>Fiscal Officer Review</status>
				<status>Pending Bank Application</status>
				<status>Applied To Bank</status>
				<status>Approved</status>
				<status>Declined</status>
			</validApplicationStatuses>
			<policies>
				<policy>
					<name>DOCUMENT_STATUS_POLICY</name>
					<stringValue>app</stringValue>
				</policy>
			</policies>
			<routePaths>
				<routePath>
					<start name="AdHoc" nextNode="Account" nextAppDocStatus="Fiscal Officer Review" />
					<role name="Account" nextNode="TravelManagerReview" nextAppDocStatus="Pending Bank Application" />
					<role name="TravelManagerReview" nextAppDocStatus="Approved" />
				</routePath>
			</routePaths>
			<routeNodes>
				<start name="AdHoc" />
				<role name="Account">
					<qualifierResolverClass>
						org.kuali.rice.krad.workflow.attribute.DataDictionaryQualifierResolver
					</qualifierResolverClass>
					<activationType>
						P
                    </activationType>
				</role>
				<role name="TravelManagerReview">
					<qualifierResolverClass>
						org.kuali.rice.krad.workflow.attribute.DataDictionaryQualifierResolver
					</qualifierResolverClass>
					<activationType>
						P
                    </activationType>
				</role>
			</routeNodes>
		</documentType>
	</documentTypes>
</data>