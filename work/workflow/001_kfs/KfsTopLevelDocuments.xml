<data xmlns="ns:workflow" xsi:schemaLocation="ns:workflow resource:WorkflowData" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <ruleAttributes xmlns="ns:workflow/RuleAttribute" xsi:schemaLocation="ns:workflow/RuleAttribute resource:RuleAttribute">
    <ruleAttribute>
      <name>FinancialSystemSearchableAttribute</name>
      <className>org.kuali.kfs.sys.document.workflow.FinancialSystemSearchableAttribute</className>
      <label>Financial System Searchable Attribute</label>
      <description>Financial System Searchable Attribute</description>
      <type>SearchableAttribute</type>
    </ruleAttribute>
    <ruleAttribute>
      <name>FinancialSystemDocumentSearchResultProcessorAttribute</name>
      <className>org.kuali.kfs.sys.document.workflow.DataDictionaryDocumentSearchCustomizer</className>
      <label>Financial System Document Search Result Processor Attribute</label>
      <description>Financial System Document Search Result Processor Attribute</description>
      <type>DocumentSearchResultProcessorAttribute</type>
    </ruleAttribute>
    <ruleAttribute>
      <name>FinancialSystemDocumentSearchGeneratorAttribute</name>
      <className>org.kuali.kfs.sys.document.workflow.DataDictionaryDocumentSearchCustomizer</className>
      <label>Financial System Document Search Generator Attribute</label>
      <description>Financial System Document Search Generator Attribute</description>
      <type>DocumentSearchGeneratorAttribute</type>
    </ruleAttribute>
    <ruleAttribute>
      <name>NoFyiActionListAttribute</name>
      <className>org.kuali.kfs.module.purap.document.workflow.PurchaseOrderActionListAttribute</className>
      <label>No FYI</label>
      <description>No FYI Action List Attribute</description>
      <type>ActionListAttribute</type>
    </ruleAttribute>
  </ruleAttributes>
  <documentTypes xmlns="ns:workflow/DocumentType" xsi:schemaLocation="ns:workflow/DocumentType resource:DocumentType">
    <documentType>
      <name>
        KFS
      </name>
      <parent>
        KualiDocument
      </parent>
      <label>
        FinancialSystemDocument
      </label>
      <serviceNamespace>
        KFS
      </serviceNamespace>
      <docHandler>
        ${application.url}/kr/maintenance.do?methodToCall=docHandler
      </docHandler>
      <postProcessorName>
        org.kuali.kfs.sys.document.workflow.PostProcessor
      </postProcessorName>
      <active>
        true
      </active>
      <attributes>
        <attribute>
          <name>
            FinancialSystemSearchableAttribute
          </name>
        </attribute>
        <attribute>
          <name>
            FinancialSystemDocumentSearchResultProcessorAttribute
          </name>
        </attribute>
        <attribute>
          <name>
            FinancialSystemDocumentSearchGeneratorAttribute
          </name>
        </attribute>
      </attributes>
      <routingVersion>
        2
      </routingVersion>
    </documentType>
    <documentType>
      <name>
        FSSM
      </name>
      <parent>
        KFS
      </parent>
      <label>
        FinancialSystemSimpleMaintenanceDocument
      </label>
      <active>
        true
      </active>
      <routingVersion>
        2
      </routingVersion>
      <routePaths>
        <routePath>
          <start name="AdHoc"/>
        </routePath>
      </routePaths>
      <routeNodes>
        <start name="AdHoc"/>
      </routeNodes>
    </documentType>
    <documentType>
      <name>
        KFSM
      </name>
      <parent>
        KFS
      </parent>
      <label>
        FinancialSystemComplexMaintenanceDocument
      </label>
      <active>
        true
      </active>
      <routingVersion>
        2
      </routingVersion>
      <routePaths>
        <routePath>
          <start name="AdHoc" nextNode="OrganizationHierarchy"/>
          <role name="OrganizationHierarchy"/>
        </routePath>
      </routePaths>
      <routeNodes>
        <start name="AdHoc"/>
        <role name="OrganizationHierarchy">
          <qualifierResolverClass>
            org.kuali.rice.kns.workflow.attribute.DataDictionaryQualifierResolver
          </qualifierResolverClass>
        </role>
      </routeNodes>
    </documentType>
    <documentType>
      <name>
        KFST
      </name>
      <parent>
        KFS
      </parent>
      <label>
        FinancialSystemTransactionalDocument
      </label>
      <docSearchHelpURL>
        default.htm?turl=WordDocuments%2Fcustomdocumentsearches.htm
      </docSearchHelpURL>
      <active>
        true
      </active>
      <security>
        <securityAttribute class="org.kuali.kfs.sys.document.workflow.SensitiveDataSecurityAttribute"/>
      </security>
      <routingVersion>
        2
      </routingVersion>
    </documentType>
    <documentType>
      <name>
        FSLO
      </name>
      <label>
        Financial System - Ledger Only
      </label>
      <active>
        true
      </active>
      <routingVersion>
        2
      </routingVersion>
      <routePaths/>
    </documentType>
  </documentTypes>
</data>
