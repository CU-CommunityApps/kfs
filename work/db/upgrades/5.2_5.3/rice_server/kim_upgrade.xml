            <column name="KIM_ATTR_DEFN_ID" valueNumeric=" (select kim_attr_defn_id from KRIM_ATTR_DEFN_T where nm='buttonName') " />
            <column name="ATTR_VAL" value="lastInventoryDateUpdateButton" />
        </insert>

        <insert tableName="KRIM_ROLE_PERM_T">
