			<column name="CPTLAST_LINE_NBR" type="NUMBER(7,0)" defaultValueNumeric="1" />
			<column name="CPTLAST_LINE_QTY" type="NUMBER(7,0)" defaultValueNumeric="0" />
			<column name="CPTLAST_LINE_AMT" type="NUMBER(19,2)" defaultValue="0.00" />
