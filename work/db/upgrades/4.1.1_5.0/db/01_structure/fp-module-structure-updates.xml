		<dropForeignKeyConstraint baseTableName="FP_CPTL_AST_INFO_DTL_T" constraintName="FP_CPTL_AST_INFO_DTL_TR1" />
		<dropPrimaryKey tableName="FP_CPTL_AST_INFO_T" />	
