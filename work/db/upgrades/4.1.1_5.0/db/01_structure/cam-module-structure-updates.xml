		</addColumn>
		<modifySql dbms='mysql'>
			<replace replace='VARCHAR2' with='VARCHAR' />
			<replace replace="DEFAULT Y" with="DEFAULT 'Y'" />
			<replace replace="DEFAULT N" with="DEFAULT 'N'" />
		</modifySql>
	</changeSet>
	
