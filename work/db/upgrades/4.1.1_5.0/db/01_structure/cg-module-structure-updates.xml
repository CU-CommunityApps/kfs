			<column name='ICR_FIN_COA_CD' type='VARCHAR2(2)' />
			<column name='ICR_FIN_ACCT_NBR' type='VARCHAR2(7)' />
			<column name='ACLN_PCT' type='VARCHAR2(45)' />
