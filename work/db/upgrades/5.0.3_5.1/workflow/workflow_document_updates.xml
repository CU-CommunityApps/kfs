<?xml version="1.0" encoding="UTF-8"?>
<data xmlns="ns:workflow" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="ns:workflow resource:WorkflowData">

 	<documentTypes xmlns="ns:workflow/DocumentType"
		xsi:schemaLocation="ns:workflow/DocumentType resource:DocumentType">
	 <documentType>
      <name>IAA</name>
      <label>Intra Account Adjustment</label>
      <parent>FP</parent>      
      <docHandler>${kfs.url}/financialIntraAccountAdjustment.do?methodToCall=docHandler</docHandler>      
      <active>
        true
      </active>
      <routePaths>
        <routePath>
          <start name="AdHoc" nextNode="Account"/>
          <role name="Account"/>
         </routePath>
      </routePaths>
      <routeNodes>
        <start name="AdHoc"/>
        <role name="Account">
          <qualifierResolverClass>org.kuali.rice.krad.workflow.attribute.DataDictionaryQualifierResolver</qualifierResolverClass>
          <activationType>P</activationType>
        </role>               
      </routeNodes>
    </documentType>
    <documentType>
	 <name>
        POA
      </name>
      <parent>
        PO
      </parent>
      <label>
        Purchase Order Amendment
      </label>
      <docHandler>
        ${kfs.url}/purapPurchaseOrder.do?methodToCall=docHandler
      </docHandler>
      <helpDefinitionURL>
        default.htm?turl=WordDocuments%2Fpurchaseorderamend.htm
      </helpDefinitionURL>
      <active>
        true
      </active>
      <routingVersion>
        2
      </routingVersion>
        <validApplicationStatuses>
        <category name="INCOMPLETE STATUSES">
	  	  <status>In Process</status>
		  <status>Awaiting Fiscal Officer Review</status>
		  <status>Awaiting New Unordered Item Review</status>
		  <status>Disapproved New Unordered Item Review</status>
		  <status>Awaiting Purchasing Approval</status>
		  <status>Awaiting Commodity Code Approval</status>
		  <status>Awaiting C and G Approval</status>
		  <status>Awaiting Budget Approval</status>
		  <status>Awaiting Tax Approval</status>
		  <status>Error occurred sending cxml</status>
		  <status>Error occurred sending fax</status>
		  <status>Out for Quote</status>
		  <status>Payment Hold</status>
		  <status>Pending Fax</status>
		  <status>Pending Print</status>
		  <status>Pending cxml</status>
		  <status>Waiting for Department</status>
		  <status>Waiting for Vendor</status>
		</category>
	
		<category name="COMPLETE STATUSES">
		  <status>Cancelled</status>
		  <status>Cancelled Change</status>
		  <status>Change in Process</status>
		  <status>Closed</status>
		  <status>Disapproved Budget</status>
		  <status>Disapproved C and G</status>
		  <status>Disapproved Change</status>
		  <status>Disapproved Commodity Code</status>
		  <status>Disapproved Purchasing</status>
		  <status>Disapproved Tax</status>
		  <status>Open</status>
		  <status>Pending Amendment</status>
		  <status>Pending Close</status>
		  <status>Pending Payment Hold</status>
		  <status>Pending Remove Hold</status>
		  <status>Pending Reopen</status>
		  <status>Pending Retransmit</status>
		  <status>Pending Void</status>
		  <status>Retired Version</status>
		  <status>Void</status>
		</category>
		
	  </validApplicationStatuses>
      <policies>
        <policy>
              <name>DOCUMENT_STATUS_POLICY</name>
              <stringValue>app</stringValue>
        </policy>
      </policies>	  
			<routePaths>
				<routePath>
					<start name="AdHoc" nextNode="HasNewUnorderedItems" />
					<split name="HasNewUnorderedItems" nextNode="RequiresItemsApproval">
						<branch name="True">
							<requests name="NewUnorderedItems" nextNode="JoinOrderedItems" />
						</branch>
						<branch name="False">
							<simple name="NoOpNewUnorderedItems" nextNode="JoinOrderedItems" />
						</branch>
						<join name="JoinOrderedItems" />
					</split>
					<split name="RequiresItemsApproval">
						<branch name="True">					
						<requests name="Account" nextNode="RequiresContractManagementReview"  nextAppDocStatus="Awaiting Purchasing Approval" />
							<split name="RequiresContractManagementReview" nextNode="Commodity">
								<branch name="True">
									<requests name="ContractManagement" nextNode="JoinRequiresContractManagementReview" nextAppDocStatus="Awaiting Commodity Code Approval" />
								</branch>
								<branch name="False">
									<requests name="NoOpRequiresContractManagementReview"
										nextNode="JoinRequiresContractManagementReview" nextAppDocStatus="Awaiting Commodity Code Approval" />
								</branch>
								<join name="JoinRequiresContractManagementReview" />
							</split>
							<requests name="Commodity" nextNode="RequiresAwardReview"  nextAppDocStatus="Awaiting C and G Approval"/>
							<split name="RequiresAwardReview" nextNode="RequiresBudgetReview">
								<branch name="True">
									<requests name="Award" nextNode="JoinRequiresAwardReview" nextAppDocStatus="Awaiting Budget Approval" />
								</branch>
								<branch name="False">
									<requests name="NoOpRequiresAwardReview" nextNode="JoinRequiresAwardReview" nextAppDocStatus="Awaiting Budget Approval" />
								</branch>
								<join name="JoinRequiresAwardReview" />
							</split>
							<split name="RequiresBudgetReview" nextNode="VendorIsEmployeeOrNonResidentAlien">
								<branch name="True">
									<requests name="Budget" nextNode="JoinRequiresBudgetReview" nextAppDocStatus="Awaiting Tax Approval" />
								</branch>
								<branch name="False">
									<simple name="NoOpBudget" nextNode="JoinRequiresBudgetReview" nextAppDocStatus="Awaiting Tax Approval"/>
								</branch>
								<join name="JoinRequiresBudgetReview" />
							</split>
							<split name="VendorIsEmployeeOrNonResidentAlien">
								<branch name="True">
									<requests name="Tax" nextNode="Join" nextAppDocStatus="Awaiting Tax Approval"/>
								</branch>
								<branch name="False">
									<simple name="NoOpVendorIsEmployeeOrNonResidentAlien"
										nextNode="Join" />
								</branch>
							</split>
						</branch>
						<branch name="False">
							<split name="RequiresItemsNonItem">
								<branch name="True">
							<requests name="AccountNonItem" nextNode="RequiresContractManagementNonItem" />
							<split name="RequiresContractManagementNonItem" nextNode="CommodityNonItem">
								<branch name="True">
									<requests name="ContractManagementNonItem" nextNode="JoinRequiresContractManagementNonItem" />
								</branch>
								<branch name="False">
									<requests name="NoOpRequiresContractManagementNonItem"
										nextNode="JoinRequiresContractManagementNonItem" />
								</branch>
								<join name="JoinRequiresContractManagementNonItem" />
							</split>
							<requests name="CommodityNonItem" nextNode="RequiresAwardNonItem" />
							<split name="RequiresAwardNonItem" nextNode="RequiresBudgetNonItem">
								<branch name="True">
									<requests name="AwardNonItem" nextNode="JoinRequiresAwardNonItem" />
								</branch>
								<branch name="False">
									<requests name="NoOpRequiresAwardNonItem" nextNode="JoinRequiresAwardNonItem" />
								</branch>
								<join name="JoinRequiresAwardNonItem" />
							</split>
							<split name="RequiresBudgetNonItem" nextNode="VendorIsEmployeeOrNonResidentAlienNonItem">
								<branch name="True">
									<requests name="BudgetNonItem" nextNode="JoinRequiresBudgetNonItem" />
								</branch>
								<branch name="False">
									<simple name="NoOpBudgetNonItem" nextNode="JoinRequiresBudgetNonItem" />
								</branch>
								<join name="JoinRequiresBudgetNonItem" />
							</split>
							<split name="VendorIsEmployeeOrNonResidentAlienNonItem">
								<branch name="True">
									<requests name="TaxNonItem" nextNode="Join" />
								</branch>
								<branch name="False">
									<simple name="NoOpVendorIsEmployeeOrNonResidentAlienNonItem"
										nextNode="Join" />
								</branch>
							</split>
						</branch>
								<branch name="False">
									<simple name="NoOpItemsNonItem" nextNode="Join" />
								</branch>
							</split>
						</branch>
						<join name="Join" />
					</split>
				</routePath>
			</routePaths>
			<routeNodes>
				<start name="AdHoc" />
				<join name="Join" />
				<role name="ContractManagement">
					<qualifierResolverClass>
						org.kuali.rice.krad.workflow.attribute.DataDictionaryQualifierResolver
					</qualifierResolverClass>
					<activationType>P</activationType>
				</role>
				<role name="Commodity">
					<qualifierResolverClass>
						org.kuali.rice.krad.workflow.attribute.DataDictionaryQualifierResolver
					</qualifierResolverClass>
					<activationType>P</activationType>
				</role>
				<role name="Award">
					<qualifierResolverClass>
						org.kuali.rice.krad.workflow.attribute.DataDictionaryQualifierResolver
					</qualifierResolverClass>
					<activationType>P</activationType>
				</role>
				<role name="Budget">
					<qualifierResolverClass>
						org.kuali.rice.krad.workflow.attribute.DataDictionaryQualifierResolver
					</qualifierResolverClass>
					<activationType>P</activationType>
				</role>
				<role name="Tax">
					<qualifierResolverClass>
						org.kuali.rice.krad.workflow.attribute.DataDictionaryQualifierResolver
					</qualifierResolverClass>
					<activationType>P</activationType>
				</role>
				<role name="ContractManagementNonItem">
					<qualifierResolverClass>
						org.kuali.rice.krad.workflow.attribute.DataDictionaryQualifierResolver
					</qualifierResolverClass>
					<activationType>P</activationType>
				</role>
				<role name="CommodityNonItem">
					<qualifierResolverClass>
						org.kuali.rice.krad.workflow.attribute.DataDictionaryQualifierResolver
					</qualifierResolverClass>
					<activationType>P</activationType>
				</role>
				<role name="AwardNonItem">
					<qualifierResolverClass>
						org.kuali.rice.krad.workflow.attribute.DataDictionaryQualifierResolver
					</qualifierResolverClass>
					<activationType>P</activationType>
				</role>
		        <role name="NewUnorderedItems">
		          <qualifierResolverClass>org.kuali.rice.krad.workflow.attribute.DataDictionaryQualifierResolver</qualifierResolverClass>
		          <activationType>P</activationType>
		        </role>
		        <role name="AccountNonItem">
		          <qualifierResolverClass>org.kuali.rice.krad.workflow.attribute.DataDictionaryQualifierResolver</qualifierResolverClass>
		          <activationType>P</activationType>
		        </role>
		        <role name="Account">
		          <qualifierResolverClass>org.kuali.rice.krad.workflow.attribute.DataDictionaryQualifierResolver</qualifierResolverClass>
		          <activationType>P</activationType>
		        </role>
				<role name="BudgetNonItem">
					<qualifierResolverClass>
						org.kuali.rice.krad.workflow.attribute.DataDictionaryQualifierResolver
					</qualifierResolverClass>
					<activationType>P</activationType>
				</role>
				<role name="TaxNonItem">
					<qualifierResolverClass>
						org.kuali.rice.krad.workflow.attribute.DataDictionaryQualifierResolver
					</qualifierResolverClass>
					<activationType>P</activationType>
				</role>
		        <split name="HasNewUnorderedItems">
		          <type>org.kuali.kfs.sys.document.workflow.SimpleBooleanSplitNode</type>
		        </split>				
				<split name="RequiresContractManagementReview">
					<type> org.kuali.kfs.sys.document.workflow.SimpleBooleanSplitNode
					</type>
				</split>
				<split name="RequiresContractManagementNonItem">
					<type> org.kuali.kfs.sys.document.workflow.SimpleBooleanSplitNode
					</type>
				</split>
				<split name="RequiresItemsApproval">
					<type> org.kuali.kfs.sys.document.workflow.SimpleBooleanSplitNode
					</type>
				</split>
				<split name="RequiresItemsNonItem">
					<type> org.kuali.kfs.sys.document.workflow.SimpleBooleanSplitNode
					</type>
				</split>
				<split name="RequiresAwardReview">
					<type>org.kuali.kfs.sys.document.workflow.SimpleBooleanSplitNode
					</type>
				</split>
				<split name="RequiresAwardNonItem">
					<type>org.kuali.kfs.sys.document.workflow.SimpleBooleanSplitNode
					</type>
				</split>
				<split name="RequiresBudgetReview">
					<type> org.kuali.kfs.sys.document.workflow.SimpleBooleanSplitNode
					</type>
				</split>
				<split name="RequiresBudgetNonItem">
					<type> org.kuali.kfs.sys.document.workflow.SimpleBooleanSplitNode
					</type>
				</split>
				<split name="VendorIsEmployeeOrNonResidentAlien">
					<type> org.kuali.kfs.sys.document.workflow.SimpleBooleanSplitNode
					</type>
				</split>
				<split name="VendorIsEmployeeOrNonResidentAlienNonItem">
					<type> org.kuali.kfs.sys.document.workflow.SimpleBooleanSplitNode
					</type>
				</split>
				<simple name="NoOpRequiresAwardReview">
					<type>org.kuali.rice.kew.engine.node.NoOpNode
					</type>
				</simple>
				<simple name="NoOpRequiresAwardNonItem">
					<type>org.kuali.rice.kew.engine.node.NoOpNode
					</type>
				</simple>				
				<simple name="NoOpRequiresContractManagementReview">
					<type>org.kuali.rice.kew.engine.node.NoOpNode
					</type>
				</simple>
				<simple name="NoOpRequiresContractManagementNonItem">
					<type>org.kuali.rice.kew.engine.node.NoOpNode
					</type>
				</simple>
				<simple name="NoOpVendorIsEmployeeOrNonResidentAlien">
					<type>org.kuali.rice.kew.engine.node.NoOpNode
					</type>
				</simple>
				<simple name="NoOpVendorIsEmployeeOrNonResidentAlienNonItem">
					<type>org.kuali.rice.kew.engine.node.NoOpNode
					</type>
				</simple>
		        <simple name="NoOpNewUnorderedItems">
		          <type>org.kuali.rice.kew.engine.node.NoOpNode</type>
		        </simple>
				<simple name="NoOpBudget">
					<type>org.kuali.rice.kew.engine.node.NoOpNode
					</type>
				</simple>
				<simple name="NoOpBudgetNonItem">
					<type>org.kuali.rice.kew.engine.node.NoOpNode
					</type>
				</simple>
				<simple name="NoOpItemsNonItem">
					<type>org.kuali.rice.kew.engine.node.NoOpNode
					</type>
				</simple>
				<join name="JoinRequiresAwardReview" />
				<join name="JoinRequiresAwardNonItem" />
				<join name="JoinOrderedItems" />
				<join name="JoinRequiresBudgetReview" />
				<join name="JoinRequiresBudgetNonItem" />       
				<join name="JoinRequiresContractManagementReview" />
				<join name="JoinRequiresContractManagementNonItem" />
			</routeNodes>
		</documentType>
		<documentType>
			<name>CATR</name>
			<parent>KFST</parent>
			<label>COA Transactional Document</label>
		</documentType>
		<documentType>
			<name>SAIP</name>
			<parent>CATR</parent>
			<label>Sub-Account Import</label>
			<docHandler>${kfs.url}/coaSubAccountImport.do?methodToCall=docHandler</docHandler>
			<active>true</active>
		</documentType>
		<documentType>
			<name>SOIP</name>
			<parent>CATR</parent>
			<label>Sub-Object Code Import</label>
			<docHandler>${kfs.url}/coaSubObjectCodeImport.do?methodToCall=docHandler</docHandler>
			<active>true</active>
		</documentType>
		<documentType>
			<name>PCIP</name>
			<parent>CATR</parent>
			<label>Project Code Import</label>
			<docHandler>${kfs.url}/coaProjectCodeImport.do?methodToCall=docHandler</docHandler>
			<active>true</active>
		</documentType>
	</documentTypes>
</data>