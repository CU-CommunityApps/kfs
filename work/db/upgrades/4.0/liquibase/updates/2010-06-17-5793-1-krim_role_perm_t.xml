<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<databaseChangeLog xmlns='http://www.liquibase.org/xml/ns/dbchangelog/1.9' xmlns:xsi='http://www.w3.org/2001/XMLSchema-instance' xsi:schemaLocation='http://www.liquibase.org/xml/ns/dbchangelog/1.9 http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-1.9.xsd'>
  <changeSet author='David' id='5793-1-1' failOnError='true' dbms=''>
    <comment>Add permission 306 to role 49 (Tax Identification Number User)  </comment>
    <insert tableName='KRIM_ROLE_PERM_T'>
      <column name='role_perm_id' value='1004' />
      <column name='obj_id' type='UUID' valueNumeric='SYS_GUID' />
      <column name='ver_nbr' valueNumeric='1' />
      <column name='role_id' value='49' />
      <column name='perm_id' value='306' />
      <column name='actv_ind' value='Y' />
    </insert>
    <modifySql dbms='mysql'>
      <replace replace='SYS_GUID' with='uuid()' />
    </modifySql>
    <modifySql dbms='mysql'>
      <replace replace='SYSDATE' with='now()' />
    </modifySql>
    <modifySql dbms='oracle'>
      <replace replace='SYS_GUID' with='sys_guid()' />
    </modifySql>
  </changeSet>
</databaseChangeLog>