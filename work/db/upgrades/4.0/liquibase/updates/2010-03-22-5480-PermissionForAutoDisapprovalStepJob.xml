<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<databaseChangeLog xmlns='http://www.liquibase.org/xml/ns/dbchangelog/1.9' xmlns:xsi='http://www.w3.org/2001/XMLSchema-instance' xsi:schemaLocation='http://www.liquibase.org/xml/ns/dbchangelog/1.9 http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-1.9.xsd'>
  <changeSet author='Muddu' id='5480-1-1' failOnError='true'>
    <comment>Setup a new permission for document type KFST</comment>
    <insert tableName='KRIM_PERM_T'>
      <column name='PERM_ID' value='1100' />
      <column name='OBJ_ID' value='SYS_GUID' />
      <column name='VER_NBR' valueNumeric='1' />
      <column name='PERM_TMPL_ID' value='3' />
      <column name='NMSPC_CD' value='KFS-SYS' />
      <column name='NM' value='Administer Routing for Document' />
      <column name='DESC_TXT' value='Allows users to open Financial System Documents via the Super search option in Document Search and take Administrative workflow actions on them such as approving the document, approving individual requests, or sending the document to a specified route node' />
      <column name='ACTV_IND' value='Y' />
    </insert>
    <modifySql dbms='mysql'>
      <replace replace='SYS_GUID' with='uuid()' />
    </modifySql>
    <modifySql dbms='mysql'>
      <replace replace='SYSDATE' with='now()' />
    </modifySql>
    <modifySql dbms='oracle'>
      <replace replace='SYS_GUID' with='sys_guid()' />
    </modifySql>
  </changeSet>
