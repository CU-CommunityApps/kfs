<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<databaseChangeLog xmlns='http://www.liquibase.org/xml/ns/dbchangelog/1.9' xmlns:xsi='http://www.w3.org/2001/XMLSchema-instance' xsi:schemaLocation='http://www.liquibase.org/xml/ns/dbchangelog/1.9 http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-1.9.xsd'>
  <changeSet author='Travis' id='5675-2-1' failOnError='true'>
    <comment> Add New LD Parameter Type for Demerger</comment>
    <insert tableName='KRNS_PARM_T'>
      <column name='NMSPC_CD' value='KFS-LD' />
      <column name='PARM_DTL_TYP_CD' value='LaborScrubberStep' />
      <column name='PARM_NM' value='DEMERGE_DOCUMENT_TYPES' />
      <column name='OBJ_ID' type='UUID' valueNumeric='SYS_GUID' />
      <column name='VER_NBR' valueNumeric='1' />
      <column name='PARM_TYP_CD' value='CONFG' />
      <column name='TXT' value='BT;ST' />
      <column name='PARM_DESC_TXT' value='Document types that will demerge remove entire document from processing all entries with the same document number when an error is detected on one entry with that document number Document types not listed here will only have error entries removed from processing; no other records will be demerged' />
      <column name='CONS_CD' value='A' />
      <column name='APPL_NMSPC_CD' value='KFS' />
    </insert>
    <modifySql dbms='mysql'>
      <replace replace='SYS_GUID' with='uuid()' />
    </modifySql>
    <modifySql dbms='mysql'>
      <replace replace='SYSDATE' with='now()' />
    </modifySql>
    <modifySql dbms='oracle'>
      <replace replace='SYS_GUID' with='sys_guid()' />
    </modifySql>
  </changeSet>
</databaseChangeLog>