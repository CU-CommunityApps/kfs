			<where>NMSPC_CD = 'KFS-FP' AND PARM_DTL_TYP_CD = 'DisbursementVoucher' AND NM = 'TAX_CONTROL_CODES_REQUIRING_TAX_REVIEW'</where>
		</update>
    </changeSet>
    
</databaseChangeLog>   
      