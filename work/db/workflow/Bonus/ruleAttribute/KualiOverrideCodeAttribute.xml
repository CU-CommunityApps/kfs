<?xml version="1.0" encoding="UTF-8"?>
<data xmlns="ns:workflow" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="ns:workflow resource:WorkflowData">
  <ruleAttributes xmlns="ns:workflow/RuleAttribute" xsi:schemaLocation="ns:workflow/RuleAttribute resource:RuleAttribute">
    <ruleAttribute>
      <name>KualiOverrideCodeAttribute</name>
      <className>org.kuali.workflow.attribute.KualiXmlRuleAttributeImpl</className>
      <label>Override Code</label>
      <description>Override Code Both Accounting Lines</description>
      <type>RuleXmlAttribute</type>
      <routingConfig>
        <fieldDef name="overrideCode" title="kuali_dd_label(SourceAccountingLine)" workflowType="ALL">
          <display>
            <type>text</type>
          </display>
          <validation required="true" />
          <fieldEvaluation>
            <xpathexpression>wf:xstreamsafe('//org.kuali.core.bo.SourceAccountingLine/overrideCode') = wf:ruledata('overrideCode') or
                                wf:xstreamsafe('//org.kuali.core.bo.TargetAccountingLine/overrideCode') = wf:ruledata('overrideCode')</xpathexpression>
          </fieldEvaluation>
        </fieldDef>
      </routingConfig>
    </ruleAttribute>
  </ruleAttributes>
</data>
