<?xml version="1.0" encoding="UTF-8"?>
<data xmlns="ns:workflow" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="ns:workflow resource:WorkflowData">
  <ruleAttributes xmlns="ns:workflow/RuleAttribute" xsi:schemaLocation="ns:workflow/RuleAttribute resource:RuleAttribute">
    <ruleAttribute>
      <name>KualiObjectCodeSubTypeAttribute</name>
      <className>org.kuali.workflow.attribute.KualiXmlRuleAttributeImpl</className>
      <label>Object Code SubType Routing</label>
      <description>Object Code SubType Both Accounting Lines Routing</description>
      <type>RuleXmlAttribute</type>
      <routingConfig>
        <fieldDef name="objectCodeSubType" title="kuali_dd_label(ObjectCode)" workflowType="ALL">
          <display>
            <type>text</type>
          </display>
          <validation required="true" />
          <quickfinder appliesTo="objectCodeSubType" draw="true" service="workflow-org.kuali.module.chart.bo.ObjSubTyp-Lookupable(workflow-code:objectCodeSubType)" />
          <fieldEvaluation>
            <xpathexpression>wf:xstreamsafe('//org.kuali.core.bo.SourceAccountingLine/objectCode/financialObjectSubTypeCode') = wf:ruledata('objectCodeSubType') or
                                wf:xstreamsafe('//org.kuali.core.bo.TargetAccountingLine/objectCode/financialObjectSubTypeCode') = wf:ruledata('objectCodeSubType')</xpathexpression>
          </fieldEvaluation>
        </fieldDef>
      </routingConfig>
    </ruleAttribute>
  </ruleAttributes>
</data>
