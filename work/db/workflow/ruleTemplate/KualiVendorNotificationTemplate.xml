<?xml version="1.0" encoding="UTF-8"?>
<data xmlns="ns:workflow" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="ns:workflow resource:WorkflowData">
  <ruleTemplates xmlns="ns:workflow/RuleTemplate" xsi:schemaLocation="ns:workflow/RuleTemplate resource:RuleTemplate">
    <ruleTemplate>
      <name>KualiVendorNotificationTemplate</name>
      <description>Kuali Vendor Notification</description>
      <attributes>
        <attribute>
          <name>KualiWorkflowInitiatorRoleAttribute</name>
          <required>true</required>
        </attribute>
      </attributes>
      <ruleDefaults>
        <description>Rule for notifying document initiators about their vendor maintenance documents.</description>
