<?xml version="1.0" encoding="UTF-8"?>
<data xmlns="ns:workflow" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="ns:workflow resource:WorkflowData">
  <ruleAttributes xmlns="ns:workflow/RuleAttribute" xsi:schemaLocation="ns:workflow/RuleAttribute resource:RuleAttribute">
    <ruleAttribute>
      <name>KualiPaymentMethodAttribute</name>
      <className>org.kuali.workflow.attribute.KualiXmlRuleAttributeImpl</className>
      <label>Payment Method Routing</label>
      <description>Payment Method Routing</description>
      <type>RuleXmlAttribute</type>
      <routingConfig>
        <fieldDef name="disbVchrPaymentMethodCode" title="kuali_dd_label(DisbursementVoucherDocument)" workflowType="ALL">
          <display>
            <type>radio</type>
            <values>kuali_values_finder_class(org.kuali.module.financial.lookup.keyvalues.PaymentMethodValuesFinder)</values>
          </display>
          <validation required="true" />
          <fieldEvaluation>
            <xpathexpression>wf:xstreamsafe('//document/disbVchrPaymentMethodCode') = wf:ruledata('disbVchrPaymentMethodCode')</xpathexpression>
          </fieldEvaluation>
        </fieldDef>
      </routingConfig>
    </ruleAttribute>
  </ruleAttributes>
</data>
