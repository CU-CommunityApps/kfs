<?xml version="1.0" encoding="UTF-8"?>
<data xmlns="ns:workflow" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="ns:workflow resource:WorkflowData">
<ruleAttributes xmlns="ns:workflow/RuleAttribute" xsi:schemaLocation="ns:workflow/RuleAttribute resource:RuleAttribute">
<ruleAttribute>
<name>KualiNewUnorderedItemsAttribute</name>
      <className>org.kuali.rice.kns.workflow.attribute.KualiXmlRuleAttributeImpl</className>
      <label>New Unordered Items Routing</label>
      <description>New Unordered Items Routing</description>
      <type>RuleXmlAttribute</type>
      <routingConfig>
        <fieldDef name="newUnorderedItem" title="kuali_dd_label(PurchaseOrderAmendmentDocument)" workflowType="ALL">
          <display>
            <type>radio</type>
            <values title="true">true</values>
            <values title="false">false</values>
          </display>
          <validation required="true" />
          <fieldEvaluation>
            <xpathexpression>wf:xstreamsafe('//document/newUnorderedItem') = wf:ruledata('newUnorderedItem')</xpathexpression>
          </fieldEvaluation>
        </fieldDef>
        <fieldDef name="receivingDeliveryCampusCode" title="kuali_dd_label(PurchaseOrderAmendmentDocument)" workflowType="ALL">
          <display>
            <type>text</type>
          </display>
          <validation required="true" />
          <quickfinder appliesTo="receivingDeliveryCampusCode" draw="true" service="workflow-org.kuali.rice.kns.bo.Campus-Lookupable(workflow-campusCode:receivingDeliveryCampusCode)" />
          <fieldEvaluation>
            <xpathexpression>wf:xstreamsafe('//document/receivingDeliveryCampusCode') = wf:ruledata('receivingDeliveryCampusCode')</xpathexpression>
          </fieldEvaluation>
        </fieldDef>
      </routingConfig>
</ruleAttribute>
</ruleAttributes>
</data>
