<?xml version="1.0" encoding="UTF-8"?>
<data xmlns="ns:workflow" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="ns:workflow resource:WorkflowData">
<ruleAttributes xmlns="ns:workflow/RuleAttribute" xsi:schemaLocation="ns:workflow/RuleAttribute resource:RuleAttribute">
<ruleAttribute>
<name>KualiSubFundGroupAttribute</name>
      <className>org.kuali.kfs.sys.document.routing.attribute.AccountLineReplacingXmlRuleAttribute</className>
      <label>SubFund Group Routing</label>
      <description>SubFund Group Both Accounting Lines Routing</description>
      <type>RuleXmlAttribute</type>
      <routingConfig>
        <fieldDef name="subFundGroupCode" title="kuali_dd_label(SubFundGroup)" workflowType="ALL">
          <display>
            <type>text</type>
          </display>
          <validation required="true" />
          <quickfinder appliesTo="subFundGroupCode" draw="true" service="workflow-org.kuali.kfs.coa.businessobject.SubFundGroup-Lookupable(null)" />
          <fieldEvaluation>
            <xpathexpression>wf:xstreamsafe('//kfs_sourceAccountingLineClass/account/subFundGroupCode') = wf:ruledata('subFundGroupCode') or wf:xstreamsafe('//kfs_targetAccountingLineClass/account/subFundGroupCode') = wf:ruledata('subFundGroupCode')</xpathexpression>
          </fieldEvaluation>
        </fieldDef>
      </routingConfig>
</ruleAttribute>
</ruleAttributes>
</data>
