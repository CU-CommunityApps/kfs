<?xml version="1.0" encoding="UTF-8"?>
<data xmlns="ns:workflow" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="ns:workflow resource:WorkflowData">
<ruleAttributes xmlns="ns:workflow/RuleAttribute" xsi:schemaLocation="ns:workflow/RuleAttribute resource:RuleAttribute">
<ruleAttribute>
<name>AwaitingReceivingStatus</name>
      <className>org.kuali.rice.kns.workflow.attribute.KualiXmlRuleAttributeImpl</className>
      <label>Awaiting Receiving Status</label>
      <description>Awaiting Receiving Status</description>
      <type>RuleXmlAttribute</type>
      <routingConfig>
        <fieldDef name="awaitingReceiving" title="kuali_dd_label(PaymentRequestDocument)" workflowType="ALL">
          <display>
            <type>radio</type>
            <values title="true">true</values>
            <values title="false">false</values>
          </display>
          <validation required="true" />
          <fieldEvaluation>
            <xpathexpression>wf:xstreamsafe('//document/awaitingReceiving') = wf:ruledata('awaitingReceiving')</xpathexpression>
          </fieldEvaluation>
        </fieldDef>
      </routingConfig>
</ruleAttribute>
</ruleAttributes>
</data>
