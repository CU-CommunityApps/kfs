<?xml version="1.0" encoding="UTF-8"?>
<data xmlns="ns:workflow" xmlns:fo="http://www.w3.org/1999/XSL/Format" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="ns:workflow resource:WorkflowData">
<documentTypes xmlns="ns:workflow/DocumentType" xsi:schemaLocation="ns:workflow/DocumentType resource:DocumentType">
<documentType>
<name>KualiRoutingFormDocument</name>
<parent>ContractsAndGrantsTransactionalDocument</parent>
<docHandler>${application.url}/researchRoutingFormMainPage.do?methodToCall=docHandler</docHandler>
<routePaths>
        <routePath>
          <start name="Adhoc Routing" nextNode="Project Director"/>
          <requests name="Project Director" nextNode="Split"/>
          <split name="Split" nextNode="ResearchRisk">
            <branch name="Adhoc Approvers Branch">
              <requests name="Adhoc Approvers" nextNode="Final Notification"/>
            </branch>
            <branch name="Org Review Branch">
              <requests name="OrganizationHierarchy" nextNode="Final Notification"/>
            </branch>
            <join name="Final Notification"/>
          </split>
          <requests name="ResearchRisk"/>
        </routePath>
      </routePaths>
<routeNodes>
<start name="Adhoc Routing"/>
<role name="Project Director">
<ruleTemplate>KualiRoutingFormProjectDirectorTemplate</ruleTemplate>
<qualifierResolverClass>org.kuali.rice.kns.workflow.attribute.DataDictionaryQualifierResolver</qualifierResolverClass>
</role>
<role name="Adhoc Approvers">
<ruleTemplate>KualiResearchAdhocApproverTemplate</ruleTemplate>
<qualifierResolverClass>org.kuali.rice.kns.workflow.attribute.DataDictionaryQualifierResolver</qualifierResolverClass>
</role>
<role name="OrganizationHierarchy">
<ruleTemplate>KualiResearchOrgReviewTemplate</ruleTemplate>
<qualifierResolverClass>org.kuali.rice.kns.workflow.attribute.DataDictionaryQualifierResolver</qualifierResolverClass>
</role>
<role name="ResearchRisk">
<ruleTemplate>KualiResearchRiskTemplate</ruleTemplate>
<qualifierResolverClass>org.kuali.rice.kns.workflow.attribute.DataDictionaryQualifierResolver</qualifierResolverClass>
</role>
<split name="Split"/>
<join name="Final Notification"/>
</routeNodes>
</documentType>
</documentTypes>
</data>
