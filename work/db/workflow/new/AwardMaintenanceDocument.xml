<?xml version="1.0" encoding="UTF-8"?>
<data xmlns="ns:workflow" xmlns:fo="http://www.w3.org/1999/XSL/Format" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="ns:workflow resource:WorkflowData">
<documentTypes xmlns="ns:workflow/DocumentType" xsi:schemaLocation="ns:workflow/DocumentType resource:DocumentType">
<documentType>
<name>AwardMaintenanceDocument</name>
<parent>ContractsAndGrantsComplexMaintenanceDocument</parent>
<docHandler>${application.url}/kr/maintenance.do?methodToCall=docHandler</docHandler>
<routePaths>
        <routePath>
          <start name="Adhoc Routing" nextNode="Org Review"/>
          <requests name="Org Review" nextNode="Research Risk"/>
          <requests name="Research Risk" nextNode="Central Review"/>
          <requests name="Central Review"/>
        </routePath>
      </routePaths>
<routeNodes>
<start name="Adhoc Routing"/>
<role name="Org Review">
<ruleTemplate>KualiOrgReviewTemplate</ruleTemplate>
<qualifierResolverClass>org.kuali.rice.kns.workflow.attribute.DataDictionaryQualifierResolver</qualifierResolverClass>
</role>
<role name="Research Risk">
<ruleTemplate>KualiResearchRiskTemplate</ruleTemplate>
<qualifierResolverClass>org.kuali.rice.kns.workflow.attribute.DataDictionaryQualifierResolver</qualifierResolverClass>
</role>
<role name="Central Review">
<ruleTemplate>KualiCentralRoutingTemplate</ruleTemplate>
<qualifierResolverClass>org.kuali.rice.kns.workflow.attribute.DataDictionaryQualifierResolver</qualifierResolverClass>
</role>
</routeNodes>
</documentType>
</documentTypes>
</data>
