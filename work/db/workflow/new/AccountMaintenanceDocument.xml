<?xml version="1.0" encoding="UTF-8"?>
<data xmlns="ns:workflow" xmlns:fo="http://www.w3.org/1999/XSL/Format" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="ns:workflow resource:WorkflowData">
<documentTypes xmlns="ns:workflow/DocumentType" xsi:schemaLocation="ns:workflow/DocumentType resource:DocumentType">
<documentType>
<name>AccountMaintenanceDocument</name>
<parent>ChartComplexMaintenanceDocument</parent>
<docHandler>${application.url}/kr/maintenance.do?methodToCall=docHandler</docHandler>
<routePaths>
        <routePath>
          <start name="Adhoc Routing" nextNode="Account Supervisor"/>
          <requests name="Account Supervisor" nextNode="Account Review"/>
          <requests name="Account Review" nextNode="Org Review"/>
          <requests name="Org Review" nextNode="Chart"/>
          <requests name="Chart" nextNode="Subfund"/>
          <requests name="Subfund" nextNode="Award"/>
          <requests name="Award"/>
        </routePath>
      </routePaths>
<routeNodes>
<start name="Adhoc Routing"/>
<role name="Account Supervisor">
<ruleTemplate>KualiAccountSupervisorTemplate</ruleTemplate>
<qualifierResolverClass>org.kuali.rice.kns.workflow.attribute.DataDictionaryQualifierResolver</qualifierResolverClass>
</role>
<role name="Account Review">
<ruleTemplate>KualiAccountTemplate</ruleTemplate>
<qualifierResolverClass>org.kuali.rice.kns.workflow.attribute.DataDictionaryQualifierResolver</qualifierResolverClass>
</role>
<role name="Org Review">
<ruleTemplate>KualiOrgReviewTemplate</ruleTemplate>
<qualifierResolverClass>org.kuali.rice.kns.workflow.attribute.DataDictionaryQualifierResolver</qualifierResolverClass>
</role>
<role name="Chart">
<ruleTemplate>KualiChartTemplate</ruleTemplate>
<qualifierResolverClass>org.kuali.rice.kns.workflow.attribute.DataDictionaryQualifierResolver</qualifierResolverClass>
</role>
<role name="Subfund">
<ruleTemplate>KualiAccountSubFundTemplate</ruleTemplate>
<qualifierResolverClass>org.kuali.rice.kns.workflow.attribute.DataDictionaryQualifierResolver</qualifierResolverClass>
</role>
<role name="Award">
<ruleTemplate>KualiAwardWorkgroupTemplate</ruleTemplate>
<qualifierResolverClass>org.kuali.rice.kns.workflow.attribute.DataDictionaryQualifierResolver</qualifierResolverClass>
</role>
</routeNodes>
</documentType>
</documentTypes>
</data>
