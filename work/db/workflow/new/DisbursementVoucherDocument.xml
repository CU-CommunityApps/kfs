<?xml version="1.0" encoding="UTF-8"?>
<data xmlns="ns:workflow" xmlns:fo="http://www.w3.org/1999/XSL/Format" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="ns:workflow resource:WorkflowData">
<documentTypes xmlns="ns:workflow/DocumentType" xsi:schemaLocation="ns:workflow/DocumentType resource:DocumentType">
<documentType>
<name>DisbursementVoucherDocument</name>
<parent>FinancialProcessingTransactionalDocument</parent>
<docHandler>${application.url}/financialDisbursementVoucher.do?methodToCall=docHandler</docHandler>
<routePaths>
				<routePath>
					<start name="AdHoc" nextNode="PayeeIsPurchaseOrderVendor"/>
					<split name="PayeeIsPurchaseOrderVendor" nextNode="Account">
						<branch name="True">
							<requests name="Purchasing" nextNode="Join"/>
						</branch>
						<branch name="False">
							<simple name="NoOp" nextNode="Join"/>
						</branch>
						<join name="Join"/>
					</split>
					<requests name="Account" nextNode="OrganizationHierarchy"/>
					<requests name="OrganizationHierarchy" nextNode="RequiresTaxReview"/>
					<split name="RequiresTaxReview" nextNode="Campus">
						<branch name="True">
							<requests name="Tax" nextNode="Join"/>
						</branch>
						<branch name="False">
							<simple name="NoOp" nextNode="Join"/>
						</branch>
						<join name="Join"/>
					</split>
					<requests name="Campus" nextNode="RequiresTravelReview"/>					
					<split name="RequiresTravelReview" nextNode="PaymentMethod">
						<branch name="True">
							<requests name="Travel" nextNode="Join"/>
						</branch>
						<branch name="False">
							<simple name="NoOp" nextNode="Join"/>
						</branch>
						<join name="Join"/>
					</split>
					<requests name="PaymentMethod" nextNode="Award"/>
					<requests name="Award"/>
				</routePath>
			</routePaths>
<routeNodes>
<start name="AdHoc"/>
<split name="PayeeIsPurchaseOrderVendor">
<type>org.kuali.kfs.sys.document.routing.SimpleBooleanSplitNode</type>
</split>
<split name="RequiresTaxReview">
<type>org.kuali.kfs.sys.document.routing.SimpleBooleanSplitNode</type>
</split>
<split name="RequiresTravelReview">
<type>org.kuali.kfs.sys.document.routing.SimpleBooleanSplitNode</type>
</split>
<simple name="NoOp">
<type>edu.iu.uis.eden.engine.node.NoOpNode</type>
</simple>
<join name="Join"/>
<role name="Purchasing">
					<qualifierResolverClass>org.kuali.rice.kns.workflow.attribute.DataDictionaryQualifierResolver</qualifierResolverClass>
				</role>
<role name="Account">
					<qualifierResolverClass>org.kuali.rice.kns.workflow.attribute.DataDictionaryQualifierResolver</qualifierResolverClass>
					<mandatoryRoute>true</mandatoryRoute>
				</role>
<role name="OrganizationHierarchy">
					<qualifierResolverClass>org.kuali.rice.kns.workflow.attribute.DataDictionaryQualifierResolver</qualifierResolverClass>
				</role>
<role name="Tax">
					<qualifierResolverClass>org.kuali.rice.kns.workflow.attribute.DataDictionaryQualifierResolver</qualifierResolverClass>
				</role>
<role name="Campus">
					<qualifierResolverClass>org.kuali.rice.kns.workflow.attribute.DataDictionaryQualifierResolver</qualifierResolverClass>
				</role>
<role name="Travel">
					<qualifierResolverClass>org.kuali.rice.kns.workflow.attribute.DataDictionaryQualifierResolver</qualifierResolverClass>
				</role>
<role name="PaymentMethod">
					<qualifierResolverClass>org.kuali.rice.kns.workflow.attribute.DataDictionaryQualifierResolver</qualifierResolverClass>
				</role>
<role name="Award">
					<qualifierResolverClass>org.kuali.rice.kns.workflow.attribute.DataDictionaryQualifierResolver</qualifierResolverClass>
				</role>
</routeNodes>
</documentType>
</documentTypes>
</data>
