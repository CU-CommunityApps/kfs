<?xml version="1.0" encoding="UTF-8"?>
<data xmlns="ns:workflow" xmlns:fo="http://www.w3.org/1999/XSL/Format" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="ns:workflow resource:WorkflowData">
<documentTypes xmlns="ns:workflow/DocumentType" xsi:schemaLocation="ns:workflow/DocumentType resource:DocumentType">
<documentType>
<name>AccountGlobalMaintenanceDocument</name>
<parent>ChartComplexMaintenanceDocument</parent>
<docHandler>${application.url}/kr/maintenance.do?methodToCall=docHandler</docHandler>
<routePaths>
        <routePath>
          <start name="Adhoc Routing" nextNode="OrganizationHierarchy"/>
          <requests name="OrganizationHierarchy" nextNode="Chart"/>
          <requests name="Chart" nextNode="RetiredAssetSoldOrGifted"/>
          <split name="RetiredAssetSoldOrGifted" nextNode="NoOp">
						<branch name="True">
							<requests name="Purchasing" nextNode="Join"/>
						</branch>
						<branch name="False">
							<simple name="NoOp" nextNode="Join"/>
						</branch>
						<join name="Join"/>
					</split>
					<simple name="NoOp"/>
        </routePath>
      </routePaths>
<routeNodes>
<start name="Adhoc Routing"/>
<role name="OrganizationHierarchy">
<!-- <ruleTemplate>KualiOrgReviewTemplate</ruleTemplate>-->
<qualifierResolverClass>org.kuali.rice.kns.workflow.attribute.DataDictionaryQualifierResolver</qualifierResolverClass>
</role>
<role name="Chart">
<!--<ruleTemplate>KualiChartTemplate</ruleTemplate>-->
<qualifierResolverClass>org.kuali.rice.kns.workflow.attribute.DataDictionaryQualifierResolver</qualifierResolverClass>
</role>
<simple name="NoOp">
<type>edu.iu.uis.eden.engine.node.NoOpNode</type>
</simple>
<join name="Join"/>
<role name="Purchasing">
					<qualifierResolverClass>org.kuali.rice.kns.workflow.attribute.DataDictionaryQualifierResolver</qualifierResolverClass>
				</role>
				<split name="RetiredAssetSoldOrGifted">
<type>org.kuali.kfs.sys.document.routing.SimpleBooleanSplitNode</type>
</split>

</routeNodes>
</documentType>
</documentTypes>
</data>
