<?xml version="1.0" encoding="UTF-8"?>
<data xmlns="ns:workflow" xmlns:fo="http://www.w3.org/1999/XSL/Format" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="ns:workflow resource:WorkflowData">
<documentTypes xmlns="ns:workflow/DocumentType" xsi:schemaLocation="ns:workflow/DocumentType resource:DocumentType">
<documentType>
<name>PaymentRequestDocument</name>
<parent>AccountsPayableTransactionalDocument</parent>
<docHandler>${application.url}/purapPaymentRequest.do?methodToCall=docHandler</docHandler>
<routePaths>
        <routePath>
          <start name="Adhoc Routing" nextNode="InvoiceAttachment"/>
          <requests name="InvoiceAttachment" nextNode="Receiving"/>
          <requests name="Receiving" nextNode="SubAccount"/>
          <requests name="SubAccount" nextNode="Account Review"/>
          <requests name="Account Review" nextNode="Org Review"/>
          <requests name="Org Review" nextNode="Tax"/>
          <requests name="Tax"/>
        </routePath>
      </routePaths>
<routeNodes>
<start name="Adhoc Routing"/>
<role name="InvoiceAttachment">
<ruleTemplate>KualiAccountsPayableTemplate</ruleTemplate>
<qualifierResolverClass>org.kuali.rice.kns.workflow.attribute.DataDictionaryQualifierResolver</qualifierResolverClass>
</role>
<role name="Receiving">
<ruleTemplate>AwaitingReceivingStatusTemplate</ruleTemplate>
<qualifierResolverClass>org.kuali.rice.kns.workflow.attribute.DataDictionaryQualifierResolver</qualifierResolverClass>
</role>
<role name="SubAccount">
<ruleTemplate>KualiSubAccountTemplate</ruleTemplate>
<qualifierResolverClass>org.kuali.rice.kns.workflow.attribute.DataDictionaryQualifierResolver</qualifierResolverClass>
</role>
<role name="Account Review">
<ruleTemplate>KualiAccountTemplate</ruleTemplate>
<qualifierResolverClass>org.kuali.rice.kns.workflow.attribute.DataDictionaryQualifierResolver</qualifierResolverClass>
<mandatoryRoute>true</mandatoryRoute>
</role>
<role name="Org Review">
<ruleTemplate>KualiOrgReviewTemplate</ruleTemplate>
<qualifierResolverClass>org.kuali.rice.kns.workflow.attribute.DataDictionaryQualifierResolver</qualifierResolverClass>
</role>
<role name="Tax">
<ruleTemplate>KualiVendorTaxReviewTemplate</ruleTemplate>
<qualifierResolverClass>org.kuali.rice.kns.workflow.attribute.DataDictionaryQualifierResolver</qualifierResolverClass>
</role>
</routeNodes>
</documentType>
</documentTypes>
</data>
