<?xml version="1.0" encoding="UTF-8"?>
<data xmlns="ns:workflow" xmlns:fo="http://www.w3.org/1999/XSL/Format" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="ns:workflow resource:WorkflowData">
<documentTypes xmlns="ns:workflow/DocumentType" xsi:schemaLocation="ns:workflow/DocumentType resource:DocumentType">
<documentType>
<name>AssetRetirementGlobalMaintenanceDocument</name>
<parent>CapitalAssetManagementComplexMaintenanceDocument</parent>
<docHandler>${application.url}/kr/maintenance.do?methodToCall=docHandler</docHandler>
<routePaths>
        
        <routePath>
          <start name="AdHoc" nextNode="Account"/>
          <requests name="Account" nextNode="OrganizationHierarchy"/>
          <requests name="OrganizationHierarchy"  nextNode="RetiredAssetTransferredExternally"/>
		  <split name="RetiredAssetTransferredExternally" nextNode="RetiredAssetSoldOrGifted">
						<branch name="True">
							<requests name="Manager"  nextNode="Join"/>
						</branch>
						<branch name="False">
							<simple name="NoOpExternal" nextNode="Join"/>
						</branch>
						<join name="Join"/>
		  </split>
		  <split name="RetiredAssetSoldOrGifted">
						<branch name="True">
							<requests name="Purchasing" nextNode="Join"/>
						</branch>
						<branch name="False">
							<simple name="NoOp" nextNode="Join"/>
						</branch>
						<join name="Join"/>
					</split>
        </routePath>
        
        
      </routePaths>
<routeNodes>
<start name="AdHoc"/>
<role name="Account">
<qualifierResolverClass>org.kuali.rice.kns.workflow.attribute.DataDictionaryQualifierResolver</qualifierResolverClass>
</role>
<role name="OrganizationHierarchy">
<qualifierResolverClass>org.kuali.rice.kns.workflow.attribute.DataDictionaryQualifierResolver</qualifierResolverClass>
</role>
<role name="Manager">
<qualifierResolverClass>org.kuali.rice.kns.workflow.attribute.DataDictionaryQualifierResolver</qualifierResolverClass>
</role>
<role name="Purchasing">
<qualifierResolverClass>org.kuali.rice.kns.workflow.attribute.DataDictionaryQualifierResolver</qualifierResolverClass>
</role>

<split name="RetiredAssetTransferredExternally">
<type>org.kuali.kfs.sys.document.routing.SimpleBooleanSplitNode</type>
</split>
<split name="RetiredAssetSoldOrGifted">
<type>org.kuali.kfs.sys.document.routing.SimpleBooleanSplitNode</type>
</split>
<simple name="NoOp">
<type>org.kuali.rice.kew.engine.node.NoOpNode</type>
</simple>
<simple name="NoOpExternal">
<type>org.kuali.rice.kew.engine.node.NoOpNode</type>
</simple>
<join name="Join"/>
</routeNodes>
</documentType>
</documentTypes>
</data>

