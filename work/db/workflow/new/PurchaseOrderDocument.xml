<?xml version="1.0" encoding="UTF-8"?>
<data xmlns="ns:workflow" xmlns:fo="http://www.w3.org/1999/XSL/Format" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="ns:workflow resource:WorkflowData">
<documentTypes xmlns="ns:workflow/DocumentType" xsi:schemaLocation="ns:workflow/DocumentType resource:DocumentType">
<documentType>
<name>PurchaseOrderDocument</name>
<parent>PurchasingTransactionalDocument</parent>
<docHandler>${application.url}/purapPurchaseOrder.do?methodToCall=docHandler</docHandler>
<routePaths>
        <routePath>
          <start name="AdHoc" nextNode="RequiresContractManagementReview"/>
          <split name="RequiresContractManagementReview" nextNode="Commodity">
          	<branch name="True">
          		<requests name="ContractManagement" nextNode="JoinRequiresContractManagementReview" />
          	</branch>
          	<branch name="False">
          		<requests name="NoOpRequiresContractManagementReview" nextNode="JoinRequiresContractManagementReview" />
          	</branch>
          	<join name="JoinRequiresContractManagementReview" />
          </split>
          <requests name="Commodity" nextNode="Award"/>
          <requests name="Award" nextNode="RequiresBudgetReview"/>
          <split name="RequiresBudgetReview" nextNode="VendorIsEmployeeOrNonResidentAlien">
						<branch name="True">
							<requests name="Budget" nextNode="JoinRequiresBudgetReview"/>
						</branch>
						<branch name="False">
							<simple name="NoOpBudget" nextNode="JoinRequiresBudgetReview"/>
						</branch>
						<join name="JoinRequiresBudgetReview"/>
					</split>
					<split name="VendorIsEmployeeOrNonResidentAlien" nextNode="TransmissionMethodIsPrint">
						<branch name="True">
							<requests name="Tax" nextNode="JoinVendorIsEmployeeOrNonResidentAlien"/>
						</branch>
						<branch name="False">
							<simple name="NoOpVendorIsEmployeeOrNonResidentAlien" nextNode="JoinVendorIsEmployeeOrNonResidentAlien"/>
						</branch>
						<join name="JoinVendorIsEmployeeOrNonResidentAlien"/>
					</split>
					<split name="TransmissionMethodIsPrint">
						<branch name="True">
							<requests name="PrintTransmission" nextNode="JoinTransmissionMethodIsPrint"/>
						</branch>
						<branch name="False">
							<simple name="NoOpPrint" nextNode="JoinTransmissionMethodIsPrint"/>
						</branch>
						<join name="JoinTransmissionMethodIsPrint"/>
					</split>
        </routePath>
      </routePaths>
<routeNodes>
<start name="AdHoc"/>
<role name="ContractManagement">
<!--<ruleTemplate>KualiInternalPurchasingTemplate</ruleTemplate>-->
<qualifierResolverClass>org.kuali.rice.kns.workflow.attribute.DataDictionaryQualifierResolver</qualifierResolverClass>
</role>
<role name="Commodity">
<!--<ruleTemplate>KualiPurApCommodityCodeTemplate</ruleTemplate>-->
<qualifierResolverClass>org.kuali.rice.kns.workflow.attribute.DataDictionaryQualifierResolver</qualifierResolverClass>
</role>
<role name="Award">
<!--<ruleTemplate>KualiPurApContractsAndGrantsTemplate</ruleTemplate>-->
<qualifierResolverClass>org.kuali.rice.kns.workflow.attribute.DataDictionaryQualifierResolver</qualifierResolverClass>
</role>
<role name="Budget">
<!--<ruleTemplate>KualiPurApBudgetReviewTemplate</ruleTemplate>-->
<qualifierResolverClass>org.kuali.rice.kns.workflow.attribute.DataDictionaryQualifierResolver</qualifierResolverClass>
</role>
<role name="Tax">
<!--<ruleTemplate>KualiVendorTaxReviewTemplate</ruleTemplate>-->
<qualifierResolverClass>org.kuali.rice.kns.workflow.attribute.DataDictionaryQualifierResolver</qualifierResolverClass>
</role>
<role name="PrintTransmission">
<!--<ruleTemplate>KualiPurchaseOrderTransmissionTemplate</ruleTemplate>-->
<qualifierResolverClass>org.kuali.rice.kns.workflow.attribute.DataDictionaryQualifierResolver</qualifierResolverClass>
</role>
<split name="RequiresContractManagementReview">
<type>org.kuali.kfs.sys.document.routing.SimpleBooleanSplitNode</type>
</split>
<split name="RequiresBudgetReview">
<type>org.kuali.kfs.sys.document.routing.SimpleBooleanSplitNode</type>
</split>
<split name="TransmissionMethodIsPrint">
<type>org.kuali.kfs.sys.document.routing.SimpleBooleanSplitNode</type>
</split>
<split name="VendorIsEmployeeOrNonResidentAlien">
<type>org.kuali.kfs.sys.document.routing.SimpleBooleanSplitNode</type>
</split>
<simple name="NoOpRequiresContractManagementReview">
<type>org.kuali.rice.kew.engine.node.NoOpNode</type>
</simple>
<simple name="NoOpVendorIsEmployeeOrNonResidentAlien">
<type>org.kuali.rice.kew.engine.node.NoOpNode</type>
</simple>
<simple name="NoOpBudget">
<type>org.kuali.rice.kew.engine.node.NoOpNode</type>
</simple>
<simple name="NoOpPrint">
<type>org.kuali.rice.kew.engine.node.NoOpNode</type>
</simple>
<join name="JoinRequiresBudgetReview" />
<join name="JoinVendorIsEmployeeOrNonResidentAlien" />
<join name="JoinTransmissionMethodIsPrint"/>
<join name="JoinRequiresContractManagementReview"/>
</routeNodes>
</documentType>
</documentTypes>
</data>
