<?xml version="1.0" encoding="UTF-8"?>
<data xmlns="ns:workflow" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="ns:workflow resource:WorkflowData">
<documentTypes xmlns="ns:workflow/DocumentType" xsi:schemaLocation="ns:workflow/DocumentType resource:DocumentType">
<documentType>
<name>DisbursementVoucherDocument</name>
      <parent>FinancialProcessingTransactionalDocument</parent>
      <label>Disbursement Voucher</label>
      <postProcessorName>org.kuali.kfs.sys.document.workflow.KualiPostProcessor</postProcessorName>
      <superUserWorkgroupName>SY_FUNCTIONAL_SUPER_USERS</superUserWorkgroupName>
      <blanketApproveWorkgroupName>SY_BLANKET_APPROVERS</blanketApproveWorkgroupName>
      <defaultExceptionWorkgroupName>SY_FUNCTIONAL_SUPER_USERS</defaultExceptionWorkgroupName>
      <docHandler>${application.url}/financialDisbursementVoucher.do?methodToCall=docHandler</docHandler>
      <active>true</active>
      <attributes>
        <attribute>
<name>KualiAccountSearchAttribute</name>
        </attribute>
        <attribute>
<name>KualiDocumentHeaderTotalAmountRange</name>
        </attribute>
        <attribute>
<name>KualiPayeeIdSearchableAttribute</name>
        </attribute>
        <attribute>
<name>KualiPayeeNameSearchableAttribute</name>
        </attribute>
        <attribute>
<name>KualiPaymentReasonSearchableAttribute</name>
        </attribute>
        <attribute>
<name>KualiDisbursementVoucherPdpDates</name>
        </attribute>
      </attributes>
      <routingVersion>1</routingVersion>
      <routePaths>
        <routePath>
          <start name="Adhoc Routing" nextNode="Account Review" />
          <requests name="Account Review" nextNode="Org Review" />
          <requests name="Org Review" nextNode="Employee Indicator" />
          <requests name="Employee Indicator" nextNode="Tax Control Code" />
          <requests name="Tax Control Code" nextNode="Alien Indicator" />
          <requests name="Alien Indicator" nextNode="Payment Reason" />
          <requests name="Payment Reason" nextNode="Payment Reason+Campus Code" />
          <requests name="Payment Reason+Campus Code" nextNode="Campus Code" />
          <requests name="Campus Code" nextNode="Alien Indicator+Payment Reason" />
          <requests name="Alien Indicator+Payment Reason" nextNode="Payment Method" />
          <requests name="Payment Method" nextNode="Award Workgroup" />
          <requests name="Award Workgroup" />
        </routePath>
      </routePaths>
      <routeNodes>
        <start name="Adhoc Routing">
          <activationType>S</activationType>
          <mandatoryRoute>false</mandatoryRoute>
          <finalApproval>false</finalApproval>
        </start>
        <requests name="Account Review">
          <activationType>S</activationType>
          <ruleTemplate>KualiAccountTemplate</ruleTemplate>
          <mandatoryRoute>false</mandatoryRoute>
          <finalApproval>false</finalApproval>
        </requests>
        <requests name="Org Review">
          <activationType>S</activationType>
          <ruleTemplate>KualiOrgReviewTemplate</ruleTemplate>
          <mandatoryRoute>false</mandatoryRoute>
          <finalApproval>false</finalApproval>
        </requests>
        <requests name="Employee Indicator">
          <activationType>P</activationType>
          <ruleTemplate>KualiEmployeeIndicatorTemplate</ruleTemplate>
          <mandatoryRoute>false</mandatoryRoute>
          <finalApproval>false</finalApproval>
        </requests>
        <requests name="Tax Control Code">
          <activationType>P</activationType>
          <ruleTemplate>KualiTaxControlCodeTemplate</ruleTemplate>
          <mandatoryRoute>false</mandatoryRoute>
          <finalApproval>false</finalApproval>
        </requests>
        <requests name="Alien Indicator">
          <activationType>P</activationType>
          <ruleTemplate>KualiAlienIndicatorTemplate</ruleTemplate>
          <mandatoryRoute>false</mandatoryRoute>
          <finalApproval>false</finalApproval>
        </requests>
        <requests name="Payment Reason">
          <activationType>P</activationType>
          <ruleTemplate>KualiPaymentReasonTemplate</ruleTemplate>
          <mandatoryRoute>false</mandatoryRoute>
          <finalApproval>false</finalApproval>
        </requests>
        <requests name="Payment Reason+Campus Code">
          <activationType>P</activationType>
          <ruleTemplate>KualiPaymentReasonCampusCodeTemplate</ruleTemplate>
          <mandatoryRoute>false</mandatoryRoute>
          <finalApproval>false</finalApproval>
        </requests>
        <requests name="Campus Code">
          <activationType>P</activationType>
          <ruleTemplate>KualiCampusCodeTemplate</ruleTemplate>
          <mandatoryRoute>false</mandatoryRoute>
          <finalApproval>false</finalApproval>
        </requests>
        <requests name="Alien Indicator+Payment Reason">
          <activationType>P</activationType>
          <ruleTemplate>KualiAlienIndicatorPaymentReasonTemplate</ruleTemplate>
          <mandatoryRoute>false</mandatoryRoute>
          <finalApproval>false</finalApproval>
        </requests>
        <requests name="Payment Method">
          <activationType>P</activationType>
          <ruleTemplate>KualiPaymentMethodTemplate</ruleTemplate>
          <mandatoryRoute>false</mandatoryRoute>
          <finalApproval>false</finalApproval>
        </requests>
        <requests name="Award Workgroup">
          <activationType>S</activationType>
          <ruleTemplate>KualiAwardWorkgroupTemplate</ruleTemplate>
          <mandatoryRoute>false</mandatoryRoute>
          <finalApproval>false</finalApproval>
        </requests>
      </routeNodes>
</documentType>
</documentTypes>
</data>
