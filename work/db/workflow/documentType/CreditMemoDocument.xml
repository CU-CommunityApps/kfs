        </attribute>
        <attribute>
          <name>KualiCreditMemoDocumentExtractedDate</name>
        </attribute>
        <attribute>
          <name>KualiCreditMemoDocumentExtractedIndicator</name>
        </attribute>
        <attribute>
          <name>KualiCreditMemoDocumentHoldIndicator</name>
        </attribute>
        <attribute>
          <name>KualiCreditMemoDocumentPaidIndicator</name>
