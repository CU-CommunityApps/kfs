      <superUserWorkgroupName>PA_AP_SUPER_USERS</superUserWorkgroupName>
      <blanketApproveWorkgroupName>PA_AP_BLANKET_APPROVERS</blanketApproveWorkgroupName>
      <defaultExceptionWorkgroupName>SY_FUNCTIONAL_SUPER_USERS</defaultExceptionWorkgroupName>
      <docHandler>${application.url}/purapCreditMemo.do?methodToCall=docHandler</docHandler>
      <active>true</active>
      <attributes>
        <attribute>
          <name>KualiCreditMemoDocSearchGeneratorAttribute</name>
        </attribute>
        <attribute>
          <name>KualiCreditMemoDocumentSearchResultProcessorAttribute</name>
        </attribute>
        <attribute>
          <name>KualiCreditMemoId</name>
        </attribute>
        <attribute>
          <name>KualiCreditMemoVendorCreditMemoNumber</name>
        </attribute>
        <attribute>
          <name>KualiCreditMemoPaymentRequestId</name>
        </attribute>
        <attribute>
          <name>KualiCreditMemoPurchaseOrderId</name>
        </attribute>
        <attribute>
          <name>KualiCreditMemoDocumentVendorName</name>
        </attribute>
        <attribute>
          <name>KualiCreditMemoDocumentVendorId</name>
        </attribute>
        <attribute>
          <name>KualiPurApDocumentOrganization</name>
        </attribute>
        <attribute>
          <name>KualiCreditMemoDocumentProcessCampus</name>
        </attribute>
        <attribute>
          <name>KualiCreditMemoDocumentCustomerNumber</name>
        </attribute>
        <attribute>
          <name>KualiCreditMemoDocumentVendorCreditMemoDate</name>
