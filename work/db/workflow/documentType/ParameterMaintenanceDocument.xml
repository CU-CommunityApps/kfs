<?xml version="1.0" encoding="UTF-8"?>
<data xmlns="ns:workflow" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="ns:workflow resource:WorkflowData">
  <documentTypes xmlns="ns:workflow/DocumentType" xsi:schemaLocation="ns:workflow/DocumentType resource:DocumentType">
    <documentType>
      <name>ParameterMaintenanceDocument</name>
      <parent>KualiBaseDocument</parent>
      <label>Parameter</label>
      <postProcessorName>org.kuali.kfs.sys.document.workflow.KualiPostProcessor</postProcessorName>
      <superUserWorkgroupName>WorkflowAdmin</superUserWorkgroupName>
      <blanketApproveWorkgroupName>WorkflowAdmin</blanketApproveWorkgroupName>
      <defaultExceptionWorkgroupName>WorkflowAdmin</defaultExceptionWorkgroupName>
      <docHandler>${application.url}/kr/maintenance.do?methodToCall=docHandler</docHandler>
      <active>true</active>
      <routingVersion>2</routingVersion>
      <routePaths>
        <routePath>
          <start name="Adhoc Routing" nextNode="Parameter Maintainers Review" />
          <requests name="Parameter Maintainers Review" />
        </routePath>
      </routePaths>
      <routeNodes>
        <start name="Adhoc Routing">
          <activationType>S</activationType>
          <mandatoryRoute>false</mandatoryRoute>
          <finalApproval>false</finalApproval>
        </start>
        <requests name="Parameter Maintainers Review">
          <activationType>S</activationType>
          <ruleTemplate>SystemParameterRoutingTemplate</ruleTemplate>
          <mandatoryRoute>false</mandatoryRoute>
          <finalApproval>false</finalApproval>
        </requests>
      </routeNodes>
    </documentType>
  </documentTypes>
</data>
