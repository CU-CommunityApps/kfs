<?xml version="1.0" encoding="UTF-8"?>
<data xmlns="ns:workflow" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="ns:workflow resource:WorkflowData">
<ruleAttributes xmlns="ns:workflow/RuleAttribute" xsi:schemaLocation="ns:workflow/RuleAttribute resource:RuleAttribute">
<ruleAttribute>
<name>KualiContractsAndGrantsProposalNumber</name>
      <className>org.kuali.rice.kns.workflow.attribute.KualiXmlSearchableAttributeImpl</className>
      <label>Contracts &amp; Grants Proposal Number</label>
      <description>Attribute to search and show search results for Proposal Number</description>
      <type>SearchableXmlAttribute</type>
      <searchingConfig>
        <fieldDef name="proposalNumber" title="kuali_dd_label(Proposal)">
          <display>
            <type>text</type>
          </display>
          <searchDefinition dataType="long" />
          <quickfinder appliesTo="proposalNumber" draw="true" service="workflow-org.kuali.kfs.module.cg.businessobject.Proposal-Lookupable(null)" />
          <fieldEvaluation>
            <xpathexpression>wf:xstreamsafe('//newMaintainableObject/businessObject/proposalNumber')</xpathexpression>
          </fieldEvaluation>
        </fieldDef>
      </searchingConfig>
</ruleAttribute>
</ruleAttributes>
</data>
