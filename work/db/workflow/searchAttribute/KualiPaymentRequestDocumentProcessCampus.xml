<?xml version="1.0" encoding="UTF-8"?>
<data xmlns="ns:workflow" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="ns:workflow resource:WorkflowData">
<ruleAttributes xmlns="ns:workflow/RuleAttribute" xsi:schemaLocation="ns:workflow/RuleAttribute resource:RuleAttribute">
<ruleAttribute>
<name>KualiPaymentRequestDocumentProcessCampus</name>
      <className>org.kuali.rice.kns.workflow.attribute.KualiXmlSearchableAttributeImpl</className>
      <label>Kuali Payment Request Document Processing Campus</label>
      <description>Attribute to identify the Processing Campus of the Payment Request Document</description>
      <type>SearchableXmlAttribute</type>
      <searchingConfig>
        <fieldDef name="paymentRequestProcessCampus" title="kuali_dd_label(PaymentRequestDocument)">
          <display>
            <type>select</type>
            <values>kuali_values_finder_class(org.kuali.rice.kns.lookup.keyvalues.CampusValuesFinder)</values>
          </display>
          <visibility>
            <column visible="false" />
          </visibility>
          <quickfinder appliesTo="campusCode" draw="true" service="workflow-org.kuali.rice.kns.bo.Campus-Lookupable(null)" />
          <fieldEvaluation>
            <xpathexpression>wf:xstreamsafe('//document/processingCampusCode')</xpathexpression>
          </fieldEvaluation>
        </fieldDef>
      </searchingConfig>
</ruleAttribute>
</ruleAttributes>
</data>
