<?xml version="1.0" encoding="UTF-8"?>
<data xmlns="ns:workflow" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="ns:workflow resource:WorkflowData">
<ruleAttributes xmlns="ns:workflow/RuleAttribute" xsi:schemaLocation="ns:workflow/RuleAttribute resource:RuleAttribute">
<ruleAttribute>
<name>KualiCapitalAssetOrganizationOwnerAccountNumber</name>
      <className>org.kuali.rice.kns.workflow.attribute.KualiXmlSearchableAttributeImpl</className>
      <label>Kuali Capital Asset Document Organization Owner Account Number</label>
      <description>Attribute to search and show search results for Asset Number in Capital Asset Documents</description>
      <type>SearchableXmlAttribute</type>
      <searchingConfig>
        <fieldDef name="organizationOwnerAccountNumber" title="kuali_dd_label(Asset)">
          <display>
            <type>text</type>
          </display>
          <visibility>
            <column visible="true" />
          </visibility>
          <quickfinder appliesTo="accountNumber" draw="true" service="workflow-org.kuali.kfs.coa.businessobject.Account-Lookupable(null)" />
          <fieldEvaluation>
            <xpathexpression>wf:xstreamsafe('//document/routingInfo/org.kuali.kfs.sys.document.workflow.RoutingData/routingSet/org.kuali.kfs.sys.document.workflow.RoutingAccount/routingAccount')</xpathexpression>
          </fieldEvaluation>
        </fieldDef>
      </searchingConfig>
</ruleAttribute>
</ruleAttributes>
</data>
