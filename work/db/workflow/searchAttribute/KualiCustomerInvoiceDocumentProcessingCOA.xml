<?xml version="1.0" encoding="UTF-8"?>
<data xmlns="ns:workflow" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="ns:workflow resource:WorkflowData">
  <ruleAttributes xmlns="ns:workflow/RuleAttribute" xsi:schemaLocation="ns:workflow/RuleAttribute resource:RuleAttribute">
    <ruleAttribute>
      <name>KualiCustomerInvoiceDocumentProcessingCOA</name>
      <className>org.kuali.workflow.attribute.KualiXmlSearchableAttributeImpl</className>
      <label>Kuali Customer Invoice Document Processing Chart of Account</label>
      <description>Attribute to identify the Processing Chart of Account of the Customer Invoice Document</description>
      <type>SearchableXmlAttribute</type>
      <searchingConfig>
        <fieldDef name="documentHeader.processingChartOfAccountCode" title="kuali_dd_label(AccountsReceivableDocumentHeader)">
          <display>
            <type>text</type>
          </display>
          <searchDefinition allowWildcards="true" caseSensitive="false" />
          <quickfinder appliesTo="processingChartOfAccountCode" draw="true" service="workflow-org.kuali.module.chart.bo.Chart-Lookupable(workflow-organizationCode:organizationCode,workflow-chartOfAccountsCode:chartOfAccountsCode)" />
          <fieldEvaluation>
            <xpathexpression>wf:xstreamsafe('//document/accountsReceivableDocumentHeader/processingChartOfAccountCode')</xpathexpression>
          </fieldEvaluation>
        </fieldDef>
      </searchingConfig>
    </ruleAttribute>
  </ruleAttributes>
</data>