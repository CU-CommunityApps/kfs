<?xml version="1.0" encoding="UTF-8"?>
<data xmlns="ns:workflow" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="ns:workflow resource:WorkflowData">
  <ruleAttributes xmlns="ns:workflow/RuleAttribute" xsi:schemaLocation="ns:workflow/RuleAttribute resource:RuleAttribute">
    <ruleAttribute>
      <name>KualiPurchaseOrderDocumentIncompleteStatuses</name>
      <className>org.kuali.workflow.module.purap.docsearch.attribute.KualiPurchaseOrderIncompleteStatusesAttribute</className>
      <label>Kuali Purchase Order Document Incomplete Statuses</label>
      <description>Attribute to identify the Purchase Order Status of the Purchase Order Document</description>
      <type>SearchableXmlAttribute</type>
      <searchingConfig>
        <fieldDef name="purchaseOrderDocumentIncompleteStatuses" title="kuali_dd_label(PurApGenericAttributes)">
          <display>
            <type>checkbox_present</type>
          </display>
          <visibility>
            <column visible="false" />
          </visibility>
          <fieldEvaluation>
            <!-- following xpath expression should match to the status code -->
            <xpathexpression>wf:xstreamsafe('//document/statusCode')</xpathexpression>
          </fieldEvaluation>
        </fieldDef>
      </searchingConfig>
    </ruleAttribute>
  </ruleAttributes>
</data>
