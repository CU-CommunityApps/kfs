<?xml version="1.0" encoding="UTF-8"?>
<data xmlns="ns:workflow" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="ns:workflow resource:WorkflowData">
<ruleAttributes xmlns="ns:workflow/RuleAttribute" xsi:schemaLocation="ns:workflow/RuleAttribute resource:RuleAttribute">
<ruleAttribute>
<name>KualiPaymentRequestDocumentInvoiceDate</name>
      <className>org.kuali.rice.kns.workflow.attribute.KualiXmlSearchableAttributeImpl</className>
      <label>Kuali Payment Request Document Invoice Date Range</label>
      <description>Attribute to identify the Invoice Date Range of the Payment Request Document</description>
      <type>SearchableXmlAttribute</type>
      <searchingConfig>
        <fieldDef name="paymentRequestInvoiceDate" title="kuali_dd_label(PaymentRequestDocument)">
          <display>
            <type>text</type>
          </display>
          <visibility>
            <column visible="false" />
          </visibility>
          <searchDefinition dataType="datetime" rangeSearch="true" />
          <fieldEvaluation>
            <xpathexpression>wf:xstreamsafe('//document/invoiceDate')</xpathexpression>
          </fieldEvaluation>
        </fieldDef>
      </searchingConfig>
</ruleAttribute>
</ruleAttributes>
</data>
