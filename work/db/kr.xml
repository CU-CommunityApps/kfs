<?xml version='1.0' encoding='UTF-8'?> 
<data xmlns="ns:workflow" xmlns:fo="http://www.w3.org/1999/XSL/Format" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="ns:workflow resource:WorkflowData">
  <documentTypes xmlns="ns:workflow/DocumentType" xsi:schemaLocation="ns:workflow/DocumentType resource:DocumentType">
    <documentType>
      <name>GSOB</name>
      <parent>COA</parent>
      <routePaths>
        <routePath>
          <start name="AdHoc" nextNode="Account"/>

          <requests name="Account" nextNode="OrganizationHierarchy"/>
          <requests name="OrganizationHierarchy" nextNode="Chart"/>
          <requests name="Chart"/>
        </routePath>
      </routePaths>
      <routeNodes>
        <start name="AdHoc"/>
        <role name="Account">
          <qualifierResolverClass>org.kuali.rice.kns.workflow.attribute.DataDictionaryQualifierResolver</qualifierResolverClass>

          <activationType>P</activationType>
        </role>
        <role name="OrganizationHierarchy">
          <qualifierResolverClass>org.kuali.rice.kns.workflow.attribute.DataDictionaryQualifierResolver</qualifierResolverClass>
          <activationType>P</activationType>
        </role>
        <role name="Chart">

          <qualifierResolverClass>org.kuali.rice.kns.workflow.attribute.DataDictionaryQualifierResolver</qualifierResolverClass>
          <activationType>P</activationType>
        </role>
      </routeNodes>
    </documentType>
    <documentType>
      <name>GORV</name>
      <parent>COA</parent>
    </documentType>
    <documentType>
      <name>INVR</name>
      <parent>ARM</parent>
      <routePaths>
        <routePath>
          <start name="AdHoc" nextNode="InvoiceRecurrenceIsInactivating"/>
          <split name="InvoiceRecurrenceIsInactivating" nextNode="InitiatedBySystemUser">
            <branch name="True">
              <requests name="RecurrenceCreator" nextNode="JoinInvoiceRecurrenceIsInactivating" />
            </branch>
            <branch name="False">
              <simple name="NoOpInvoiceRecurrenceIsInactivating" nextNode="JoinInvoiceRecurrenceIsInactivating" />
            </branch>
            <join name="JoinInvoiceRecurrenceIsInactivating" />
          </split>
          <split name="InitiatedBySystemUser">
          	<branch name="True">
          		<simple name="NoOpInitiatedBySystemUser" nextNode="JoinInitiatedBySystemUser" />
          	</branch>
          	<branch name="False">
	          <requests name="Account" nextNode="Recurrence" />
	          <requests name="Recurrence" nextNode="JoinInitiatedBySystemUser" />
          	</branch>
            <join name="JoinInitiatedBySystemUser"/>
          </split>
        </routePath>
      </routePaths>
      <routeNodes>
        <start name="AdHoc"/>
        <role name="RecurrenceCreator">
          <qualifierResolverClass>org.kuali.rice.kns.workflow.attribute.DataDictionaryQualifierResolver</qualifierResolverClass>
          <activationType>P</activationType>
        </role>
        <role name="Account">
          <qualifierResolverClass>org.kuali.rice.kns.workflow.attribute.DataDictionaryQualifierResolver</qualifierResolverClass>
          <activationType>P</activationType>
        </role>
        <role name="Recurrence">
          <qualifierResolverClass>org.kuali.rice.kns.workflow.attribute.DataDictionaryQualifierResolver</qualifierResolverClass>
          <activationType>P</activationType>
        </role>
        <split name="InvoiceRecurrenceIsInactivating">
          <type>org.kuali.kfs.sys.document.workflow.SimpleBooleanSplitNode</type>
        </split>
        <simple name="NoOpInvoiceRecurrenceIsInactivating">
          <type>org.kuali.rice.kew.engine.node.NoOpNode</type>
        </simple>
        <join name="JoinInvoiceRecurrenceIsInactivating"/>
        <split name="InitiatedBySystemUser">
          <type>org.kuali.kfs.sys.document.workflow.SimpleBooleanSplitNode</type>
        </split>
        <simple name="NoOpInitiatedBySystemUser">
          <type>org.kuali.rice.kew.engine.node.NoOpNode</type>
        </simple>
        <join name="JoinInitiatedBySystemUser"/>
      </routeNodes>
    </documentType>
    <documentType>
      <name>AWRD</name>
      <parent>CGM</parent>
      <routePaths>
        <routePath>
          <start name="AdHoc" nextNode="OrganizationHierarchy"/>
          <requests name="OrganizationHierarchy" />
        </routePath>
      </routePaths>
      <routeNodes>
        <start name="AdHoc"/>
        <role name="OrganizationHierarchy">
          <qualifierResolverClass>org.kuali.rice.kns.workflow.attribute.DataDictionaryQualifierResolver</qualifierResolverClass>
          <activationType>P</activationType>
        </role>
      </routeNodes>
    </documentType>
    <documentType>
      <name>PMTY</name>
      <parent>PRSM</parent>
      <label>CapitalAssetSystemState</label>
      <helpDefinitionURL>default.htm?turl=WordDocuments%2Fcapitalassetsystemstatemaintenancedocument.htm</helpDefinitionURL>
    </documentType>
    <documentType>
      <name>BC</name>
      <parent>BCT</parent>
      <label>Budget Construction Document</label>
      <postProcessorName />
      <routePaths />
    </documentType>
    <documentType>
      <name>GDLG</name>
      <parent>COA</parent>
      <routePaths>
        <routePath>
          <start name="AdHoc" nextNode="OrganizationHierarchy"/>
          <requests name="OrganizationHierarchy" nextNode="Chart"/>
          <requests name="Chart" nextNode="Account"/>
          <requests name="Account"/>
        </routePath>
      </routePaths>
      <routeNodes>
        <start name="AdHoc"/>
        <role name="OrganizationHierarchy">
          <qualifierResolverClass>org.kuali.rice.kns.workflow.attribute.DataDictionaryQualifierResolver</qualifierResolverClass>
          <activationType>P</activationType>
        </role>
        <role name="Chart">
          <qualifierResolverClass>org.kuali.rice.kns.workflow.attribute.DataDictionaryQualifierResolver</qualifierResolverClass>
          <activationType>P</activationType>
        </role>
        <role name="Account">
          <qualifierResolverClass>org.kuali.rice.kns.workflow.attribute.DataDictionaryQualifierResolver</qualifierResolverClass>
          <activationType>P</activationType>
        </role>
      </routeNodes>
    </documentType>
  </documentTypes>
</data>