<documentType>
<name>PaymentRequestDocument</name>
<parent>AccountsPayableTransactionalDocument</parent>
<docHandler>${application.url}/purapPaymentRequest.do?methodToCall=docHandler</docHandler>
<routePaths>
        <routePath>
          <start name="AdHoc" nextNode="RequiresImageAttachment"/>
          <split name="RequiresImageAttachment" nextNode="PurchaseWasReceived">
			<branch name="True">
				<requests name="ImageAttachment" nextNode="JoinRequiresImageAttachment"/>
			</branch>
			<branch name="False">
				<simple name="NoOpRequiresImageAttachment" nextNode="JoinRequiresImageAttachment"/>
			</branch>
			<join name="JoinRequiresImageAttachment"/>
		  </split> 
          <split name="PurchaseWasReceived" nextNode="SubAccount">
						<branch name="True">
							<requests name="Receiving" nextNode="JoinReceiving"/>
						</branch>
						<branch name="False">
							<simple name="NoOpReceiving" nextNode="JoinReceiving"/>
						</branch>
						<join name="JoinReceiving"/>
					</split>
          <requests name="SubAccount" nextNode="Account"/>
          <requests name="Account" nextNode="AccountingOrganizationHierarchy"/>
          <requests name="AccountingOrganizationHierarchy" nextNode="Tax"/>
          <split name="VendorIsEmployeeOrNonResidentAlien">
						<branch name="True">
							<requests name="Tax" nextNode="JoinVendorIsEmployeeOrNonResidentAlien"/>
						</branch>
						<branch name="False">
							<simple name="NoOpVendorIsEmployeeOrNonResidentAlien" nextNode="JoinVendorIsEmployeeOrNonResidentAlien"/>
						</branch>
						<join name="JoinVendorIsEmployeeOrNonResidentAlien"/>
