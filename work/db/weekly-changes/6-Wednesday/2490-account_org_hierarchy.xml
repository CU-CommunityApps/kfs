<documentType>
<name>BudgetAdjustmentDocument</name>
<parent>FinancialProcessingTransactionalDocument</parent>
<docHandler>${application.url}/financialBudgetAdjustment.do?methodToCall=docHandler</docHandler>
<routePaths>
        <routePath>
          <start name="AdHoc" nextNode="RequiresFullApproval"/>
          <split name="RequiresFullApproval">
            <branch name="True">
              <requests name="Account" nextNode="AccountingOrganizationHierarchy"/>
              <requests name="AccountingOrganizationHierarchy" nextNode="SubFund"/>
              <requests name="SubFund" nextNode="Award"/>
              <requests name="Award" nextNode="JoinRequiresFullApproval"/>
            </branch>
            <branch name="False">
              <simple name="Do Nothing" nextNode="JoinRequiresFullApproval"/>
            </branch>
            <join name="JoinRequiresFullApproval"/>
          </split>
         
        </routePath>
      </routePaths>
<routeNodes>
<start name="AdHoc"/>
<role name="Account">
<qualifierResolverClass>org.kuali.rice.kns.workflow.attribute.DataDictionaryQualifierResolver</qualifierResolverClass>
</role>
<role name="AccountingOrganizationHierarchy">
<qualifierResolverClass>org.kuali.rice.kns.workflow.attribute.DataDictionaryQualifierResolver</qualifierResolverClass>
</role>
<role name="SubFund">
<qualifierResolverClass>org.kuali.rice.kns.workflow.attribute.DataDictionaryQualifierResolver</qualifierResolverClass>
</role>
<role name="Award">
<qualifierResolverClass>org.kuali.rice.kns.workflow.attribute.DataDictionaryQualifierResolver</qualifierResolverClass>
</role>
<split name="RequiresFullApproval">
<type>org.kuali.kfs.sys.document.workflow.SimpleBooleanSplitNode</type>
</split>
<simple name="Do Nothing">
<type>org.kuali.rice.kew.engine.node.NoOpNode</type>
</simple>

<join name="JoinRequiresFullApproval"/>
</routeNodes>
</documentType>

<documentType>
<name>DisbursementVoucherDocument</name>
<parent>FinancialProcessingTransactionalDocument</parent>
<docHandler>${application.url}/financialDisbursementVoucher.do?methodToCall=docHandler</docHandler>
<routePaths>
				<routePath>
					<start name="AdHoc" nextNode="PayeeIsPurchaseOrderVendor"/>
					<split name="PayeeIsPurchaseOrderVendor" nextNode="Account">
						<branch name="True">
							<requests name="Purchasing" nextNode="JoinPayeeIsPurchaseOrderVendor"/>
						</branch>
						<branch name="False">
							<simple name="NoOpPayee" nextNode="JoinPayeeIsPurchaseOrderVendor"/>
						</branch>
						<join name="JoinPayeeIsPurchaseOrderVendor"/>
					</split>
					<requests name="Account" nextNode="AccountingOrganizationHierarchy"/>
					<requests name="AccountingOrganizationHierarchy" nextNode="RequiresTaxReview"/>
					<split name="RequiresTaxReview" nextNode="Campus">
						<branch name="True">
							<requests name="Tax" nextNode="JoinRequiresTaxReview"/>
						</branch>
						<branch name="False">
							<simple name="NoOpTaxReview" nextNode="JoinRequiresTaxReview"/>
						</branch>
						<join name="JoinRequiresTaxReview"/>
					</split>
					<requests name="Campus" nextNode="RequiresTravelReview"/>					
					<split name="RequiresTravelReview" nextNode="PaymentMethod">
						<branch name="True">
							<requests name="Travel" nextNode="JoinRequiresTravelReview"/>
						</branch>
						<branch name="False">
							<simple name="NoOpTravelReview" nextNode="JoinRequiresTravelReview"/>
						</branch>
						<join name="JoinRequiresTravelReview"/>
					</split>
					<requests name="PaymentMethod" nextNode="Award"/>
					<requests name="Award"/>
				</routePath>
			</routePaths>
<routeNodes>
<start name="AdHoc"/>
<split name="PayeeIsPurchaseOrderVendor">
<type>org.kuali.kfs.sys.document.workflow.SimpleBooleanSplitNode</type>
</split>
<split name="RequiresTaxReview">
<type>org.kuali.kfs.sys.document.workflow.SimpleBooleanSplitNode</type>
</split>
<split name="RequiresTravelReview">
<type>org.kuali.kfs.sys.document.workflow.SimpleBooleanSplitNode</type>
</split>
<simple name="NoOpPayee">
<type>org.kuali.rice.kew.engine.node.NoOpNode</type>
</simple>
<simple name="NoOpTaxReview">
<type>org.kuali.rice.kew.engine.node.NoOpNode</type>
</simple>
<simple name="NoOpTravelReview">
<type>org.kuali.rice.kew.engine.node.NoOpNode</type>
</simple>
<join name="JoinPayeeIsPurchaseOrderVendor"/>
<join name="JoinRequiresTaxReview"/>
<join name="JoinRequiresTravelReview"/>
<role name="Purchasing">
					<qualifierResolverClass>org.kuali.rice.kns.workflow.attribute.DataDictionaryQualifierResolver</qualifierResolverClass>
				</role>
<role name="Account">
					<qualifierResolverClass>org.kuali.rice.kns.workflow.attribute.DataDictionaryQualifierResolver</qualifierResolverClass>
					<mandatoryRoute>true</mandatoryRoute>
				</role>
<role name="AccountingOrganizationHierarchy">
					<qualifierResolverClass>org.kuali.rice.kns.workflow.attribute.DataDictionaryQualifierResolver</qualifierResolverClass>
				</role>
<role name="Tax">
					<qualifierResolverClass>org.kuali.rice.kns.workflow.attribute.DataDictionaryQualifierResolver</qualifierResolverClass>
				</role>
<role name="Campus">
					<qualifierResolverClass>org.kuali.rice.kns.workflow.attribute.DataDictionaryQualifierResolver</qualifierResolverClass>
				</role>
<role name="Travel">
					<qualifierResolverClass>org.kuali.rice.kns.workflow.attribute.DataDictionaryQualifierResolver</qualifierResolverClass>
				</role>
<role name="PaymentMethod">
					<qualifierResolverClass>org.kuali.rice.kns.workflow.attribute.DataDictionaryQualifierResolver</qualifierResolverClass>
				</role>
<role name="Award">
					<qualifierResolverClass>org.kuali.rice.kns.workflow.attribute.DataDictionaryQualifierResolver</qualifierResolverClass>
				</role>
</routeNodes>
</documentType>

