<documentType>
	<name>CashControlDocument</name>
	<parent>AccountsReceivableTransactionalDocument</parent>
	<docHandler>
		${application.url}/arCashControlDocument.do?methodToCall=docHandler
	</docHandler>
	<routePaths>
		<routePath>
			<start name="AdHoc" nextNode="Lockbox" />
			<requests name="Lockbox"
				nextNode="AssociatedWithElectronicPayment" />
			<split name="AssociatedWithElectronicPayment">
				<branch name="True">
					<requests name="ElectronicPayment" nextNode="Join" />
				</branch>
				<branch name="False">
					<simple name="NoOp" nextNode="Join" />
				</branch>
				<join name="Join" />
			</split>
		</routePath>
	</routePaths>
	<routeNodes>
		<start name="AdHoc" />
		<role name="Lockbox">
			<qualifierResolverClass>
				org.kuali.rice.kns.workflow.attribute.DataDictionaryQualifierResolver
			</qualifierResolverClass>
			<mandatoryRoute>true</mandatoryRoute>
		</role>
		<role name="ElectronicPayment">
			<qualifierResolverClass>
				org.kuali.rice.kns.workflow.attribute.DataDictionaryQualifierResolver
			</qualifierResolverClass>
		</role>
		<split name="AssociatedWithElectronicPayment">
			<type>org.kuali.kfs.sys.document.routing.SimpleBooleanSplitNode</type>
		</split>
		<simple name="NoOp">
			<type>org.kuali.rice.kew.engine.node.NoOpNode</type>
		</simple>
		<join name="Join" />
	</routeNodes>
</documentType>
