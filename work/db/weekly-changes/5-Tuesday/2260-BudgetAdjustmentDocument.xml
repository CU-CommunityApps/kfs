<?xml version="1.0" encoding="UTF-8"?>
<data xmlns="ns:workflow" xmlns:fo="http://www.w3.org/1999/XSL/Format" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="ns:workflow resource:WorkflowData">
<documentTypes xmlns="ns:workflow/DocumentType" xsi:schemaLocation="ns:workflow/DocumentType resource:DocumentType">
<documentType>
<name>BudgetAdjustmentDocument</name>
<parent>FinancialProcessingTransactionalDocument</parent>
<docHandler>${application.url}/financialBudgetAdjustment.do?methodToCall=docHandler</docHandler>
<routePaths>
        <routePath>
          <start name="AdHoc" nextNode="RequiresFullApproval"/>
          <split name="RequiresFullApproval">
            <branch name="True">
              <requests name="Account" nextNode="OrganizationHierarchy"/>
              <requests name="OrganizationHierarchy" nextNode="SubFund"/>
              <requests name="SubFund" nextNode="Award"/>
              <requests name="Award" nextNode="JoinRequiresFullApproval"/>
            </branch>
            <branch name="False">
              <simple name="Do Nothing" nextNode="JoinRequiresFullApproval"/>
            </branch>
            <join name="JoinRequiresFullApproval"/>
          </split>
         
        </routePath>
      </routePaths>
<routeNodes>
<start name="AdHoc"/>
<role name="Account">
<!--<ruleTemplate>KualiAccountTemplate</ruleTemplate>-->
<qualifierResolverClass>org.kuali.rice.kns.workflow.attribute.DataDictionaryQualifierResolver</qualifierResolverClass>
</role>
<role name="OrganizationHierarchy">
<!--<ruleTemplate>KualiOrgReviewTemplate</ruleTemplate>-->
<qualifierResolverClass>org.kuali.rice.kns.workflow.attribute.DataDictionaryQualifierResolver</qualifierResolverClass>
</role>
<role name="SubFund">
<!--<ruleTemplate>KualiSubFundTemplate</ruleTemplate>-->
<qualifierResolverClass>org.kuali.rice.kns.workflow.attribute.DataDictionaryQualifierResolver</qualifierResolverClass>
</role>
<role name="Award">
<!--<ruleTemplate>KualiAwardWorkgroupTemplate</ruleTemplate>-->
<qualifierResolverClass>org.kuali.rice.kns.workflow.attribute.DataDictionaryQualifierResolver</qualifierResolverClass>
</role>
<split name="RequiresFullApproval">
<type>org.kuali.kfs.sys.document.routing.SimpleBooleanSplitNode</type>
</split>
<simple name="Do Nothing">
<type>org.kuali.rice.kew.engine.node.NoOpNode</type>
</simple>

<join name="JoinRequiresFullApproval"/>
</routeNodes>
</documentType>
</documentTypes>
</data>
