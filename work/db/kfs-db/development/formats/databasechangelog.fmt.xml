<Record name="databasechangelog" type="delimited">
  <Field name="id"                              type="string"     nullable="true"  trim="false" delimiter="|"  />
  <Field name="author"                          type="string"     nullable="true"  trim="false" delimiter="|"  />
  <Field name="filename"                        type="string"     nullable="true"  trim="false" delimiter="|"  />
  <Field name="dateexecuted"                    type="date"       nullable="true"  trim="false" delimiter="|"  format="yyyyMMddHHmmss" />
  <Field name="md5sum"                          type="string"     nullable="true"  trim="false" delimiter="|"  />
  <Field name="description"                     type="string"     nullable="true"  trim="false" delimiter="|"  />
  <Field name="comments"                        type="string"     nullable="true"  trim="false" delimiter="|"  />
  <Field name="tag"                             type="string"     nullable="true"  trim="false" delimiter="|"  />
  <Field name="liquibase"                       type="string"     nullable="true"  trim="false" delimiter="|\r\n" />
</Record>