        <form-bean name="JournalVoucherForm" type="org.kuali.module.financial.web.struts.form.JournalVoucherForm" />
        <form-bean name="IndirectCostAdjustmentForm" type="org.kuali.module.financial.web.struts.form.IndirectCostAdjustmentForm" />
        <form-bean name="NonCheckDisbursementForm" type="org.kuali.module.financial.web.struts.form.NonCheckDisbursementForm" />
        <form-bean name="PreEncumbranceForm" type="org.kuali.module.financial.web.struts.form.PreEncumbranceForm" />
        <form-bean name="DisbursementVoucherForm" type="org.kuali.module.financial.web.struts.form.DisbursementVoucherForm" />
        <form-bean name="CashManagementForm" type="org.kuali.module.financial.web.struts.form.CashManagementForm" />
        <form-bean name="CashManagementStatusForm" type="org.kuali.module.financial.web.struts.form.CashManagementStatusForm" />
        <form-bean name="ProcurementCardForm" type="org.kuali.module.financial.web.struts.form.ProcurementCardForm" />
        <form-bean name="BudgetAdjustmentForm" type="org.kuali.module.financial.web.struts.form.BudgetAdjustmentForm" />
        <form-bean name="YearEndBudgetAdjustmentForm" type="org.kuali.module.financial.web.struts.form.YearEndBudgetAdjustmentForm" />

        <!-- KRA Module Bean Definitions -->
        <form-bean name="BudgetForm" type="org.kuali.module.kra.budget.web.struts.form.BudgetForm" />
        <form-bean name="BudgetParametersForm" type="org.kuali.module.kra.budget.web.struts.form.BudgetForm" />
        <form-bean name="BudgetOverviewForm" type="org.kuali.module.kra.budget.web.struts.form.BudgetForm" />
        <form-bean name="BudgetTemplateForm" type="org.kuali.module.kra.budget.web.struts.form.BudgetForm" />
        <form-bean name="BudgetNonpersonnelForm" type="org.kuali.module.kra.budget.web.struts.form.BudgetForm" />
        <form-bean name="BudgetNonpersonnelCopyOverForm" type="org.kuali.module.kra.budget.web.struts.form.BudgetForm" />
        <form-bean name="BudgetPersonnelForm" type="org.kuali.module.kra.budget.web.struts.form.BudgetForm" />
        <form-bean name="BudgetPersonnelForm" type="org.kuali.module.kra.budget.web.struts.form.BudgetForm" />
        <form-bean name="BudgetCostShareForm" type="org.kuali.module.kra.budget.web.struts.form.BudgetForm" />
        <form-bean name="BudgetOutputForm" type="org.kuali.module.kra.budget.web.struts.form.BudgetForm" />
        <form-bean name="BudgetModularForm" type="org.kuali.module.kra.budget.web.struts.form.BudgetForm" />
        <form-bean name="BudgetIndirectCostForm" type="org.kuali.module.kra.budget.web.struts.form.BudgetForm" />
        <form-bean name="BudgetAuditModeForm" type="org.kuali.module.kra.budget.web.struts.form.BudgetForm" />
        <form-bean name="BudgetPermissionsForm" type="org.kuali.module.kra.budget.web.struts.form.BudgetForm" />
        <form-bean name="BudgetNotesForm" type="org.kuali.module.kra.budget.web.struts.form.BudgetForm" />

        <form-bean name="RoutingForm" type="org.kuali.module.kra.routingform.web.struts.form.RoutingForm" />
        <form-bean name="RfMainPageForm" type="org.kuali.module.kra.routingform.web.struts.form.RoutingForm" />
        <form-bean name="RfPersonnelForm" type="org.kuali.module.kra.routingform.web.struts.form.RoutingForm" />
        <form-bean name="RfResearchRisksForm" type="org.kuali.module.kra.routingform.web.struts.form.RoutingForm" />
        <form-bean name="RfProjectDetailsForm" type="org.kuali.module.kra.routingform.web.struts.form.RoutingForm" />
        <form-bean name="RfAgencySpecificForm" type="org.kuali.module.kra.routingform.web.struts.form.RoutingForm" />
        <form-bean name="RfSF424Form" type="org.kuali.module.kra.routingform.web.struts.form.RoutingForm" />
        <form-bean name="RfAttachmentsForm" type="org.kuali.module.kra.routingform.web.struts.form.RoutingForm" />
        <form-bean name="RfNotesForm" type="org.kuali.module.kra.routingform.web.struts.form.RoutingForm" />
        <form-bean name="RfOutputForm" type="org.kuali.module.kra.routingform.web.struts.form.RoutingForm" />
        <form-bean name="RfTemplateForm" type="org.kuali.module.kra.routingform.web.struts.form.RoutingForm" />
        <form-bean name="RfAuditModeForm" type="org.kuali.module.kra.routingform.web.struts.form.RoutingForm" />
        <form-bean name="RfPermissionsForm" type="org.kuali.module.kra.routingform.web.struts.form.RoutingForm" />
        <form-bean name="RfApprovalsForm" type="org.kuali.module.kra.routingform.web.struts.form.RoutingForm" />

        <!-- General Ledger Bean Definitions -->
        <form-bean name="CorrectionForm" type="org.kuali.module.gl.web.struts.form.CorrectionForm" />
        <form-bean name="BalanceInquiryForm" type="org.kuali.module.gl.web.struts.form.BalanceInquiryForm" />

        <!-- PURAP Module Bean Definitions -->
        <form-bean name="RequisitionForm" type="org.kuali.module.purap.web.struts.form.RequisitionForm" />

    </form-beans>

    <global-exceptions>
        <exception type="org.kuali.core.exceptions.AuthorizationException" key="meaningless" path="/authorizationException.do" />
        <exception type="org.kuali.module.financial.exceptions.CashDrawerStateException" key="unused" path="/cashManagementStatus.do" />

        <exception type="org.kuali.core.exceptions.UnknownDocumentIdException" key="error.document.noLongerExistsInKuali" path="/jsp/core/Exception.jsp" />
        <exception type="org.kuali.core.exceptions.FileUploadLimitExceededException" key="error.document.attachmentMaximumFileSizeExceeded" path="/jsp/core/Exception.jsp" />
    </global-exceptions>


    <global-forwards>
        <forward name="error" path="/jsp/core/KualiError.jsp" />
        <forward name="save" path="/index.jsp" redirect="true" />
        <forward name="cancel" path="/index.jsp" redirect="true" />
        <forward name="close" path="/index.jsp" redirect="true" />

        <forward name="portal" path="/index.jsp" redirect="true" />
    </global-forwards>

    <action-mappings>
        <!--  Core Action Mappings -->
        <action path="/portal" name="KualiForm" type="org.kuali.core.web.struts.action.KualiSimpleAction">
            <forward name="basic" path="/portal.jsp" />
        </action>

        <action path="/index" name="KualiForm" type="org.kuali.core.web.struts.action.KualiSimpleAction">
            <forward name="basic" path="/portal.jsp" />
        </action>

        <action path="/Exception" name="KualiForm" type="org.kuali.core.web.struts.action.KualiExceptionAction">
            <forward name="basic" path="/jsp/core/Exception.jsp" />
        </action>

        <action path="/jstlTest" name="KualiForm" type="org.kuali.core.web.struts.action.KualiSimpleAction">
            <forward name="basic" path="/jsp/core/jstlTest.jsp" />
        </action>

        <action path="/authorizationException" name="KualiForm" type="org.kuali.core.web.struts.action.KualiAuthorizationExceptionAction">
            <forward name="basic" path="/jsp/core/authorizationException.jsp" />
        </action>

        <action path="/questionPrompt" name="QuestionPromptForm" scope="request" parameter="methodToCall" type="org.kuali.core.web.struts.action.QuestionPromptAction">
            <forward name="basic" path="/jsp/core/KualiQuestion.jsp" />
        </action>

        <action path="/lookup" name="LookupForm" scope="request" attribute="KualiForm" parameter="methodToCall" type="org.kuali.core.web.struts.action.KualiLookupAction">
            <forward name="basic" path="/jsp/core/KualiLookup.jsp" />
        </action>

        <action path="/inquiry" name="InquiryForm" scope="request" attribute="KualiForm" parameter="methodToCall" type="org.kuali.core.web.struts.action.KualiInquiryAction">
            <forward name="basic" path="/jsp/core/KualiInquiry.jsp" />
        </action>

        <action path="/balanceInquiryReportMenu" name="KualiBalanceInquiryReportMenuForm" scope="request" attribute="KualiForm" parameter="methodToCall" type="org.kuali.core.web.struts.action.KualiBalanceInquiryReportMenuAction">
            <forward name="basic" path="/jsp/core/KualiBalanceInquiryReportMenu.jsp" />
        </action>

        <action path="/maintenance" name="KualiMaintenanceForm" attribute="KualiForm" scope="request" parameter="methodToCall" validate="false" input="/jsp/core/KualiMaintenanceDocument.jsp" type="org.kuali.core.web.struts.action.KualiMaintenanceDocumentAction">
            <forward name="basic" path="/jsp/core/KualiMaintenanceDocument.jsp" />
        </action>

        <action path="/help" name="KualiHelpForm" scope="request" attribute="KualiForm" parameter="methodToCall" type="org.kuali.core.web.struts.action.KualiHelpAction">
            <forward name="basic" path="/jsp/core/KualiHelp.jsp" />
        </action>

        <action path="/dvPerDiem" name="DisbursementVoucherForm" scope="request" attribute="KualiForm" parameter="methodToCall" type="org.kuali.core.web.struts.action.KualiHelpAction">
            <forward name="basic" path="/jsp/core/KualiHelp.jsp" />
            <forward name="dvPerDiemLinks" path="/jsp/modules/financial/DisbursementVoucherTravelPerDiemLinks.jsp" />
        </action>

        <action path="/loadObjectInfo" parameter="methodToCall" scope="request" type="org.kuali.core.web.struts.action.LoadObjectInfoAction">
            <forward name="chart" path="/jsp/xmlhttp/chart.jsp" />
            <forward name="account" path="/jsp/xmlhttp/account.jsp" />
            <forward name="subAccount" path="/jsp/xmlhttp/subAccount.jsp" />
            <forward name="objectCode" path="/jsp/xmlhttp/object.jsp" />
            <forward name="subObjectCode" path="/jsp/xmlhttp/subObject.jsp" />
            <forward name="project" path="/jsp/xmlhttp/project.jsp" />
            <forward name="organization" path="/jsp/xmlhttp/organization.jsp" />
            <forward name="objectType" path="/jsp/xmlhttp/objectType.jsp" />
            <forward name="user" path="/jsp/xmlhttp/user.jsp" />
            <forward name="originationCode" path="/jsp/xmlhttp/originationCode.jsp" />
            <forward name="documentType" path="/jsp/xmlhttp/documentType.jsp" />
            <forward name="bank" path="/jsp/xmlhttp/bank.jsp" />
            <forward name="bankAccount" path="/jsp/xmlhttp/bankAccount.jsp" />
            <forward name="objectTypeCode" path="/jsp/xmlhttp/objectTypeCode.jsp" />
            <forward name="accountICR" path="/jsp/xmlhttp/accountICR.jsp" />
        </action>

        <!--  Module Action Mappings -->
        <action path="/financial*" name="{1}Form" input="/jsp/modules/financial/{1}.jsp" type="org.kuali.module.financial.web.struts.action.{1}Action" scope="request" parameter="methodToCall" validate="true" attribute="KualiForm">
            <forward name="basic" path="/jsp/modules/financial/{1}.jsp" />
        </action>

        <action path="/depositWizard" name="DepositWizardForm" scope="request" attribute="KualiForm" parameter="methodToCall" type="org.kuali.module.financial.web.struts.action.DepositWizardAction">
            <forward name="basic" path="/jsp/modules/financial/DepositWizard.jsp" />
        </action>

        <action path="/generalLedger*" name="{1}Form" input="/jsp/modules/generalLedger/{1}.jsp" type="org.kuali.module.gl.web.struts.action.{1}Action" scope="request" parameter="methodToCall" validate="true" attribute="KualiForm">
            <forward name="basic" path="/jsp/modules/generalLedger/{1}.jsp" />
        </action>

        <action path="/glBalanceInquiry" name="BalanceInquiryForm" scope="request" attribute="KualiForm" parameter="methodToCall" type="org.kuali.module.gl.web.struts.action.BalanceInquiryAction">
            <forward name="basic" path="/jsp/modules/generalLedger/BalanceInquiry.jsp" />
        </action>

        <action path="/glModifiedInquiry" name="BalanceInquiryForm" scope="request" attribute="KualiForm" parameter="methodToCall" type="org.kuali.module.gl.web.struts.action.BalanceInquiryAction">
            <forward name="basic" path="/jsp/modules/generalLedger/ModifiedInquiry.jsp" />
        </action>

        <action path="/glAccountBalanceByConsolidationLookup" name="BalanceInquiryForm" scope="request" attribute="KualiForm" parameter="methodToCall" type="org.kuali.module.gl.web.struts.action.BalanceInquiryAction">
            <forward name="basic" path="/jsp/modules/generalLedger/AccountBalanceByConsolidationLookup.jsp" />
        </action>

        <action path="/researchBudget*" name="Budget{1}Form" input="/jsp/modules/research/budget/{1}.jsp" type="org.kuali.module.kra.budget.web.struts.action.Budget{1}Action" scope="request" parameter="methodToCall" validate="true" attribute="KualiForm">
            <forward name="basic" path="/jsp/modules/research/budget/Budget{1}.jsp" />
            <forward name="parameters" path="/jsp/modules/research/budget/BudgetParameters.jsp" redirect="false" />
            <forward name="overview" path="/jsp/modules/research/budget/BudgetOverview.jsp" redirect="false" />
            <forward name="template" path="/jsp/modules/research/budget/BudgetTemplate.jsp" redirect="false" />
            <forward name="nonpersonnel" path="/jsp/modules/research/budget/BudgetNonpersonnel.jsp" redirect="false" />
            <forward name="nonpersonnelCopyOver" path="/jsp/modules/research/budget/BudgetNonpersonnelCopyOver.jsp" redirect="false" />
            <forward name="personnel" path="/jsp/modules/research/budget/BudgetPersonnel.jsp" redirect="false" />
            <forward name="costshare" path="/jsp/modules/research/budget/BudgetCostShare.jsp" redirect="false" />
            <forward name="output" path="/jsp/modules/research/budget/BudgetOutput.jsp" redirect="false" />
            <forward name="modular" path="/jsp/modules/research/budget/BudgetModular.jsp" redirect="false" />
            <forward name="indirectcost" path="/jsp/modules/research/budget/BudgetIndirectCost.jsp" redirect="false" />
            <forward name="auditmode" path="/jsp/modules/research/budget/BudgetAuditMode.jsp" redirect="false" />
            <forward name="permissions" path="/jsp/modules/research/budget/BudgetPermissions.jsp" redirect="false" />
            <forward name="notes" path="/jsp/modules/research/budget/BudgetNotes.jsp" redirect="false" />
        </action>

        <action path="/researchRf*" name="Rf{1}Form" input="/jsp/modules/research/routingform/{1}.jsp" type="org.kuali.module.kra.routingform.web.struts.action.Rf{1}Action" scope="request" parameter="methodToCall" validate="true" attribute="KualiForm">
            <forward name="basic" path="/jsp/modules/research/routingform/Rf{1}.jsp" />
            <forward name="mainpage" path="/jsp/modules/research/routingform/RfMainPage.jsp" redirect="false" />
            <forward name="personnel" path="/jsp/modules/research/routingform/RfPersonnel.jsp" redirect="false" />
            <forward name="researchrisks" path="/jsp/modules/research/routingform/RfResearchRisks.jsp" redirect="false" />
            <forward name="projectdetails" path="/jsp/modules/research/routingform/RfProjectDetails.jsp" redirect="false" />       
            <forward name="agencyspecific" path="/jsp/modules/research/routingform/RfAgencySpecific.jsp" redirect="false" />       
            <forward name="sf424" path="/jsp/modules/research/routingform/RfSF424.jsp" redirect="false" />       
            <forward name="attachments" path="/jsp/modules/research/routingform/RfAttachments.jsp" redirect="false" />       
            <forward name="notes" path="/jsp/modules/research/routingform/RfNotes.jsp" redirect="false" />       
            <forward name="output" path="/jsp/modules/research/routingform/RfOutput.jsp" redirect="false" />       
            <forward name="template" path="/jsp/modules/research/routingform/RfTemplate.jsp" redirect="false" />       
            <forward name="auditmode" path="/jsp/modules/research/routingform/RfAuditMode.jsp" redirect="false" />
            <forward name="permissions" path="/jsp/modules/research/routingform/RfPermissions.jsp" redirect="false" />
            <forward name="approvals" path="/jsp/modules/research/routingform/RfApprovals.jsp" redirect="false" />
        </action>

        <action path="/cashManagementStatus" name="CashManagementStatusForm" type="org.kuali.module.financial.web.struts.action.CashManagementStatusAction" scope="request" attribute="KualiForm">
            <forward name="basic" path="/jsp/modules/financial/CashManagementStatus.jsp" />
        </action>

        <action path="/purap*" name="{1}Form" input="/jsp/modules/purap/{1}.jsp" type="org.kuali.module.purap.web.struts.action.{1}Action" scope="request" parameter="methodToCall" validate="true" attribute="KualiForm">
            <forward name="basic" path="/jsp/modules/purap/{1}.jsp" />
        </action>

    </action-mappings>

    <controller processorClass="org.kuali.core.web.struts.action.KualiRequestProcessor" maxFileSize="5M" />

    <message-resources parameter="ApplicationResources" />
    <message-resources key="kraResources" parameter="org.kuali.module.kra.budget.KraResources" />

    <plug-in className="org.kuali.core.web.struts.pojo.PojoPlugin" />
</struts-config>
