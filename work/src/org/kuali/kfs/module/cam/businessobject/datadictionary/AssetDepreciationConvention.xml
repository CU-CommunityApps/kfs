<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "../dataDictionary-1_0.dtd">
<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.module.cams.bo.AssetDepreciationConvention">
 
        <inquiry>
            <title>Asset Depreciation Convention Inquiry</title>
             
            <inquirySections>
                <inquirySection title="Header Text">
                    <inquiryFields>
                        <field attributeName="depreciationConventionCode" />
                        <field attributeName="financialObjectSubTypeCode" />
                    </inquiryFields>
                </inquirySection>
            </inquirySections>  
        </inquiry>

        <lookup>
            <title>Asset Depreciation Convention Lookup</title>

            <instructions>Look up an Asset Depreciation Convention</instructions>

            <defaultSort sortAscending="true">
                <sortAttributes>
                    <sortAttribute attributeName="financialObjectSubTypeCode" />
                </sortAttributes>
            </defaultSort>

            <lookupFields>
                <lookupField attributeName="depreciationConventionCode" required="false" />
                <lookupField attributeName="financialObjectSubTypeCode" required="false" />
            </lookupFields>

            <resultFields>
                <field attributeName="depreciationConventionCode" />
                <field attributeName="financialObjectSubTypeCode" />
            </resultFields>
        </lookup>

        <attributes>

            <attribute name="depreciationConventionCode" forceUppercase="true">
                <label>Depreciation Convention Code</label>
                <shortLabel>Code</shortLabel>

                <maxLength>2</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="4" />
                </control>

                <summary>depreciationConventionCode</summary>
                <description>depreciationConventionCode description...</description>
			</attribute>

            <attribute name="financialObjectSubTypeCode" forceUppercase="true">
                <label>Financial Object Sub Type Code</label>
                <shortLabel>Code</shortLabel>

                <maxLength>2</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="4" />
                </control>

                <summary>financialObjectSubTypeCode</summary>
                <description>financialObjectSubTypeCode description...</description>
			</attribute>

            <attributeReference name="versionNumber" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="versionNumber" />

        </attributes>
    </businessObject>
</dictionaryEntry>
