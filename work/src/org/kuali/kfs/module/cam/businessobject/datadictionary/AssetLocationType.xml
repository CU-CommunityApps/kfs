<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "../dataDictionary-1_0.dtd">
<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.module.cams.bo.AssetLocationType">
 
        <inquiry>
            <title>Asset Location Type Inquiry</title>
             
            <inquirySections>
                <inquirySection title="Header Text">
                    <inquiryFields>
                        <field attributeName="active" />
                        <field attributeName="assetLocationTypeCode" />
                        <field attributeName="assetLocationTypeName" />
                    </inquiryFields>
                </inquirySection>
            </inquirySections>  
        </inquiry>

        <lookup>
            <title>Asset Location Type Lookup</title>

            <instructions>Look up an Asset Location Type</instructions>

            <defaultSort sortAscending="true">
                <sortAttributes>
                    <sortAttribute attributeName="assetLocationTypeCode" />
                </sortAttributes>
            </defaultSort>

            <lookupFields>
                <lookupField attributeName="active" required="false" />
                <lookupField attributeName="assetLocationTypeCode" required="false" />
                <lookupField attributeName="assetLocationTypeName" required="false" />
            </lookupFields>

            <resultFields>
                <field attributeName="active" />
                <field attributeName="assetLocationTypeCode" />
                <field attributeName="assetLocationTypeName" />
            </resultFields>
        </lookup>

        <attributes>

            <attribute name="active" forceUppercase="true">
                <label>Active</label>
                <shortLabel>Active</shortLabel>

                <maxLength>1</maxLength>

                <control>
                    <checkbox />
                </control>

                <summary>active</summary>
                <description>active description...</description>
			</attribute>

            <attribute name="assetLocationTypeCode" forceUppercase="true">
                <label>Asset Location Type Code</label>
                <shortLabel>Code</shortLabel>

                <maxLength>2</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="4" />
                </control>

                <summary>assetLocationTypeCode</summary>
                <description>assetLocationTypeCode description...</description>
			</attribute>

            <attribute name="assetLocationTypeName" forceUppercase="true">
                <label>Asset Location Type Name</label>
                <shortLabel>Name</shortLabel>

                <maxLength>30</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="32" />
                </control>

                <summary>assetLocationTypeName</summary>
                <description>assetLocationTypeName description...</description>
			</attribute>

            <attributeReference name="versionNumber" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="versionNumber" />

        </attributes>
    </businessObject>
</dictionaryEntry>
