<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "../dataDictionary-1_0.dtd">
<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.module.cams.bo.Pretag">
 
        <inquiry>
            <title>Pretag Inquiry</title>
             
            <inquirySections>
                <inquirySection title="Header Text">
                    <inquiryFields>
                        <field attributeName="purchaseOrderNumber" />
                        <field attributeName="lineItemNumber" />
                        <field attributeName="quantityInvoiced" />
                        <field attributeName="capitalAssetTypeCode" />
                        <field attributeName="manufacturerName" />
                        <field attributeName="manufacturerModelNumber" />
                        <field attributeName="vendorName" />
                        <field attributeName="organizationCode" />
                        <field attributeName="organizationDescription" />
                        <field attributeName="organizationInventoryName" />
	                    <field attributeName="personUniversal.personName" />
                        <field attributeName="organizationText" />
                        <field attributeName="assetTopsDescription" />
                        <field attributeName="chartOfAccountsCode" />
                        <field attributeName="pretagCreateDate" />
                        <field attributeName="active" />
                    </inquiryFields>
                </inquirySection>
                
                <inquirySection title="Pretag Details">
                    <inquiryFields>
                        <inquiryCollection name="pretagDetails" businessObjectClass="org.kuali.module.cams.bo.PretagDetail"  summaryTitle="Pretag Detail" >
                        	<field attributeName="campusTagNumber" />
                        	<field attributeName="serialNumber" />
                        	<field attributeName="organizationTagNumber" />
                        	<field attributeName="governmentTagNumber" />
                        	<field attributeName="nationalStockNumber" />
                        	<field attributeName="campusCode" />
                        	<field attributeName="buildingCode" />
                        	<field attributeName="buildingRoomNumber" />
                        	<field attributeName="buildingSubRoomNumber" />
                        	<field attributeName="pretagTagCreateDate" />
                        	<field attributeName="active" />
                        	<inquirySummaryFields>
                                <inquirySummaryField attributeName="campusTagNumber" />
                                <inquirySummaryField attributeName="serialNumber" />
                            </inquirySummaryFields>
                        </inquiryCollection>
                    </inquiryFields>
                </inquirySection>                           
                
            </inquirySections>  
        </inquiry>

        <lookup>
            <title>Pretag Lookup</title>

            <instructions>Look up a Pretag</instructions>

            <defaultSort sortAscending="true">
                <sortAttributes>
                    <sortAttribute attributeName="purchaseOrderNumber" />
                    <sortAttribute attributeName="lineItemNumber" />
               </sortAttributes>
            </defaultSort>

            <lookupFields>
                <lookupField attributeName="purchaseOrderNumber" required="false" />
                <lookupField attributeName="lineItemNumber" required="false" />
                <lookupField attributeName="quantityInvoiced" required="false" />
                <lookupField attributeName="pretagDetails.campusTagNumber" required="false" />
                <lookupField attributeName="pretagDetails.serialNumber" required="false" />
                <lookupField attributeName="assetTopsDescription" required="false" />
                <lookupField attributeName="active" required="false" />
                	<defaultValue>Y</defaultValue>
                </lookupField>
            </lookupFields>

            <resultFields>
                <field attributeName="purchaseOrderNumber" />
                <field attributeName="lineItemNumber" />
                <field attributeName="quantityInvoiced" />
                <field attributeName="campusTagNumbers"/>
                <field attributeName="serialNumbers"/>
                <field attributeName="assetTopsDescription" />
                <field attributeName="active" />
            </resultFields>
        </lookup>

        <attributes>
            <attributeReference name="capitalAssetTypeCode" sourceClassName="org.kuali.module.cams.bo.AssetType" sourceAttributeName="capitalAssetTypeCode" >
            	<label>Captial Asset Type Code</label>
                <shortLabel>Asset Type Code</shortLabel>
                <required>true</required>
            	<control>
            		<select valuesFinderClass="org.kuali.module.cams.lookup.keyvalues.AssetTypeValuesFinder" />
            	</control>
            </attributeReference>

            <attributeReference name="organizationCode" sourceClassName="org.kuali.module.chart.bo.Org" sourceAttributeName="organizationCode">
                <label>Organization Code</label>
                <shortLabel>Org Cd</shortLabel>
                <required>false</required>
            </attributeReference>

            <attributeReference name="personUniversal.personUserIdentifier" sourceClassName="org.kuali.core.bo.user.UniversalUser" sourceAttributeName="personUserIdentifier">
                <label>Representative Universal Identifier</label>
                <shortLabel>Identifier</shortLabel>
                <required>false</required>
                <control>
                    <kualiUser universalIdAttributeName="representativeUniversalIdentifier" userIdAttributeName="personUniversal.personUserIdentifier" personNameAttributeName="personUniversal.personName" />
                </control>
            </attributeReference>
            

            <attributeReference name="personUniversal.personName" sourceClassName="org.kuali.core.bo.user.UniversalUser" sourceAttributeName="personName">
                <required>false</required>
                <control>
                    <hidden />
                </control>
            </attributeReference>

            <attributeReference name="active" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="activeIndicator" />

            <attributeReference name="chartOfAccountsCode" sourceClassName="org.kuali.module.chart.bo.Chart" sourceAttributeName="chartOfAccountsCode">
                <label>Chart of Accounts Code</label>
                <shortLabel>Fiscal Chart</shortLabel>
                <required>true</required>
                <control>
                    <select valuesFinderClass="org.kuali.module.chart.lookup.keyvalues.ChartValuesFinder" />
                </control>
            </attributeReference>
            
            <attributeReference name="purchaseOrderNumber" sourceClassName="org.kuali.module.cams.bo.PendingPurchaseOrder" sourceAttributeName="purchaseOrderNumber">
                <label>Purchase Order Number</label>
                <shortLabel>Number</shortLabel>
                <required>true</required>
            </attributeReference>

            <attributeReference name="pretagCreateDate" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="genericTimestamp">
                <label>Pretag Create Date</label>
                <shortLabel>Date</shortLabel>
            </attributeReference>

            <attribute name="assetTopsDescription">
                <label>Purchase Order Line Item Description</label>
                <shortLabel>Description</shortLabel>

                <maxLength>2000</maxLength>
                <validationPattern><anyCharacter allowWhitespace="true" /></validationPattern>

                <control>
                    <textarea rows="5" cols="80"/>
                </control>

                <summary>assetTopsDescription</summary>
                <description>assetTopsDescription description...</description>
			</attribute>

            <attribute name="lineItemNumber" >
                <label>Line Item Number</label>
                <shortLabel>Number</shortLabel>

                <maxLength>12</maxLength>
                <validationPattern><numeric /></validationPattern>

                <control>
                    <text size="14" />
                </control>

                <summary>lineItemNumber</summary>
                <description>lineItemNumber description...</description>
			</attribute>

            <attribute name="manufacturerModelNumber" forceUppercase="true">
                <label>Manufacturer Model Number</label>
                <shortLabel>Number</shortLabel>

                <maxLength>25</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="27" />
                </control>

                <summary>manufacturerModelNumber</summary>
                <description>manufacturerModelNumber description...</description>
			</attribute>

            <attribute name="manufacturerName" forceUppercase="true">
                <label>Manufacturer Name</label>
                <shortLabel>Name</shortLabel>

                <maxLength>40</maxLength>
                <validationPattern><anyCharacter allowWhitespace="true"/></validationPattern>

                <control>
                    <text size="42" />
                </control>

                <summary>manufacturerName</summary>
                <description>manufacturerName description...</description>
			</attribute>

            <attribute name="organizationDescription" >
                <label>Organization Description</label>
                <shortLabel>Description</shortLabel>

                <maxLength>255</maxLength>
                <validationPattern><anyCharacter allowWhitespace="true" /></validationPattern>

                <control>
                    <textarea rows="5" cols="80"/>
                </control>

                <summary>organizationDescription</summary>
                <description>organizationDescription description...</description>
			</attribute>

            <attribute name="organizationInventoryName" forceUppercase="true">
                <label>Organization Inventory Name</label>
                <shortLabel>Name</shortLabel>

                <maxLength>40</maxLength>
                <validationPattern><anyCharacter allowWhitespace="true"/></validationPattern>

                <control>
                    <text size="42" />
                </control>

                <summary>organizationInventoryName</summary>
                <description>organizationInventoryName description...</description>
			</attribute>

            <attribute name="organizationText" >
                <label>Organization Text</label>
                <shortLabel>Text</shortLabel>

                <maxLength>255</maxLength>
                <validationPattern><anyCharacter allowWhitespace="true" /></validationPattern>

                <control>
                    <textarea rows="5" cols="80"/>
                </control>

                <summary>organizationText</summary>
                <description>organizationText description...</description>
			</attribute>

            <attribute name="quantityInvoiced" >
                <label>Quantity Invoiced</label>
                <shortLabel>Invoiced</shortLabel>

                <maxLength>9</maxLength>
                <validationPattern><floatingPoint /></validationPattern>

                <control>
                    <text size="11" />
                </control>

                <summary>quantityInvoiced</summary>
                <description>quantityInvoiced description...</description>
			</attribute>

            <attribute name="vendorName" forceUppercase="true">
                <label>Vendor Name</label>
                <shortLabel>Name</shortLabel>

                <maxLength>40</maxLength>
                <validationPattern><anyCharacter allowWhitespace="true" /></validationPattern>

                <control>
                    <text size="42" />
                </control>

                <summary>vendorName</summary>
                <description>vendorName description...</description>
			</attribute>
			
            <attribute name="campusTagNumbers" >
                <label>Campus Tag Number</label>
                <shortLabel>Number</shortLabel>

                <maxLength>8</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="10" />
                </control>
                <summary>campusTagNumber</summary>
                <description>campusTagNumber description...</description>
			</attribute>
			 			
            <attribute name="serialNumbers" >
                <label>Serial Number</label>
                <shortLabel>Number</shortLabel>

                <maxLength>25</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="27" />
                </control>

                <summary>serialNumber</summary>
                <description>serialNumber description...</description>
			</attribute>

            <attributeReference name="pretagDetails.campusTagNumber" sourceClassName="org.kuali.module.cams.bo.PretagDetail" sourceAttributeName="campusTagNumber"/>
            <attributeReference name="pretagDetails.serialNumber" sourceClassName="org.kuali.module.cams.bo.PretagDetail" sourceAttributeName="serialNumber"/>

            <attributeReference name="versionNumber" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="versionNumber" />

        </attributes>
 
        <collections>
            <collection name="pretagDetails">
                <label>Pretag Details</label>
                <shortLabel>Details</shortLabel>
                <elementLabel>Details</elementLabel>
             </collection>
        </collections> 
           
        <relationships>
            <relationship objectAttributeName="personUniversal" >
                <primitiveAttribute sourceName="representativeUniversalIdentifier" targetName="personUniversalIdentifier" />
				<supportAttribute sourceName="personUniversal.personUserIdentifier" targetName="personUserIdentifier" identifier="true" />
				<supportAttribute sourceName="personUniversal.personName" targetName="personName" />
            </relationship>
        </relationships>

    </businessObject>
</dictionaryEntry>
