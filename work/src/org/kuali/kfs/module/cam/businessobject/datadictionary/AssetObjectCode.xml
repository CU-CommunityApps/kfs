<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "../dataDictionary-1_0.dtd">
<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.module.cams.bo.AssetObjectCode">
 
        <inquiry>
            <title>Asset Object Code Inquiry</title>
             
            <inquirySections>
                <inquirySection title="Header Text">
                    <inquiryFields>
                        <field attributeName="universityFiscalYear" />
                        <field attributeName="chartOfAccountsCode" />
                        <field attributeName="financialObjectSubTypeCode" />
                        <field attributeName="capitalizationFinancialObjectCode" />
                        <field attributeName="accumulatedDepreciationFinancialObjectCode" />
                        <field attributeName="depreciationExpenseFinancialObjectCode" />
                        <field attributeName="active" />
                    </inquiryFields>
                </inquirySection>
            </inquirySections>  
        </inquiry>

        <lookup>
            <title>Asset Object Code Lookup</title>

            <instructions>Look up an Asset Object Code</instructions>

            <defaultSort sortAscending="true">
                <sortAttributes>
                    <sortAttribute attributeName="universityFiscalYear" />
                    <sortAttribute attributeName="chartOfAccountsCode" />
                    <sortAttribute attributeName="financialObjectSubTypeCode" />
                </sortAttributes>
            </defaultSort>

            <lookupFields>
			    <lookupField attributeName="universityFiscalYear" required="true">
                    <defaultValueFinderClass>org.kuali.kfs.lookup.valuefinder.FiscalYearFinder</defaultValueFinderClass>
                </lookupField>
                <lookupField attributeName="chartOfAccountsCode" required="false" />
                <lookupField attributeName="financialObjectSubTypeCode" required="false" />
                <lookupField attributeName="capitalizationFinancialObjectCode" required="false" />
                <lookupField attributeName="accumulatedDepreciationFinancialObjectCode" required="false" />
                <lookupField attributeName="depreciationExpenseFinancialObjectCode" required="false" />
                <lookupField attributeName="active" required="false">
                	<defaultValue>Y</defaultValue>
                </lookupField>
            </lookupFields>

            <resultFields>
                <field attributeName="universityFiscalYear" />
                <field attributeName="chartOfAccountsCode" />
                <field attributeName="financialObjectSubTypeCode" />
                <field attributeName="capitalizationFinancialObjectCode" />
                <field attributeName="accumulatedDepreciationFinancialObjectCode" />
                <field attributeName="depreciationExpenseFinancialObjectCode" />
                <field attributeName="active" />
            </resultFields>
        </lookup>

        <attributes>

            <attributeReference name="universityFiscalYear" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="genericFiscalYear" />

			<attributeReference name="chartOfAccountsCode" sourceClassName="org.kuali.module.chart.bo.Chart" sourceAttributeName="chartOfAccountsCode" />

            <attributeReference name="financialObjectSubTypeCode" sourceClassName="org.kuali.module.chart.bo.ObjectCode" sourceAttributeName="financialObjectSubTypeCode" />

            <attributeReference name="capitalizationFinancialObjectCode" sourceClassName="org.kuali.module.chart.bo.ObjectCode" sourceAttributeName="financialObjectCode" >
                <label>Capitalization Financial Object Code</label>
                <shortLabel>Code</shortLabel>
            </attributeReference>
        
            <attributeReference name="accumulatedDepreciationFinancialObjectCode" sourceClassName="org.kuali.module.chart.bo.ObjectCode" sourceAttributeName="financialObjectCode" >
                <label>Accumulated Depreciation Financial Object Code</label>
                <shortLabel>Code</shortLabel>
            </attributeReference>

            <attributeReference name="depreciationExpenseFinancialObjectCode" sourceClassName="org.kuali.module.chart.bo.ObjectCode" sourceAttributeName="financialObjectCode" >
                <label>Depreciation Expense Financial Object Code</label>
                <shortLabel>Code</shortLabel>
            </attributeReference>

            <attributeReference name="active" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="activeIndicator" />

            <attributeReference name="versionNumber" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="versionNumber" />

        </attributes>
    </businessObject>
</dictionaryEntry>
