                    <inquiryFields>
                        <field attributeName="buildingCode" />
                        <field attributeName="buildingRoomNumber" />
                        <field attributeName="buildingSubRoomNumber" />
                        <field attributeName="campusCode" />
                        <field attributeName="campusTagNumber" />
                        <field attributeName="capitalAssetNumber" />
