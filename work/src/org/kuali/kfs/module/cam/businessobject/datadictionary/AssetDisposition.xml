<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="AssetDisposition" parent="AssetDisposition-parentBean" />

  <bean id="AssetDisposition-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.kfs.module.cam.businessobject.AssetDisposition" />
    <property name="inquiryDefinition" >
      <ref bean="AssetDisposition-inquiryDefinition" />
    </property>
    <property name="lookupDefinition" >
      <ref bean="AssetDisposition-lookupDefinition" />
    </property>
    <property name="attributes" >
      <list>
        <ref bean="AssetDisposition-capitalAssetNumber" />
        <ref bean="AssetDisposition-dispositionCode" />
        <ref bean="AssetDisposition-dispositionDate" />
        <ref bean="AssetDisposition-dispositionOriginationCode" />
        <ref bean="AssetDisposition-originalCapitalAssetNumber" />
        <ref bean="AssetDisposition-versionNumber" />
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->


  <bean id="AssetDisposition-capitalAssetNumber" parent="AssetDisposition-capitalAssetNumber-parentBean" />

  <bean id="AssetDisposition-capitalAssetNumber-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="capitalAssetNumber" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Capital Asset Number" />
    <property name="shortLabel" value="Number" />
    <property name="maxLength" value="12" />
    <property name="validationPattern" >
      <bean parent="NumericValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="14" />
    </property>
    <property name="summary" value="capitalAssetNumber" />
    <property name="description" value="capitalAssetNumber description..." />
  </bean>
  <bean id="AssetDisposition-dispositionCode" parent="AssetDisposition-dispositionCode-parentBean" />

  <bean id="AssetDisposition-dispositionCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="dispositionCode" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Disposition Code" />
    <property name="shortLabel" value="Code" />
    <property name="maxLength" value="2" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="4" />
    </property>
    <property name="summary" value="dispositionCode" />
    <property name="description" value="dispositionCode description..." />
  </bean>
  <bean id="AssetDisposition-dispositionDate" parent="AssetDisposition-dispositionDate-parentBean" />

  <bean id="AssetDisposition-dispositionDate-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="dispositionDate" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Disposition Date" />
    <property name="shortLabel" value="Date" />
    <property name="maxLength" value="7" />
    <property name="validationPattern" >
      <bean parent="DateValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="9" />
    </property>
    <property name="summary" value="dispositionDate" />
    <property name="description" value="dispositionDate description..." />
  </bean>
  <bean id="AssetDisposition-dispositionOriginationCode" parent="AssetDisposition-dispositionOriginationCode-parentBean" />

  <bean id="AssetDisposition-dispositionOriginationCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="dispositionOriginationCode" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Disposition Origination Code" />
    <property name="shortLabel" value="Code" />
    <property name="maxLength" value="2" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="4" />
    </property>
    <property name="summary" value="dispositionOriginationCode" />
    <property name="description" value="dispositionOriginationCode description..." />
  </bean>
  <bean id="AssetDisposition-originalCapitalAssetNumber" parent="AssetDisposition-originalCapitalAssetNumber-parentBean" />

  <bean id="AssetDisposition-originalCapitalAssetNumber-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="originalCapitalAssetNumber" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Original Capital Asset Number" />
    <property name="shortLabel" value="Number" />
    <property name="maxLength" value="12" />
    <property name="validationPattern" >
      <bean parent="NumericValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="14" />
    </property>
    <property name="summary" value="originalCapitalAssetNumber" />
    <property name="description" value="originalCapitalAssetNumber description..." />
  </bean>
  <bean id="AssetDisposition-versionNumber" parent="AssetDisposition-versionNumber-parentBean" />

  <bean id="AssetDisposition-versionNumber-parentBean" abstract="true" parent="GenericAttributes-versionNumber">
  </bean>

<!-- Business Object Inquiry Definition -->


  <bean id="AssetDisposition-inquiryDefinition" parent="AssetDisposition-inquiryDefinition-parentBean" />

  <bean id="AssetDisposition-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
    <property name="title" value="Asset Disposition Inquiry" />
    <property name="inquirySections" >
      <list>
        <bean parent="InquirySectionDefinition">
          <property name="title" value="Header Text" />
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields" >
            <list>
              <bean parent="FieldDefinition"
                    p:attributeName="capitalAssetNumber" />
              <bean parent="FieldDefinition"
                    p:attributeName="dispositionCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="dispositionDate" />
              <bean parent="FieldDefinition"
                    p:attributeName="dispositionOriginationCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="originalCapitalAssetNumber" />
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Business Object Lookup Definition -->


  <bean id="AssetDisposition-lookupDefinition" parent="AssetDisposition-lookupDefinition-parentBean" />

  <bean id="AssetDisposition-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
    <property name="title" value="Asset Disposition Lookup" />
    <property name="instructions" value="Look up an Asset Disposition" />
    <property name="defaultSort" >
      <bean parent="SortDefinition">
      </bean>
    </property>
    <property name="lookupFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="capitalAssetNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="dispositionCode" />
        <bean parent="FieldDefinition"
              p:attributeName="dispositionDate" />
        <bean parent="FieldDefinition"
              p:attributeName="dispositionOriginationCode" />
        <bean parent="FieldDefinition"
              p:attributeName="originalCapitalAssetNumber" />
      </list>
    </property>
    <property name="resultFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="capitalAssetNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="dispositionCode" />
        <bean parent="FieldDefinition"
              p:attributeName="dispositionDate" />
        <bean parent="FieldDefinition"
              p:attributeName="dispositionOriginationCode" />
        <bean parent="FieldDefinition"
              p:attributeName="originalCapitalAssetNumber" />
      </list>
    </property>
  </bean>
</beans>
