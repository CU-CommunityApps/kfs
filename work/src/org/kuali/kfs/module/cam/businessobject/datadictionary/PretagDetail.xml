<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "../dataDictionary-1_0.dtd">
<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.module.cams.bo.PretagDetail">
 
        <inquiry>
            <title>Pretag Detail Inquiry</title>
             
            <inquirySections>
                <inquirySection title="Header Text">
                    <inquiryFields>
                        <field attributeName="purchaseOrderNumber" />
                        <field attributeName="lineItemNumber" />
                        <field attributeName="campusTagNumber" />
                        <field attributeName="serialNumber" />
                        <field attributeName="organizationTagNumber" />
                        <field attributeName="governmentTagNumber" />
                        <field attributeName="nationalStockNumber" />                       
                        <field attributeName="campusCode" />
                        <field attributeName="buildingCode" />
                        <field attributeName="buildingRoomNumber" />
                        <field attributeName="buildingSubRoomNumber" />
                         <field attributeName="pretagTagCreateDate" />
                    </inquiryFields>
                </inquirySection>
            </inquirySections>  
        </inquiry>

        <attributes>

            <attributeReference name="purchaseOrderNumber" sourceClassName="org.kuali.module.cams.bo.Pretag" sourceAttributeName="purchaseOrderNumber" >
                 <required>true</required>
                <control>
                    <hidden />
                </control>
            </attributeReference>
            
            <attributeReference name="lineItemNumber" sourceClassName="org.kuali.module.cams.bo.Pretag" sourceAttributeName="lineItemNumber" >
                <required>true</required>
                <control>
                    <hidden />
                </control>
             </attributeReference>
 
            <attributeReference name="campusCode" sourceClassName="org.kuali.core.bo.Campus" sourceAttributeName="campusCode">
                <control>
                    <select valuesFinderClass="org.kuali.core.lookup.keyvalues.CampusValuesFinder" />
                </control>
            </attributeReference>
            
            <attributeReference name="pretagTagCreateDate" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="genericTimestamp">
                <label>Pretag Tag Create Date</label>
                <shortLabel>Date</shortLabel>
            </attributeReference>


            <attributeReference name="buildingCode" sourceClassName="org.kuali.kfs.bo.Building" sourceAttributeName="buildingCode" />

            <attributeReference name="buildingRoomNumber" sourceClassName="org.kuali.kfs.bo.Room" sourceAttributeName="buildingRoomNumber" />

            <attribute name="buildingSubRoomNumber" forceUppercase="true">
                <label>Building Sub Room Number</label>
                <shortLabel>Number</shortLabel>

                <maxLength>2</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="4" />
                </control>

                <summary>buildingSubRoomNumber</summary>
                <description>buildingSubRoomNumber description...</description>
			</attribute>


            <attribute name="campusTagNumber" forceUppercase="true">
                <label>Tag Number</label>
                <shortLabel>Number</shortLabel>

                <maxLength>8</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="10" />
                </control>

                <summary>campusTagNumber</summary>
                <description>campusTagNumber description...</description>
			</attribute>

            <attribute name="organizationTagNumber" forceUppercase="true">
                <label>Organization Tag Number</label>
                <shortLabel>Number</shortLabel>

                <maxLength>8</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="10" />
                </control>

                <summary>organizationTagNumber</summary>
                <description>organizationTagNumber description...</description>
			</attribute>
			
            <attribute name="governmentTagNumber" forceUppercase="true">
                <label>Government Tag Number</label>
                <shortLabel>Number</shortLabel>

                <maxLength>12</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="14" />
                </control>

                <summary>governmentTagNumber</summary>
                <description>governmentTagNumber description...</description>
			</attribute>
			
            <attribute name="nationalStockNumber" forceUppercase="true">
                <label>National Stock Number (NSN)</label>
                <shortLabel>Number</shortLabel>

                <maxLength>25</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="27" />
                </control>

                <summary>nationalStockNumber</summary>
                <description>nationalStockNumber description...</description>
			</attribute>

            <attribute name="serialNumber" forceUppercase="true">
                <label>Serial Number</label>
                <shortLabel>Number</shortLabel>

                <maxLength>25</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="27" />
                </control>

                <summary>serialNumber</summary>
                <description>serialNumber description...</description>
			</attribute>

            <attributeReference name="versionNumber" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="versionNumber" />

        </attributes>
    </businessObject>
</dictionaryEntry>
