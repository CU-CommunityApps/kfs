        <ref bean="PendingPurchaseOrder-deliverToAddress1" />
        <ref bean="PendingPurchaseOrder-deliverToAddress2" />
        <ref bean="PendingPurchaseOrder-deliverToName" />
        <ref bean="PendingPurchaseOrder-departmentRepresentative" />
        <ref bean="PendingPurchaseOrder-purchaseOrderNumber" />
        <ref bean="PendingPurchaseOrder-requestAddress1" />
        <ref bean="PendingPurchaseOrder-requestAddress2" />
        <ref bean="PendingPurchaseOrder-requestName" />
