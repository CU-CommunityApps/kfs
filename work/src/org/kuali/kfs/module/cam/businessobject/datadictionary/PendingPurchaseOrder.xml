<!-- Business Object Inquiry Definition -->


  <bean id="PendingPurchaseOrder-inquiryDefinition" parent="PendingPurchaseOrder-inquiryDefinition-parentBean" />

  <bean id="PendingPurchaseOrder-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
    <property name="title" value="Pending Purchase Order Inquiry" />
    <property name="inquirySections" >
      <list>
        <bean parent="InquirySectionDefinition">
          <property name="title" value="Header Text" />
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields" >
            <list>
              <bean parent="FieldDefinition"
                    p:attributeName="deliverToAddress1" />
              <bean parent="FieldDefinition"
                    p:attributeName="deliverToAddress2" />
              <bean parent="FieldDefinition"
                    p:attributeName="deliverToName" />
              <bean parent="FieldDefinition"
                    p:attributeName="departmentRepresentative" />
              <bean parent="FieldDefinition"
                    p:attributeName="purchaseOrderNumber" />
