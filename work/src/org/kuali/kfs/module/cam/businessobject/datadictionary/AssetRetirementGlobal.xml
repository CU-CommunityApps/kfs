<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "../dataDictionary-1_0.dtd">
<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.module.cams.bo.AssetRetirementGlobal">
 
        <inquiry>
            <title>Asset Retirement Global Inquiry</title>
             
            <inquirySections>
                <inquirySection title="Header Text">
                    <inquiryFields>
                        <field attributeName="documentNumber" />
                        <field attributeName="inventoryStatusCode" />
                        <field attributeName="mergedTargetCapitalAssetNumber" />
                        <field attributeName="retirementDate" />
                        <field attributeName="retirementReasonCode" />
                    </inquiryFields>
                </inquirySection>
            </inquirySections>  
        </inquiry>

        <lookup>
            <title>Asset Retirement Global Lookup</title>

            <instructions>Look up an Asset Retirement Global</instructions>

            <defaultSort sortAscending="true">
                <sortAttributes>
                    <sortAttribute attributeName="documentNumber" />
                </sortAttributes>
            </defaultSort>

            <lookupFields>
                <lookupField attributeName="documentNumber" required="false" />
                <lookupField attributeName="inventoryStatusCode" required="false" />
                <lookupField attributeName="mergedTargetCapitalAssetNumber" required="false" />
                <lookupField attributeName="retirementDate" required="false" />
                <lookupField attributeName="retirementReasonCode" required="false" />
            </lookupFields>

            <resultFields>
                <field attributeName="documentNumber" />
                <field attributeName="inventoryStatusCode" />
                <field attributeName="mergedTargetCapitalAssetNumber" />
                <field attributeName="retirementDate" />
                <field attributeName="retirementReasonCode" />
            </resultFields>
        </lookup>

        <attributes>

            <attribute name="documentNumber" forceUppercase="true">
                <label>Document Number</label>
                <shortLabel>Number</shortLabel>

                <maxLength>14</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="16" />
                </control>

                <summary>documentNumber</summary>
                <description>documentNumber description...</description>
			</attribute>

            <attribute name="inventoryStatusCode" forceUppercase="true">
                <label>Inventory Status Code</label>
                <shortLabel>Code</shortLabel>

                <maxLength>1</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="3" />
                </control>

                <summary>inventoryStatusCode</summary>
                <description>inventoryStatusCode description...</description>
			</attribute>

            <attribute name="mergedTargetCapitalAssetNumber" forceUppercase="true">
                <label>Merged Target Capital Asset Number</label>
                <shortLabel>Number</shortLabel>

                <maxLength>12</maxLength>
                <validationPattern><numeric /></validationPattern>

                <control>
                    <text size="14" />
                </control>

                <summary>mergedTargetCapitalAssetNumber</summary>
                <description>mergedTargetCapitalAssetNumber description...</description>
			</attribute>

            <attributeReference name="retirementDate" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="genericDate">
                <label>Retirement Date</label>
                <shortLabel>Date</shortLabel>
            </attributeReference>
            
            <attribute name="retirementReasonCode" forceUppercase="true">
                <label>Retirement Reason Code</label>
                <shortLabel>Code</shortLabel>

                <maxLength>1</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                	<select
                		valuesFinderClass="org.kuali.core.lookup.keyvalues.PersistableBusinessObjectValuesFinder"
						businessObjectClass="org.kuali.module.cams.bo.AssetRetirementReason"
						keyAttribute="retirementReasonCode"
						labelAttribute="retirementReasonName" includeKeyInLabel="true" />
                    <!-- text size="3" /-->
                </control>

                <summary>retirementReasonCode</summary>
                <description>retirementReasonCode description...</description>
			</attribute>

            <attributeReference name="sharedRetirementInfo.retirementChartOfAccountsCode" sourceClassName="org.kuali.module.cams.bo.AssetRetirementGlobalDetail" sourceAttributeName="retirementChartOfAccountsCode" />
            <attributeReference name="sharedRetirementInfo.retirementAccountNumber" sourceClassName="org.kuali.module.cams.bo.AssetRetirementGlobalDetail" sourceAttributeName="retirementAccountNumber" />
            <attributeReference name="sharedRetirementInfo.retirementContactName" sourceClassName="org.kuali.module.cams.bo.AssetRetirementGlobalDetail" sourceAttributeName="retirementContactName" />
            <attributeReference name="sharedRetirementInfo.retirementInstitutionName" sourceClassName="org.kuali.module.cams.bo.AssetRetirementGlobalDetail" sourceAttributeName="retirementInstitutionName" />
            <attributeReference name="sharedRetirementInfo.retirementStreetAddress" sourceClassName="org.kuali.module.cams.bo.AssetRetirementGlobalDetail" sourceAttributeName="retirementStreetAddress" />
            <attributeReference name="sharedRetirementInfo.retirementCityName" sourceClassName="org.kuali.module.cams.bo.AssetRetirementGlobalDetail" sourceAttributeName="retirementCityName" />
            <attributeReference name="sharedRetirementInfo.retirementStateCode" sourceClassName="org.kuali.module.cams.bo.AssetRetirementGlobalDetail" sourceAttributeName="retirementStateCode" />
            <attributeReference name="sharedRetirementInfo.retirementZipCode" sourceClassName="org.kuali.module.cams.bo.AssetRetirementGlobalDetail" sourceAttributeName="retirementZipCode" />
            <attributeReference name="sharedRetirementInfo.retirementCountryCode" sourceClassName="org.kuali.module.cams.bo.AssetRetirementGlobalDetail" sourceAttributeName="retirementCountryCode" />
            <attributeReference name="sharedRetirementInfo.retirementPhoneNumber" sourceClassName="org.kuali.module.cams.bo.AssetRetirementGlobalDetail" sourceAttributeName="retirementPhoneNumber" />
            <attributeReference name="sharedRetirementInfo.estimatedSellingPrice" sourceClassName="org.kuali.module.cams.bo.AssetRetirementGlobalDetail" sourceAttributeName="estimatedSellingPrice" />
            <attributeReference name="sharedRetirementInfo.salePrice" sourceClassName="org.kuali.module.cams.bo.AssetRetirementGlobalDetail" sourceAttributeName="salePrice" />
            <attributeReference name="sharedRetirementInfo.cashReceiptFinancialDocumentNumber" sourceClassName="org.kuali.module.cams.bo.AssetRetirementGlobalDetail" sourceAttributeName="cashReceiptFinancialDocumentNumber" />
            <attributeReference name="sharedRetirementInfo.handlingFeeAmount" sourceClassName="org.kuali.module.cams.bo.AssetRetirementGlobalDetail" sourceAttributeName="handlingFeeAmount" />
            <attributeReference name="sharedRetirementInfo.preventiveMaintenanceAmount" sourceClassName="org.kuali.module.cams.bo.AssetRetirementGlobalDetail" sourceAttributeName="preventiveMaintenanceAmount" />
            <attributeReference name="sharedRetirementInfo.buyerDescription" sourceClassName="org.kuali.module.cams.bo.AssetRetirementGlobalDetail" sourceAttributeName="buyerDescription" />
            <attributeReference name="sharedRetirementInfo.paidCaseNumber" sourceClassName="org.kuali.module.cams.bo.AssetRetirementGlobalDetail" sourceAttributeName="paidCaseNumber" />
            
            <attributeReference name="versionNumber" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="versionNumber" />

        </attributes>
    </businessObject>
</dictionaryEntry>
