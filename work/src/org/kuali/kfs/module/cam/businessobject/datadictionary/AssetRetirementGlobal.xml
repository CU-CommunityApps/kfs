<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "../dataDictionary-1_0.dtd">
<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.module.cams.bo.AssetRetirementGlobal">
 
        <inquiry>
            <title>Asset Retirement Global Inquiry</title>
             
            <inquirySections>
                <inquirySection title="Header Text">
                    <inquiryFields>
                        <field attributeName="documentNumber" />
                        <field attributeName="inventoryStatusCode" />
                        <field attributeName="mergedTargetCapitalAssetNumber" />
                        <field attributeName="retirementDate" />
                        <field attributeName="retirementReasonCode" />
                    </inquiryFields>
                </inquirySection>
            </inquirySections>  
        </inquiry>

        <lookup>
            <title>Asset Retirement Global Lookup</title>

            <instructions>Look up an Asset Retirement Global</instructions>

            <defaultSort sortAscending="true">
                <sortAttributes>
                    <sortAttribute attributeName="documentNumber" />
                </sortAttributes>
            </defaultSort>

            <lookupFields>
                <lookupField attributeName="documentNumber" required="false" />
                <lookupField attributeName="inventoryStatusCode" required="false" />
                <lookupField attributeName="mergedTargetCapitalAssetNumber" required="false" />
                <lookupField attributeName="retirementDate" required="false" />
                <lookupField attributeName="retirementReasonCode" required="false" />
            </lookupFields>

            <resultFields>
                <field attributeName="documentNumber" />
                <field attributeName="inventoryStatusCode" />
                <field attributeName="mergedTargetCapitalAssetNumber" />
                <field attributeName="retirementDate" />
                <field attributeName="retirementReasonCode" />
            </resultFields>
        </lookup>

        <attributes>

            <attribute name="documentNumber" forceUppercase="true">
                <label>Document Number</label>
                <shortLabel>Number</shortLabel>

                <maxLength>14</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="16" />
                </control>

                <summary>documentNumber</summary>
                <description>documentNumber description...</description>
			</attribute>

            <attribute name="inventoryStatusCode" forceUppercase="true">
                <label>Inventory Status Code</label>
                <shortLabel>Code</shortLabel>

                <maxLength>1</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="3" />
                </control>

                <summary>inventoryStatusCode</summary>
                <description>inventoryStatusCode description...</description>
			</attribute>

            <attribute name="mergedTargetCapitalAssetNumber" forceUppercase="true">
                <label>Merged Target Capital Asset Number</label>
                <shortLabel>Number</shortLabel>

                <maxLength>12</maxLength>
                <validationPattern><numeric /></validationPattern>

                <control>
                    <text size="14" />
                </control>

                <summary>mergedTargetCapitalAssetNumber</summary>
                <description>mergedTargetCapitalAssetNumber description...</description>
			</attribute>

            <attribute name="retirementDate" forceUppercase="true">
                <label>Retirement Date</label>
                <shortLabel>Date</shortLabel>

                <maxLength>7</maxLength>
                <validationPattern><date /></validationPattern>

                <control>
                    <text size="9" />
                </control>

                <summary>retirementDate</summary>
                <description>retirementDate description...</description>
			</attribute>

            <attribute name="retirementReasonCode" forceUppercase="true">
                <label>Retirement Reason Code</label>
                <shortLabel>Code</shortLabel>

                <maxLength>1</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="3" />
                </control>

                <summary>retirementReasonCode</summary>
                <description>retirementReasonCode description...</description>
			</attribute>

            <attributeReference name="versionNumber" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="versionNumber" />

        </attributes>
    </businessObject>
</dictionaryEntry>
