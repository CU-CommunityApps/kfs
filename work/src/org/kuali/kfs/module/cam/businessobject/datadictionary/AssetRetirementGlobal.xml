<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="AssetRetirementGlobal" parent="AssetRetirementGlobal-parentBean" />

  <bean id="AssetRetirementGlobal-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.module.cams.bo.AssetRetirementGlobal" />
    <property name="inquiryDefinition" >
      <ref bean="AssetRetirementGlobal-inquiryDefinition" />
    </property>
    <property name="lookupDefinition" >
      <ref bean="AssetRetirementGlobal-lookupDefinition" />
    </property>
    <property name="attributes" >
      <list>
        <ref bean="AssetRetirementGlobal-documentNumber" />
        <ref bean="AssetRetirementGlobal-inventoryStatusCode" />
        <ref bean="AssetRetirementGlobal-mergedTargetCapitalAssetNumber" />
        <ref bean="AssetRetirementGlobal-retirementDate" />
        <ref bean="AssetRetirementGlobal-retirementReasonCode" />
        <ref bean="AssetRetirementGlobal-sharedRetirementInfo.retirementChartOfAccountsCode" />
        <ref bean="AssetRetirementGlobal-sharedRetirementInfo.retirementAccountNumber" />
        <ref bean="AssetRetirementGlobal-sharedRetirementInfo.retirementContactName" />
        <ref bean="AssetRetirementGlobal-sharedRetirementInfo.retirementInstitutionName" />
        <ref bean="AssetRetirementGlobal-sharedRetirementInfo.retirementStreetAddress" />
        <ref bean="AssetRetirementGlobal-sharedRetirementInfo.retirementCityName" />
        <ref bean="AssetRetirementGlobal-sharedRetirementInfo.retirementStateCode" />
        <ref bean="AssetRetirementGlobal-sharedRetirementInfo.retirementZipCode" />
        <ref bean="AssetRetirementGlobal-sharedRetirementInfo.retirementCountryCode" />
        <ref bean="AssetRetirementGlobal-sharedRetirementInfo.retirementPhoneNumber" />
        <ref bean="AssetRetirementGlobal-sharedRetirementInfo.estimatedSellingPrice" />
        <ref bean="AssetRetirementGlobal-sharedRetirementInfo.salePrice" />
        <ref bean="AssetRetirementGlobal-sharedRetirementInfo.cashReceiptFinancialDocumentNumber" />
        <ref bean="AssetRetirementGlobal-sharedRetirementInfo.handlingFeeAmount" />
        <ref bean="AssetRetirementGlobal-sharedRetirementInfo.preventiveMaintenanceAmount" />
        <ref bean="AssetRetirementGlobal-sharedRetirementInfo.buyerDescription" />
        <ref bean="AssetRetirementGlobal-sharedRetirementInfo.paidCaseNumber" />
        <ref bean="AssetRetirementGlobal-versionNumber" />
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->


  <bean id="AssetRetirementGlobal-documentNumber" parent="AssetRetirementGlobal-documentNumber-parentBean" />

  <bean id="AssetRetirementGlobal-documentNumber-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="documentNumber" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Document Number" />
    <property name="shortLabel" value="Number" />
    <property name="maxLength" value="14" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="16" />
    </property>
    <property name="summary" value="documentNumber" />
    <property name="description" value="documentNumber description..." />
  </bean>
  <bean id="AssetRetirementGlobal-inventoryStatusCode" parent="AssetRetirementGlobal-inventoryStatusCode-parentBean" />

  <bean id="AssetRetirementGlobal-inventoryStatusCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="inventoryStatusCode" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Inventory Status Code" />
    <property name="shortLabel" value="Code" />
    <property name="maxLength" value="1" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="3" />
    </property>
    <property name="summary" value="inventoryStatusCode" />
    <property name="description" value="inventoryStatusCode description..." />
  </bean>
  <bean id="AssetRetirementGlobal-mergedTargetCapitalAssetNumber" parent="AssetRetirementGlobal-mergedTargetCapitalAssetNumber-parentBean" />

  <bean id="AssetRetirementGlobal-mergedTargetCapitalAssetNumber-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="mergedTargetCapitalAssetNumber" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Merged Target Capital Asset Number" />
    <property name="shortLabel" value="Number" />
    <property name="maxLength" value="12" />
    <property name="validationPattern" >
      <bean parent="NumericValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="14" />
    </property>
    <property name="summary" value="mergedTargetCapitalAssetNumber" />
    <property name="description" value="mergedTargetCapitalAssetNumber description..." />
  </bean>
  <bean id="AssetRetirementGlobal-retirementDate" parent="AssetRetirementGlobal-retirementDate-parentBean" />

  <bean id="AssetRetirementGlobal-retirementDate-parentBean" abstract="true" parent="GenericAttributes-genericDate">
    <property name="name" value="retirementDate" />
    <property name="label" value="Retirement Date" />
    <property name="shortLabel" value="Date" />
  </bean>
  <bean id="AssetRetirementGlobal-retirementReasonCode" parent="AssetRetirementGlobal-retirementReasonCode-parentBean" />

  <bean id="AssetRetirementGlobal-retirementReasonCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="retirementReasonCode" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Retirement Reason Code" />
    <property name="shortLabel" value="Code" />
    <property name="maxLength" value="1" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="SelectControlDefinition"
            p:businessObjectClass="org.kuali.module.cams.bo.AssetRetirementReason"
            p:valuesFinderClass="org.kuali.core.lookup.keyvalues.PersistableBusinessObjectValuesFinder"
            p:includeKeyInLabel="true"
            p:keyAttribute="retirementReasonCode"
            p:labelAttribute="retirementReasonName" />
    </property>
    <property name="summary" value="retirementReasonCode" />
    <property name="description" value="retirementReasonCode description..." />
  </bean>
  <bean id="AssetRetirementGlobal-sharedRetirementInfo.retirementChartOfAccountsCode" parent="AssetRetirementGlobal-sharedRetirementInfo.retirementChartOfAccountsCode-parentBean" />

  <bean id="AssetRetirementGlobal-sharedRetirementInfo.retirementChartOfAccountsCode-parentBean" abstract="true" parent="AssetRetirementGlobalDetail-retirementChartOfAccountsCode">
    <property name="name" value="sharedRetirementInfo.retirementChartOfAccountsCode" />
  </bean>
  <bean id="AssetRetirementGlobal-sharedRetirementInfo.retirementAccountNumber" parent="AssetRetirementGlobal-sharedRetirementInfo.retirementAccountNumber-parentBean" />

  <bean id="AssetRetirementGlobal-sharedRetirementInfo.retirementAccountNumber-parentBean" abstract="true" parent="AssetRetirementGlobalDetail-retirementAccountNumber">
    <property name="name" value="sharedRetirementInfo.retirementAccountNumber" />
  </bean>
  <bean id="AssetRetirementGlobal-sharedRetirementInfo.retirementContactName" parent="AssetRetirementGlobal-sharedRetirementInfo.retirementContactName-parentBean" />

  <bean id="AssetRetirementGlobal-sharedRetirementInfo.retirementContactName-parentBean" abstract="true" parent="AssetRetirementGlobalDetail-retirementContactName">
    <property name="name" value="sharedRetirementInfo.retirementContactName" />
  </bean>
  <bean id="AssetRetirementGlobal-sharedRetirementInfo.retirementInstitutionName" parent="AssetRetirementGlobal-sharedRetirementInfo.retirementInstitutionName-parentBean" />

  <bean id="AssetRetirementGlobal-sharedRetirementInfo.retirementInstitutionName-parentBean" abstract="true" parent="AssetRetirementGlobalDetail-retirementInstitutionName">
    <property name="name" value="sharedRetirementInfo.retirementInstitutionName" />
  </bean>
  <bean id="AssetRetirementGlobal-sharedRetirementInfo.retirementStreetAddress" parent="AssetRetirementGlobal-sharedRetirementInfo.retirementStreetAddress-parentBean" />

  <bean id="AssetRetirementGlobal-sharedRetirementInfo.retirementStreetAddress-parentBean" abstract="true" parent="AssetRetirementGlobalDetail-retirementStreetAddress">
    <property name="name" value="sharedRetirementInfo.retirementStreetAddress" />
  </bean>
  <bean id="AssetRetirementGlobal-sharedRetirementInfo.retirementCityName" parent="AssetRetirementGlobal-sharedRetirementInfo.retirementCityName-parentBean" />

  <bean id="AssetRetirementGlobal-sharedRetirementInfo.retirementCityName-parentBean" abstract="true" parent="AssetRetirementGlobalDetail-retirementCityName">
    <property name="name" value="sharedRetirementInfo.retirementCityName" />
  </bean>
  <bean id="AssetRetirementGlobal-sharedRetirementInfo.retirementStateCode" parent="AssetRetirementGlobal-sharedRetirementInfo.retirementStateCode-parentBean" />

  <bean id="AssetRetirementGlobal-sharedRetirementInfo.retirementStateCode-parentBean" abstract="true" parent="AssetRetirementGlobalDetail-retirementStateCode">
    <property name="name" value="sharedRetirementInfo.retirementStateCode" />
    <property name="control" >
      <bean parent="SelectControlDefinition"
            p:businessObjectClass="org.kuali.kfs.bo.State"
            p:valuesFinderClass="org.kuali.kfs.lookup.keyvalues.StateValuesFinder" />
    </property>
  </bean>
  <bean id="AssetRetirementGlobal-sharedRetirementInfo.retirementZipCode" parent="AssetRetirementGlobal-sharedRetirementInfo.retirementZipCode-parentBean" />

  <bean id="AssetRetirementGlobal-sharedRetirementInfo.retirementZipCode-parentBean" abstract="true" parent="AssetRetirementGlobalDetail-retirementZipCode">
    <property name="name" value="sharedRetirementInfo.retirementZipCode" />
  </bean>
  <bean id="AssetRetirementGlobal-sharedRetirementInfo.retirementCountryCode" parent="AssetRetirementGlobal-sharedRetirementInfo.retirementCountryCode-parentBean" />

  <bean id="AssetRetirementGlobal-sharedRetirementInfo.retirementCountryCode-parentBean" abstract="true" parent="AssetRetirementGlobalDetail-retirementCountryCode">
    <property name="name" value="sharedRetirementInfo.retirementCountryCode" />
    <property name="control" >
      <bean parent="SelectControlDefinition"
            p:valuesFinderClass="org.kuali.kfs.lookup.keyvalues.CountryValuesFinder" />
    </property>
  </bean>
  <bean id="AssetRetirementGlobal-sharedRetirementInfo.retirementPhoneNumber" parent="AssetRetirementGlobal-sharedRetirementInfo.retirementPhoneNumber-parentBean" />

  <bean id="AssetRetirementGlobal-sharedRetirementInfo.retirementPhoneNumber-parentBean" abstract="true" parent="AssetRetirementGlobalDetail-retirementPhoneNumber">
    <property name="name" value="sharedRetirementInfo.retirementPhoneNumber" />
  </bean>
  <bean id="AssetRetirementGlobal-sharedRetirementInfo.estimatedSellingPrice" parent="AssetRetirementGlobal-sharedRetirementInfo.estimatedSellingPrice-parentBean" />

  <bean id="AssetRetirementGlobal-sharedRetirementInfo.estimatedSellingPrice-parentBean" abstract="true" parent="AssetRetirementGlobalDetail-estimatedSellingPrice">
    <property name="name" value="sharedRetirementInfo.estimatedSellingPrice" />
  </bean>
  <bean id="AssetRetirementGlobal-sharedRetirementInfo.salePrice" parent="AssetRetirementGlobal-sharedRetirementInfo.salePrice-parentBean" />

  <bean id="AssetRetirementGlobal-sharedRetirementInfo.salePrice-parentBean" abstract="true" parent="AssetRetirementGlobalDetail-salePrice">
    <property name="name" value="sharedRetirementInfo.salePrice" />
  </bean>
  <bean id="AssetRetirementGlobal-sharedRetirementInfo.cashReceiptFinancialDocumentNumber" parent="AssetRetirementGlobal-sharedRetirementInfo.cashReceiptFinancialDocumentNumber-parentBean" />

  <bean id="AssetRetirementGlobal-sharedRetirementInfo.cashReceiptFinancialDocumentNumber-parentBean" abstract="true" parent="AssetRetirementGlobalDetail-cashReceiptFinancialDocumentNumber">
    <property name="name" value="sharedRetirementInfo.cashReceiptFinancialDocumentNumber" />
  </bean>
  <bean id="AssetRetirementGlobal-sharedRetirementInfo.handlingFeeAmount" parent="AssetRetirementGlobal-sharedRetirementInfo.handlingFeeAmount-parentBean" />

  <bean id="AssetRetirementGlobal-sharedRetirementInfo.handlingFeeAmount-parentBean" abstract="true" parent="AssetRetirementGlobalDetail-handlingFeeAmount">
    <property name="name" value="sharedRetirementInfo.handlingFeeAmount" />
  </bean>
  <bean id="AssetRetirementGlobal-sharedRetirementInfo.preventiveMaintenanceAmount" parent="AssetRetirementGlobal-sharedRetirementInfo.preventiveMaintenanceAmount-parentBean" />

  <bean id="AssetRetirementGlobal-sharedRetirementInfo.preventiveMaintenanceAmount-parentBean" abstract="true" parent="AssetRetirementGlobalDetail-preventiveMaintenanceAmount">
    <property name="name" value="sharedRetirementInfo.preventiveMaintenanceAmount" />
  </bean>
  <bean id="AssetRetirementGlobal-sharedRetirementInfo.buyerDescription" parent="AssetRetirementGlobal-sharedRetirementInfo.buyerDescription-parentBean" />

  <bean id="AssetRetirementGlobal-sharedRetirementInfo.buyerDescription-parentBean" abstract="true" parent="AssetRetirementGlobalDetail-buyerDescription">
    <property name="name" value="sharedRetirementInfo.buyerDescription" />
  </bean>
  <bean id="AssetRetirementGlobal-sharedRetirementInfo.paidCaseNumber" parent="AssetRetirementGlobal-sharedRetirementInfo.paidCaseNumber-parentBean" />

  <bean id="AssetRetirementGlobal-sharedRetirementInfo.paidCaseNumber-parentBean" abstract="true" parent="AssetRetirementGlobalDetail-paidCaseNumber">
    <property name="name" value="sharedRetirementInfo.paidCaseNumber" />
  </bean>
  <bean id="AssetRetirementGlobal-versionNumber" parent="AssetRetirementGlobal-versionNumber-parentBean" />

  <bean id="AssetRetirementGlobal-versionNumber-parentBean" abstract="true" parent="GenericAttributes-versionNumber">
  </bean>

<!-- Business Object Inquiry Definition -->


  <bean id="AssetRetirementGlobal-inquiryDefinition" parent="AssetRetirementGlobal-inquiryDefinition-parentBean" />

  <bean id="AssetRetirementGlobal-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
    <property name="title" value="Asset Retirement Global Inquiry" />
    <property name="inquirySections" >
      <list>
        <bean parent="InquirySectionDefinition">
          <property name="title" value="Header Text" />
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields" >
            <list>
              <bean parent="FieldDefinition"
                    p:attributeName="documentNumber" />
              <bean parent="FieldDefinition"
                    p:attributeName="inventoryStatusCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="mergedTargetCapitalAssetNumber" />
              <bean parent="FieldDefinition"
                    p:attributeName="retirementDate" />
              <bean parent="FieldDefinition"
                    p:attributeName="retirementReasonCode" />
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Business Object Lookup Definition -->


  <bean id="AssetRetirementGlobal-lookupDefinition" parent="AssetRetirementGlobal-lookupDefinition-parentBean" />

  <bean id="AssetRetirementGlobal-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
    <property name="title" value="Asset Retirement Global Lookup" />
    <property name="instructions" value="Look up an Asset Retirement Global" />
    <property name="defaultSort" >
      <bean parent="SortDefinition">
      </bean>
    </property>
    <property name="lookupFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="documentNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="inventoryStatusCode" />
        <bean parent="FieldDefinition"
              p:attributeName="mergedTargetCapitalAssetNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="retirementDate" />
        <bean parent="FieldDefinition"
              p:attributeName="retirementReasonCode" />
      </list>
    </property>
    <property name="resultFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="documentNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="inventoryStatusCode" />
        <bean parent="FieldDefinition"
              p:attributeName="mergedTargetCapitalAssetNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="retirementDate" />
        <bean parent="FieldDefinition"
              p:attributeName="retirementReasonCode" />
      </list>
    </property>
  </bean>
</beans>
