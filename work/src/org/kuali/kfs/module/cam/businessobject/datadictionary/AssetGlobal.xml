<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "../../../core/datadictionary/dataDictionary-1_0.dtd">

<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.module.cams.bo.AssetGlobal">
        <attributes>
<!--  ### Asset Detail Information ### -->
            <attributeReference name="assetHeader.organizationOwnerChartOfAccountsCode" sourceClassName="org.kuali.module.cams.bo.AssetHeader" sourceAttributeName="organizationOwnerChartOfAccountsCode">
                
            </attributeReference>

            <attributeReference name="assetHeader.organizationOwnerAccountNumber" sourceClassName="org.kuali.module.cams.bo.AssetHeader" sourceAttributeName="organizationOwnerAccountNumber">
                
			</attributeReference>		
			
			
			
			
			<attributeReference name="assetHeader.agencyNumber" sourceClassName="org.kuali.module.cams.bo.AssetHeader" sourceAttributeName="agencyNumber">
				
			</attributeReference>
			
			<attributeReference name="acquisitionTypeCode" sourceClassName="org.kuali.module.cams.bo.Asset" sourceAttributeName="acquisitionTypeCode">
				
			</attributeReference>
			
			<attributeReference name="inventoryStatusCode" sourceClassName="org.kuali.module.cams.bo.Asset" sourceAttributeName="inventoryStatusCode">
				
			</attributeReference>

            <attributeReference name="conditionCode" sourceClassName="org.kuali.module.cams.bo.AssetCondition" sourceAttributeName="assetConditionCode">
                
            </attributeReference>
			
			<attributeReference name="capitalAssetDescription" sourceClassName="org.kuali.module.cams.bo.Asset" sourceAttributeName="capitalAssetDescription">
				
			</attributeReference>
			
            <attributeReference name="capitalAssetTypeCode" sourceClassName="org.kuali.module.cams.bo.AssetType" sourceAttributeName="capitalAssetTypeCode">
               <label>Asset Type Code</label>
            </attributeReference>
			
			<attributeReference name="vendorName" sourceClassName="org.kuali.module.cams.bo.Asset" sourceAttributeName="vendorName">
				
			</attributeReference>
			
			<attributeReference name="manufacturerName" sourceClassName="org.kuali.module.cams.bo.Asset" sourceAttributeName="manufacturerName">
				
			</attributeReference>
			
			<attributeReference name="manufacturerModelNumber" sourceClassName="org.kuali.module.cams.bo.Asset" sourceAttributeName="manufacturerModelNumber">
				
			</attributeReference>
			
			<attributeReference name="organizationText" sourceClassName="org.kuali.module.cams.bo.AssetOrganization" sourceAttributeName="organizationText">
				<label>Organization Text</label>
			</attributeReference>
  
            <attributeReference name="assetHeader.representativeUniversalIdentifier" sourceClassName="org.kuali.module.cams.bo.AssetHeader" sourceAttributeName="representativeUniversalIdentifier" >
                <label>Asset Representitive</label>
                <displayLabelAttribute>assetHeader.assetRepresentative.personUserIdentifier</displayLabelAttribute>
                <required>false</required>
                <control>
                    <hidden />
                </control>
			</attributeReference>
			
			<attributeReference name="assetHeader.assetRepresentative.personUserIdentifier" sourceClassName="org.kuali.core.bo.user.UniversalUser" sourceAttributeName="personUserIdentifier">
                <label>Asset Representitive</label>
                <required>false</required>
                <control>
                	<kualiUser universalIdAttributeName="assetHeader.representativeUniversalIdentifier" userIdAttributeName="assetHeader.assetRepresentative.personUserIdentifier" personNameAttributeName="assetHeader.assetRepresentative.personName" />
                </control>
			</attributeReference>
				
            <attributeReference name="assetHeader.assetRepresentative.personName" sourceClassName="org.kuali.core.bo.user.UniversalUser" sourceAttributeName="personName">
                <label>Asset Representitive</label>
                <required>false</required>
                <control>
                    <hidden />
                </control>
            </attributeReference>
			
			<attributeReference name="lastInventoryDate" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="genericDate">
				<label>Last Inventory Date</label>
			</attributeReference>
			
			<attributeReference name="createDate" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="genericDate">
				<label>Create Date</label>
			</attributeReference>
						
			<attributeReference name="capitalAssetInServiceDate" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="genericDate">
				<label>In-service Date</label>
			</attributeReference>
			
			<attributeReference name="capitalAssetDepreciationDate" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="genericDate">
				<label>Deprecation Date</label>
			</attributeReference>
			




			<!-- 
			<attributeReference name="landCountyName" sourceClassName="org.kuali.module.cams.bo.Asset" sourceAttributeName="landCountyName">
				<label>Land Country</label>
			</attributeReference>
			
			<attributeReference name="landAcreageSize" sourceClassName="org.kuali.module.cams.bo.Asset" sourceAttributeName="landAcreageSize">
				<label>Land Acreage</label>
			</attributeReference>
					
			<attributeReference name="landParcelNumber" sourceClassName="org.kuali.module.cams.bo.Asset" sourceAttributeName="landParcelNumber">
				<label>Land Parcel Number</label>
			</attributeReference>
			-->
	
			<attribute name="landCountyName" forceUppercase="true">
                <label>Land County Name</label>
                <shortLabel>Country</shortLabel>
                <maxLength>22</maxLength>
                <validationPattern><anyCharacter /></validationPattern>
                <control>
                    <text size="20" />
                </control>
                <summary>landCountyName</summary>
                <description>landCountyName description...</description>
			</attribute>
			
			<attribute name="landAcreageSize" forceUppercase="true">
                <label>Land Acreage</label>
                <shortLabel>Acreage</shortLabel>
                <maxLength>9</maxLength>
                <validationPattern><numeric /></validationPattern>
                <control>
                    <text size="7" />
                </control>
                <summary>landAcreageSize</summary>
                <description>landAcreageSize description...</description>
			</attribute>			
			
			<attribute name="landParcelNumber" forceUppercase="true">
                <label>Land Parcel Number</label>
                <shortLabel>Parcel</shortLabel>
                <maxLength>32</maxLength>
                <validationPattern><anyCharacter /></validationPattern>
                <control>
                    <text size="30" />
                </control>
                <summary>landParcelNumber</summary>
                <description>landParcelNumber description...</description>
			</attribute>			
			
<!--  ### Add Payment ### -->
			
			
			<!-- display total cost -->
            <attribute name="financialDocumentNextLineNumber" >
                <label>Financial Document Next LineNumber</label>
                <shortLabel>financialDocumentNextLineNumber</shortLabel>
                <maxLength>32</maxLength>
                <validationPattern><anyCharacter /></validationPattern>
                <control>
                    <hidden/>
                </control>
                <summary>financialDocumentNextLineNumber</summary>
                <description>financialDocumentNextLineNumber description...</description>
			</attribute>		

			<attributeReference name="versionNumber" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="versionNumber" />

        </attributes>
        
        <relationships>
         	<relationship objectAttributeName="assetHeader.assetRepresentative" >
            	<primitiveAttribute sourceName="assetHeader.representativeUniversalIdentifier" targetName="personUniversalIdentifier" />
				<supportAttribute sourceName="assetHeader.assetRepresentative.personUserIdentifier" targetName="personUserIdentifier" identifier="true" />
				<supportAttribute sourceName="assetHeader.assetRepresentative.personName" targetName="personName" />
        	</relationship>
        	<relationship objectAttributeName="assetHeader.organizationOwnerChartOfAccounts" >
	            <primitiveAttribute sourceName="assetHeader.organizationOwnerChartOfAccountsCode" targetName="chartOfAccountsCode" />
	        </relationship>
			<relationship objectAttributeName="assetHeader.organizationOwnerAccount" >
	            <primitiveAttribute sourceName="assetHeader.organizationOwnerChartOfAccountsCode" targetName="chartOfAccountsCode" />
	            <primitiveAttribute sourceName="assetHeader.organizationOwnerAccountNumber" targetName="accountNumber" />
	        </relationship>
        	
			
        </relationships>

        <objectLabel>Asset Global</objectLabel>
        
    </businessObject>
</dictionaryEntry>
