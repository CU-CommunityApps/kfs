<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "../../../core/datadictionary/dataDictionary-1_0.dtd">

<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.module.cams.bo.AssetGlobal">

        <inquiry>
            <title>Asset Global Inquiry</title>
            <inquirySections>
                <inquirySection title="Header Text">
                    <inquiryFields>
                        <field attributeName="organizationOwnerChartOfAccountsCode" />
                        <field attributeName="organizationOwnerAccountNumber" />
                        <!-- org. owner -->
                        <field attributeName="agencyNumber" />
                        <field attributeName="acquisitionTypeCode" />
                        <field attributeName="inventoryStatusCode" />
                        <field attributeName="conditionCode" />
                        <field attributeName="capitalAssetTypeCode" />
                        <!-- asset rep -->
                    </inquiryFields>
                </inquirySection>
            </inquirySections>  
        </inquiry>

        <lookup>
            <lookupableID>assetLookupable</lookupableID>
            <title>Asset Global Lookup</title>
            
            <menubar>
                <![CDATA[<a href="maintenance.do?methodToCall=start&businessObjectClassName=org.kuali.module.cams.bo.AssetGlobal"><img src="${kr.externalizable.images.url}tinybutton-createnewglobal.gif" alt="create new global" width="98" height="15"/></a>]]>
            </menubar>
            
            <instructions>Look up a Global Asset</instructions>
            
            <defaultSort sortAscending="true">
                <sortAttributes>
                    <sortAttribute attributeName="organizationOwnerChartOfAccountsCode" />
                </sortAttributes>
            </defaultSort>
            
            <lookupFields>
                <lookupField attributeName="organizationOwnerChartOfAccountsCode" required="false"/>
                <lookupField attributeName="organizationOwnerAccountNumber" required="false"/>
                <lookupField attributeName="agencyNumber" required="false"/>
                <lookupField attributeName="acquisitionTypeCode" required="false"/>
                <lookupField attributeName="inventoryStatusCode" required="false"/>               
            </lookupFields>
            
            <resultFields>
                <field attributeName="organizationOwnerChartOfAccountsCode" />
                <field attributeName="organizationOwnerAccountNumber" />
                <!-- asset rep -->
                <field attributeName="leoOrganizationOwner" />
                <field attributeName="agencyNumber" />
                <field attributeName="acquisitionTypeCode" />
                <field attributeName="inventoryStatusCode" />
                <field attributeName="conditionCode" />
                <field attributeName="capitalAssetDescription" />
                <field attributeName="capitalAssetTypeCode" />
                <field attributeName="vendorName" />
				<field attributeName="manufacturerName" />
				<field attributeName="manufacturerModelNumber" />
				<!-- asset rep -->
				<field attributeName="leoAssetRepresentitive" />
				<field attributeName="organizationText" />
				<field attributeName="lastInventoryDate" />
				<field attributeName="createDate" />
				<field attributeName="financialDocumentPostingYear" />
				<field attributeName="financialDocumentPostingPeriodCode" />
				<field attributeName="capitalAssetInServiceDate" />
				<!-- deprec. date -->
				<field attributeName="leoDeprecationDate" />
            </resultFields>
        </lookup>

        <attributes>
<!--  ### Asset Detail Information ### -->
            <attributeReference name="organizationOwnerChartOfAccountsCode" sourceClassName="org.kuali.module.cams.bo.Asset" sourceAttributeName="organizationOwnerChartOfAccountsCode">
            	<label>Owner Chart</label>
			</attributeReference>
			
			<attributeReference name="organizationOwnerAccountNumber" sourceClassName="org.kuali.module.cams.bo.Asset" sourceAttributeName="organizationOwnerAccountNumber">
				<label>Owner Account Number</label>
			</attributeReference>
			
			<!-- org. owner -->
			<attribute name="leoOrganizationOwner" forceUppercase="true">
                <label>Organization Owner</label>
                <shortLabel>Need More Info</shortLabel>
                <maxLength>2</maxLength>
                <validationPattern><anyCharacter allowWhitespace="true"/></validationPattern>
                <control>
                    <text size="2" />
                </control>
                <summary>leoOrganizationOwner</summary>
                <description>leoOrganizationOwner....</description>
			</attribute>
			
			<attributeReference name="agencyNumber" sourceClassName="org.kuali.module.cams.bo.Asset" sourceAttributeName="agencyNumber">
				<label>Owner</label>
			</attributeReference>
			
			<attributeReference name="acquisitionTypeCode" sourceClassName="org.kuali.module.cams.bo.Asset" sourceAttributeName="acquisitionTypeCode">
				<label>Acquistion Type</label>
			</attributeReference>
			
			<attributeReference name="inventoryStatusCode" sourceClassName="org.kuali.module.cams.bo.Asset" sourceAttributeName="inventoryStatusCode">
				<label>Acquistion Status</label>
			</attributeReference>

            <attribute name="conditionCode" forceUppercase="true">
                <label>Condition Code</label>
                <shortLabel>Condition Code</shortLabel>
                <maxLength>1</maxLength>
                <validationPattern><anyCharacter /></validationPattern>
                <control>
                    <text size="3" />
                </control>
                <summary>conditionCode</summary>
                <description>conditionCode description...</description>
			</attribute>
			
            <attribute name="capitalAssetDescription" forceUppercase="true">
                <label>Description</label>
                <shortLabel>Description</shortLabel>
                <maxLength>2000</maxLength>
                <validationPattern><anyCharacter allowWhitespace="true"/></validationPattern>
                <control>
                    <textarea rows="5" cols="80"/>
                </control>
                <summary>capitalAssetDescription</summary>
                <description>capitalAssetDescription description...</description>
			</attribute>
			
            <attribute name="capitalAssetTypeCode" forceUppercase="true">
                <label>Asset Type</label>
                <shortLabel>Asset Type</shortLabel>
                <maxLength>7</maxLength>
                <validationPattern><anyCharacter /></validationPattern>
                <control>
                    <text size="9" />
                </control>
                <summary>AssetType</summary>
                <description>capitalAssetTypeCode description...</description>
			</attribute>
			
			<attributeReference name="vendorName" sourceClassName="org.kuali.module.cams.bo.Asset" sourceAttributeName="vendorName">
				<label>Vendor</label>
			</attributeReference>
			
			<attributeReference name="manufacturerName" sourceClassName="org.kuali.module.cams.bo.Asset" sourceAttributeName="manufacturerName">
				<label>Manufacturer</label>
			</attributeReference>
			
			<attributeReference name="manufacturerModelNumber" sourceClassName="org.kuali.module.cams.bo.Asset" sourceAttributeName="manufacturerModelNumber">
				<label>Model Number</label>
			</attributeReference>
			
			<attributeReference name="organizationText" sourceClassName="org.kuali.module.cams.bo.AssetOrganization" sourceAttributeName="organizationText">
				<label>Organization Text</label>
			</attributeReference>
			
			<!-- asset rep -->
			<attribute name="leoAssetRepresentitive" forceUppercase="true">
                <label>Asset Representitive</label>
                <shortLabel>Need More Info</shortLabel>
                <maxLength>2</maxLength>
                <validationPattern><anyCharacter allowWhitespace="true"/></validationPattern>
                <control>
                    <text size="2" />
                </control>
                <summary>leoAssetRepresentitive</summary>
                <description>leoAssetRepresentitive....</description>
			</attribute>
			
			<attributeReference name="lastInventoryDate" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="genericDate">
				<label>Last Inventory Date</label>
			</attributeReference>
			
			<attributeReference name="createDate" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="genericDate">
				<label>Create Date</label>
			</attributeReference>
			
			<attributeReference name="financialDocumentPostingYear" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="genericFiscalYear">
				<label>Fiscal Year</label>
			</attributeReference>
			
			<attributeReference name="financialDocumentPostingPeriodCode" sourceClassName="org.kuali.module.cams.bo.Asset" sourceAttributeName="financialDocumentPostingPeriodCode">
				<label>Period</label>
			</attributeReference>
			
			<attributeReference name="capitalAssetInServiceDate" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="genericDate">
				<label>In-service Date</label>
			</attributeReference>
			
			<!-- deprec. date -->
			<attribute name="leoDeprecationDate" forceUppercase="true">
                <label>Deprecation Date</label>
                <shortLabel>Need More Info</shortLabel>
                <maxLength>2</maxLength>
                <validationPattern><anyCharacter allowWhitespace="true"/></validationPattern>
                <control>
                    <text size="2" />
                </control>
                <summary>leoDeprecationDate</summary>
                <description>leoDeprecationDate....</description>
			</attribute>

<!--  ### Unique Asset Information ### -->

			<!-- nbr of assets to create -->
			<attribute name="leoAssetsToCreate" forceUppercase="true">
                <label>Number of Assets to Create</label>
                <shortLabel>Need More Info</shortLabel>
                <maxLength>2</maxLength>
                <validationPattern><anyCharacter allowWhitespace="true"/></validationPattern>
                <control>
                    <text size="2" />
                </control>
                <summary>leoAssetsToCreate</summary>
                <description>leoAssetsToCreate....</description>
			</attribute>
			
			<attributeReference name="capitalAssetNumber" sourceClassName="org.kuali.module.cams.bo.Asset" sourceAttributeName="capitalAssetNumber">
				<label>Asset Number</label>
			</attributeReference>
			
			<attributeReference name="campusTagNumber" sourceClassName="org.kuali.module.cams.bo.Asset" sourceAttributeName="campusTagNumber">
				<label>Tag Number</label>
			</attributeReference>
			
			<attributeReference name="serialNumber" sourceClassName="org.kuali.module.cams.bo.Asset" sourceAttributeName="serialNumber">
				<label>Serial Number</label>
			</attributeReference>

			<!-- government tag -->
			<attribute name="leoGovernmentTag" forceUppercase="true">
                <label>Government Tag</label>
                <shortLabel>Need More Info</shortLabel>
                <maxLength>2</maxLength>
                <validationPattern><anyCharacter allowWhitespace="true"/></validationPattern>
                <control>
                    <text size="2" />
                </control>
                <summary>leoGovernmentTag</summary>
                <description>leoGovernmentTag....</description>
			</attribute>

			<!-- national stock number -->
			<attribute name="leoNationalStockNbr" forceUppercase="true">
                <label>National Stock Number</label>
                <shortLabel>Need More Info</shortLabel>
                <maxLength>2</maxLength>
                <validationPattern><anyCharacter allowWhitespace="true"/></validationPattern>
                <control>
                    <text size="2" />
                </control>
                <summary>leoNationalStockNbr</summary>
                <description>leoNationalStockNbr....</description>
			</attribute>

			<attributeReference name="organizationInventoryName" sourceClassName="org.kuali.module.cams.bo.Asset" sourceAttributeName="organizationInventoryName">
				<label>Organization Inventory Name</label>
			</attributeReference>
			
			<attributeReference name="organizationAssetTypeIdentifier" sourceClassName="org.kuali.module.cams.bo.AssetOrganization" sourceAttributeName="organizationAssetTypeIdentifier">
				<label>Organization Asset Type</label>
			</attributeReference>

<!--  ### Land Information ### -->
			<attributeReference name="landCountyName" sourceClassName="org.kuali.module.cams.bo.Asset" sourceAttributeName="landCountyName">
				<label>Land Country</label>
			</attributeReference>
			
			<attributeReference name="landAcreageSize" sourceClassName="org.kuali.module.cams.bo.Asset" sourceAttributeName="landAcreageSize">
				<label>Land Acreage</label>
			</attributeReference>
					
			<attributeReference name="landParcelNumber" sourceClassName="org.kuali.module.cams.bo.Asset" sourceAttributeName="landParcelNumber">
				<label>Land Parcel Number</label>
			</attributeReference>

<!--  ### Add Payment ### -->
			<attributeReference name="chartOfAccountsCode" sourceClassName="org.kuali.module.cams.bo.AssetPayment" sourceAttributeName="chartOfAccountsCode">
				<label>Chart</label>
			</attributeReference>

			<attributeReference name="accountNumber" sourceClassName="org.kuali.module.cams.bo.AssetPayment" sourceAttributeName="accountNumber">
				<label>Account</label>
			</attributeReference>

			<attributeReference name="subAccountNumber" sourceClassName="org.kuali.module.cams.bo.AssetPayment" sourceAttributeName="subAccountNumber">
				<label>Sub Account</label>
			</attributeReference>

			<attributeReference name="financialObjectCode" sourceClassName="org.kuali.module.cams.bo.AssetPayment" sourceAttributeName="financialObjectCode">
				<label>Object</label>
			</attributeReference>

			<attributeReference name="financialSubObjectCode" sourceClassName="org.kuali.module.cams.bo.AssetPayment" sourceAttributeName="financialSubObjectCode">
				<label>Sub Object</label>
			</attributeReference>
			
			<attributeReference name="projectCode" sourceClassName="org.kuali.module.cams.bo.AssetPayment" sourceAttributeName="projectCode">
				<label>Project</label>
			</attributeReference>

			<attributeReference name="organizationReferenceId" sourceClassName="org.kuali.module.cams.bo.AssetPayment" sourceAttributeName="organizationReferenceId">
				<label>Org. Ref. ID</label>
			</attributeReference>

			<!-- financial document number -->
			<attribute name="leoDocumentNumber" forceUppercase="true">
                <label>Financial Document Number</label>
                <shortLabel>dup/exists???</shortLabel>
                <maxLength>2</maxLength>
                <validationPattern><anyCharacter allowWhitespace="true"/></validationPattern>
                <control>
                    <text size="2" />
                </control>
                <summary>leoDocumentNumber</summary>
                <description>leoDocumentNumber....</description>
			</attribute>

			<attributeReference name="financialDocumentTypeCode" sourceClassName="org.kuali.module.cams.bo.AssetPayment" sourceAttributeName="financialDocumentTypeCode">
				<label>Document Type Code</label>
			</attributeReference>

			<attributeReference name="purchaseOrderNumber" sourceClassName="org.kuali.module.cams.bo.AssetPayment" sourceAttributeName="purchaseOrderNumber">
				<label>Purchase Order Number</label>
			</attributeReference>

			<attributeReference name="requisitionNumber" sourceClassName="org.kuali.module.cams.bo.AssetPayment" sourceAttributeName="requisitionNumber">
				<label>Requisition</label>
			</attributeReference>

			<attributeReference name="financialDocumentPostingDate" sourceClassName="org.kuali.module.cams.bo.AssetPayment" sourceAttributeName="financialDocumentPostingDate">
				<label>Financial Post Date</label>
			</attributeReference>

			<!-- fiscal year -->
			<attribute name="leoFinancialDocumentPostingYear">
                <label>Fiscal Year</label>
                <shortLabel>dup/exists???</shortLabel>
                <maxLength>2</maxLength>
                <validationPattern><anyCharacter allowWhitespace="true"/></validationPattern>
                <control>
                    <text size="2" />
                </control>
                <summary>leoFinancialDocumentPostingYear</summary>
                <description>leoFinancialDocumentPostingYear....</description>
			</attribute>

			<!-- period -->
			<attribute name="leoFinancialDocumentPostingPeriodCode">
                <label>Period</label>
                <shortLabel>dup/exists???</shortLabel>
                <maxLength>2</maxLength>
                <validationPattern><anyCharacter allowWhitespace="true"/></validationPattern>
                <control>
                    <text size="2" />
                </control>
                <summary>leoFinancialDocumentPostingPeriodCode</summary>
                <description>leoFinancialDocumentPostingPeriodCode....</description>
			</attribute>

			<attributeReference name="accountChargeAmount" sourceClassName="org.kuali.module.cams.bo.AssetPayment" sourceAttributeName="accountChargeAmount">
				<label>Account Amount</label>
			</attributeReference>

			<!-- display total cost -->
            <attribute name="leoAddPaymentsTotalCost">
                <label>Display Total Cost</label>
                <shortLabel>total</shortLabel>
                <maxLength>19</maxLength>
                <validationPattern>
                    <fixedPoint allowNegative="false" precision="19" scale="2" />
                </validationPattern>
                <required>true</required>
                <control>
                    <text size="8" />
                </control>
                <summary>leoAddPaymentsTotalCost</summary>
                <description>leoAddPaymentsTotalCost....</description>
            </attribute>	

			<attributeReference name="versionNumber" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="versionNumber" />

        </attributes>
        
