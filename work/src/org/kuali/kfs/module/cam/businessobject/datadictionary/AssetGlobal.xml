<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "../../../core/datadictionary/dataDictionary-1_0.dtd">

<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.module.cams.bo.AssetGlobal">

        <inquiry>
            <title>Asset Global Inquiry</title>
            <inquirySections>
                <inquirySection title="Header Text">
                    <inquiryFields>
                        <field attributeName="organizationOwnerChartOfAccountsCode" />
                        <field attributeName="organizationOwnerAccountNumber" />
                        <field attributeName="agencyNumber" />
                        <field attributeName="acquisitionTypeCode" />
                        <field attributeName="inventoryStatusCode" />
                    </inquiryFields>
                </inquirySection>
            </inquirySections>  
        </inquiry>

        <lookup>
            <lookupableID>assetLookupable</lookupableID>
            <title>Asset Global Lookup</title>
            
            <menubar>
                <![CDATA[<a href="maintenance.do?methodToCall=start&businessObjectClassName=org.kuali.module.cams.bo.AssetGlobal"><img src="${kr.externalizable.images.url}tinybutton-createnewglobal.gif" alt="create new global" width="98" height="15"/></a>]]>
            </menubar>
            
            <instructions>Look up a Global Asset</instructions>
            
            <defaultSort sortAscending="true">
                <sortAttributes>
                    <sortAttribute attributeName="organizationOwnerChartOfAccountsCode" />
                </sortAttributes>
            </defaultSort>
            
            <lookupFields>
                <!-- <lookupField attributeName="assetOrganization.organizationTagNumber" required="false" noLookup="true" /> -->
                <!-- <defaultValueFinderClass>org.kuali.kfs.lookup.valuefinder.FiscalYearFinder</defaultValueFinderClass> -->
                <lookupField attributeName="organizationOwnerChartOfAccountsCode" required="false"/>
                <lookupField attributeName="organizationOwnerAccountNumber" required="false"/>
                <lookupField attributeName="agencyNumber" required="false"/>
                <lookupField attributeName="acquisitionTypeCode" required="false"/>
                <lookupField attributeName="inventoryStatusCode" required="false"/>               
            </lookupFields>
            
            <resultFields>
                <field attributeName="organizationOwnerChartOfAccountsCode" />
                <field attributeName="organizationOwnerAccountNumber" />
                <field attributeName="agencyNumber" />
                <field attributeName="acquisitionTypeCode" />
                <field attributeName="inventoryStatusCode" />
            </resultFields>
        </lookup>

        <attributes>
<!--  ### temp holder - remove from BO when finished ### -->
            <attribute name="needMoreInfo">
                <label>Need More Info</label>
                <shortLabel>Need More Info</shortLabel>
                <maxLength>2</maxLength>
                <validationPattern><anyCharacter allowWhitespace="true"/></validationPattern>
                <control>
                    <text size="2" />
                </control>
                <summary>needMoreInfo</summary>
                <description>This is a temp holder used for testing....</description>
			</attribute>

<!--  ### Global Asset Maintenance ### -->        
            <attribute name="capitalAssetDescription" forceUppercase="true">
                <label>Capital Asset Description</label>
                <shortLabel>Description</shortLabel>
                <maxLength>2000</maxLength>
                <validationPattern><anyCharacter allowWhitespace="true"/></validationPattern>
                <control>
                    <textarea rows="5" cols="80"/>
                </control>
                <summary>capitalAssetDescription</summary>
                <description>capitalAssetDescription description...</description>
			</attribute>
			
            <attribute name="capitalAssetTypeCode" forceUppercase="true">
                <label>Capital Asset Type Code</label>
                <shortLabel>Code</shortLabel>
                <maxLength>7</maxLength>
                <validationPattern><anyCharacter /></validationPattern>
                <control>
                    <text size="9" />
                </control>
                <summary>capitalAssetTypeCode</summary>
                <description>capitalAssetTypeCode description...</description>
			</attribute>
			
            <attribute name="conditionCode" forceUppercase="true">
                <label>Condition Code</label>
                <shortLabel>Code</shortLabel>
                <maxLength>1</maxLength>
                <validationPattern><anyCharacter /></validationPattern>
                <control>
                    <text size="3" />
                </control>
                <summary>conditionCode</summary>
                <description>conditionCode description...</description>
			</attribute>
			
            <attributeReference name="versionNumber" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="versionNumber" />
            
<!--  ### Asset Detail Information ### -->
			<!-- <attributeReference name="organizationOwnerAccount.organizationCode" sourceClassName="org.kuali.module.cams.bo.Account" sourceAttributeName="organizationOwnerAccount" /> -->
            <attributeReference name="organizationOwnerChartOfAccountsCode" sourceClassName="org.kuali.module.cams.bo.Asset" sourceAttributeName="organizationOwnerChartOfAccountsCode">
            	<label>Owner Chart</label>
			</attributeReference>
			
			<attributeReference name="organizationOwnerAccountNumber" sourceClassName="org.kuali.module.cams.bo.Asset" sourceAttributeName="organizationOwnerAccountNumber">
				<label>Owner Account Number</label>
			</attributeReference>
			
			<attributeReference name="agencyNumber" sourceClassName="org.kuali.module.cams.bo.Asset" sourceAttributeName="agencyNumber">
				<label>Owner</label>
			</attributeReference>
			
			<attributeReference name="acquisitionTypeCode" sourceClassName="org.kuali.module.cams.bo.Asset" sourceAttributeName="acquisitionTypeCode">
				<label>Acquistion Type</label>
			</attributeReference>
			
			<attributeReference name="inventoryStatusCode" sourceClassName="org.kuali.module.cams.bo.Asset" sourceAttributeName="inventoryStatusCode">
				<label>Acquistion Status</label>
			</attributeReference>

<!--  ### Land Information ###
            <attributeReference name="landCountyName" sourceClassName="org.kuali.module.cams.bo.Asset" sourceAttributeName="landCountyName">
				<label>Land Country</label>
				<shortLabel>landCountyName</shortLabel>
				<maxLength>4</maxLength>
                <validationPattern><anyCharacter allowWhitespace="true" /></validationPattern>
                <control>
                    <text size="4" />
                </control>
                <summary>landCountyName</summary>
                <description>Land country name.</description>
			</attributeReference>
			
            <attributeReference name="landAcreageSize" sourceClassName="org.kuali.module.cams.bo.Asset" sourceAttributeName="landAcreageSize">
				<label>Land Acreage</label>
				<shortLabel>landAcreageSize</shortLabel>
				<maxLength>10</maxLength>
                <validationPattern><anyCharacter allowWhitespace="true" /></validationPattern>
                <control>
                    <text size="10" />
                </control>
                <summary>landAcreageSize</summary>
                <description>Land acreage size.</description>
			</attributeReference>
            
            <attributeReference name="landParcelNumber" sourceClassName="org.kuali.module.cams.bo.Asset" sourceAttributeName="landParcelNumber">
				<label>Land Parcel Number</label>
				<shortLabel>landParcelNumber</shortLabel>
				<maxLength>10</maxLength>
                <validationPattern><anyCharacter allowWhitespace="true" /></validationPattern>
                <control>
                    <text size="10" />
                </control>
                <summary>landParcelNumber</summary>
                <description>Land parcel number.</description>
			</attributeReference>
-->
			
			<attributeReference name="landCountyName" sourceClassName="org.kuali.module.cams.bo.Asset" sourceAttributeName="landCountyName">
				<label>Land Country</label>
			</attributeReference>
			
			<attributeReference name="landAcreageSize" sourceClassName="org.kuali.module.cams.bo.Asset" sourceAttributeName="landAcreageSize">
				<label>Land Acreage</label>
			</attributeReference>
					
			<attributeReference name="landParcelNumber" sourceClassName="org.kuali.module.cams.bo.Asset" sourceAttributeName="landParcelNumber">
				<label>Land Parcel Number</label>
			</attributeReference>
			
        </attributes>
        
<!-- ## on hold
        <relationships>
			<relationship objectAttributeName="organizationOwnerAccount" >
				<primitiveAttribute sourceName="personUniversalIdentifier" targetName="personUniversalIdentifier" />
				<supportAttribute sourceName="organizationOwnerAccount.personUserIdentifier" targetName="personUserIdentifier" identifier="true" />
			</relationship>
		</relationships>
