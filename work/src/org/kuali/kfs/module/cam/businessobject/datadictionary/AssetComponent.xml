                <summary>componentOrganizationTagNumber</summary>
                <description>componentOrganizationTagNumber description...</description>
			</attribute>

            <attribute name="componentOrganizationText" forceUppercase="true">
                <label>Component Organization Text</label>
                <shortLabel>Text</shortLabel>
                <maxLength>255</maxLength>
                <validationPattern ><anyCharacter allowWhitespace="true" /></validationPattern>
