                    </inquiryFields>
                </inquirySection>
            </inquirySections>  
        </inquiry>

        <lookup>
            <title>Asset Component Lookup</title>

            <instructions>Look up an Asset Component</instructions>

            <defaultSort sortAscending="true">
                <sortAttributes>
                    <sortAttribute attributeName="capitalAssetNumber" />
                    <sortAttribute attributeName="componentNumber" />
                </sortAttributes>
            </defaultSort>

            <lookupFields>
                <lookupField attributeName="capitalAssetNumber" required="false" />
                <lookupField attributeName="componentConditionCode" required="false" />
                <lookupField attributeName="componentContactPhoneNumber" required="false" />
                <lookupField attributeName="componentDescription" required="false" />
                <lookupField attributeName="componentEstimatedLifetimeLimit" required="false" />
                <lookupField attributeName="componentManufacturerModelNumber" required="false" />
                <lookupField attributeName="componentManufacturerName" required="false" />
                <lookupField attributeName="componentNumber" required="false" />
                <lookupField attributeName="componentOrganizationTagNumber" required="false" />
                <lookupField attributeName="componentOrganizationText" required="false" />
                <lookupField attributeName="componentReplacementAmount" required="false" />
                <lookupField attributeName="componentSerialNumber" required="false" />
                <lookupField attributeName="componentVendorName" required="false" />
                <lookupField attributeName="componentWarrantyBeginningDate" required="false" />
                <lookupField attributeName="componentWarrantyContactName" required="false" />
                <lookupField attributeName="componentWarrantyEndingDate" required="false" />
                <lookupField attributeName="componentWarrantyNumber" required="false" />
                <lookupField attributeName="componentWarrantyPurchaseOrderNumber" required="false" />
                <lookupField attributeName="componentWarrantyText" required="false" />
                <lookupField attributeName="componentWarrantyPhoneNumber" required="false" />
                <lookupField attributeName="governmentTagNumber" required="false" />
                <lookupField attributeName="nationalStockNumber" required="false" /> 
            </lookupFields>

            <resultFields>
                <field attributeName="capitalAssetNumber" />
                <field attributeName="componentConditionCode" />
                <field attributeName="componentContactPhoneNumber" />
                <field attributeName="componentDescription" />
                <field attributeName="componentEstimatedLifetimeLimit" />
                <field attributeName="componentManufacturerModelNumber" />
                <field attributeName="componentManufacturerName" />
                <field attributeName="componentNumber" />
                <field attributeName="componentOrganizationTagNumber" />
                <field attributeName="componentOrganizationText" />
                <field attributeName="componentReplacementAmount" />
                <field attributeName="componentSerialNumber" />
                <field attributeName="componentVendorName" />
                <field attributeName="componentWarrantyBeginningDate" />
                <field attributeName="componentWarrantyContactName" />
                <field attributeName="componentWarrantyEndingDate" />
                <field attributeName="componentWarrantyNumber" />
                <field attributeName="componentWarrantyPurchaseOrderNumber" />
                <field attributeName="componentWarrantyText" />
                <field attributeName="componentWarrantyPhoneNumber" />
                <field attributeName="governmentTagNumber" />
                <field attributeName="nationalStockNumber" />
            </resultFields>
        </lookup>

        <attributes>

           <attributeReference name="capitalAssetNumber"
            	sourceClassName="org.kuali.module.cams.bo.Asset"
            	sourceAttributeName="capitalAssetNumber" />

            <attribute name="componentConditionCode" forceUppercase="true">
                <label>Component Condition Code</label>
                <shortLabel>Code</shortLabel>
                <maxLength>1</maxLength>
                <validationPattern ><anyCharacter allowWhitespace="true" /></validationPattern>
                <control>
					<select
						valuesFinderClass="org.kuali.core.lookup.keyvalues.PersistableBusinessObjectValuesFinder"
						businessObjectClass="org.kuali.module.cams.bo.AssetCondition"
						keyAttribute="assetConditionCode"
						labelAttribute="assetConditionName" includeKeyInLabel="false" />
				</control>
                <summary>componentConditionCode</summary>
                <description>componentConditionCode description...</description>
			</attribute>
            <attribute name="componentContactPhoneNumber" forceUppercase="true">
                <label>Component Contact Phone Number</label>
                <shortLabel>Number</shortLabel>
                <maxLength>10</maxLength>
                <validationPattern ><anyCharacter allowWhitespace="true" /></validationPattern>
                <control>
                    <text size="12" />
                </control>
                <summary>componentContactPhoneNumber</summary>
                <description>componentContactPhoneNumber description...</description>
			</attribute>

            <attribute name="componentDescription" forceUppercase="true">
                <label>Component Description</label>
                <shortLabel>Description</shortLabel>
                <maxLength>255</maxLength>
                <validationPattern ><anyCharacter allowWhitespace="true" /></validationPattern>
                <control>
                    <textarea rows="5" cols="80"/>
                </control>
                <summary>componentDescription</summary>
                <description>componentDescription description...</description>
			</attribute>

            <attribute name="componentEstimatedLifetimeLimit" forceUppercase="true">
                <label>Component Estimated Lifetime Limit</label>
                <shortLabel>Limit</shortLabel>
                <maxLength>5</maxLength>
                <validationPattern><numeric /></validationPattern>
                <control>
                    <text size="7" />
                </control>
                <summary>componentEstimatedLifetimeLimit</summary>
                <description>componentEstimatedLifetimeLimit description...</description>
			</attribute>

            <attribute name="componentManufacturerModelNumber" forceUppercase="true">
