                <shortLabel>Number</shortLabel>
                <maxLength>5</maxLength>
                <validationPattern><numeric /></validationPattern>
                <control>
                    <text size="7" />
                </control>
                <summary>componentNumber</summary>
                <description>componentNumber description...</description>
			</attribute>

            <attribute name="componentOrganizationTagNumber" forceUppercase="true">
                <label>Component Organization Tag Number</label>
                <shortLabel>Number</shortLabel>
                <maxLength>8</maxLength>
                <validationPattern ><anyCharacter allowWhitespace="true" /></validationPattern>
                <control>
                    <text size="10" />
