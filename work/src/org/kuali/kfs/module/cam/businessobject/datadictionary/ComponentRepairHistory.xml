		<lookup>
			<title>Component Repair History Lookup</title>
			<instructions>
				Look up a Component Repair History
			</instructions>
			<defaultSort sortAscending="true">
				<sortAttributes>
					<sortAttribute attributeName="capitalAssetNumber" />
					<sortAttribute attributeName="componentNumber" />
					<sortAttribute
						attributeName="componentIncidentDate" />
				</sortAttributes>
			</defaultSort>
			<lookupFields>
				<lookupField attributeName="capitalAssetNumber"
					required="false" />
				<lookupField
					attributeName="componentEstimatedRepairDate" required="false" />
				<lookupField attributeName="componentIncidentDate"
					required="false" />
				<lookupField attributeName="componentNumber"
					required="false" />
				<lookupField attributeName="componentProblemDescription"
					required="false" />
				<lookupField attributeName="componentRepairAmount"
					required="false" />
				<lookupField attributeName="componentRepairContactName"
					required="false" />
				<lookupField attributeName="componentRepairDate"
					required="false" />
				<lookupField attributeName="componentRepairNoteText"
					required="false" />
				<lookupField
					attributeName="componentRepairSolutionDescription"
					required="false" />
			</lookupFields>
			<resultFields>
				<field attributeName="capitalAssetNumber" />
				<field attributeName="componentEstimatedRepairDate" />
				<field attributeName="componentIncidentDate" />
				<field attributeName="componentNumber" />
				<field attributeName="componentProblemDescription" />
				<field attributeName="componentRepairAmount" />
				<field attributeName="componentRepairContactName" />
				<field attributeName="componentRepairDate" />
				<field attributeName="componentRepairNoteText" />
				<field
					attributeName="componentRepairSolutionDescription" />
			</resultFields>
		</lookup>
		<attributes>
			<attributeReference name="capitalAssetNumber"
				sourceClassName="org.kuali.module.cams.bo.Asset"
				sourceAttributeName="capitalAssetNumber" />
				
			<attributeReference name="componentNumber"
				sourceClassName="org.kuali.module.cams.bo.AssetComponent"
				sourceAttributeName="componentNumber" />
				
			<attributeReference name="componentIncidentDate"
				sourceClassName="org.kuali.core.bo.AttributeReferenceDummy"
				sourceAttributeName="genericDate">
				<label>Incident Date</label>
				<shortLabel>Date</shortLabel>
			</attributeReference>
			
			<attributeReference name="componentEstimatedRepairDate"
				sourceClassName="org.kuali.core.bo.AttributeReferenceDummy"
				sourceAttributeName="genericDate">
				<label>Estimated Repair Date Date</label>
				<shortLabel>Date</shortLabel>
			</attributeReference>
			
			<attribute name="componentProblemDescription"
				forceUppercase="true">
				<label>Component Problem Description</label>
				<shortLabel>Description</shortLabel>
				<maxLength>255</maxLength>
				<validationPattern>
					<anyCharacter allowWhitespace="true" />
				</validationPattern>
				<control>
					<textarea rows="5" cols="80" />
				</control>
				<summary>componentProblemDescription</summary>
				<description>
					componentProblemDescription description...
				</description>
