<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="ComponentRepairHistory" parent="ComponentRepairHistory-parentBean" />

  <bean id="ComponentRepairHistory-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.module.cams.bo.ComponentRepairHistory" />
    <property name="inquiryDefinition" >
      <ref bean="ComponentRepairHistory-inquiryDefinition" />
    </property>
    <property name="lookupDefinition" >
      <ref bean="ComponentRepairHistory-lookupDefinition" />
    </property>
    <property name="attributes" >
      <list>
        <ref bean="ComponentRepairHistory-capitalAssetNumber" />
        <ref bean="ComponentRepairHistory-componentNumber" />
        <ref bean="ComponentRepairHistory-componentIncidentDate" />
        <ref bean="ComponentRepairHistory-componentEstimatedRepairDate" />
        <ref bean="ComponentRepairHistory-componentProblemDescription" />
        <ref bean="ComponentRepairHistory-componentRepairAmount" />
        <ref bean="ComponentRepairHistory-componentRepairContactName" />
        <ref bean="ComponentRepairHistory-componentRepairDate" />
        <ref bean="ComponentRepairHistory-componentRepairNoteText" />
        <ref bean="ComponentRepairHistory-componentRepairSolutionDescription" />
        <ref bean="ComponentRepairHistory-versionNumber" />
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->


  <bean id="ComponentRepairHistory-capitalAssetNumber" parent="ComponentRepairHistory-capitalAssetNumber-parentBean" />

  <bean id="ComponentRepairHistory-capitalAssetNumber-parentBean" abstract="true" parent="Asset-capitalAssetNumber">
  </bean>
  <bean id="ComponentRepairHistory-componentNumber" parent="ComponentRepairHistory-componentNumber-parentBean" />

  <bean id="ComponentRepairHistory-componentNumber-parentBean" abstract="true" parent="AssetComponent-componentNumber">
  </bean>
  <bean id="ComponentRepairHistory-componentIncidentDate" parent="ComponentRepairHistory-componentIncidentDate-parentBean" />

  <bean id="ComponentRepairHistory-componentIncidentDate-parentBean" abstract="true" parent="GenericAttributes-genericDate">
    <property name="name" value="componentIncidentDate" />
    <property name="label" value="Incident Date" />
    <property name="shortLabel" value="Date" />
  </bean>
  <bean id="ComponentRepairHistory-componentEstimatedRepairDate" parent="ComponentRepairHistory-componentEstimatedRepairDate-parentBean" />

  <bean id="ComponentRepairHistory-componentEstimatedRepairDate-parentBean" abstract="true" parent="GenericAttributes-genericDate">
    <property name="name" value="componentEstimatedRepairDate" />
    <property name="label" value="Estimated Repair Date Date" />
    <property name="shortLabel" value="Date" />
  </bean>
  <bean id="ComponentRepairHistory-componentProblemDescription" parent="ComponentRepairHistory-componentProblemDescription-parentBean" />

  <bean id="ComponentRepairHistory-componentProblemDescription-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="componentProblemDescription" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Component Problem Description" />
    <property name="shortLabel" value="Description" />
    <property name="maxLength" value="255" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern"
            p:allowWhitespace="true" />
    </property>
    <property name="control" >
      <bean parent="TextareaControlDefinition"
            p:rows="5"
            p:cols="80" />
    </property>
    <property name="summary" value="componentProblemDescription" />
    <property name="description" value="componentProblemDescription description..." />
  </bean>
  <bean id="ComponentRepairHistory-componentRepairAmount" parent="ComponentRepairHistory-componentRepairAmount-parentBean" />

  <bean id="ComponentRepairHistory-componentRepairAmount-parentBean" abstract="true" parent="GenericAttributes-genericAmount">
    <property name="name" value="componentRepairAmount" />
    <property name="label" value="Repair Amount" />
    <property name="shortLabel" value="Amount" />
    <property name="formatterClass" value="org.kuali.core.web.format.CurrencyFormatter" />
  </bean>
  <bean id="ComponentRepairHistory-componentRepairContactName" parent="ComponentRepairHistory-componentRepairContactName-parentBean" />

  <bean id="ComponentRepairHistory-componentRepairContactName-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="componentRepairContactName" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Component Repair Contact Name" />
    <property name="shortLabel" value="Name" />
    <property name="maxLength" value="40" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern"
            p:allowWhitespace="true" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="42" />
    </property>
    <property name="summary" value="componentRepairContactName" />
    <property name="description" value="componentRepairContactName description..." />
  </bean>
  <bean id="ComponentRepairHistory-componentRepairDate" parent="ComponentRepairHistory-componentRepairDate-parentBean" />

  <bean id="ComponentRepairHistory-componentRepairDate-parentBean" abstract="true" parent="GenericAttributes-genericDate">
    <property name="name" value="componentRepairDate" />
    <property name="label" value="Repair Date Date" />
    <property name="shortLabel" value="Date" />
  </bean>
  <bean id="ComponentRepairHistory-componentRepairNoteText" parent="ComponentRepairHistory-componentRepairNoteText-parentBean" />

  <bean id="ComponentRepairHistory-componentRepairNoteText-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="componentRepairNoteText" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Component Repair Note Text" />
    <property name="shortLabel" value="Text" />
    <property name="maxLength" value="255" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern"
            p:allowWhitespace="true" />
    </property>
    <property name="control" >
      <bean parent="TextareaControlDefinition"
            p:rows="5"
            p:cols="80" />
    </property>
    <property name="summary" value="componentRepairNoteText" />
    <property name="description" value="componentRepairNoteText description..." />
  </bean>
  <bean id="ComponentRepairHistory-componentRepairSolutionDescription" parent="ComponentRepairHistory-componentRepairSolutionDescription-parentBean" />

  <bean id="ComponentRepairHistory-componentRepairSolutionDescription-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="componentRepairSolutionDescription" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Component Repair Solution Description" />
    <property name="shortLabel" value="Description" />
    <property name="maxLength" value="255" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern"
            p:allowWhitespace="true" />
    </property>
    <property name="control" >
      <bean parent="TextareaControlDefinition"
            p:rows="5"
            p:cols="80" />
    </property>
    <property name="summary" value="componentRepairSolutionDescription" />
    <property name="description" value="componentRepairSolutionDescription description..." />
  </bean>
  <bean id="ComponentRepairHistory-versionNumber" parent="ComponentRepairHistory-versionNumber-parentBean" />

  <bean id="ComponentRepairHistory-versionNumber-parentBean" abstract="true" parent="GenericAttributes-versionNumber">
  </bean>

<!-- Business Object Inquiry Definition -->


  <bean id="ComponentRepairHistory-inquiryDefinition" parent="ComponentRepairHistory-inquiryDefinition-parentBean" />

  <bean id="ComponentRepairHistory-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
    <property name="title" value="Component Repair History Inquiry" />
    <property name="inquirySections" >
      <list>
        <bean parent="InquirySectionDefinition">
          <property name="title" value="Header Text" />
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields" >
            <list>
              <bean parent="FieldDefinition"
                    p:attributeName="capitalAssetNumber" />
              <bean parent="FieldDefinition"
                    p:attributeName="componentEstimatedRepairDate" />
              <bean parent="FieldDefinition"
                    p:attributeName="componentIncidentDate" />
              <bean parent="FieldDefinition"
                    p:attributeName="componentNumber" />
              <bean parent="FieldDefinition"
                    p:attributeName="componentProblemDescription" />
              <bean parent="FieldDefinition"
                    p:attributeName="componentRepairAmount" />
              <bean parent="FieldDefinition"
                    p:attributeName="componentRepairContactName" />
              <bean parent="FieldDefinition"
                    p:attributeName="componentRepairDate" />
              <bean parent="FieldDefinition"
                    p:attributeName="componentRepairNoteText" />
              <bean parent="FieldDefinition"
                    p:attributeName="componentRepairSolutionDescription" />
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Business Object Lookup Definition -->


  <bean id="ComponentRepairHistory-lookupDefinition" parent="ComponentRepairHistory-lookupDefinition-parentBean" />

  <bean id="ComponentRepairHistory-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
    <property name="title" value="Component Repair History Lookup" />
    <property name="instructions" value="Look up a Component Repair History" />
    <property name="defaultSort" >
      <bean parent="SortDefinition">
      </bean>
    </property>
    <property name="lookupFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="capitalAssetNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="componentEstimatedRepairDate" />
        <bean parent="FieldDefinition"
              p:attributeName="componentIncidentDate" />
        <bean parent="FieldDefinition"
              p:attributeName="componentNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="componentProblemDescription" />
        <bean parent="FieldDefinition"
              p:attributeName="componentRepairAmount" />
        <bean parent="FieldDefinition"
              p:attributeName="componentRepairContactName" />
        <bean parent="FieldDefinition"
              p:attributeName="componentRepairDate" />
        <bean parent="FieldDefinition"
              p:attributeName="componentRepairNoteText" />
        <bean parent="FieldDefinition"
              p:attributeName="componentRepairSolutionDescription" />
      </list>
    </property>
    <property name="resultFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="capitalAssetNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="componentEstimatedRepairDate" />
        <bean parent="FieldDefinition"
              p:attributeName="componentIncidentDate" />
        <bean parent="FieldDefinition"
              p:attributeName="componentNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="componentProblemDescription" />
        <bean parent="FieldDefinition"
              p:attributeName="componentRepairAmount" />
        <bean parent="FieldDefinition"
              p:attributeName="componentRepairContactName" />
        <bean parent="FieldDefinition"
              p:attributeName="componentRepairDate" />
        <bean parent="FieldDefinition"
              p:attributeName="componentRepairNoteText" />
        <bean parent="FieldDefinition"
              p:attributeName="componentRepairSolutionDescription" />
      </list>
    </property>
  </bean>
</beans>
