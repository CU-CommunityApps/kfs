				sourceAttributeName="genericAmount">
				<label>Repair Amount</label>
				<shortLabel>Amount</shortLabel>
				<formatterClass>org.kuali.core.web.format.CurrencyFormatter</formatterClass>
			</attributeReference>
			
			<attribute name="componentRepairContactName"
				forceUppercase="true">
				<label>Component Repair Contact Name</label>
				<shortLabel>Name</shortLabel>
				<maxLength>40</maxLength>
				<validationPattern>
					<anyCharacter allowWhitespace="true" />
				</validationPattern>
				<control>
					<text size="42" />
				</control>
				<summary>componentRepairContactName</summary>
				<description>
					componentRepairContactName description...
				</description>
			</attribute>
			
			<attributeReference name="componentRepairDate"
				sourceClassName="org.kuali.core.bo.AttributeReferenceDummy"
				sourceAttributeName="genericDate">
				<label>Repair Date Date</label>
				<shortLabel>Date</shortLabel>
			</attributeReference>
			
			<attribute name="componentRepairNoteText"
				forceUppercase="true">
				<label>Component Repair Note Text</label>
				<shortLabel>Text</shortLabel>
				<maxLength>255</maxLength>
				<validationPattern>
					<anyCharacter allowWhitespace="true" />
				</validationPattern>
				<control>
					<textarea rows="5" cols="80" />
				</control>
				<summary>componentRepairNoteText</summary>
				<description>
					componentRepairNoteText description...
				</description>
			</attribute>

			<attribute name="componentRepairSolutionDescription"
				forceUppercase="true">
				<label>Component Repair Solution Description</label>
				<shortLabel>Description</shortLabel>
				<maxLength>255</maxLength>
				<validationPattern>
