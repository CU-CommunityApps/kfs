		<inquiry>
			<title>Component Repair History Inquiry</title>
			<inquirySections>
				<inquirySection title="Header Text">
					<inquiryFields>
						<field attributeName="capitalAssetNumber" />
						<field
							attributeName="componentEstimatedRepairDate" />
						<field attributeName="componentIncidentDate" />
						<field attributeName="componentNumber" />
						<field
							attributeName="componentProblemDescription" />
						<field attributeName="componentRepairAmount" />
						<field
							attributeName="componentRepairContactName" />
						<field attributeName="componentRepairDate" />
						<field attributeName="componentRepairNoteText" />
						<field
							attributeName="componentRepairSolutionDescription" />
					</inquiryFields>
				</inquirySection>
			</inquirySections>
