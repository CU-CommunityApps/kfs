<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="AssetObjectCodeMaintenanceDocument" parent="AssetObjectCodeMaintenanceDocument-parentBean" />

  <bean id="AssetObjectCodeMaintenanceDocument-parentBean" abstract="true" parent="MaintenanceDocumentEntry">
    <property name="businessObjectClass" value="org.kuali.kfs.module.cam.businessobject.AssetObjectCode" />
    <property name="maintainableClass" value="org.kuali.rice.kns.maintenance.KualiMaintainableImpl" />
    <property name="maintainableSections" >
      <list>
        <ref bean="AssetObjectCodeMaintenanceDocument-EditAssetObjectCode" />
      </list>
    </property>
    <property name="defaultExistenceChecks" >
      <list>
        <bean parent="ReferenceDefinition"
              p:attributeName="capitalizationFinancialObject"
              p:attributeToHighlightOnFail="capitalizationFinancialObjectCode" />
        <bean parent="ReferenceDefinition"
              p:attributeName="accumulatedDepreciationFinancialObject"
              p:attributeToHighlightOnFail="accumulatedDepreciationFinancialObjectCode" />
        <bean parent="ReferenceDefinition"
              p:attributeName="depreciationExpenseFinancialObject"
              p:attributeToHighlightOnFail="depreciationExpenseFinancialObjectCode" />
      </list>
    </property>
    <property name="lockingKeys" >
      <list>
        <value>universityFiscalYear</value>
        <value>chartOfAccountsCode</value>
        <value>financialObjectSubTypeCode</value>
      </list>
    </property>
    <property name="label" value="Asset Object Code" />
    <property name="shortLabel" value="Asset Object Code" />
    <property name="summary" value="Asset Object Code maintenance document" />
    <property name="description" value="Document used to create or update Asset Object Code objects" />
    <property name="documentTypeName" value="AssetObjectCodeMaintenanceDocument" />
    <property name="businessRulesClass" value="org.kuali.rice.kns.maintenance.rules.MaintenanceDocumentRuleBase" />
    <property name="documentAuthorizerClass" value="org.kuali.kfs.sys.document.authorization.FinancialSystemMaintenanceDocumentAuthorizerBase" />
    <property name="documentTypeCode" value="COBJ" />
    <property name="authorizations" >
      <list>
        <bean parent="AuthorizationDefinition">
          <property name="action" value="initiate" />
          <property name="authorizedGroups" >
            <set>
              <value>SY_MAINTENANCE_USERS</value>
            </set>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Maintenance Section Definitions -->


  <bean id="AssetObjectCodeMaintenanceDocument-EditAssetObjectCode" parent="AssetObjectCodeMaintenanceDocument-EditAssetObjectCode-parentBean" />

  <bean id="AssetObjectCodeMaintenanceDocument-EditAssetObjectCode-parentBean" abstract="true" parent="MaintainableSectionDefinition">
    <property name="id" value="Edit Asset Object Code" />
    <property name="title" value="Edit Asset Object Code" />
    <property name="maintainableItems" >
      <list>
        <bean parent="MaintainableFieldDefinition"
              p:name="universityFiscalYear"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="chartOfAccountsCode"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="financialObjectSubTypeCode"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="capitalizationFinancialObjectCode" />
        <bean parent="MaintainableFieldDefinition"
              p:name="accumulatedDepreciationFinancialObjectCode" />
        <bean parent="MaintainableFieldDefinition"
              p:name="depreciationExpenseFinancialObjectCode" />
        <bean parent="MaintainableFieldDefinition"
              p:name="active" />
      </list>
    </property>
  </bean>
</beans>
