<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="BarcodeInventoryErrorDocument" parent="BarcodeInventoryErrorDocument-parentBean" />

  <bean id="BarcodeInventoryErrorDocument-parentBean" abstract="true" parent="TransactionalDocumentEntry">
    <property name="label" value="Barcode Inventory Error" />
    <property name="shortLabel" value="Barcode Inventory Error" />
    <property name="summary" value="&amp;nbsp;" />
    <property name="description" value="Barcode Inventory Error Document" />
    <property name="documentTypeName" value="BarcodeInventoryErrorDocument" />
    
    <property name="documentClass" value="org.kuali.module.cams.document.BarcodeInventoryErrorDocument" />
    <property name="businessRulesClass" value="org.kuali.module.cams.rules.BarcodeInventoryErrorDocumentRule" />
    <property name="documentAuthorizerClass" value="org.kuali.module.cams.document.authorization.BarcodeInventoryErrorDocumentAuthorizer"/>
    
    <property name="documentTypeCode" value="BCIE" />
    <property name="authorizations" >
      <list>
        <bean parent="AuthorizationDefinition">
          <property name="action" value="initiate" />
          <property name="authorizedGroups" >
            <set>
              <value>kualiUniversalGroup</value>
            </set>
          </property>
        </bean>
      </list>
    </property>
    <property name="attributes" >
      <list>
        <ref bean="BarcodeInventoryErrorDocument-documentNumber" />
        <ref bean="BarcodeInventoryErrorDocument-uploaderUniversalIdentifier" />
        <ref bean="BarcodeInventoryErrorDocument-versionNumber" />
      </list>
    </property>
  </bean>
 
 <!-- Attribute Definitions -->


  <bean id="BarcodeInventoryErrorDocument-documentNumber" parent="BarcodeInventoryErrorDocument-documentNumber-parentBean" />
  <bean id="BarcodeInventoryErrorDocument-documentNumber-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="documentNumber" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Document Number" />
    <property name="shortLabel" value="Number" />
    <property name="maxLength" value="14" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="16" />
    </property>
    <property name="summary" value="documentNumber" />
    <property name="description" value="documentNumber description..." />
  </bean>
  
  <bean id="BarcodeInventoryErrorDocument-uploaderUniversalIdentifier" parent="BarcodeInventoryErrorDocument-uploaderUniversalIdentifier-parentBean" />
  <bean id="BarcodeInventoryErrorDocument-uploaderUniversalIdentifier-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="uploaderUniversalIdentifier" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Uploader Universal Identifier" />
    <property name="shortLabel" value="Identifier" />
    <property name="maxLength" value="10" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="12" />
    </property>
    <property name="summary" value="uploaderUniversalIdentifier" />
    <property name="description" value="uploaderUniversalIdentifier description..." />
  </bean>
  
  
  <bean id="BarcodeInventoryErrorDocument-versionNumber" parent="BarcodeInventoryErrorDocument-versionNumber-parentBean" />
  <bean id="BarcodeInventoryErrorDocument-versionNumber-parentBean" abstract="true" parent="GenericAttributes-versionNumber">
  </bean>


</beans>
