<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean name="AssetFabricationMaintenanceDocument" parent="AssetMaintenanceDocument">
    <property name="businessObjectClass" value="org.kuali.kfs.module.cam.businessobject.AssetFabrication" />
    <property name="label" value="Asset Fabrication" />
    <property name="shortLabel" value="Asset Fabrication" />
    <property name="documentTypeName" value="AssetFabricationMaintenanceDocument" />
    <property name="documentTypeCode" value="FR" />
    <property name="workflowProperties" >
      <ref bean="AssetFabricationMaintenanceDocument-workflowProperties" />
    </property>
  </bean>

  <bean id="AssetFabricationMaintenanceDocument-workflowProperties" parent="AssetFabricationMaintenanceDocument-workflowProperties-parentBean" />

  <bean id="AssetFabricationMaintenanceDocument-workflowProperties-parentBean" abstract="true" parent="WorkflowProperties">
    <property name="workflowPropertyGroups" >
      <list>
        <bean parent="WorkflowPropertyGroup">
          <property name="workflowProperties" >
            <list>
              <bean parent="WorkflowProperty"
                    p:path="newMaintainableObject.businessObject" />
                <bean parent="WorkflowProperty"
                    p:path="newMaintainableObject.businessObject.organizationOwnerAccount" />    
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>
</beans>
