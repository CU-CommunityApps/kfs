<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean name="AssetFabricationMaintenanceDocument" parent="AssetMaintenanceDocument">
    <property name="businessObjectClass" value="org.kuali.kfs.module.cam.businessobject.AssetFabrication" />
    <property name="label" value="Asset Fabrication" />
    <property name="shortLabel" value="Asset Fabrication" />
    <property name="documentTypeName" value="AssetFabricationMaintenanceDocument" />
    <property name="documentTypeCode" value="FR" />
    <property name="workflowAttributes" >
      <ref bean="AssetFabricationMaintenanceDocument-workflowAttributes" />
    </property>
  </bean>

  <bean id="AssetFabricationMaintenanceDocument-workflowAttributes" parent="AssetFabricationMaintenanceDocument-workflowAttributes-parentBean" />

  <bean id="AssetFabricationMaintenanceDocument-workflowAttributes-parentBean" abstract="true" parent="WorkflowAttributes">
    <property name="searchingTypeDefinitions">
    		<list>
    			<bean class="org.kuali.rice.kns.datadictionary.SearchingTypeDefinition">
    				<property name="searchingAttribute">
    					<bean class="org.kuali.rice.kns.datadictionary.SearchingAttribute">
    						<property name="businessObjectClassName" value="org.kuali.kfs.module.cam.businessobject.Asset" />
    						<property name="attributeName" value="capitalAssetNumber" />
    					</bean>
    				</property>
    				<property name="documentValues">
    					<list>
    						<value>newMaintainableObject.businessObject.capitalAssetNumber</value>
    					</list>
    				</property>
    			</bean>
    		</list>
    	</property>
    	<property name="routingTypeDefinitions">
    		<map>
    			<entry key="Account" value-ref="RoutingType-CapitalAssetManagementComplexMaintenanceDocument-Account" />
    			<entry key="OrganizationHierarchy" value-ref="RoutingType-CapitalAssetManagementComplexMaintenanceDocument-OrganizationHierarchy" />
    			<entry key="Management" value-ref="RoutingType-AssetFabricationMaintenanceDocument-Management" />
    		</map>
    	</property>
  </bean>
  
    
  <bean id="RoutingType-AssetFabricationMaintenanceDocument-Management" class="org.kuali.rice.kns.datadictionary.RoutingTypeDefinition">
    	<property name="routingAttributes">
    		<list>
		        <ref bean="RoutingAttribute-chartOfAccountsCode" />
		        <ref bean="RoutingAttribute-organizationCode" />
    		</list>
    	</property>
    	<property name="documentValuePathGroups">
    		<list>
    			<bean class="org.kuali.rice.kns.datadictionary.DocumentValuePathGroup">
    				<property name="documentValues">
	    				<list>
	    					<value>newMaintainableObject.businessObject.organizationOwnerChartOfAccountsCode</value>
	    					<value>newMaintainableObject.businessObject.organizationOwnerAccount.organizationCode</value>	    					
	    				</list>
    				</property>
    			</bean>
    		</list>
    	</property>
    </bean>
</beans>
