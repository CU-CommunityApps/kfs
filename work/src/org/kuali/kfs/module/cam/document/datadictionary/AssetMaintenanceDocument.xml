<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "../../../../core/datadictionary/dataDictionary-1_0.dtd">

<dictionaryEntry>
    <maintenanceDocument>
        <businessObjectClass>org.kuali.module.cams.bo.Asset</businessObjectClass>
        <maintainableClass>org.kuali.module.cams.maintenance.AssetMaintainableImpl</maintainableClass>
        <businessRulesClass>org.kuali.core.maintenance.rules.MaintenanceDocumentRuleBase</businessRulesClass>
        <documentAuthorizerClass>org.kuali.core.document.authorization.MaintenanceDocumentAuthorizerBase</documentAuthorizerClass>
        <authorizations>
            <authorization action="initiate">
                <workgroups>
                    <workgroup>KUALI_ROLE_MAINTENANCE</workgroup>
                </workgroups>
            </authorization>
        </authorizations>

        <documentTypeName>AssetMaintenanceDocument</documentTypeName>
        <documentTypeCode>AA</documentTypeCode>

        <label>Asset</label>

        <summary>Asset maintenance document</summary>
        <description>Document used to create or update Asset objects</description>

        <lockingKeys>
            <field attributeName="capitalAssetNumber" />
        </lockingKeys>
        
        <maintainableSections>
            <maintainableSection title="Asset Detail Info">
                <maintainableItems>
                	<subSectionHeader name="Asset Detail Info" />
                    <!-- where to get these information? Owner, Org, Federal Contribution,  Aquisition Type -->
                    <!--  maintainableField name="capitalAssetNumber" required="true" / -->
                    <maintainableField name="organizationOwnerChartOfAccountsCode" />
                    <maintainableField name="organizationOwnerAccountNumber" />
                    <maintainableField name="agencyNumber" />
                    <maintainableField name="acquisitionTypeCode" />
                    <maintainableField name="conditionCode" required="true" />
                    <maintainableField name="capitalAssetDescription" required="true" />
                    
                    
                    <maintainableField name="capitalAssetTypeCode" required="true" />
                    <maintainableField name="vendorName" />
                    <maintainableField name="manufacturerName" />
                    
                    <maintainableField name="manufacturerModelNumber" />
					<maintainableField name="serialNumber" />
                    
                    <maintainableField name="campusTagNumber" />
                    <maintainableField name="oldTagNumber" />
                    <maintainableField name="lastInventoryDate" />
                    <maintainableField name="inventoryStatusCode" />
                    
                    <maintainableField name="createDate" required="true" />
                    
                    <maintainableField name="financialDocumentPostingPeriodCode" />
					<maintainableField name="financialDocumentPostingYear" />
					
                    
                    
                    <maintainableField name="totalCostAmount" />
                   
					
					<maintainableField name="active">
                        <defaultValue>true</defaultValue>
                    </maintainableField>
                    <maintainableField name="versionNumber" />
					
					<subSectionHeader name="Location - On Campus" />
					<maintainableField name="campusCode" />
					<maintainableField name="buildingCode" />
					<maintainableField name="buildingRoomNumber" />
					<maintainableField name="buildingSubRoomNumber" />
					
					<!--  subSectionHeader name="Location - Off Campus - Where to get the ifo for this From? Name, Address, City, State, Zip?" /-->
					
					
					</maintainableItems>
            </maintainableSection>
            
            <maintainableSection title="Organization Info">
                <maintainableItems>
                	<!-- Organization Tag, Asset Representative, Org Asset Type, Organization Text, Receive Date -->
                	<maintainableField name="organizationInventoryName" />
                	<maintainableField name="representativeUniversalIdentifier" />
                 </maintainableItems>
            </maintainableSection>
            
            <maintainableSection title="Depreciation">
                <maintainableItems>
					
					<maintainableField name="primaryDepreciationMethodCode" />
					<maintainableField name="secondaryDepreciationMethodCode" />
					<maintainableField name="replacementAmount" />
					<maintainableField name="salePrice" />
					<maintainableField name="estimatedSellingPrice" />
					<maintainableField name="salvageAmount" />
					
				</maintainableItems>
			</maintainableSection>
            
            <maintainableSection title="Retire">
                <maintainableItems>
					
					<maintainableField name="retirementChartOfAccountsCode" />
					<maintainableField name="retirementAccountNumber" />
					<maintainableField name="retirementReasonCode" />
					<maintainableField name="retirementPeriodCode" />
					<maintainableField name="retirementFiscalYear" />
					
				</maintainableItems>
			</maintainableSection>
            
            <maintainableSection title="Find a proper home for these attributes?">
              	
              	<maintainableItems>
            			
					
					 <!--  Where to put the following information? -->
                    <maintainableField name="receiveDate" required="true" />
                    <maintainableField name="loanReturnDate" />
					<maintainableField name="loanDate" />
					<maintainableField name="expectedReturnDate" />
					
					<maintainableField name="cashReceiptFinancialDocumentNumber" />
					
					<maintainableField name="estimatedFabricationCompletionDate" />
					<maintainableField name="fabricationEstimatedTotalAmount" />
					<maintainableField name="transferOfFundsFinancialDocumentNumber" />
					<maintainableField name="insuranceChargeAccountNumber" />
					<maintainableField name="insuranceChartOfAccountsCode" />

					
					<maintainableField name="campusPoliceDepartmentCaseNumber" />
					<maintainableField name="inventoryScannedCode" />
					<maintainableField name="signatureCode" />
				    
					
                </maintainableItems>
            </maintainableSection>
        </maintainableSections>
    </maintenanceDocument>
</dictionaryEntry>