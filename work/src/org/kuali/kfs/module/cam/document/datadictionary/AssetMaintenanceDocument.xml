<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="AssetMaintenanceDocument" parent="AssetMaintenanceDocument-parentBean" />

  <bean id="AssetMaintenanceDocument-parentBean" abstract="true" parent="MaintenanceDocumentEntry">
    <property name="businessObjectClass" value="org.kuali.kfs.module.cam.businessobject.Asset" />
    <property name="maintainableClass" value="org.kuali.kfs.module.cam.document.AssetMaintainableImpl" />
    <property name="maintainableSections" >
      <list>
        <ref bean="AssetMaintenanceDocument-AssetDetailInformation" />
        <ref bean="AssetMaintenanceDocument-AssetLocation" />
        <ref bean="AssetMaintenanceDocument-OrganizationInformation" />
        <ref bean="AssetMaintenanceDocument-FabricationInformation" />
        <ref bean="AssetMaintenanceDocument-LandInformation" />
        <ref bean="AssetMaintenanceDocument-AssetDepreciationInformation" />
        <ref bean="AssetMaintenanceDocument-ViewMergeHistory" />
        <ref bean="AssetMaintenanceDocument-ViewAssetSeparateHistory" />
        <ref bean="AssetMaintenanceDocument-ViewRetirementInformation" />
        <ref bean="AssetMaintenanceDocument-ViewEquipmentLoanInformation" />
        <ref bean="AssetMaintenanceDocument-Warranty" />
        <ref bean="AssetMaintenanceDocument-RepairHistory" />
        <ref bean="AssetMaintenanceDocument-Components" />
        <ref bean="AssetMaintenanceDocument-PaymentInformation" />
      </list>
    </property>
    <property name="defaultExistenceChecks" >
      <list>
        <bean parent="ReferenceDefinition"
              p:attributeName="agency"
              p:attributeToHighlightOnFail="agencyNumber" />
        <bean parent="ReferenceDefinition"
              p:attributeName="campus"
              p:attributeToHighlightOnFail="campusCode" />
        <bean parent="ReferenceDefinition"
              p:attributeName="building"
              p:attributeToHighlightOnFail="buildingCode" />
        <bean parent="ReferenceDefinition"
              p:attributeName="buildingRoom"
              p:attributeToHighlightOnFail="buildingRoomNumber" />
        <bean parent="ReferenceDefinition"
              p:attributeName="capitalAssetType"
              p:attributeToHighlightOnFail="capitalAssetTypeCode" />
        <bean parent="ReferenceDefinition"
              p:attributeName="organizationOwnerAccount"
              p:attributeToHighlightOnFail="organizationOwnerAccountNumber" />
        <bean parent="ReferenceDefinition"
              p:attributeName="organizationOwnerChartOfAccounts"
              p:attributeToHighlightOnFail="organizationOwnerChartOfAccountsCode" />
        <bean parent="ReferenceDefinition"
              p:attributeName="condition"
              p:attributeToHighlightOnFail="conditionCode" />
        <bean parent="ReferenceDefinition"
              p:attributeName="inventoryStatus"
              p:attributeToHighlightOnFail="inventoryStatusCode" />     
        <bean parent="ReferenceDefinition"
              p:attributeName="acquisitionType"
              p:attributeToHighlightOnFail="acquisitionTypeCode"/>
      </list>
    </property>
    <property name="lockingKeys" >
      <list>
        <value>capitalAssetNumber</value>
      </list>
    </property>
    <property name="label" value="Asset" />
    <property name="shortLabel" value="Asset" />
    <property name="summary" value="Asset maintenance document" />
    <property name="description" value="Document used to create or update Asset objects" />
    <property name="documentTypeName" value="AssetMaintenanceDocument" />
    <property name="businessRulesClass" value="org.kuali.kfs.module.cam.document.validation.impl.AssetRule" />
    <property name="documentAuthorizerClass" value="org.kuali.kfs.module.cam.document.authorization.AssetAuthorizer" />
    <property name="documentTypeCode" value="CASM" />
    <property name="workflowAttributes" >
      <ref bean="AssetMaintenanceDocument-workflowAttributes" />
    </property>
    <property name="authorizations" >
      <list>
        <bean parent="AuthorizationDefinition">
          <property name="action" value="initiate" />
          <property name="authorizedGroups" >
            <set>
              <value>kualiUniversalGroup</value>
            </set>
          </property>
        </bean>
      </list>
    </property>
    <property name="additionalSectionsFile" value="/jsp/modules/cams/AssetPaymentsLookupLink.jsp"></property>
  </bean>

