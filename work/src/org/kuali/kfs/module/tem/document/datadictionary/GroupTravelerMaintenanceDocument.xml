<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

<bean id="GroupTravelerMaintenanceDocument" parent="GroupTravelerMaintenanceDocument-parentBean" />

<bean id="GroupTravelerMaintenanceDocument-parentBean" abstract="true" parent="MaintenanceDocumentEntry">
	<property name="businessObjectClass" value="org.kuali.kfs.module.tem.businessobject.GroupTraveler" />
	<property name="maintainableClass"   value="org.kuali.rice.kns.maintenance.KualiMaintainableImpl" />
	<property name="maintainableSections" >
	<list>
		<ref bean="GroupTravelerMaintenanceDocument-Section" />
	</list>
	</property>
	<property name="allowsNewOrCopy" value="true" />
	<property name="lockingKeys">
	<list>
		<value>id</value>
	</list>
	</property>
	<property name="documentTypeName"        value="TGT" />
	<property name="documentAuthorizerClass" value="org.kuali.kfs.sys.document.authorization.FinancialSystemMaintenanceDocumentAuthorizerBase" />

</bean>

<bean id="GroupTravelerMaintenanceDocument-Section" parent="GroupTravelerMaintenanceDocument-Section-parentBean" />
<bean id="GroupTravelerMaintenanceDocument-Section-parentBean" abstract="true" parent="MaintainableSectionDefinition">
		<property name="id"    value="GroupTraveler" />
		<property name="title" value="Edit GroupTraveler" />
		<property name="maintainableItems"><list>
			<bean parent="MaintainableFieldDefinition" p:name="id" p:required="false"/>
			<bean parent="MaintainableFieldDefinition" p:name="name" p:required="true"/>
			<bean parent="MaintainableFieldDefinition" p:name="travelerTypeCode" p:required="true"/>
		</list></property>
</bean>
</beans>