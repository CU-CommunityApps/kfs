<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

<bean id="AccountingDocumentRelationshipMaintenanceDocument" parent="AccountingDocumentRelationshipMaintenanceDocument-parentBean" />

<bean id="AccountingDocumentRelationshipMaintenanceDocument-parentBean" abstract="true" parent="MaintenanceDocumentEntry">
	<property name="businessObjectClass" value="org.kuali.kfs.module.tem.businessobject.AccountingDocumentRelationship" />
	<property name="maintainableClass"   value="org.kuali.kfs.module.tem.document.maintenance.AccountingDocumentRelationshipMaintainable" />
	<property name="maintainableSections" >
	<list>
		<ref bean="AccountingDocumentRelationshipMaintenanceDocument-Section" />
	</list>
	</property>
	<property name="allowsNewOrCopy" value="true" />
	<property name="lockingKeys">
	<list>
	</list>
	</property>
	<property name="documentTypeName"        value="ADR" />
	<property name="documentAuthorizerClass" value="org.kuali.kfs.sys.document.authorization.FinancialSystemMaintenanceDocumentAuthorizerBase" />
	<property name="businessRulesClass" value="org.kuali.kfs.module.tem.document.validation.impl.AccountingDocumentRelationshipValidation" />
</bean>

<bean id="AccountingDocumentRelationshipMaintenanceDocument-Section" parent="AccountingDocumentRelationshipMaintenanceDocument-Section-parentBean" />
<bean id="AccountingDocumentRelationshipMaintenanceDocument-Section-parentBean" abstract="true" parent="MaintainableSectionDefinition">
		<property name="id"    value="AccountingDocumentRelationship" />
		<property name="title" value="Edit AccountingDocumentRelationship" />
		<property name="maintainableItems"><list>
			<bean parent="MaintainableFieldDefinition" p:name="id" p:required="false"/>
			<bean parent="MaintainableFieldDefinition" p:name="documentNumber" p:required="true"/>
			<bean parent="MaintainableFieldDefinition" p:name="relDocumentNumber" p:required="true"/>
			<bean parent="MaintainableFieldDefinition" p:name="description" p:required="false"/>
			<bean parent="MaintainableFieldDefinition" p:name="active" p:required="true"/>
		</list></property>
</bean>
</beans>