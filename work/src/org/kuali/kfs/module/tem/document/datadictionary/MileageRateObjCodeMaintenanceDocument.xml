<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

	<bean id="MileageRateObjCodeMaintenanceDocument" parent="MileageRateObjCodeMaintenanceDocument-parentBean" />	
	<bean id="MileageRateObjCodeMaintenanceDocument-parentBean" abstract="true" parent="MaintenanceDocumentEntry">
		<property name="businessObjectClass" value="org.kuali.kfs.module.tem.businessobject.MileageRateObjCode" />
		<property name="maintainableClass"   value="org.kuali.rice.kns.maintenance.KualiMaintainableImpl" />
		<property name="maintainableSections" >
			<list>
				<ref bean="MileageRateObjCodeMaintenanceDocument-Section" />
			</list>
		</property>
		<property name="allowsNewOrCopy" value="true" />
		<property name="lockingKeys">
			<list>
				<value>id</value>
			</list>
		</property>
		<property name="documentTypeName"        value="TMRC" />
		<property name="documentAuthorizerClass" value="org.kuali.kfs.sys.document.authorization.FinancialSystemMaintenanceDocumentAuthorizerBase" />
	</bean>
	
	<bean id="MileageRateObjCodeMaintenanceDocument-Section" parent="MileageRateObjCodeMaintenanceDocument-Section-parentBean" />
	<bean id="MileageRateObjCodeMaintenanceDocument-Section-parentBean" abstract="true" parent="MaintainableSectionDefinition">
			<property name="id"    value="MileageRateObjCode" />
			<property name="title" value="Edit MileageRateObjCode" />
			<property name="maintainableItems">
				<list>
					<bean parent="MaintainableFieldDefinition" p:name="id" p:required="false"/>
					<bean parent="MaintainableFieldDefinition" p:name="travelerTypeCode" p:required="true"/>
					<bean parent="MaintainableFieldDefinition" p:name="tripTypeCode" p:required="true"/>
					<bean parent="MaintainableFieldDefinition" p:name="mileageRateId" p:required="true"/>
					<bean parent="MaintainableFieldDefinition" p:name="financialObjectCode" p:required="true"/>
					<bean parent="MaintainableFieldDefinition" p:name="documentType" p:required="true"/>
					<bean parent="MaintainableFieldDefinition" p:name="active" p:required="true"/>
				</list>
			</property>
	</bean>
</beans>