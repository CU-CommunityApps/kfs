<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

	<bean id="TravelAgencyAuditMaintenanceDocument" parent="TravelAgencyAuditMaintenanceDocument-parentBean" />

	<bean id="TravelAgencyAuditMaintenanceDocument-parentBean" abstract="true" parent="MaintenanceDocumentEntry">
		<property name="businessObjectClass" value="org.kuali.kfs.module.tem.businessobject.AgencyStagingData" />
		<property name="maintainableClass" value="org.kuali.kfs.module.tem.document.maintenance.TravelAgencyAuditMaintainable" />
		<property name="maintainableSections">
			<list>
				<ref bean="TravelAgencyAuditOverview-Section" />
                <ref bean="TripInformation-Section" />
                <ref bean="TravelerInformation-Section" />
				<ref bean="AccountingInformation-Section" />
				<ref bean="Airline-Section" />
				<ref bean="Lodging-Section" />
				<ref bean="RentalCar-Section" />
			</list>
		</property>
		<property name="allowsNewOrCopy" value="false" />
		<property name="allowsRecordDeletion" value="true" />
		<property name="lockingKeys">
			<list>
				<value>id</value>
			</list>
		</property>
		<property name="documentTypeName" value="TAAR" />
        <property name="documentAuthorizerClass" value="org.kuali.kfs.module.tem.document.authorization.TravelAgencyAuditAuthorizer"/>
		<property name="businessRulesClass" value="org.kuali.kfs.module.tem.document.validation.impl.TravelAgencyAuditValidation" />
		<property name="documentPresentationControllerClass" value="org.kuali.kfs.module.tem.document.authorization.TravelAgencyAuditPresentationController" />
	</bean>

	<bean id="TravelAgencyAuditOverview-Section" parent="TravelAgencyAuditOverview-Section-parentBean" />
	<bean id="TravelAgencyAuditOverview-Section-parentBean" abstract="true" parent="MaintainableSectionDefinition">
		<property name="id" value="TravelAgencyAudit" />
		<property name="title" value="Edit Travel Agency Data" />
		<property name="maintainableItems">
			<list>
			    <bean parent="MaintainableSubSectionHeaderDefinition" p:name="Travel Agency Data"/>
                <bean parent="MaintainableFieldDefinition" p:name="agencyDataId" p:required="false" />
                <bean parent="MaintainableFieldDefinition" p:name="creditCardOrAgencyCode" p:required="false" />
                <bean parent="MaintainableFieldDefinition" p:name="agency" p:required="true" />
                <bean parent="MaintainableFieldDefinition" p:name="otherCompanyName" p:required="false" />
                <bean parent="MaintainableFieldDefinition" p:name="agencyFileName" p:required="false" />
                <bean parent="MaintainableFieldDefinition" p:name="merchantName" p:required="false" />
                <bean parent="MaintainableFieldDefinition" p:name="billingCycleDate" p:required="false" />
                <bean parent="MaintainableFieldDefinition" p:name="transactionPostingDate" p:unconditionallyReadOnly="true" p:required="true" />
                <bean parent="MaintainableFieldDefinition" p:name="processingTimestamp" p:unconditionallyReadOnly="true" />
				
			</list>
		</property>
	</bean>
	
	<bean id="TripInformation-Section" parent="TripInformation-Section-parentBean" />
    <bean id="TripInformation-Section-parentBean" abstract="true" parent="MaintainableSectionDefinition">
        <property name="id" value="TravelAgencyAudit" />
        <property name="title" value="Edit Trip Data" />
        <property name="maintainableItems">
            <list>
                <bean parent="MaintainableFieldDefinition" p:name="tripId" p:required="true" />
                <bean parent="MaintainableFieldDefinition" p:name="tripInvoiceNumber" p:required="true" />
                <bean parent="MaintainableFieldDefinition" p:name="tripTravelerTypeId" />
                <bean parent="MaintainableFieldDefinition" p:name="otherAmount" />
                
            </list>
        </property>
    </bean>
    
    <bean id="TravelerInformation-Section" parent="TravelerInformation-Section-parentBean" />
    <bean id="TravelerInformation-Section-parentBean" abstract="true" parent="MaintainableSectionDefinition">
        <property name="id" value="TravelAgencyAudit" />
        <property name="title" value="Edit Traveler Data" />
        <property name="maintainableItems">
            <list>
                <bean parent="MaintainableFieldDefinition" p:name="travelerName" p:required="true" />
                <bean parent="MaintainableFieldDefinition" p:name="travelerId" p:required="false" />
                <bean parent="MaintainableFieldDefinition" p:name="travelerNetworkId" p:required="false" />
                <bean parent="MaintainableFieldDefinition" p:name="tripExpenseAmount" p:required="true" />
                <bean parent="MaintainableFieldDefinition" p:name="alternateTripId" p:required="true" />
                <bean parent="MaintainableFieldDefinition" p:name="tripArrangerName" p:required="false" />
                
            </list>
        </property>
    </bean>

	<bean id="Lodging-Section" parent="Lodging-Section-parentBean" />
	<bean id="Lodging-Section-parentBean" abstract="true" parent="MaintainableSectionDefinition">
		<property name="id" value="TravelAgencyAudit" />
		<property name="title" value="Lodging" />
		<property name="maintainableItems">
			<list>
			    <bean parent="MaintainableSubSectionHeaderDefinition" p:name="Lodging"/>
			    <bean parent="MaintainableFieldDefinition" p:name="lodgingItineraryNumber" />
			 	<bean parent="MaintainableFieldDefinition" p:name="lodgingPropertyName"/>
				<bean parent="MaintainableFieldDefinition" p:name="lodgingAmount"  />
				<bean parent="MaintainableFieldDefinition" p:name="lodgingPrepayDate" />
				<bean parent="MaintainableFieldDefinition" p:name="lodgingPrepayDaysNumber"/>
				<bean parent="MaintainableFieldDefinition" p:name="lodgingPropertyCityName" />
				<bean parent="MaintainableFieldDefinition" p:name="tripLodgingArrivalDate" />
				<bean parent="MaintainableFieldDefinition" p:name="lodgingBookingDate"/>
                <bean parent="MaintainableFieldDefinition" p:name="lodgingDepartureDate" />
				<bean parent="MaintainableFieldDefinition" p:name="lodgingPropertyStateCode" />
				<bean parent="MaintainableFieldDefinition" p:name="lodgingCountryName"/>
			</list>
		</property>
	</bean>
	<bean id="Airline-Section" parent="Airline-Section-parentBean" />
	<bean id="Airline-Section-parentBean" abstract="true" parent="MaintainableSectionDefinition">
		<property name="id" value="TravelAgencyAudit" />
		<property name="title" value="Airline" />
		<property name="maintainableItems">
			<list>
			    <bean parent="MaintainableSubSectionHeaderDefinition" p:name="Airline"/>
                <bean parent="MaintainableFieldDefinition" p:name="airBookDate" />
                <bean parent="MaintainableFieldDefinition" p:name="pnrNumber" />
			    <bean parent="MaintainableFieldDefinition" p:name="airTicketNumber" />
			    <bean parent="MaintainableFieldDefinition" p:name="airServiceFeeNumber" />
			 	<bean parent="MaintainableFieldDefinition" p:name="airServiceFeeAmount"  />
			 	<bean parent="MaintainableFieldDefinition" p:name="airCarrierCode"/>
				<bean parent="MaintainableFieldDefinition" p:name="tripDepartureDate"/>
				<bean parent="MaintainableFieldDefinition" p:name="airTransactionAmount" p:required="false"/>
				<bean parent="MaintainableFieldDefinition" p:name="tripReturnDate"/>
				<bean parent="MaintainableFieldDefinition" p:name="airBaseFareAmount"/>
				<bean parent="MaintainableFieldDefinition" p:name="fareSaverCode" />
				<bean parent="MaintainableFieldDefinition" p:name="airTaxAmount"/>
				<bean parent="MaintainableFieldDefinition" p:name="airLowFareAmount"/>
				<bean parent="MaintainableFieldDefinition" p:name="airSegmentId" />
				<bean parent="MaintainableFieldDefinition" p:name="airTicketClass"/>
				<bean parent="MaintainableFieldDefinition" p:name="airReasonCode"  />
                <bean parent="MaintainableFieldDefinition" p:name="transactionUniqueId"  />
			</list>
		</property>
	</bean>
	<bean id="RentalCar-Section" parent="RentalCar-Section-parentBean" />
	<bean id="RentalCar-Section-parentBean" abstract="true" parent="MaintainableSectionDefinition">
		<property name="id" value="TravelAgencyAudit" />
		<property name="title" value="Rental Car" />
		<property name="maintainableItems">
			<list>
				<bean parent="MaintainableSubSectionHeaderDefinition" p:name="Rental Car"/>
			    <bean parent="MaintainableFieldDefinition" p:name="rentalCarItineraryNumber" />
			  	<bean parent="MaintainableFieldDefinition" p:name="rentalCarCompanyName"/>
                <bean parent="MaintainableFieldDefinition" p:name="rentalCarOpenDate" />
				<bean parent="MaintainableFieldDefinition" p:name="rentalCarAmount" />
				<bean parent="MaintainableFieldDefinition" p:name="rentalCarBillAmount"  />
				<bean parent="MaintainableFieldDefinition" p:name="rentalCarNumberOfDays" />
				<bean parent="MaintainableFieldDefinition" p:name="rentalCarFuelAmount" p:required="false"/>
				<bean parent="MaintainableFieldDefinition" p:name="rentalCarAdditionalAmount"  />
				<bean parent="MaintainableFieldDefinition" p:name="rentalCarCloseDate"/>
				<bean parent="MaintainableFieldDefinition" p:name="rentalCarTaxAmount" />
				<bean parent="MaintainableFieldDefinition" p:name="rentalCarDetailText" />
				<bean parent="MaintainableFieldDefinition" p:name="rentalCarSurchargeAmount"/>
				<bean parent="MaintainableFieldDefinition" p:name="rentalCarGovernmentSurchargeAmount"/>
				
			</list>
		</property>
	</bean>
	
	 <bean id="AccountingInformation-Section" parent="AccountingInformation-Section-parentBean"/>
	
	<bean id="AccountingInformation-Section-parentBean" abstract="true" parent="MaintainableSectionDefinition">
    <property name="defaultOpen" value="true"/>  
    <property name="id" value="accountingInfo"/>
    <property name="title" value="Accounting Information"/>
    <property name="maintainableItems">
      <list>
              <bean parent="MaintainableFieldDefinition" p:name="distributionCode" />
        <bean parent="MaintainableCollectionDefinition">
          <property name="name" value="tripAccountingInformation"/>
          <property name="businessObjectClass" value="org.kuali.kfs.module.tem.businessobject.TripAccountingInformation"/>
          <property name="maintainableFields">
            <list>
              <bean parent="MaintainableFieldDefinition" p:name="tripAccountNumber" />
              <bean parent="MaintainableFieldDefinition" p:name="tripSubAccountNumber" />
              <bean parent="MaintainableFieldDefinition" p:name="objectCode"/>
              <bean parent="MaintainableFieldDefinition" p:name="subObjectCode"/>
              <bean parent="MaintainableFieldDefinition" p:name="projectCode"/>
              <bean parent="MaintainableFieldDefinition" p:name="organizationReference" />
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>
  
  <bean id="ConferenceRegistration-Section" parent="ConferenceRegistration-Section-parentBean" />
    <bean id="ConferenceRegistration-Section-parentBean" abstract="true" parent="MaintainableSectionDefinition">
        <property name="id" value="TravelAgencyAudit" />
        <property name="title" value="Conference Registration" />
        <property name="maintainableItems">
            <list>
                <bean parent="MaintainableSubSectionHeaderDefinition" p:name="Conference Registration"/>
                <bean parent="MaintainableFieldDefinition" p:name="registrationCompanyName" />
                <bean parent="MaintainableFieldDefinition" p:name="registrationAmount"/>
                
            </list>
        </property>
    </bean>

</beans>