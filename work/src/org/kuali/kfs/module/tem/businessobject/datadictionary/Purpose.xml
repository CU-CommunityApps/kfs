<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:p="http://www.springframework.org/schema/p" xmlns:dd="http://rice.kuali.org/dd"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd http://rice.kuali.org/dd http://rice.kuali.org/dd/dd.xsd">

	<bean id="Purpose" parent="Purpose-parentBean" />
	<bean id="Purpose-parentBean" abstract="true" parent="BusinessObjectEntry" >
		<property name="businessObjectClass" value="org.kuali.kfs.module.tem.businessobject.Purpose" />
		<property name="lookupDefinition" ref="Purpose-lookupDefinition" />
		<property name="inquiryDefinition" ref="Purpose-inquiryDefinition" />
		<property name="objectLabel" value="Purpose" />
		<property name="attributes">
		<list>
			<ref bean="Purpose-documentType" />
			<ref bean="Purpose-purposeCode" />
			<ref bean="Purpose-purposeName" />
			<ref bean="Purpose-purposeDescription" />
			<ref bean="Purpose-reviewRequiredIndicator" />
			<ref bean="Purpose-active" />
			<ref bean="Purpose-versionNumber" />
		</list>
		</property>
	</bean>
	<bean id="Purpose-documentType" parent="Purpose-documentType-parentBean" />
	<bean id="Purpose-documentType-parentBean" parent="AttributeDefinition">
		<property name="name" value="documentType" />
		<property name="label" value="Document Type" />
		<property name="shortLabel" value="Document Type" />
		<property name="maxLength" value="4" />
		<property name="required" value="true" />
		<property name="summary" value="Document Type" />
		<property name="description" value="Document Type" />
		<property name="control"><bean parent="TextControlDefinition" p:size="4"/></property>
	</bean>
	<bean id="Purpose-purposeCode" parent="Purpose-purposeCode-parentBean" />
	<bean id="Purpose-purposeCode-parentBean" parent="AttributeDefinition">
		<property name="name" value="purposeCode" />
		<property name="label" value="Purpose Code" />
		<property name="shortLabel" value="Purpose Code" />
		<property name="maxLength" value="4" />
		<property name="required" value="true" />
		<property name="summary" value="Purpose Code" />
		<property name="description" value="Purpose Code" />
		<property name="control"><bean parent="TextControlDefinition" p:size="4"/></property>
	</bean>
	<bean id="Purpose-purposeName" parent="Purpose-purposeName-parentBean" />
	<bean id="Purpose-purposeName-parentBean" parent="AttributeDefinition">
		<property name="name" value="purposeName" />
		<property name="label" value="Purpose Name" />
		<property name="shortLabel" value="Purpose Name" />
		<property name="maxLength" value="40" />
		<property name="summary" value="Purpose Name" />
		<property name="description" value="Purpose Name" />
		<property name="control"><bean parent="TextControlDefinition" p:size="40"/></property>
	</bean>
	<bean id="Purpose-purposeDescription" parent="Purpose-purposeDescription-parentBean" />
	<bean id="Purpose-purposeDescription-parentBean" parent="AttributeDefinition">
		<property name="name" value="purposeDescription" />
		<property name="label" value="Purpose Description" />
		<property name="shortLabel" value="Purpose Description" />
		<property name="maxLength" value="100" />
		<property name="summary" value="Purpose Description" />
		<property name="description" value="Purpose Description" />
		<property name="control"><bean parent="TextControlDefinition" p:size="100"/></property>
	</bean>
	<bean id="Purpose-reviewRequiredIndicator" parent="Purpose-reviewRequiredIndicator-parentBean" />
	<bean id="Purpose-reviewRequiredIndicator-parentBean" parent="GenericAttributes-genericBoolean">
		<property name="name" value="reviewRequiredIndicator" />
		<property name="label" value="Review Required Indicator" />
		<property name="shortLabel" value="Review Required Indicator" />
		<property name="summary" value="Review Required Indicator" />
		<property name="description" value="Review Required Indicator" />
	</bean>
	<bean id="Purpose-active" parent="Purpose-active-parentBean" />
	<bean id="Purpose-active-parentBean" parent="GenericAttributes-genericBoolean">
		<property name="name" value="active" />
		<property name="label" value="Active" />
		<property name="shortLabel" value="Active" />
		<property name="required" value="true" />
		<property name="summary" value="Active" />
		<property name="description" value="Active" />
	</bean>
	<bean id="Purpose-versionNumber" parent="Purpose-versionNumber-parentBean" />
	<bean id="Purpose-versionNumber-parentBean" parent="GenericAttributes-versionNumber">
		<property name="name" value="versionNumber" />
		<property name="label" value="Version Number" />
		<property name="shortLabel" value="Version Number" />
		<property name="summary" value="Version Number" />
		<property name="description" value="Version Number" />
	</bean>
	<bean id="Purpose-inquiryDefinition" parent="Purpose-inquiryDefinition-parentBean" />
	<bean id="Purpose-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
		<property name="title" value="Purpose Inquiry"/>
		<property name="inquirySections">
		<list>
			<bean parent="InquirySectionDefinition">
				<property name="title" value="Purpose Inquiry"/>
				<property name="numberOfColumns" value="1"/>
				<property name="inquiryFields">
				<list>
					<bean parent="FieldDefinition" p:attributeName="documentType" />
					<bean parent="FieldDefinition" p:attributeName="purposeCode" />
					<bean parent="FieldDefinition" p:attributeName="purposeName" />
					<bean parent="FieldDefinition" p:attributeName="purposeDescription" />
					<bean parent="FieldDefinition" p:attributeName="reviewRequiredIndicator" />
					<bean parent="FieldDefinition" p:attributeName="active" />
				</list>
				</property>
			</bean>
		</list>
		</property>
	</bean>
	<bean id="Purpose-lookupDefinition" parent="Purpose-lookupDefinition-parentBean" />
	<bean id="Purpose-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
		<property name="title" value="Purpose Lookup" />
		<property name="defaultSort">
		<bean parent="SortDefinition">
			<property name="attributeNames"><list>
			</list></property>
		</bean>
		</property>
		<property name="lookupFields"><list>
			<bean parent="FieldDefinition" p:attributeName="documentType" />
			<bean parent="FieldDefinition" p:attributeName="purposeCode" />
			<bean parent="FieldDefinition" p:attributeName="purposeName" />
			<bean parent="FieldDefinition" p:attributeName="purposeDescription" />
			<bean parent="FieldDefinition" p:attributeName="reviewRequiredIndicator" />
			<bean parent="FieldDefinition" p:attributeName="active" />
		</list>
		</property>
		<property name="resultFields">
		<list>
			<bean parent="FieldDefinition" p:attributeName="documentType" />
			<bean parent="FieldDefinition" p:attributeName="purposeCode" />
			<bean parent="FieldDefinition" p:attributeName="purposeName" />
			<bean parent="FieldDefinition" p:attributeName="purposeDescription" />
			<bean parent="FieldDefinition" p:attributeName="reviewRequiredIndicator" />
			<bean parent="FieldDefinition" p:attributeName="active" />
		</list>
		</property>
	</bean>
</beans>