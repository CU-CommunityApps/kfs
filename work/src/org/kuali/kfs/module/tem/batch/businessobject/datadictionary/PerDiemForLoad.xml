<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:p="http://www.springframework.org/schema/p" xmlns:dd="http://rice.kuali.org/dd"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd http://rice.kuali.org/dd http://rice.kuali.org/dd/dd.xsd">

	<bean id="PerDiemForLoad" parent="PerDiemForLoad-parentBean" />
	<bean id="PerDiemForLoad-parentBean" abstract="true" parent="PerDiem-parentBean" >
		<property name="businessObjectClass" value="org.kuali.kfs.module.tem.batch.businessobject.PerDiemForLoad" />
		<property name="titleAttribute" value="id" />
		<property name="objectLabel" value="Per Diem For Load" />
		<property name="attributes">
		<list>
			<ref bean="PerDiemForLoad-lineNumber" />
			<ref bean="PerDiemForLoad-id" />
			<ref bean="PerDiemForLoad-tripTypeCode" />
			<ref bean="PerDiemForLoad-countryState" />
			<ref bean="PerDiemForLoad-county" />
			<ref bean="PerDiemForLoad-primaryDestination" />
			<ref bean="PerDiemForLoad-seasonBeginDate" />
			<ref bean="PerDiemForLoad-effectiveFromDate" />
            <ref bean="PerDiemForLoad-effectiveToDate" />
			<ref bean="PerDiemForLoad-breakfast" />
			<ref bean="PerDiemForLoad-lunch" />
			<ref bean="PerDiemForLoad-dinner" />
			<ref bean="PerDiemForLoad-incidentals" />
			<ref bean="PerDiemForLoad-mealsAndIncidentals" />
			<ref bean="PerDiemForLoad-active" />
			<ref bean="PerDiemForLoad-versionNumber" />
		</list>
		</property>
	</bean>
	
	<bean id="PerDiemForLoad-id" parent="PerDiem-id"/>
	
	<bean id="PerDiemForLoad-lineNumber" parent="PerDiem-lineNumber" />
	
	<bean id="PerDiemForLoad-tripTypeCode" parent="PerDiem-tripTypeCode" />
	
	<bean id="PerDiemForLoad-countryState" parent="PerDiem-countryState" />
	
	<bean id="PerDiemForLoad-county" parent="PerDiem-county" >
		<property name="required" value="false" />
	</bean>
	
	<bean id="PerDiemForLoad-primaryDestination" parent="PerDiem-primaryDestination" />
	
	<bean id="PerDiemForLoad-seasonBeginDate" parent="PerDiem-seasonBeginDate"/>
	
	
	<bean id="PerDiemForLoad-effectiveFromDate" parent="PerDiem-effectiveFromDate" >
		<property name="required" value="true" />		
	</bean>
	
	<bean id="PerDiemForLoad-effectiveToDate" parent="PerDiem-effectiveToDate" />
	
	<bean id="PerDiemForLoad-breakfast" parent="PerDiem-breakfast" />

	<bean id="PerDiemForLoad-lunch" parent="PerDiem-lunch" />

	<bean id="PerDiemForLoad-dinner" parent="PerDiem-dinner" />

	<bean id="PerDiemForLoad-incidentals" parent="PerDiem-incidentals" />

	<bean id="PerDiemForLoad-mealsAndIncidentals" parent="PerDiem-mealsAndIncidentals" />

	<bean id="PerDiemForLoad-active" parent="PerDiem-active" />
	
	<bean id="PerDiemForLoad-versionNumber" parent="PerDiem-versionNumber" />
</beans>
