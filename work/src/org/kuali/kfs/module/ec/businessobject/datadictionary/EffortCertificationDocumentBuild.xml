<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "../dataDictionary-1_0.dtd">
<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.module.effort.bo.EffortCertificationDocumentBuild">

        <inquiry>
            <title>Effort Certification Detail Build Inquiry</title>

            <inquirySections><inquirySection title="Header Text"><inquiryFields>
                <field attributeName="effortCertificationBuildNumber" />
                
                <field attributeName="effortCertificationReportNumber" />           
                <field attributeName="effortCertificationDocumentCode" />                
                
                <field attributeName="universityFiscalYear" />
                <field attributeName="emplid" />
            </inquiryFields></inquirySection></inquirySections>
        </inquiry>

        <lookup>
            <title>Effoft Certification Detail Build Lookup</title>

            <instructions>Look up Effort Certification Detail Build</instructions>

            <defaultSort sortAscending="true">
                <sortAttributes>
                    <sortAttribute attributeName="effortCertificationBuildNumber" />
                    <sortAttribute attributeName="emplid" />
                </sortAttributes>
            </defaultSort>

            <lookupFields>
            	<lookupField attributeName="effortCertificationReportNumber" required="true" />
                <lookupField attributeName="universityFiscalYear" required="true" />
                <lookupField attributeName="emplid" required="true" />
                <lookupField attributeName="effortCertificationBuildNumber" required="false" />
            </lookupFields>

            <resultFields>
                <field attributeName="effortCertificationBuildNumber" />
                <field attributeName="emplid" />
                                                
                <field attributeName="universityFiscalYear" />
                
                <field attributeName="effortCertificationReportNumber" />           
                <field attributeName="effortCertificationDocumentCode" />                
            </resultFields>
        </lookup>

        <attributes>

            <attribute name="effortCertificationDocumentCode" forceUppercase="true">
                <label>Effort Certification Document Code</label>
                <shortLabel>Code</shortLabel>

                <maxLength>2</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="4" />
                </control>

                <summary>Effort Certification DocumentCode</summary>
                <description>Effort Certification LaborDocumentCode description...</description>
			</attribute>
			
			<attribute name="effortCertificationBuildNumber" forceUppercase="true">
                <label>Effort Certification Build Number</label>
                <shortLabel>Number</shortLabel>

                <maxLength>19</maxLength>
                <validationPattern><numeric /></validationPattern>

                <control>
                    <text size="21" />
                </control>

                <summary>effortCertificationBuildNumber</summary>
                <description>effortCertificationBuildNumber description...</description>
			</attribute>	

            <attributeReference name="emplid" sourceClassName="org.kuali.core.bo.user.UniversalUser" sourceAttributeName="personPayrollIdentifier"/>

            <attributeReference name="universityFiscalYear" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="genericFiscalYear" />
			
			<attributeReference name="effortCertificationReportNumber" sourceClassName="org.kuali.module.effort.bo.EffortCertificationReportDefinition" sourceAttributeName="effortCertificationReportNumber" />

            <attributeReference name="versionNumber" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="versionNumber" />
        </attributes>
        
        <relationships>
            <relationship objectAttributeName="employee" >
                <primitiveAttribute sourceName="employee.personUniversalIdentifier" targetName="personUniversalIdentifier" />
				<supportAttribute sourceName="emplid" targetName="personPayrollIdentifier" identifier="true" />
	        </relationship>
        </relationships>
    </businessObject>
</dictionaryEntry>
