<!DOCTYPE dictionaryEntry PUBLIC "-//Kuali Project//DTD Data Dictionary 1.0//EN" "../../../../../../../../rice/kns/src/main/resources/org/kuali/core/datadictionary/dataDictionary-1_0.dtd">
<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.module.effort.bo.OutstandingCertificationsByReport">
       <lookup>
       		<lookupableID>outstandingCertificationsByReportLookupable</lookupableID> 
            <title>Outstanding Certifications By Report</title>

            <instructions>Find Outstanding Reports</instructions>

            <defaultSort sortAscending="true">
                <sortAttributes>
                    <sortAttribute attributeName="effortCertificationReportNumber" />
                </sortAttributes>
            </defaultSort>

            <lookupFields>
            	<lookupField attributeName="universityFiscalYear" />
                <lookupField attributeName="effortCertificationReportNumber" />
            </lookupFields>

            <resultFields>
            	<field attributeName="universityFiscalYear" />
            	<field attributeName="effortCertificationReportNumber" />
            	<field attributeName="chartOfAccountsCode" /> 
				<field attributeName="organizationCode" /> 
				<field attributeName="outstandingCertificationCount" /> 
            </resultFields>
        </lookup>

        <attributes>
			<attribute name="universityFiscalYear" forceUppercase="true">
                <label>University Fiscal Year</label>
                <shortLabel>year</shortLabel>

                <maxLength>4</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="4" />
                </control>

                <summary>University Fiscal Year</summary>
                <description>University Fiscal Year</description>
			</attribute>
			
			<attribute name="effortCertificationReportNumber" forceUppercase="true">
                <label>Effort Certification Report Number</label>
                <shortLabel>report number</shortLabel>

                <maxLength>4</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="4" />
                </control>

                <summary>Effort Certification Report Number</summary>
                <description>Effort Certification Report Number</description>
			</attribute>
			
			<attribute name="chartOfAccountsCode" forceUppercase="true">
                <label>Chart Of Accounts Code</label>
                <shortLabel>chart</shortLabel>

                <maxLength>4</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="4" />
                </control>

                <summary>Chart Of Accounts Code</summary>
                <description>Chart Of Accounts Code</description>
			</attribute>
			
			<attribute name="outstandingCertificationCount" forceUppercase="true">
                <label>Number of Outstanding Certifications</label>
                <shortLabel>count</shortLabel>

                <maxLength>10</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="4" />
                </control>

                <summary>Number of Outstanding Certifications</summary>
                <description>Number of Outstanding Certifications</description>
			</attribute>
			
        </attributes>
          
    </businessObject>
</dictionaryEntry>
