<!DOCTYPE dictionaryEntry PUBLIC "-//Kuali Project//DTD Data Dictionary 1.0//EN" "../../../../../../../../rice/kns/src/main/resources/org/kuali/core/datadictionary/dataDictionary-1_0.dtd">
<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.module.effort.bo.OutstandingCertificationsByReport">
       <lookup>
       		<lookupableID>outstandingCertificationsByReportLookupable</lookupableID> 
            <title>Outstanding Certifications By Report</title>

            <instructions>Find Outstanding Reports</instructions>

            <defaultSort sortAscending="true">
                <sortAttributes>
                    <sortAttribute attributeName="effortCertificationReportNumber" />
                </sortAttributes>
            </defaultSort>

            <lookupFields>
            	<lookupField attributeName="universityFiscalYear" required="true" />
                <lookupField attributeName="effortCertificationReportNumber" required="true" />
            </lookupFields>

            <resultFields>
            	<field attributeName="universityFiscalYear" />
            	<field attributeName="effortCertificationReportNumber" />
            	<field attributeName="chartOfAccountsCode" /> 
				<field attributeName="organizationCode" /> 
				<field attributeName="outstandingCertificationCount" /> 
            </resultFields>
        </lookup>

        <attributes>
			<attributeReference name="universityFiscalYear" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="genericFiscalYear"/>
			
			<attributeReference name="effortCertificationReportNumber" sourceClassName="org.kuali.module.effort.bo.EffortCertificationReportDefinition" sourceAttributeName="effortCertificationReportNumber" />
            
			<attributeReference name="chartOfAccountsCode" sourceClassName="org.kuali.module.chart.bo.Chart" sourceAttributeName="chartOfAccountsCode"/>
			
			<attributeReference name="organizationCode" sourceClassName="org.kuali.module.chart.bo.Org" sourceAttributeName="organizationCode" />
			
			<attribute name="outstandingCertificationCount" forceUppercase="true">
                <label>Number of Outstanding Certifications</label>
                <shortLabel>count</shortLabel>

                <maxLength>10</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="4" />
                </control>

                <summary>Number of Outstanding Certifications</summary>
                <description>Number of Outstanding Certifications</description>
			</attribute>
			
        </attributes>
          
    </businessObject>
</dictionaryEntry>
