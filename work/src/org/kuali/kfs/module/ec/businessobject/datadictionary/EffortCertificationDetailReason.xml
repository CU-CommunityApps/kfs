<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "../dataDictionary-1_0.dtd">
<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.module.effort.bo.EffortCertificationDetailReason">


        <inquiry>
            <title>Effort Certification Detail Line Reason Inquiry</title>

            <inquirySections><inquirySection title="Header Text"><inquiryFields>
                <field attributeName="a21LaborDetailLineReasonDescription" />
                <field attributeName="a21LaborEffortCategoryCode" />
                <field attributeName="accountNumber" />
                <field attributeName="chartOfAccountsCode" />
                <field attributeName="documentNumber" />
            </inquiryFields></inquirySection></inquirySections>
        </inquiry>

        <lookup>
            <title>Effort Certification Detail Line Reason Lookup</title>

            <instructions>Look up an Effort Certification Detail Line Reason</instructions>

            <defaultSort sortAscending="true">
                <sortAttributes>
                    <sortAttribute attributeName="a21LaborEffortCategoryCode" />
                </sortAttributes>
            </defaultSort>

            <lookupFields>
                <lookupField attributeName="a21LaborDetailLineReasonDescription" required="false" />
                <lookupField attributeName="a21LaborEffortCategoryCode" required="false" />
                <lookupField attributeName="accountNumber" required="false" />
                <lookupField attributeName="chartOfAccountsCode" required="false" />
                <lookupField attributeName="documentNumber" required="false" />
            </lookupFields>

            <resultFields>
                <field attributeName="a21LaborDetailLineReasonDescription" />
                <field attributeName="a21LaborEffortCategoryCode" />
                <field attributeName="accountNumber" />
                <field attributeName="chartOfAccountsCode" />
                <field attributeName="documentNumber" />
            </resultFields>
        </lookup>

        <attributes>

            <attribute name="a21LaborDetailLineReasonDescription" forceUppercase="true">
                <label>Effort Certification Labor Detail Line Reason Description</label>
                <shortLabel>Description</shortLabel>

                <maxLength>200</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="202" />
                </control>

                <summary>a21LaborDetailLineReasonDescription</summary>
                <description>a21LaborDetailLineReasonDescription description...</description>
			</attribute>

            <attribute name="a21LaborEffortCategoryCode" forceUppercase="true">
                <label>Effort Certification Labor Effort Category Code</label>
                <shortLabel>Code</shortLabel>

                <maxLength>2</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="4" />
                </control>

                <summary>a21LaborEffortCategoryCode</summary>
                <description>a21LaborEffortCategoryCode description...</description>
			</attribute>

			<attributeReference name="accountNumber" sourceClassName="org.kuali.module.chart.bo.Account" sourceAttributeName="accountNumber" />
			<attributeReference name="chartOfAccountsCode" sourceClassName="org.kuali.module.chart.bo.Chart" sourceAttributeName="chartOfAccountsCode" />
            <attributeReference name="documentNumber" sourceClassName="org.kuali.core.bo.DocumentHeader" sourceAttributeName="documentNumber"/>
            <attributeReference name="versionNumber" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="versionNumber" />

        </attributes>
    </businessObject>
