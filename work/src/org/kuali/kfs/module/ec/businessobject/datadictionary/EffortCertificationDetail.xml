<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "../dataDictionary-1_0.dtd">
<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.module.effort.bo.EffortCertificationDetail">

        <inquiry>
            <title>Effort Certification Detail Line Inquiry</title>

            <inquirySections><inquirySection title="Header Text"><inquiryFields>
                <field attributeName="documentNumber" />
                <field attributeName="financialDocumentPostingYear" />
                
                <field attributeName="chartOfAccountsCode" />                                
                <field attributeName="accountNumber" />   
                <field attributeName="subAccountNumber" />
                <field attributeName="positionNumber" />
                <field attributeName="financialObjectCode" />
                <field attributeName="sourceChartOfAccountsCode" />
                <field attributeName="sourceAccountNumber" />
                
                <field attributeName="effortCertificationPayrollAmount" />
                <field attributeName="effortCertificationCalculatedOverallPercent" />
                <field attributeName="effortCertificationUpdatedOverallPercent" />
                
                <field attributeName="costShareSourceSubAccountNumber" /> 
                <field attributeName="effortCertificationOriginalPayrollAmount" />               
            </inquiryFields></inquirySection></inquirySections>
        </inquiry>

        <lookup>
            <title>Effort Certification Detail Line Lookup</title>

            <instructions>Look up an Effort Certification Detail Line</instructions>

            <defaultSort sortAscending="true">
                <sortAttributes>
                    <sortAttribute attributeName="documentNumber" />
                    <sortAttribute attributeName="chartOfAccountsCode" />
                    <sortAttribute attributeName="accountNumber" />
                </sortAttributes>
            </defaultSort>

            <lookupFields>
                <lookupField attributeName="documentNumber" required="true" />
                <lookupField attributeName="financialDocumentPostingYear" required="false" />
                
                <lookupField attributeName="chartOfAccountsCode" required="false" />
                <lookupField attributeName="accountNumber" required="false" />
                <lookupField attributeName="subAccountNumber" required="false" />
                <lookupField attributeName="financialObjectCode" required="false" />
                <lookupField attributeName="positionNumber" required="false" />
            </lookupFields>

            <resultFields>
                <field attributeName="documentNumber" />
                <field attributeName="sourceChartOfAccountsCode" />
                <field attributeName="sourceAccountNumber" />
                
                <field attributeName="financialDocumentPostingYear" />               
                <field attributeName="chartOfAccountsCode" />                                
                <field attributeName="accountNumber" />   
                <field attributeName="subAccountNumber" />
                <field attributeName="positionNumber" />
                <field attributeName="financialObjectCode" />
                
                <field attributeName="effortCertificationPayrollAmount" />
            </resultFields>
        </lookup>

        <attributes>			
            <attributeReference name="documentNumber" sourceClassName="org.kuali.core.bo.DocumentHeader" sourceAttributeName="documentNumber"/>

            <attributeReference name="effortCertificationOriginalPayrollAmount" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="genericAmount">
                <label>Salary Orig</label>
                <shortLabel>Salary Orig</shortLabel>

                <summary>Effort Certification LaborOriginalPayrollAmount</summary>
                <description>Effort Certification LaborOriginalPayrollAmount description...</description>
                <formatterClass>org.kuali.core.web.format.CurrencyFormatter</formatterClass>
			</attributeReference>

            <attributeReference name="effortCertificationPayrollAmount" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="genericAmount">
                <label>Salary</label>
                <shortLabel>Salary</shortLabel>

                <summary>Effort Certification LaborPayrollAmount</summary>
                <description>Effort Certification LaborPayrollAmount description...</description>
                <formatterClass>org.kuali.core.web.format.CurrencyFormatter</formatterClass>
			</attributeReference>
			
			<attributeReference name="effortCertificationCalculatedOverallPercent" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="percent">
                <label>Original Effort</label>
                <shortLabel>Effort</shortLabel>

                <summary>Effort Certification LaborCalculatedOverallPercent</summary>
                <description>Effort Certification LaborCalculatedOverallPercent description...</description>
                <formatterClass>org.kuali.core.web.format.IntegerFormatter</formatterClass>                
			</attributeReference>			

            <attributeReference name="effortCertificationUpdatedOverallPercent" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="percent">
                <label>Effort</label>
                <shortLabel>Effort</shortLabel>

                <summary>Effort Certification LaborUpdatedOverallPercent</summary>
                <description>Effort Certification LaborUpdatedOverallPercent description...</description>
                <formatterClass>org.kuali.core.web.format.IntegerFormatter</formatterClass>
			</attributeReference>		

			<attributeReference name="accountNumber" sourceClassName="org.kuali.module.chart.bo.Account" sourceAttributeName="accountNumber" >
				<label>Account</label>
			</attributeReference>
            <attributeReference name="chartOfAccountsCode" sourceClassName="org.kuali.module.chart.bo.Chart" sourceAttributeName="chartOfAccountsCode" >
            	<label>Chart</label>
            	<control>
                  <select valuesFinderClass="org.kuali.module.chart.lookup.keyvalues.SimpleChartValuesFinder" />
               </control> 
			</attributeReference>
            <attributeReference name="subAccountNumber" sourceClassName="org.kuali.module.chart.bo.SubAccount" sourceAttributeName="subAccountNumber" >
            	<label>CS Sub-Acct</label>
			</attributeReference>

            <attributeReference name="financialDocumentPostingYear" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="genericFiscalYear">
                <label>Financial Document Posting Year</label>
                <shortLabel>Year</shortLabel>

                <summary>financialDocumentPostingYear</summary>
                <description>financialDocumentPostingYear description...</description>
			</attributeReference>

			<attributeReference name="financialObjectCode" sourceClassName="org.kuali.module.chart.bo.ObjectCode" sourceAttributeName="financialObjectCode">
				<label>Object Code</label>
			</attributeReference>

            <attributeReference name="positionNumber" sourceClassName="org.kuali.module.labor.bo.PositionData" sourceAttributeName="positionNumber" >
				<label>Position Number</label>
				<shortLabel>Position</shortLabel>
			</attributeReference>
			
			<attribute name="sourceAccountNumber" forceUppercase="true">
                <label>Source Account</label>
                <shortLabel>Source Account</shortLabel>
				
				<maxLength>7</maxLength>

                <control>
                    <text size="10" />
                </control>
                <summary>sourceAccountNumber</summary>
                <description>sourceAccountNumber description...</description>
			</attribute>

            <attribute name="sourceChartOfAccountsCode" forceUppercase="true">
                <label>Source Chart</label>
                <shortLabel>Source Chart</shortLabel>
                
                <maxLength>2</maxLength>

                <control>
                    <text size="2" />
                </control>
                
                <summary>sourceFinancialChartOfAccountsCode</summary>
                <description>sourceFinancialChartOfAccountsCode description...</description>
			</attribute>
						            
            <attribute name="costShareSourceSubAccountNumber" forceUppercase="true">
                <label> Src Sub-Acct</label>
                <shortLabel> Src Sub-Acct</shortLabel>
                
                <maxLength>5</maxLength>
                <control>
                    <text size="5" />
                </control>
                
                <summary>costShareSourceSubAccountNumber</summary>
                <description>costShareSourceSubAccountNumber description...</description>
			</attribute>
			
			<attributeReference name="originalFringeBenefitAmount" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="genericAmount">
                <label>F-Ben Orig </label>
                <shortLabel>F-Ben Orig </shortLabel>

                <summary>F-Ben Orig </summary>
                <description>F-Ben Orig </description>
                <formatterClass>org.kuali.core.web.format.CurrencyFormatter</formatterClass>
			</attributeReference>

			<attributeReference name="fringeBenefitAmount" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="genericAmount">
                <label>F-Ben</label>
                <shortLabel>F-Ben</shortLabel>

                <summary>F-Ben</summary>
                <description>F-Ben</description>
                <formatterClass>org.kuali.core.web.format.CurrencyFormatter</formatterClass>
			</attributeReference>			

            <attributeReference name="versionNumber" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="versionNumber" />
        </attributes>
    </businessObject>
</dictionaryEntry>
