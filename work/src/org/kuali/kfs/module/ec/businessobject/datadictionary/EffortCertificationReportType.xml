<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "../dataDictionary-1_0.dtd">
<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.module.effort.bo.EffortCertificationReportType">


        <inquiry>
            <title>Effort Certification Report Type Code Inquiry</title>

            <inquirySections><inquirySection title="Header Text"><inquiryFields>
                <field attributeName="effortCertificationReportTypeCode" />
                <field attributeName="effortCertificationReportDescription" />
                <field attributeName="rowActiveIndicator" />
            </inquiryFields></inquirySection></inquirySections>
        </inquiry>

        <lookup>
            <title>Effort Certification Report Type Code Lookup</title>

            <instructions>Look up an Effort Certification Report Type Code</instructions>

            <defaultSort sortAscending="true">
                <sortAttributes>
                    <sortAttribute attributeName="effortCertificationReportTypeCode" />
                </sortAttributes>
            </defaultSort>

            <lookupFields>
                <lookupField attributeName="effortCertificationReportTypeCode" required="false" />
                <lookupField attributeName="rowActiveIndicator" required="false" />
            </lookupFields>

            <resultFields>
                <field attributeName="effortCertificationReportTypeCode" />
                <field attributeName="effortCertificationReportDescription" />
                <field attributeName="rowActiveIndicator" />
            </resultFields>
        </lookup>

        <attributes>

            <attribute name="effortCertificationReportDescription" forceUppercase="true">
                <label>Effort Certification Report Description</label>
                <shortLabel>Description</shortLabel>

                <maxLength>40</maxLength>
                <validationPattern><anyCharacter allowWhitspaces="true"/></validationPattern>

                <control>
                    <text size="42" />
                </control>

                <summary>Effort Certification Report Description</summary>
                <description>Effort Certification Report Description</description>
			</attribute>

            <attribute name="effortCertificationReportTypeCode" forceUppercase="true">
                <label>Effort Certification Report Type Code</label>
                <shortLabel>Code</shortLabel>

                <maxLength>2</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                     <text size="4" />
                </control>

                <summary>Effort Certification Report Type Code</summary>
                <description>Effort Certification Report Type Code</description>
			</attribute>

            <attribute name="rowActiveIndicator" forceUppercase="true">
                <label>Row Active Indicator</label>
                <shortLabel>Indicator</shortLabel>

                <maxLength>1</maxLength>

                <control>
                    <checkbox />
                </control>

                <summary>row Active Indicator</summary>
                <description>row Active Indicator</description>
			</attribute>

            <attributeReference name="versionNumber" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="versionNumber" />

        </attributes>
    </businessObject>
</dictionaryEntry>
