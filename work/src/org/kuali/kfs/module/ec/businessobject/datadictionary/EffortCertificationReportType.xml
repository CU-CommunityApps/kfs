<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "../dataDictionary-1_0.dtd">
<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.module.effort.bo.EffortCertificationReportType">


        <inquiry>
            <title>Report Type Code</title>

            <inquirySections><inquirySection title="Header Text"><inquiryFields>
                <field attributeName="effortCertificationReportTypeCode" />
                <field attributeName="effortCertificationReportDescription" />
                <field attributeName="active" />
            </inquiryFields></inquirySection></inquirySections>
        </inquiry>

        <lookup>
            <title>Report Type Lookup</title>

            <instructions>Look Up Effort Certification Report Type</instructions>

            <defaultSort sortAscending="true">
                <sortAttributes>
                    <sortAttribute attributeName="effortCertificationReportTypeCode" />
                </sortAttributes>
            </defaultSort>

            <lookupFields>
                <lookupField attributeName="effortCertificationReportTypeCode" required="false" />
                <lookupField attributeName="active" required="false" />
            </lookupFields>

            <resultFields>
                <field attributeName="effortCertificationReportTypeCode" />
                <field attributeName="effortCertificationReportDescription" />
                <field attributeName="active" />
            </resultFields>
        </lookup>
		
        <attributes>

            <attribute name="effortCertificationReportDescription" forceUppercase="true">
                <label>Report Description</label>
                <shortLabel>Description</shortLabel>

                <maxLength>40</maxLength>
                <validationPattern><anyCharacter allowWhitespace="true"/></validationPattern>

                <control>
                    <text size="42" />
                </control>

                <summary>Effort Certification Report Description</summary>
                <description>Effort Certification Report Description</description>
			</attribute>

            <attribute name="effortCertificationReportTypeCode" forceUppercase="true">
                <label>Report Type Code</label>
                <shortLabel>Code</shortLabel>

                <maxLength>2</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                     <text size="4" />
                </control>

                <summary>Effort Certification Report Type Code</summary>
                <description>Effort Certification Report Type Code</description>
			</attribute>

            <attributeReference name="active" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="activeIndicator"/>
            <attributeReference name="versionNumber" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="versionNumber" />

        </attributes>
    </businessObject>
</dictionaryEntry>
