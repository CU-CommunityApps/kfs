<?xml version="1.0" encoding="UTF-8"?>
<data xmlns="ns:workflow" xmlns:fo="http://www.w3.org/1999/XSL/Format" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="ns:workflow resource:WorkflowData">
<documentTypes xmlns="ns:workflow/DocumentType" xsi:schemaLocation="ns:workflow/DocumentType resource:DocumentType">
<documentType>
<name>EffortCertificationDocument</name>
<parent>EffortCertificationTransactionalDocument</parent>
<docHandler>${application.url}/effortEffortCertification.do?methodToCall=docHandler</docHandler>
<routePaths>
        <routePath>
          <start name="AdHoc" nextNode="ProjectManagement"/>
          <requests name="ProjectManagement" nextNode="Account"/>
          <requests name="Account" nextNode="OrganizationHierarchy"/>
          <requests name="OrganizationHierarchy" nextNode="DoAwardSplit"/>
          <split name="DoAwardSplit" nextNode="DoRecreateSplit">
            <branch name="True">
              <requests name="Award" nextNode="AwardJoin"/>
            </branch>
            <branch name="False">
              <simple name="AwardDoNothing" nextNode="AwardJoin"/>
            </branch>
            <join name="AwardJoin"/>
          </split>
          <split name="DoRecreateSplit" >
            <branch name="True">
              <requests name="Management" nextNode="RecreateJoin"/>
            </branch>
            <branch name="False">
              <simple name="RecreateDoNothing" nextNode="RecreateJoin"/>
            </branch>
            <join name="RecreateJoin"/>
          </split>
        </routePath>
      </routePaths>
<routeNodes>
<start name="AdHoc"/>
<role name="ProjectManagement">
<!--<ruleTemplate>KualiProjectDirectorTemplate</ruleTemplate>-->
<qualifierResolverClass>org.kuali.rice.kns.workflow.attribute.DataDictionaryQualifierResolver</qualifierResolverClass>
</role>
<role name="Account">
<!--<ruleTemplate>KualiAccountTemplate</ruleTemplate>-->
<qualifierResolverClass>org.kuali.rice.kns.workflow.attribute.DataDictionaryQualifierResolver</qualifierResolverClass>
</role>
<role name="OrganizationHierarchy">
<!--<ruleTemplate>KualiOrgReviewTemplate</ruleTemplate>-->
<qualifierResolverClass>org.kuali.rice.kns.workflow.attribute.DataDictionaryQualifierResolver</qualifierResolverClass>
</role>
<role name="Award">
<!--<ruleTemplate>KualiAwardWorkgroupTemplate</ruleTemplate>-->
<qualifierResolverClass>org.kuali.rice.kns.workflow.attribute.DataDictionaryQualifierResolver</qualifierResolverClass>
</role>
<role name="Recreate">
<!--<ruleTemplate>WorkflowDocumentTemplate</ruleTemplate>-->
<qualifierResolverClass>org.kuali.rice.kns.workflow.attribute.DataDictionaryQualifierResolver</qualifierResolverClass>
</role>
<split name="DoAwardSplit">
<type>org.kuali.kfs.sys.document.workflow.SimpleBooleanSplitNode</type>
</split>
<split name="DoRecreateSplit">
<type>org.kuali.kfs.sys.document.workflow.SimpleBooleanSplitNode</type>
</split>
<simple name="AwardDoNothing">
<type>org.kuali.rice.kew.engine.node.NoOpNode</type>
</simple>
<simple name="RecreateDoNothing">
<type>org.kuali.rice.kew.engine.node.NoOpNode</type>
</simple>
<join name="AwardJoin"/>
<join name="RecreateJoin"/>
</routeNodes>
</documentType>
</documentTypes>
</data>
