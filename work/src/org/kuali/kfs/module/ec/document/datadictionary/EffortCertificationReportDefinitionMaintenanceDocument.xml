<!DOCTYPE dictionaryEntry PUBLIC "-//Kuali Project//DTD Data Dictionary 1.0//EN" "../../../../../../../../../rice/kns/src/main/resources/org/kuali/core/datadictionary/dataDictionary-1_0.dtd">

<dictionaryEntry>
	<maintenanceDocument>
        <businessObjectClass>org.kuali.module.effort.bo.EffortCertificationReportDefinition</businessObjectClass>
        <maintainableClass>org.kuali.core.maintenance.KualiMaintainableImpl</maintainableClass>
        <businessRulesClass>org.kuali.module.effort.rules.EffortCertificationReportDefinitionRule</businessRulesClass>
		<preRulesCheckClass>org.kuali.module.effort.rules.EffortCertificationReportDefinitionMaintenanceDocumentPreRules</preRulesCheckClass>
        <documentAuthorizerClass>org.kuali.core.document.authorization.MaintenanceDocumentAuthorizerBase</documentAuthorizerClass>
        <authorizations>
            <authorization action="initiate">
                <workgroups>
                    <workgroup>KUALI_EFFORT_ADMIN</workgroup>
                </workgroups>
            </authorization>
        </authorizations>

		<documentTypeName>EffortCertificationReportDefinitionMaintenanceDocument</documentTypeName>
		<documentTypeCode>ARPT</documentTypeCode>

		<label>Report Definition Maintenance Document</label>
		<shortLabel>Effort Cert Report Maint Doc</shortLabel>
		
		<summary>Effort Certification Report Definition Maintenance Document</summary>
		<description>Document used to create or update Effort Certification Report Maintenance Document</description>

        <lockingKeys>
            <field attributeName="universityFiscalYear" />
			<field attributeName="effortCertificationReportNumber" />
		</lockingKeys>  
		<defaultExistenceChecks>
			<reference attributeName="options" attributeToHighlightOnFail="universityFiscalYear" />
			<reference attributeName="effortCertificationPeriodStatusCode" attributeToHighlightOnFail="effortCertificationReportPeriodStatusCode" />
			<reference attributeName="reportBeginFiscalYear" attributeToHighlightOnFail="effortCertificationReportBeginFiscalYear" />
			<reference attributeName="reportBeginPeriod" attributeToHighlightOnFail="effortCertificationReportBeginPeriodCode" />
			<reference attributeName="reportEndFiscalYear" attributeToHighlightOnFail="effortCertificationReportEndFiscalYear" />
			<reference attributeName="reportEndPeriod" attributeToHighlightOnFail="effortCertificationReportEndPeriodCode" />
			<reference attributeName="expenseTransferYear" attributeToHighlightOnFail="expenseTransferFiscalYear" />
			<reference attributeName="expenseTransferFiscalPeriod" attributeToHighlightOnFail="expenseTransferFiscalPeriodCode" />
			<reference attributeName="effortCertificationReportType" activeIndicatorAttributeName="active" attributeToHighlightOnFail="effortCertificationReportTypeCode" />
			<reference collection="effortCertificationReportPositions" attributeName="positionObjectGroup" activeIndicatorAttributeName="rowActiveIndicator" attributeToHighlightOnFail="effortCertificationReportPositionObjectGroupCode" />
		</defaultExistenceChecks>
		
	    <maintainableSections>
	        <maintainableSection title="Edit Report Definition">
	        	<maintainableItems>
	        		<maintainableField name="universityFiscalYear" required="true" />
	                <maintainableField name="effortCertificationReportNumber" required="true" />
	                <maintainableField name="effortCertificationReportPeriodTitle" required="true" />
	                <maintainableField name="effortCertificationReportPeriodStatusCode" required="true" />
	                <maintainableField name="effortCertificationReportBeginFiscalYear" required="true" />
	                <maintainableField name="effortCertificationReportBeginPeriodCode" required="true" />
	                <maintainableField name="effortCertificationReportEndFiscalYear" required="true" />
	                <maintainableField name="effortCertificationReportEndPeriodCode" required="true" />
	        	    <maintainableField name="effortCertificationReportTypeCode" required="true" />
	        	    <maintainableField name="expenseTransferFiscalYear" required="true" />
	                <maintainableField name="expenseTransferFiscalPeriodCode" required="true" />
	                <maintainableField name="effortCertificationReportReturnDate" />
	                <maintainableField name="active" required="true" >
			        	<defaultValue>true</defaultValue>
					</maintainableField>
	                <maintainableField name="versionNumber" />
	         	</maintainableItems>
	        </maintainableSection>
	        <maintainableSection title="Edit Report Position Object Group Codes">
				<maintainableItems>
					<maintainableCollection name="effortCertificationReportPositions" 
						businessObjectClass="org.kuali.module.effort.bo.EffortCertificationReportPosition"
						sourceClassName="org.kuali.module.labor.bo.PositionObjectGroup"
						sourceAttributeName="effortCertificationReportPositions"
						summaryTitle="Position Object Group Codes">
						<maintainableField name="effortCertificationReportPositionObjectGroupCode" required="true" />
						<maintainableField name="active" required="true" >
			            	<defaultValue>true</defaultValue>
						</maintainableField>
			            <maintainableField name="newCollectionRecord" />
			            <maintainableField name="versionNumber" />
			            <summaryFields>
                            <summaryField name="effortCertificationReportPositionObjectGroupCode" />
                        </summaryFields>
                        <duplicateIdentificationFields>
                        	<duplicateIdentificationField name="effortCertificationReportPositionObjectGroupCode" />
					    	<duplicateIdentificationField name="universityFiscalYear" />
							<duplicateIdentificationField name="effortCertificationReportNumber" />
						</duplicateIdentificationFields>
			        </maintainableCollection>
				</maintainableItems>
			</maintainableSection>
	    </maintainableSections>
	</maintenanceDocument>
</dictionaryEntry>
