        <ref bean="PurchasingAccountsPayableItemAsset-documentNumber" />
        <ref bean="PurchasingAccountsPayableItemAsset-accountsPayableLineItemIdentifier" />
        <ref bean="PurchasingAccountsPayableItemAsset-capitalAssetBuilderLineNumber" />
        <ref bean="PurchasingAccountsPayableItemAsset-accountsPayableLineItemDescription" />
