<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="GeneralLedgerEntryAssetDetail" parent="GeneralLedgerEntryAssetDetail-parentBean" />

  <bean id="GeneralLedgerEntryAssetDetail-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.kfs.module.cab.businessobject.GeneralLedgerEntryAssetDetail" />
    <property name="inquiryDefinition" >
      <ref bean="GeneralLedgerEntryAssetDetail-inquiryDefinition" />
    </property>
    <property name="lookupDefinition" >
      <ref bean="GeneralLedgerEntryAssetDetail-lookupDefinition" />
    </property>
    <property name="objectLabel" value="General Ledger Entry Asset Detail" />
    <property name="attributes" >
      <list>
        <ref bean="GeneralLedgerEntryAssetDetail-generalLedgerAccountIdentifier" />
        <ref bean="GeneralLedgerEntryAssetDetail-capitalAssetBuilderLineNumber" />
        <ref bean="GeneralLedgerEntryAssetDetail-capitalAssetNumber" />
        <ref bean="GeneralLedgerEntryAssetDetail-newAssetIndicator" />
        <ref bean="GeneralLedgerEntryAssetDetail-campusTagNumber" />
        <ref bean="GeneralLedgerEntryAssetDetail-serialNumber" />
        <ref bean="GeneralLedgerEntryAssetDetail-campusCode" />
        <ref bean="GeneralLedgerEntryAssetDetail-buildingCode" />
        <ref bean="GeneralLedgerEntryAssetDetail-buildingRoomNumber" />
        <ref bean="GeneralLedgerEntryAssetDetail-buildingSubRoomNumber" />
        <ref bean="GeneralLedgerEntryAssetDetail-newCollectionRecord" />
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->

  <bean id="GeneralLedgerEntryAssetDetail-generalLedgerAccountIdentifier" parent="GeneralLedgerEntryAssetDetail-generalLedgerAccountIdentifier-parentBean" />

  <bean id="GeneralLedgerEntryAssetDetail-generalLedgerAccountIdentifier-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="generalLedgerAccountIdentifier" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="General Ledger Account Identifier" />
    <property name="shortLabel" value="Number" />
    <property name="maxLength" value="12" />
    <property name="validationPattern" >
      <bean parent="NumericValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="14" />
    </property>
    <property name="summary" value="generalLedgerAccountIdentifier" />
    <property name="description" value="generalLedgerAccountIdentifier description..." />
  </bean>
  
  <bean id="GeneralLedgerEntryAssetDetail-capitalAssetBuilderLineNumber" parent="GeneralLedgerEntryAssetDetail-capitalAssetBuilderLineNumber-parentBean" />

  <bean id="GeneralLedgerEntryAssetDetail-capitalAssetBuilderLineNumber-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="capitalAssetBuilderLineNumber" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Capital Asset Builder Line Number" />
    <property name="shortLabel" value="Number" />
    <property name="maxLength" value="3" />
    <property name="validationPattern" >
      <bean parent="NumericValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="5" />
    </property>
    <property name="summary" value="capitalAssetBuilderLineNumber" />
    <property name="description" value="capitalAssetBuilderLineNumber description..." />
  </bean>

  <bean id="GeneralLedgerEntryAssetDetail-capitalAssetNumber" parent="GeneralLedgerEntryAssetDetail-capitalAssetNumber-parentBean" />

  <bean id="GeneralLedgerEntryAssetDetail-capitalAssetNumber-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="capitalAssetNumber" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Capital Asset Number" />
    <property name="shortLabel" value="Number" />
    <property name="maxLength" value="12" />
    <property name="validationPattern" >
      <bean parent="NumericValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="14" />
    </property>
    <property name="summary" value="capitalAssetNumber" />
    <property name="description" value="capitalAssetNumber description..." />
  </bean>

  <bean id="GeneralLedgerEntryAssetDetail-newAssetIndicator" parent="GeneralLedgerEntryAssetDetail-newAssetIndicator-parentBean" />

  <bean id="GeneralLedgerEntryAssetDetail-newAssetIndicator-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="newAssetIndicator" />
    <property name="forceUppercase" value="false" />
    <property name="label" value="New Asset Indicator" />
    <property name="shortLabel" value="New Asset Indicator" />
    <property name="maxLength" value="1" />
    <property name="control" >
      <bean parent="CheckboxControlDefinition" />
    </property>
    <property name="summary" value="newAssetIndicator" />
    <property name="description" value="newAssetIndicator description..." />
  </bean>

  <bean id="GeneralLedgerEntryAssetDetail-buildingCode" parent="GeneralLedgerEntryAssetDetail-buildingCode-parentBean" />

  <bean id="GeneralLedgerEntryAssetDetail-buildingCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="buildingCode" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Building Code" />
    <property name="shortLabel" value="Code" />
    <property name="maxLength" value="10" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="12" />
    </property>
    <property name="summary" value="buildingCode" />
    <property name="description" value="buildingCode description..." />
  </bean>
  <bean id="GeneralLedgerEntryAssetDetail-buildingRoomNumber" parent="GeneralLedgerEntryAssetDetail-buildingRoomNumber-parentBean" />

  <bean id="GeneralLedgerEntryAssetDetail-buildingRoomNumber-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="buildingRoomNumber" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Building Room Number" />
    <property name="shortLabel" value="Number" />
    <property name="maxLength" value="8" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="10" />
    </property>
    <property name="summary" value="buildingRoomNumber" />
    <property name="description" value="buildingRoomNumber description..." />
  </bean>
  <bean id="GeneralLedgerEntryAssetDetail-buildingSubRoomNumber" parent="GeneralLedgerEntryAssetDetail-buildingSubRoomNumber-parentBean" />

  <bean id="GeneralLedgerEntryAssetDetail-buildingSubRoomNumber-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="buildingSubRoomNumber" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Building Sub Room Number" />
    <property name="shortLabel" value="Number" />
    <property name="maxLength" value="2" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="4" />
    </property>
    <property name="summary" value="buildingSubRoomNumber" />
    <property name="description" value="buildingSubRoomNumber description..." />
  </bean>
  
  <bean id="GeneralLedgerEntryAssetDetail-serialNumber" parent="GeneralLedgerEntryAssetDetail-serialNumber-parentBean" />

  <bean id="GeneralLedgerEntryAssetDetail-serialNumber-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="serialNumber" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Serial Number" />
    <property name="shortLabel" value="Number" />
    <property name="maxLength" value="25" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="27" />
    </property>
    <property name="summary" value="serialNumber" />
    <property name="description" value="serialNumber description..." />
  </bean>
  
  <bean id="GeneralLedgerEntryAssetDetail-campusCode" parent="GeneralLedgerEntryAssetDetail-campusCode-parentBean" />

  <bean id="GeneralLedgerEntryAssetDetail-campusCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="campusCode" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Campus Code" />
    <property name="shortLabel" value="Code" />
    <property name="maxLength" value="2" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="4" />
    </property>
    <property name="summary" value="campusCode" />
    <property name="description" value="campusCode description..." />
  </bean>
  <bean id="GeneralLedgerEntryAssetDetail-campusTagNumber" parent="GeneralLedgerEntryAssetDetail-campusTagNumber-parentBean" />

  <bean id="GeneralLedgerEntryAssetDetail-campusTagNumber-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="campusTagNumber" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="New Tag Number" />
    <property name="shortLabel" value="Number" />
    <property name="maxLength" value="8" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="10" />
    </property>
    <property name="summary" value="campusTagNumber" />
    <property name="description" value="campusTagNumber description..." />
  </bean>
  
  <bean id="GeneralLedgerEntryAssetDetail-newCollectionRecord" parent="GeneralLedgerEntryAssetDetail-newCollectionRecord-parentBean" />

  <bean id="GeneralLedgerEntryAssetDetail-newCollectionRecord-parentBean" abstract="true" parent="GenericAttributes-newCollectionRecord" />

<!-- Business Object Inquiry Definition -->

  <bean id="GeneralLedgerEntryAssetDetail-inquiryDefinition" parent="GeneralLedgerEntryAssetDetail-inquiryDefinition-parentBean" />

  <bean id="GeneralLedgerEntryAssetDetail-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
    <property name="title" value="General Ledger Entry Asset Detail Inquiry" />
    <property name="inquirySections" >
      <list>
        <bean parent="InquirySectionDefinition">
          <property name="title" value="" />
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields" >
            <list>
              <bean parent="FieldDefinition"
                    p:attributeName="generalLedgerAccountIdentifier" />
              <bean parent="FieldDefinition"
                    p:attributeName="capitalAssetBuilderLineNumber" />
              <bean parent="FieldDefinition"
                    p:attributeName="capitalAssetNumber" />
              <bean parent="FieldDefinition"
                    p:attributeName="newAssetIndicator" />
              <bean parent="FieldDefinition"
                    p:attributeName="buildingCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="buildingRoomNumber" />
              <bean parent="FieldDefinition"
                    p:attributeName="buildingSubRoomNumber" />
              <bean parent="FieldDefinition"
                    p:attributeName="serialNumber" />
              <bean parent="FieldDefinition"
                    p:attributeName="campusCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="campusTagNumber" />
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Business Object Lookup Definition -->


  <bean id="GeneralLedgerEntryAssetDetail-lookupDefinition" parent="GeneralLedgerEntryAssetDetail-lookupDefinition-parentBean" />

  <bean id="GeneralLedgerEntryAssetDetail-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
    <property name="title" value="General Ledger Entry Asset Detail Lookup" />
    <property name="instructions" value="Look up an General Ledger Entry Asset Detail" />
    <property name="defaultSort" >
      <bean parent="SortDefinition">
      </bean>
    </property>
    <property name="lookupFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="generalLedgerAccountIdentifier" />
        <bean parent="FieldDefinition"
              p:attributeName="capitalAssetBuilderLineNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="capitalAssetNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="newAssetIndicator" />
        <bean parent="FieldDefinition"
              p:attributeName="buildingCode" />
        <bean parent="FieldDefinition"
              p:attributeName="buildingRoomNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="buildingSubRoomNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="serialNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="campusCode" />
        <bean parent="FieldDefinition"
              p:attributeName="campusTagNumber" />
      </list>
    </property>
    <property name="resultFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="generalLedgerAccountIdentifier" />
        <bean parent="FieldDefinition"
              p:attributeName="capitalAssetBuilderLineNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="capitalAssetNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="newAssetIndicator" />
        <bean parent="FieldDefinition"
              p:attributeName="buildingCode" />
        <bean parent="FieldDefinition"
              p:attributeName="buildingRoomNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="buildingSubRoomNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="serialNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="campusCode" />
        <bean parent="FieldDefinition"
              p:attributeName="campusTagNumber" />
      </list>
    </property>
  </bean>
</beans>
