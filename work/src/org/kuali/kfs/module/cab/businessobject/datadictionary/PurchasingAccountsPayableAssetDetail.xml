<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="PurchasingAccountsPayableAssetDetail" parent="PurchasingAccountsPayableAssetDetail-parentBean" />

  <bean id="PurchasingAccountsPayableAssetDetail-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.kfs.module.cab.businessobject.PurchasingAccountsPayableAssetDetail" />
    <property name="inquiryDefinition" >
      <ref bean="PurchasingAccountsPayableAssetDetail-inquiryDefinition" />
    </property>
    <property name="lookupDefinition" >
      <ref bean="PurchasingAccountsPayableAssetDetail-lookupDefinition" />
    </property>
    <property name="objectLabel" value="Purchasing Accounts Payable Asset Detail" />
    <property name="attributes" >
      <list>
        <ref bean="PurchasingAccountsPayableAssetDetail-documentNumber" />
        <ref bean="PurchasingAccountsPayableAssetDetail-accountsPayableLineItemIdentifier" />
        <ref bean="PurchasingAccountsPayableAssetDetail-capitalAssetBuilderLineNumber" />
        <ref bean="PurchasingAccountsPayableAssetDetail-capitalAssetNumber" />
        <ref bean="PurchasingAccountsPayableAssetDetail-newAssetIndicator" />
        <ref bean="PurchasingAccountsPayableAssetDetail-campusTagNumber" />
        <ref bean="PurchasingAccountsPayableAssetDetail-serialNumber" />
        <ref bean="PurchasingAccountsPayableAssetDetail-campusCode" />
        <ref bean="PurchasingAccountsPayableAssetDetail-buildingCode" />
        <ref bean="PurchasingAccountsPayableAssetDetail-buildingRoomNumber" />
        <ref bean="PurchasingAccountsPayableAssetDetail-buildingSubRoomNumber" />
        <ref bean="PurchasingAccountsPayableAssetDetail-versionNumber" />
        <ref bean="PurchasingAccountsPayableAssetDetail-newCollectionRecord" />
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->

  <bean id="PurchasingAccountsPayableAssetDetail-documentNumber" parent="PurchasingAccountsPayableAssetDetail-documentNumber-parentBean" />

  <bean id="PurchasingAccountsPayableAssetDetail-documentNumber-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="documentNumber" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Document Number" />
    <property name="shortLabel" value="Number" />
    <property name="maxLength" value="12" />
    <property name="validationPattern" >
      <bean parent="NumericValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="14" />
    </property>
    <property name="summary" value="documentNumber" />
    <property name="description" value="documentNumber description..." />
  </bean>
  
  <bean id="PurchasingAccountsPayableAssetDetail-accountsPayableLineItemIdentifier" parent="PurchasingAccountsPayableAssetDetail-accountsPayableLineItemIdentifier-parentBean" />

  <bean id="PurchasingAccountsPayableAssetDetail-accountsPayableLineItemIdentifier-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="accountsPayableLineItemIdentifier" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Accounts Payable Line Item Identifier" />
    <property name="shortLabel" value="Number" />
    <property name="maxLength" value="3" />
    <property name="validationPattern" >
      <bean parent="NumericValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="5" />
    </property>
    <property name="summary" value="accountsPayableLineItemIdentifier" />
    <property name="description" value="accountsPayableLineItemIdentifier description..." />
  </bean>

  <bean id="PurchasingAccountsPayableAssetDetail-capitalAssetBuilderLineNumber" parent="PurchasingAccountsPayableAssetDetail-capitalAssetBuilderLineNumber-parentBean" />

