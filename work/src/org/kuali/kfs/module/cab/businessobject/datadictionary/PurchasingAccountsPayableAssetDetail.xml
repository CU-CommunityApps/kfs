    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="4" />
    </property>
    <property name="summary" value="campusCode" />
    <property name="description" value="campusCode description..." />
  </bean>
  <bean id="PurchasingAccountsPayableAssetDetail-campusTagNumber" parent="PurchasingAccountsPayableAssetDetail-campusTagNumber-parentBean" />

  <bean id="PurchasingAccountsPayableAssetDetail-campusTagNumber-parentBean" abstract="true" parent="AttributeDefinition">
