    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="10" />
    </property>
    <property name="summary" value="buildingRoomNumber" />
    <property name="description" value="buildingRoomNumber description..." />
  </bean>
  <bean id="PurchasingAccountsPayableAssetDetail-buildingSubRoomNumber" parent="PurchasingAccountsPayableAssetDetail-buildingSubRoomNumber-parentBean" />

  <bean id="PurchasingAccountsPayableAssetDetail-buildingSubRoomNumber-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="buildingSubRoomNumber" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Building Sub Room Number" />
    <property name="shortLabel" value="Number" />
    <property name="maxLength" value="2" />
    <property name="validationPattern" >
