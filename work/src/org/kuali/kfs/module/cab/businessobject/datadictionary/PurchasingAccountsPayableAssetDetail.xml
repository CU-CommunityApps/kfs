    <property name="name" value="capitalAssetBuilderLineNumber" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Capital Asset Builder Line Number" />
    <property name="shortLabel" value="Number" />
    <property name="maxLength" value="3" />
    <property name="validationPattern" >
      <bean parent="NumericValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="5" />
    </property>
    <property name="summary" value="capitalAssetBuilderLineNumber" />
    <property name="description" value="capitalAssetBuilderLineNumber description..." />
  </bean>

  <bean id="PurchasingAccountsPayableAssetDetail-capitalAssetNumber" parent="PurchasingAccountsPayableAssetDetail-capitalAssetNumber-parentBean" />

  <bean id="PurchasingAccountsPayableAssetDetail-capitalAssetNumber-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="capitalAssetNumber" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Capital Asset Number" />
    <property name="shortLabel" value="Number" />
    <property name="maxLength" value="12" />
    <property name="validationPattern" >
      <bean parent="NumericValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="14" />
    </property>
    <property name="summary" value="capitalAssetNumber" />
    <property name="description" value="capitalAssetNumber description..." />
  </bean>

  <bean id="PurchasingAccountsPayableAssetDetail-newAssetIndicator" parent="PurchasingAccountsPayableAssetDetail-newAssetIndicator-parentBean" />

  <bean id="PurchasingAccountsPayableAssetDetail-newAssetIndicator-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="newAssetIndicator" />
    <property name="forceUppercase" value="false" />
    <property name="label" value="New Asset Indicator" />
    <property name="shortLabel" value="New Asset Indicator" />
    <property name="maxLength" value="1" />
    <property name="control" >
      <bean parent="CheckboxControlDefinition" />
    </property>
    <property name="summary" value="newAssetIndicator" />
    <property name="description" value="newAssetIndicator description..." />
  </bean>

  <bean id="PurchasingAccountsPayableAssetDetail-buildingCode" parent="PurchasingAccountsPayableAssetDetail-buildingCode-parentBean" />

  <bean id="PurchasingAccountsPayableAssetDetail-buildingCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="buildingCode" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Building Code" />
    <property name="shortLabel" value="Code" />
    <property name="maxLength" value="10" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="12" />
    </property>
    <property name="summary" value="buildingCode" />
    <property name="description" value="buildingCode description..." />
  </bean>
  <bean id="PurchasingAccountsPayableAssetDetail-buildingRoomNumber" parent="PurchasingAccountsPayableAssetDetail-buildingRoomNumber-parentBean" />

  <bean id="PurchasingAccountsPayableAssetDetail-buildingRoomNumber-parentBean" abstract="true" parent="AttributeDefinition">
