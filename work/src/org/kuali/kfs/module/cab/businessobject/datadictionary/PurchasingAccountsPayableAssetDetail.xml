  <bean id="PurchasingAccountsPayableAssetDetail-serialNumber" parent="PurchasingAccountsPayableAssetDetail-serialNumber-parentBean" />

  <bean id="PurchasingAccountsPayableAssetDetail-serialNumber-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="serialNumber" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Serial Number" />
    <property name="shortLabel" value="Number" />
    <property name="maxLength" value="25" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="27" />
    </property>
    <property name="summary" value="serialNumber" />
