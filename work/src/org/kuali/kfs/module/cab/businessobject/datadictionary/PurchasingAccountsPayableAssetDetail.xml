  <bean id="PurchasingAccountsPayableAssetDetail-versionNumber" parent="PurchasingAccountsPayableAssetDetail-versionNumber-parentBean" />

  <bean id="PurchasingAccountsPayableAssetDetail-versionNumber-parentBean" abstract="true" parent="GenericAttributes-versionNumber" />

  <bean id="PurchasingAccountsPayableAssetDetail-newCollectionRecord" parent="PurchasingAccountsPayableAssetDetail-newCollectionRecord-parentBean" />

  <bean id="PurchasingAccountsPayableAssetDetail-newCollectionRecord-parentBean" abstract="true" parent="GenericAttributes-newCollectionRecord" />

<!-- Business Object Inquiry Definition -->

  <bean id="PurchasingAccountsPayableAssetDetail-inquiryDefinition" parent="PurchasingAccountsPayableAssetDetail-inquiryDefinition-parentBean" />

  <bean id="PurchasingAccountsPayableAssetDetail-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
    <property name="title" value="Purchasing Accounts Payable Asset Detail Inquiry" />
    <property name="inquirySections" >
      <list>
        <bean parent="InquirySectionDefinition">
          <property name="title" value="" />
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields" >
            <list>
              <bean parent="FieldDefinition"
                    p:attributeName="documentNumber" />
			  <bean parent="FieldDefinition"
                    p:attributeName="accountsPayableLineItemIdentifier" />                    
              <bean parent="FieldDefinition"
                    p:attributeName="capitalAssetBuilderLineNumber" />
              <bean parent="FieldDefinition"
                    p:attributeName="capitalAssetNumber" />
              <bean parent="FieldDefinition"
                    p:attributeName="newAssetIndicator" />
              <bean parent="FieldDefinition"
                    p:attributeName="buildingCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="buildingRoomNumber" />
              <bean parent="FieldDefinition"
                    p:attributeName="buildingSubRoomNumber" />
              <bean parent="FieldDefinition"
                    p:attributeName="serialNumber" />
              <bean parent="FieldDefinition"
                    p:attributeName="campusCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="campusTagNumber" />
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Business Object Lookup Definition -->


  <bean id="PurchasingAccountsPayableAssetDetail-lookupDefinition" parent="PurchasingAccountsPayableAssetDetail-lookupDefinition-parentBean" />

  <bean id="PurchasingAccountsPayableAssetDetail-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
    <property name="title" value="Purchasing Accounts Payable Asset Detail Lookup" />
    <property name="instructions" value="Look up an Purchasing Accounts Payable Asset Detail" />
    <property name="defaultSort" >
      <bean parent="SortDefinition">
      </bean>
    </property>
    <property name="lookupFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="documentNumber" />
		<bean parent="FieldDefinition"
              p:attributeName="accountsPayableLineItemIdentifier" />              
        <bean parent="FieldDefinition"
              p:attributeName="capitalAssetBuilderLineNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="capitalAssetNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="newAssetIndicator" />
        <bean parent="FieldDefinition"
              p:attributeName="buildingCode" />
        <bean parent="FieldDefinition"
              p:attributeName="buildingRoomNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="buildingSubRoomNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="serialNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="campusCode" />
        <bean parent="FieldDefinition"
              p:attributeName="campusTagNumber" />
      </list>
    </property>
    <property name="resultFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="documentNumber" />
		<bean parent="FieldDefinition"
              p:attributeName="accountsPayableLineItemIdentifier" />              
        <bean parent="FieldDefinition"
              p:attributeName="capitalAssetBuilderLineNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="capitalAssetNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="newAssetIndicator" />
        <bean parent="FieldDefinition"
              p:attributeName="buildingCode" />
        <bean parent="FieldDefinition"
              p:attributeName="buildingRoomNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="buildingSubRoomNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="serialNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="campusCode" />
        <bean parent="FieldDefinition"
              p:attributeName="campusTagNumber" />
      </list>
    </property>
  </bean>
</beans>
