<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="PaymentMediumMaintenanceDocument" parent="PaymentMediumMaintenanceDocument-parentBean" />

  <bean id="PaymentMediumMaintenanceDocument-parentBean" abstract="true" parent="MaintenanceDocumentEntry">
    <property name="businessObjectClass" value="org.kuali.module.ar.bo.PaymentMedium" />
    <property name="maintainableClass" value="org.kuali.core.maintenance.KualiMaintainableImpl" />
    <property name="maintainableSections" >
      <list>
        <ref bean="PaymentMediumMaintenanceDocument-EditPaymentMedium" />
      </list>
    </property>
    <property name="lockingKeys" >
      <list>
        <value>customerPaymentMediumCode</value>
      </list>
    </property>
    <property name="label" value="Payment Medium" />
    <property name="shortLabel" value="PayMedium" />
    <property name="summary" value="Payment Medium maintenance document" />
    <property name="description" value="Document used to create or update Payment Medium objects" />
    <property name="documentTypeName" value="PaymentMediumMaintenanceDocument" />
    <property name="documentAuthorizerClass" value="org.kuali.core.document.authorization.MaintenanceDocumentAuthorizerBase" />
    <property name="documentTypeCode" value="PMED" />
    <property name="authorizations" >
      <list>
        <bean parent="AuthorizationDefinition">
          <property name="action" value="initiate" />
          <property name="authorizedGroups" >
            <set>
              <value>SY_MAINTENANCE_USERS</value>
            </set>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Maintenance Section Definitions -->


  <bean id="PaymentMediumMaintenanceDocument-EditPaymentMedium" parent="PaymentMediumMaintenanceDocument-EditPaymentMedium-parentBean" />

  <bean id="PaymentMediumMaintenanceDocument-EditPaymentMedium-parentBean" abstract="true" parent="MaintainableSectionDefinition">
    <property name="id" value="Edit Payment Medium" />
    <property name="title" value="Edit Payment Medium" />
    <property name="maintainableItems" >
      <list>
        <bean parent="MaintainableFieldDefinition"
              p:name="customerPaymentMediumCode"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="customerPaymentMediumDescription"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="active"
              p:defaultValue="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="versionNumber" />
      </list>
    </property>
  </bean>
</beans>
