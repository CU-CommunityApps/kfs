        </property>
    </bean>
    <bean id="SearchingType-AccountsReceivableDocument-processingOrganizationCode" class="org.kuali.rice.kns.datadictionary.SearchingTypeDefinition">
        <property name="searchingAttribute" ref="SearchingAttribute-processingOrganizationCode" />
