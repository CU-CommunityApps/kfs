    </bean>  
    <bean id="SearchingAttribute-financialDocumentReferenceInvoiceNumber" class="org.kuali.rice.kns.datadictionary.SearchingAttribute" parent="WorkflowAttributeMetaData-financialDocumentReferenceInvoiceNumber" />
    
	<bean id="WorkflowAttributeMetaData-lockboxNumber" abstract="true">
  		<property name="businessObjectClassName" value="org.kuali.kfs.module.ar.businessobject.SystemInformation" />
