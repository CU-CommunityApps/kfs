    </bean>
    <bean id="SearchingAttribute-customerName" class="org.kuali.rice.kns.datadictionary.SearchingAttribute" parent="WorkflowAttributeMetaData-customerName" />
    
    <bean id="WorkflowAttributeMetaData-billingChartOfAccountsCode" abstract="true">
