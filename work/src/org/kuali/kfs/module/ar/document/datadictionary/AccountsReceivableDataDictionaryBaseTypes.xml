    	<property name="attributeName" value="billByChartOfAccountCode" />
    </bean>  
    <bean id="SearchingAttribute-billingChartOfAccountsCode" class="org.kuali.rice.kns.datadictionary.SearchingAttribute" parent="WorkflowAttributeMetaData-billingChartOfAccountsCode" />

    <bean id="WorkflowAttributeMetaData-billingOrganizationCode" abstract="true">
