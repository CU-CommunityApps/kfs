    </bean>  
    <bean id="SearchingAttribute-processingChartOfAccountsCode" class="org.kuali.rice.kns.datadictionary.SearchingAttribute" parent="WorkflowAttributeMetaData-processingChartOfAccountsCode" />

    <bean id="WorkflowAttributeMetaData-processingOrganizationCode" abstract="true">
