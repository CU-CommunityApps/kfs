	<bean id="SearchingType-AccountsReceivableDocument-customerNumber" class="org.kuali.rice.kns.datadictionary.SearchingTypeDefinition">
        <property name="searchingAttribute" ref="SearchingAttribute-customerNumber" />
        <property name="documentValues">
        	<list>
        		<value>accountsReceivableDocumentHeader.customerNumber</value>
        	</list>
        </property>
    </bean>
	<bean id="SearchingType-AccountsReceivableDocument-customerName" class="org.kuali.rice.kns.datadictionary.SearchingTypeDefinition">
        <property name="searchingAttribute" ref="SearchingAttribute-customerName" />
        <property name="documentValues">
        	<list>
        		<value>accountsReceivableDocumentHeader.customer.customerName</value>
        	</list>
        </property>
    </bean>
	<bean id="SearchingType-AccountsReceivableDocument-processingChartOfAccountsCode" class="org.kuali.rice.kns.datadictionary.SearchingTypeDefinition">
        <property name="searchingAttribute" ref="SearchingAttribute-processingChartOfAccountsCode" />
        <property name="documentValues">
        	<list>
