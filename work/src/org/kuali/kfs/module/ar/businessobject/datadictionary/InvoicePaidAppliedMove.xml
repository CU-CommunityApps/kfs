<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="InvoicePaidAppliedMove" parent="InvoicePaidAppliedMove-parentBean" />

  <bean id="InvoicePaidAppliedMove-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.kfs.module.ar.businessobject.InvoicePaidAppliedMove" />
    <property name="inquiryDefinition" >
      <ref bean="InvoicePaidAppliedMove-inquiryDefinition" />
    </property>
    <property name="lookupDefinition" >
      <ref bean="InvoicePaidAppliedMove-lookupDefinition" />
    </property>
    <property name="objectLabel" value="Invoice Paid Applied Move" />
    <property name="attributes" >
      <list>
        <ref bean="InvoicePaidAppliedMove-documentNumber" />
        <ref bean="InvoicePaidAppliedMove-financialDocumentReferenceInvoiceNumber" />
        <ref bean="InvoicePaidAppliedMove-invoiceItemAppliedAmount" />
        <ref bean="InvoicePaidAppliedMove-invoiceItemNumber" />
        <ref bean="InvoicePaidAppliedMove-paidAppliedItemNumber" />
        <ref bean="InvoicePaidAppliedMove-universityFiscalPeriodCode" />
        <ref bean="InvoicePaidAppliedMove-universityFiscalYear" />
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->


  <bean id="InvoicePaidAppliedMove-documentNumber" parent="InvoicePaidAppliedMove-documentNumber-parentBean" />

  <bean id="InvoicePaidAppliedMove-documentNumber-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="documentNumber" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Document Number" />
    <property name="shortLabel" value="Number" />
    <property name="maxLength" value="14" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="16" />
    </property>
  </bean>
  <bean id="InvoicePaidAppliedMove-financialDocumentReferenceInvoiceNumber" parent="InvoicePaidAppliedMove-financialDocumentReferenceInvoiceNumber-parentBean" />

  <bean id="InvoicePaidAppliedMove-financialDocumentReferenceInvoiceNumber-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="financialDocumentReferenceInvoiceNumber" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Financial Document Reference Invoice Number" />
    <property name="shortLabel" value="Number" />
    <property name="maxLength" value="14" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="16" />
    </property>
  </bean>
  <bean id="InvoicePaidAppliedMove-invoiceItemAppliedAmount" parent="InvoicePaidAppliedMove-invoiceItemAppliedAmount-parentBean" />

  <bean id="InvoicePaidAppliedMove-invoiceItemAppliedAmount-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="invoiceItemAppliedAmount" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Invoice Item Applied Amount" />
    <property name="shortLabel" value="Amount" />
    <property name="maxLength" value="19" />
    <property name="validationPattern" >
      <bean parent="FloatingPointValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="21" />
    </property>
  </bean>
  <bean id="InvoicePaidAppliedMove-invoiceItemNumber" parent="InvoicePaidAppliedMove-invoiceItemNumber-parentBean" />

  <bean id="InvoicePaidAppliedMove-invoiceItemNumber-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="invoiceItemNumber" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Invoice Item Number" />
    <property name="shortLabel" value="Number" />
    <property name="maxLength" value="7" />
    <property name="validationPattern" >
      <bean parent="NumericValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="9" />
    </property>
  </bean>
  <bean id="InvoicePaidAppliedMove-paidAppliedItemNumber" parent="InvoicePaidAppliedMove-paidAppliedItemNumber-parentBean" />

  <bean id="InvoicePaidAppliedMove-paidAppliedItemNumber-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="paidAppliedItemNumber" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Paid Applied Item Number" />
    <property name="shortLabel" value="Number" />
    <property name="maxLength" value="7" />
    <property name="validationPattern" >
      <bean parent="NumericValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="9" />
    </property>
  </bean>
  <bean id="InvoicePaidAppliedMove-universityFiscalPeriodCode" parent="InvoicePaidAppliedMove-universityFiscalPeriodCode-parentBean" />

  <bean id="InvoicePaidAppliedMove-universityFiscalPeriodCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="universityFiscalPeriodCode" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="University Fiscal Period Code" />
    <property name="shortLabel" value="Code" />
    <property name="maxLength" value="2" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="4" />
    </property>
  </bean>
  <bean id="InvoicePaidAppliedMove-universityFiscalYear" parent="InvoicePaidAppliedMove-universityFiscalYear-parentBean" />

  <bean id="InvoicePaidAppliedMove-universityFiscalYear-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="universityFiscalYear" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="University Fiscal Year" />
    <property name="shortLabel" value="Year" />
    <property name="maxLength" value="4" />
    <property name="validationPattern" >
      <bean parent="NumericValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="6" />
    </property>
  </bean>

<!-- Business Object Inquiry Definition -->


  <bean id="InvoicePaidAppliedMove-inquiryDefinition" parent="InvoicePaidAppliedMove-inquiryDefinition-parentBean" />

  <bean id="InvoicePaidAppliedMove-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
    <property name="title" value="Invoice Paid Applied Move Inquiry" />
    <property name="inquirySections" >
      <list>
        <bean parent="InquirySectionDefinition">
          <property name="title" value="" />
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields" >
            <list>
              <bean parent="FieldDefinition"
                    p:attributeName="documentNumber" />
              <bean parent="FieldDefinition"
                    p:attributeName="financialDocumentReferenceInvoiceNumber" />
              <bean parent="FieldDefinition"
                    p:attributeName="invoiceItemAppliedAmount" />
              <bean parent="FieldDefinition"
                    p:attributeName="invoiceItemNumber" />
              <bean parent="FieldDefinition"
                    p:attributeName="paidAppliedItemNumber" />
              <bean parent="FieldDefinition"
                    p:attributeName="universityFiscalPeriodCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="universityFiscalYear" />
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Business Object Lookup Definition -->


  <bean id="InvoicePaidAppliedMove-lookupDefinition" parent="InvoicePaidAppliedMove-lookupDefinition-parentBean" />

  <bean id="InvoicePaidAppliedMove-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
    <property name="title" value="Invoice Paid Applied Move Lookup" />
    <property name="instructions" value="Look up an Invoice Paid Applied Move" />
    <property name="defaultSort" >
      <bean parent="SortDefinition">
      </bean>
    </property>
    <property name="lookupFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="documentNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="financialDocumentReferenceInvoiceNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="invoiceItemAppliedAmount" />
        <bean parent="FieldDefinition"
              p:attributeName="invoiceItemNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="paidAppliedItemNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="universityFiscalPeriodCode" />
        <bean parent="FieldDefinition"
              p:attributeName="universityFiscalYear" />
      </list>
    </property>
    <property name="resultFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="documentNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="financialDocumentReferenceInvoiceNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="invoiceItemAppliedAmount" />
        <bean parent="FieldDefinition"
              p:attributeName="invoiceItemNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="paidAppliedItemNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="universityFiscalPeriodCode" />
        <bean parent="FieldDefinition"
              p:attributeName="universityFiscalYear" />
      </list>
    </property>
  </bean>
</beans>
