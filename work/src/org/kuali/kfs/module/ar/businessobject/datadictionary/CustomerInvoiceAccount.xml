	<bean id="CustomerInvoiceAccount" parent="CustomerInvoiceAccount-parentBean" />

	<bean id="CustomerInvoiceAccount-parentBean" abstract="true"
		parent="BusinessObjectEntry">
		<property name="businessObjectClass"
			value="org.kuali.kfs.module.ar.businessobject.CustomerInvoiceAccount" />
		<property name="inquiryDefinition">
			<ref bean="CustomerInvoiceAccount-inquiryDefinition" />
		</property>
		<property name="lookupDefinition">
			<ref bean="CustomerInvoiceAccount-lookupDefinition" />
		</property>
		<property name="objectLabel" value="Customer Invoice Account" />
		<property name="attributes">
			<list>
				<ref bean="CustomerInvoiceAccount-chartOfAccountsCode" />
				<ref bean="CustomerInvoiceAccount-accountNumber" />
				<ref bean="CustomerInvoiceAccount-account.accountName" />
				<ref bean="CustomerInvoiceAccount-objectCode" />
				<ref bean="CustomerInvoiceAccount-subObjectCode" />
				<ref bean="CustomerInvoiceAccount-subAccountNumber" />
				<ref bean="CustomerInvoiceAccount-accountType" />
				<ref bean="CustomerInvoiceAccount-active" />
			</list>
		</property>
	</bean>

	<!-- Attribute Definitions -->


	<bean id="CustomerInvoiceAccount-chartOfAccountsCode" parent="CustomerInvoiceAccount-chartOfAccountsCode-parentBean" />
	<bean id="CustomerInvoiceAccount-chartOfAccountsCode-parentBean"
		abstract="true" parent="ChartAttribute-SelectControl">
		<property name="name" value="chartOfAccountsCode" />
	</bean>

	<bean id="CustomerInvoiceAccount-accountNumber" parent="CustomerInvoiceAccount-accountNumber-parentBean" />
	<bean id="CustomerInvoiceAccount-accountNumber-parentBean" abstract="true"
		parent="AccountAttribute">
		<property name="name" value="accountNumber" />
	</bean>

	<bean id="CustomerInvoiceAccount-account.accountName" parent="CustomerInvoiceAccount-account.accountName-parentBean" />
	<bean id="CustomerInvoiceAccount-account.accountName-parentBean"
		abstract="true" parent="Account-accountName">
		<property name="name" value="account.accountName" />
	</bean>

	<bean id="CustomerInvoiceAccount-objectCode" parent="CustomerInvoiceAccount-objectCode-parentBean" />
	<bean id="CustomerInvoiceAccount-objectCode-parentBean" abstract="true"
		parent="ObjectCodeAttribute">
		<property name="name" value="objectCode" />
	</bean>

	<bean id="CustomerInvoiceAccount-subObjectCode" parent="CustomerInvoiceAccount-subObjectCode-parentBean" />
	<bean id="CustomerInvoiceAccount-subObjectCode-parentBean" abstract="true"
		parent="SubObjectAttribute">
		<property name="name" value="subObjectCode" />
	</bean>

	<bean id="CustomerInvoiceAccount-subAccountNumber" parent="CustomerInvoiceAccount-subAccountNumber-parentBean" />
	<bean id="CustomerInvoiceAccount-subAccountNumber-parentBean"
		abstract="true" parent="SubAccountAttribute">
		<property name="name" value="subAccountNumber" />
	</bean>


	<bean id="CustomerInvoiceAccount-accountType" parent="CustomerInvoiceAccount-accountType-parentBean" />

	<bean id="CustomerInvoiceAccount-accountType-parentBean" abstract="true"
		parent="AttributeDefinition">
		<property name="name" value="accountType" />
		<property name="label" value="Type of Account" />
		<property name="shortLabel" value="Type of Account" />
		<property name="maxLength" value="45" />
		<property name="control">
			<bean parent="SelectControlDefinition"
				p:valuesFinderClass="org.kuali.kfs.integration.cg.businessobject.options.AccountTypeValuesFinder"
				p:includeKeyInLabel="false" />
		</property>
	</bean>

	<bean id="CustomerInvoiceAccount-active" parent="CustomerInvoiceAccount-active-parentBean" />
	<bean id="CustomerInvoiceAccount-active-parentBean" abstract="true"
		parent="GenericAttributes-activeIndicator">
		<property name="name" value="active" />
	</bean>


	<!-- Business Object Inquiry Definition -->


	<bean id="CustomerInvoiceAccount-inquiryDefinition" parent="CustomerInvoiceAccount-inquiryDefinition-parentBean" />

	<bean id="CustomerInvoiceAccount-inquiryDefinition-parentBean"
		abstract="true" parent="InquiryDefinition">
		<property name="title" value="Customer Account Inquiry" />
		<property name="inquirySections">
			<list>
				<bean parent="InquirySectionDefinition">
					<property name="title" value="Customer Invoice Account Details" />
					<property name="numberOfColumns" value="1" />
					<property name="inquiryFields">
						<list>
							<bean parent="FieldDefinition" p:attributeName="accountNumber" />
							<bean parent="FieldDefinition" p:attributeName="chartOfAccountsCode" />
							<bean parent="FieldDefinition" p:attributeName="objectCode" />
							<bean parent="FieldDefinition" p:attributeName="subObjectCode" />
							<bean parent="FieldDefinition" p:attributeName="subAccountNumber" />
							<bean parent="FieldDefinition" p:attributeName="accountType" />
						</list>
					</property>
				</bean>
			</list>
		</property>
	</bean>

	<!-- Business Object Lookup Definition -->


	<bean id="CustomerInvoiceAccount-lookupDefinition" parent="CustomerInvoiceAccount-lookupDefinition-parentBean" />

	<bean id="CustomerInvoiceAccount-lookupDefinition-parentBean"
		abstract="true" parent="LookupDefinition">
		<property name="title" value="Customer Account Lookup" />

		<property name="defaultSort">
			<bean parent="SortDefinition">
			</bean>
		</property>
		<property name="lookupFields">
			<list>
				<bean parent="FieldDefinition" p:attributeName="accountNumber" />
				<bean parent="FieldDefinition" p:attributeName="chartOfAccountsCode" />
				<bean parent="FieldDefinition" p:attributeName="objectCode" />
				<bean parent="FieldDefinition" p:attributeName="subObjectCode" />
				<bean parent="FieldDefinition" p:attributeName="subAccountNumber" />
				<bean parent="FieldDefinition" p:attributeName="accountType" />
			</list>
		</property>
		<property name="resultFields">
			<list>
				<bean parent="FieldDefinition" p:attributeName="accountNumber" />
				<bean parent="FieldDefinition" p:attributeName="chartOfAccountsCode" />
				<bean parent="FieldDefinition" p:attributeName="objectCode" />
				<bean parent="FieldDefinition" p:attributeName="subObjectCode" />
				<bean parent="FieldDefinition" p:attributeName="subAccountNumber" />
				<bean parent="FieldDefinition" p:attributeName="accountType" />
			</list>
		</property>
	</bean>
</beans>
