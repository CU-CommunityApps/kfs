	<bean id="ContractsGrantsLOCReviewDetail" parent="ContractsGrantsLOCReviewDetail-parentBean" />
	<bean id="ContractsGrantsLOCReviewDetail-parentBean" abstract="true" parent="BusinessObjectEntry">
		<property name="businessObjectClass" value="org.kuali.kfs.module.ar.businessobject.ContractsGrantsLOCReviewDetail" />
		<property name="objectLabel" value="Contracts Grants LOC Review Detail" />
		<property name="attributes">
			<list>
				<ref bean="ContractsGrantsLOCReviewDetail-documentNumber" />
				<ref bean="ContractsGrantsLOCReviewDetail-proposalNumber" />
				<ref bean="ContractsGrantsLOCReviewDetail-awardDocumentNumber" />
				<ref bean="ContractsGrantsLOCReviewDetail-chartOfAccountsCode" />
				<ref bean="ContractsGrantsLOCReviewDetail-agencyNumber" />
				<ref bean="ContractsGrantsLOCReviewDetail-customerNumber" />
				<ref bean="ContractsGrantsLOCReviewDetail-awardBeginningDate" />
				<ref bean="ContractsGrantsLOCReviewDetail-awardEndingDate" />
				<ref bean="ContractsGrantsLOCReviewDetail-accountNumber" />
				<ref bean="ContractsGrantsLOCReviewDetail-accountExpirationDate" />
				<ref bean="ContractsGrantsLOCReviewDetail-accountDescription" />
				<ref bean="ContractsGrantsLOCReviewDetail-awardBudgetAmount" />
				<ref bean="ContractsGrantsLOCReviewDetail-claimOnCashBalance" />
				<ref bean="ContractsGrantsLOCReviewDetail-fundsNotDrawn" />
				<ref bean="ContractsGrantsLOCReviewDetail-hiddenAmountToDraw" />
				<ref bean="ContractsGrantsLOCReviewDetail-amountToDraw" />
				<ref bean="ContractsGrantsLOCReviewDetail-locAmount" />
				<ref bean="ContractsGrantsLOCReviewDetail-amountAvailableToDraw" />
			</list>
		</property>
	</bean>

	<bean id="ContractsGrantsLOCReviewDetail-documentNumber" parent="ContractsGrantsLOCReviewDetail-documentNumber-parentBean" />

	<bean id="ContractsGrantsLOCReviewDetail-documentNumber-parentBean" abstract="true" parent="DocumentHeader-documentNumber">
		<property name="forceUppercase" value="true" />
	</bean>

	<bean id="ContractsGrantsLOCReviewDetail-proposalNumber" parent="ContractsGrantsLOCReviewDetail-proposalNumber-parentBean" />

	<bean id="ContractsGrantsLOCReviewDetail-proposalNumber-parentBean" abstract="true" parent="AttributeDefinition">
		<property name="name" value="proposalNumber" />
		<property name="label" value="Proposal Number" />
		<property name="shortLabel" value="Proposal Number" />
		<property name="maxLength" value="10" />
		<property name="required" value="false" />
		<property name="control">
			<bean parent="TextControlDefinition" p:size="12" />
		</property>
	</bean>

	<bean id="ContractsGrantsLOCReviewDetail-awardDocumentNumber" parent="ContractsGrantsLOCReviewDetail-awardDocumentNumber-parentBean" />

	<bean id="ContractsGrantsLOCReviewDetail-awardDocumentNumber-parentBean" abstract="true" parent="AttributeDefinition">
		<property name="name" value="awardDocumentNumber" />
		<property name="label" value="Award Document Number" />
		<property name="shortLabel" value="Number" />
		<property name="maxLength" value="12" />
		<property name="validationPattern">
			<ref bean="AnyCharacterValidation" />
		</property>
		<property name="control">
			<bean parent="TextControlDefinition" p:size="14" />
		</property>
	</bean>

	<bean id="ContractsGrantsLOCReviewDetail-chartOfAccountsCode" parent="ContractsGrantsLOCReviewDetail-chartOfAccountsCode-parentBean" />
	<bean id="ContractsGrantsLOCReviewDetail-chartOfAccountsCode-parentBean" abstract="true" parent="ChartAttribute-TextControl">
		<property name="required" value="false" />
	</bean>

	<bean id="ContractsGrantsLOCReviewDetail-accountNumber" parent="ContractsGrantsLOCReviewDetail-accountNumber-parentBean" />

	<bean id="ContractsGrantsLOCReviewDetail-accountNumber-parentBean" parent="AccountAttribute">
		<property name="name" value="accountNumber" />
		<property name="label" value="Account Number" />
		<property name="shortLabel" value="CntrctAcct" />
		<property name="required" value="false" />
	</bean>
	<bean id="ContractsGrantsLOCReviewDetail-accountExpirationDate" parent="ContractsGrantsLOCReviewDetail-accountExpirationDate-parentBean" />

	<bean id="ContractsGrantsLOCReviewDetail-accountExpirationDate-parentBean" abstract="true" parent="GenericAttributes-genericTimestamp">
		<property name="name" value="accountExpirationDate" />
		<property name="label" value="Account Expiration Date" />
		<property name="shortLabel" value="ExpDate" />
	</bean>
	<bean id="ContractsGrantsLOCReviewDetail-accountDescription" parent="ContractsGrantsLOCReviewDetail-accountDescription-parentBean" />

	<bean id="ContractsGrantsLOCReviewDetail-accountDescription-parentBean" abstract="true" parent="AttributeDefinition">
		<property name="name" value="accountDescription" />
		<property name="label" value="Account Description" />
		<property name="shortLabel" value="Account Description" />
		<property name="maxLength" value="45" />
		<property name="required" value="false" />
		<property name="control">
			<bean parent="TextControlDefinition" p:size="45" />
		</property>
	</bean>

	<bean id="ContractsGrantsLOCReviewDetail-agencyNumber" parent="ContractsGrantsLOCReviewDetail-agencyNumber-parentBean" />

	<bean id="ContractsGrantsLOCReviewDetail-agencyNumber-parentBean" abstract="true" parent="GenericAttributes-agencyNumber">
	</bean>

	<bean id="ContractsGrantsLOCReviewDetail-customerNumber" parent="ContractsGrantsLOCReviewDetail-customerNumber-parentBean" />

	<bean id="ContractsGrantsLOCReviewDetail-customerNumber-parentBean" abstract="true" parent="Customer-customerNumber">
		<property name="name" value="customerNumber" />
		<property name="label" value="Customer Number" />
		<property name="shortLabel" value="Customer Number" />
	</bean>

	<bean id="ContractsGrantsLOCReviewDetail-awardBeginningDate" parent="ContractsGrantsLOCReviewDetail-awardBeginningDate-parentBean" />

	<bean id="ContractsGrantsLOCReviewDetail-awardBeginningDate-parentBean" abstract="true" parent="GenericAttributes-genericDate">
		<property name="name" value="awardBeginningDate" />
		<property name="label" value="Award Start Date" />
		<property name="shortLabel" value="Start" />
	</bean>

	<bean id="ContractsGrantsLOCReviewDetail-awardEndingDate" parent="ContractsGrantsLOCReviewDetail-awardEndingDate-parentBean" />

	<bean id="ContractsGrantsLOCReviewDetail-awardEndingDate-parentBean" abstract="true" parent="GenericAttributes-genericDate">
		<property name="name" value="awardEndingDate" />
		<property name="label" value="Award Stop Date" />
		<property name="shortLabel" value="Stop" />
	</bean>

	<bean id="ContractsGrantsLOCReviewDetail-awardBudgetAmount" parent="ContractsGrantsLOCReviewDetail-awardBudgetAmount-parentBean" />
	<bean id="ContractsGrantsLOCReviewDetail-awardBudgetAmount-parentBean" abstract="true" parent="GenericAttributes-genericAmount">
		<property name="name" value="awardBudgetAmount" />
		<property name="label" value="Award Budget Amount" />
		<property name="shortLabel" value="Award Budget Amount" />
		<property name="required" value="false" />
	</bean>

	<bean id="ContractsGrantsLOCReviewDetail-claimOnCashBalance" parent="ContractsGrantsLOCReviewDetail-claimOnCashBalance-parentBean" />
	<bean id="ContractsGrantsLOCReviewDetail-claimOnCashBalance-parentBean" abstract="true" parent="GenericAttributes-genericAmount">
		<property name="name" value="claimOnCashBalance" />
		<property name="label" value="Claim On Cash Balance" />
		<property name="shortLabel" value="Claim On Cash Balance" />
		<property name="required" value="false" />
	</bean>

	<bean id="ContractsGrantsLOCReviewDetail-fundsNotDrawn" parent="ContractsGrantsLOCReviewDetail-fundsNotDrawn-parentBean" />
	<bean id="ContractsGrantsLOCReviewDetail-fundsNotDrawn-parentBean" abstract="true" parent="GenericAttributes-genericAmount">
		<property name="name" value="fundsNotDrawn" />
		<property name="label" value="Funds Not Drawn" />
		<property name="shortLabel" value="Funds Not Drawn" />
		<property name="required" value="false" />
	</bean>

	<bean id="ContractsGrantsLOCReviewDetail-amountToDraw" parent="ContractsGrantsLOCReviewDetail-amountToDraw-parentBean" />
	<bean id="ContractsGrantsLOCReviewDetail-amountToDraw-parentBean" abstract="true" parent="GenericAttributes-genericAmount">
		<property name="name" value="amountToDraw" />
		<property name="label" value="Amount To Draw" />
		<property name="shortLabel" value="Amount To Draw" />
		<property name="required" value="false" />
	</bean>


	<bean id="ContractsGrantsLOCReviewDetail-hiddenAmountToDraw" parent="ContractsGrantsLOCReviewDetail-hiddenAmountToDraw-parentBean" />
	<bean id="ContractsGrantsLOCReviewDetail-hiddenAmountToDraw-parentBean" abstract="true" parent="GenericAttributes-genericAmount">
		<property name="name" value="hiddenAmountToDraw" />
		<property name="label" value="hidden Amount To Draw" />
		<property name="shortLabel" value="hidden Amount To Draw" />
		<property name="required" value="false" />
	</bean>

	<bean id="ContractsGrantsLOCReviewDetail-locAmount" parent="ContractsGrantsLOCReviewDetail-locAmount-parentBean" />
	<bean id="ContractsGrantsLOCReviewDetail-locAmount-parentBean" abstract="true" parent="GenericAttributes-genericAmount">
		<property name="name" value="locAmount" />
		<property name="label" value="LOC Amount" />
		<property name="shortLabel" value="LOC Amount" />
		<property name="required" value="false" />
	</bean>

	<bean id="ContractsGrantsLOCReviewDetail-amountAvailableToDraw" parent="ContractsGrantsLOCReviewDetail-amountAvailableToDraw-parentBean" />
	<bean id="ContractsGrantsLOCReviewDetail-amountAvailableToDraw-parentBean" abstract="true" parent="GenericAttributes-genericAmount">
		<property name="name" value="amountAvailableToDraw" />
		<property name="label" value="Award Amount Available To Draw" />
		<property name="shortLabel" value="Award Amount Available To Draw" />
		<property name="required" value="false" />
	</bean>
  
</beans>
