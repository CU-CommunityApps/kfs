	<bean id="DunningLetterDistributionOnDemandLookupResult" parent="DunningLetterDistributionOnDemandLookupResult-parentBean" />

	<bean id="DunningLetterDistributionOnDemandLookupResult-parentBean" abstract="true" parent="BusinessObjectEntry">
		<property name="businessObjectClass" value="org.kuali.kfs.module.ar.businessobject.DunningLetterDistributionOnDemandLookupResult" />
		<property name="objectLabel" value="Dunning Letter Distribution On Demand Lookup Result" />
		<property name="lookupDefinition">
			<ref bean="DunningLetterDistributionOnDemandLookupResult-lookupDefinition" />
		</property>
		<property name="attributes">
			<list>
				<ref bean="DunningLetterDistributionOnDemandLookupResult-principalId" />
 				<ref bean="DunningLetterDistributionOnDemandLookupResult-collector.principalName" />
				<ref bean="DunningLetterDistributionOnDemandLookupResult-proposalNumber" />
				<ref bean="DunningLetterDistributionOnDemandLookupResult-accountNumber" />
				<ref bean="DunningLetterDistributionOnDemandLookupResult-agencyNumber" />
				<ref bean="DunningLetterDistributionOnDemandLookupResult-customerNumber" />
				<ref bean="DunningLetterDistributionOnDemandLookupResult-campaignID" />
				<ref bean="DunningLetterDistributionOnDemandLookupResult-agingBucket" />
				<ref bean="DunningLetterDistributionOnDemandLookupResult-awardTotal" />
				<ref bean="DunningLetterDistributionOnDemandLookupResult-invoiceDocumentNumber" />
			</list>
		</property>
				<property name="relationships">
			<list>
				<bean parent="RelationshipDefinition">
		          <property name="objectAttributeName" value="collector"/>
		          <property name="primitiveAttributes">
		            <list>
		              <bean parent="PrimitiveAttributeDefinition" p:sourceName="principalId" p:targetName="principalId"/>
		            </list>
		          </property>
		          <property name="supportAttributes">
		            <list>
		              <bean parent="SupportAttributeDefinition" p:sourceName="collector.principalName" p:targetName="principalName"/>
		              <bean parent="SupportAttributeDefinition" p:sourceName="userLookupRoleNamespaceCode" p:targetName="lookupRoleNamespaceCode"/>
		              <bean parent="SupportAttributeDefinition" p:sourceName="userLookupRoleName" p:targetName="lookupRoleName"/>                    
		            </list>
		          </property>
		        </bean>
				<bean parent="RelationshipDefinition" p:objectAttributeName="award">
					<property name="primitiveAttributes">
						<list>
							<bean parent="PrimitiveAttributeDefinition" p:sourceName="proposalNumber" p:targetName="proposalNumber" />
						</list>
					</property>
				</bean>
				<bean parent="RelationshipDefinition" p:objectAttributeName="customer">
					<property name="primitiveAttributes">
						<list>
							<bean parent="PrimitiveAttributeDefinition" p:sourceName="customerNumber" p:targetName="customerNumber" />
						</list>
					</property>
				</bean>
				<bean parent="RelationshipDefinition" p:objectAttributeName="agency">
					<property name="primitiveAttributes">
						<list>
							<bean parent="PrimitiveAttributeDefinition" p:sourceName="agencyNumber" p:targetName="agencyNumber" />
						</list>
					</property>
				</bean>
			</list>
		</property>
	</bean>

	<!-- Attribute Definitions -->
  <bean id="DunningLetterDistributionOnDemandLookupResult-collector.principalName" parent="DunningLetterDistributionOnDemandLookupResult-collector.principalName-parentBean"/>

  <bean id="DunningLetterDistributionOnDemandLookupResult-collector.principalName-parentBean" abstract="true" parent="PersonImpl-principalName">
    <property name="name" value="collector.principalName"/>
    <property name="label" value="Collector"/>
    <property name="required" value="false"/>
    <property name="maxLength" value="40"/>
  </bean>

  <bean id="DunningLetterDistributionOnDemandLookupResult-principalId" parent="DunningLetterDistributionOnDemandLookupResult-principalId-parentBean"/>

  <bean id="DunningLetterDistributionOnDemandLookupResult-principalId-parentBean" abstract="true" parent="PersonImpl-principalId">
    <property name="name" value="principalId"/>
    <property name="forceUppercase" value="true"/>
    <property name="label" value="Collector"/>
    <property name="shortLabel" value="Collector"/>
    <property name="maxLength" value="40"/>
     <property name="control">
       <ref bean="HiddenControl" />
     </property>
  </bean>

	<bean id="DunningLetterDistributionOnDemandLookupResult-proposalNumber" parent="DunningLetterDistributionOnDemandLookupResult-proposalNumber-parentBean" />
	<bean id="DunningLetterDistributionOnDemandLookupResult-proposalNumber-parentBean" abstract="true" parent="ExternalizableAttributeDefinitionProxy"
		p:sourceExternalizableBusinessObjectInterface="org.kuali.kfs.integration.cg.ContractsAndGrantsAward" p:sourceAttributeName="proposalNumber">
		<property name="name" value="proposalNumber" />
		<property name="label" value="Proposal Number" />
	</bean>
	
	<bean id="DunningLetterDistributionOnDemandLookupResult-accountNumber" parent="DunningLetterDistributionOnDemandLookupResult-accountNumber-parentBean" />

	<bean id="DunningLetterDistributionOnDemandLookupResult-accountNumber-parentBean" abstract="true" parent="AccountAttribute" />

	<bean id="DunningLetterDistributionOnDemandLookupResult-agencyNumber" parent="DunningLetterDistributionOnDemandLookupResult-agencyNumber-parentBean" />
	<bean id="DunningLetterDistributionOnDemandLookupResult-agencyNumber-parentBean" abstract="true" parent="ExternalizableAttributeDefinitionProxy"
		p:sourceExternalizableBusinessObjectInterface="org.kuali.kfs.integration.cg.ContractsAndGrantsAgency" p:sourceAttributeName="agencyNumber">
		<property name="name" value="agencyNumber" />
		<property name="label" value="Agency Number" />
	</bean>
	
	<bean id="DunningLetterDistributionOnDemandLookupResult-invoiceDocumentNumber" parent="DunningLetterDistributionOnDemandLookupResult-invoiceDocumentNumber-parentBean" />
	<bean id="DunningLetterDistributionOnDemandLookupResult-invoiceDocumentNumber-parentBean" abstract="true" parent="DocumentHeader-documentNumber" >
	<property name="name" value="invoiceDocumentNumber" />
	<property name="label" value="Invoice Document Number" />
	</bean>


	<bean id="DunningLetterDistributionOnDemandLookupResult-customerNumber" parent="DunningLetterDistributionOnDemandLookupResult-customerNumber-parentBean" />
	<bean id="DunningLetterDistributionOnDemandLookupResult-customerNumber-parentBean" abstract="true" parent="Customer-customerNumber" />

	
	<bean id="DunningLetterDistributionOnDemandLookupResult-campaignID" parent="DunningLetterDistributionOnDemandLookupResult-campaignID-parentBean" />
	<bean id="DunningLetterDistributionOnDemandLookupResult-campaignID-parentBean" abstract="true" parent="DunningCampaign-campaignID" />
	

	<bean id="DunningLetterDistributionOnDemandLookupResult-awardTotal" parent="DunningLetterDistributionOnDemandLookupResult-awardTotal-parentBean" />
	<bean id="DunningLetterDistributionOnDemandLookupResult-awardTotal-parentBean" abstract="true" parent="GenericAttributes-genericAmount">
		<property name="name" value="awardTotal" />
		<property name="label" value="Award Total Amount" />
		<property name="shortLabel" value="Total" />
		<property name="formatterClass" value="org.kuali.rice.core.web.format.CurrencyFormatter" />
	</bean>

		<bean id="DunningLetterDistributionOnDemandLookupResult-agingBucket" parent="DunningLetterDistributionOnDemandLookupResult-agingBucket-parentBean" />

	<bean id="DunningLetterDistributionOnDemandLookupResult-agingBucket-parentBean" abstract="true"
		parent="AttributeDefinition">
		<property name="name" value="agingBucket" />
		<property name="label" value="Aging Bucket" />
		<property name="maxLength" value="40"/>
		<property name="shortLabel" value="agingBucket" />
		<property name="control">
			<bean parent="SelectControlDefinition"
				p:valuesFinderClass="org.kuali.kfs.module.ar.businessobject.options.DaysPastDueValuesFinder"
				p:includeKeyInLabel="false" />
		</property>
	</bean>

	<!-- Business Object Lookup Definition -->

	<bean id="DunningLetterDistributionOnDemandLookupResult-lookupDefinition" parent="DunningLetterDistributionOnDemandLookupResult-lookupDefinition-parentBean" />

	<bean id="DunningLetterDistributionOnDemandLookupResult-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
		<property name="title" value="Dunning Letter Distribution On Demand Lookup" />
		<property name="lookupableID" value="dunningLetterDistributionOnDemandLookupResultLookupable" />

		<property name="defaultSort">
			<bean parent="SortDefinition">
				<property name="attributeNames">
					<list>
						<value>agencyNumber</value>
					</list>
				</property>
			</bean>
		</property>

		<property name="lookupFields">
			<list>		
				<bean parent="FieldDefinition" p:required="false" p:attributeName="collector.principalName" />
				<bean parent="FieldDefinition" p:required="false" p:attributeName="principalId" />		
				<bean parent="FieldDefinition" p:required="false" p:attributeName="agencyNumber" />
				<bean parent="FieldDefinition" p:required="false" p:attributeName="accountNumber" />
				<bean parent="FieldDefinition" p:required="false" p:attributeName="customerNumber" />
				<bean parent="FieldDefinition" p:required="false" p:attributeName="proposalNumber" />
				<bean parent="FieldDefinition" p:required="false" p:attributeName="awardTotal" />
				<bean parent="FieldDefinition" p:required="false" p:attributeName="campaignID" />
				<bean parent="FieldDefinition" p:required="false" p:attributeName="agingBucket" />
				<bean parent="FieldDefinition" p:required="false" p:attributeName="invoiceDocumentNumber" />		
			</list>
		</property>
		<property name="resultFields">
			<list>
				<bean parent="FieldDefinition" p:required="false" p:attributeName="agencyNumber" />
				<bean parent="FieldDefinition" p:required="false" p:attributeName="customerNumber" />
				<bean parent="FieldDefinition" p:required="false" p:attributeName="proposalNumber" />
				<bean parent="FieldDefinition" p:required="false" p:attributeName="awardTotal" />
				<bean parent="FieldDefinition" p:required="false" p:attributeName="campaignID" />								
			</list>
		</property>
	</bean>
</beans>
