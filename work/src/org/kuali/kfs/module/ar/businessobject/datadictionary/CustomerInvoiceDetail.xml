<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "../dataDictionary-1_0.dtd">
<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.module.ar.bo.CustomerInvoiceDetail">
 
        <inquiry>
            <title>Invoice Detail Inquiry</title>
             
            <inquirySections>
                <inquirySection title="Header Text">
                    <inquiryFields>
                        <field attributeName="documentNumber" />
                        <field attributeName="chartOfAccountsCode" />
                        <field attributeName="accountNumber" />
                        <field attributeName="subAccountNumber" />
                        <field attributeName="financialObjectCode" />
                        <field attributeName="financialSubObjectCode" />
                        <field attributeName="projectCode" />
                        <field attributeName="organizationReferenceId" />
						<field attributeName="accountsReceivableObjectCode" />
                        <field attributeName="accountsReceivableSubObjectCode" />
						<field attributeName="financialDocumentOverrideCode" />
                        <field attributeName="financialDocumentPostingYear" />
                        <field attributeName="invoiceItemQuantity" />
                        <field attributeName="invoiceItemCode" />
                        <field attributeName="invoiceItemUnitOfMeasureCode" />
                        <field attributeName="invoiceItemDescription" />
                        <field attributeName="invoiceItemUnitPrice" />
                        <field attributeName="invoiceItemTotalAmount" />
                        <field attributeName="invoiceItemServiceDate" />
                        <field attributeName="invoiceItemNumber" />
                    </inquiryFields>
                </inquirySection>
            </inquirySections>  
        </inquiry>

        <lookup>
            <title>Invoice Detail Lookup</title>

            <instructions>Look up an Invoice Detail</instructions>

            <defaultSort sortAscending="true">
                <sortAttributes>
                    <sortAttribute attributeName="documentNumber" />
                    <sortAttribute attributeName="invoiceItemNumber" />
                </sortAttributes>
            </defaultSort>

            <lookupFields>
                <lookupField attributeName="accountNumber" required="false" />
                <lookupField attributeName="chartOfAccountsCode" required="false" />
                <lookupField attributeName="documentNumber" required="false" />
                <lookupField attributeName="financialDocumentOverrideCode" required="false" />
                <lookupField attributeName="financialDocumentPostingYear" required="false" />
                <lookupField attributeName="financialObjectCode" required="false" />
                <lookupField attributeName="financialSubObjectCode" required="false" />
				<lookupField attributeName="accountsReceivableObjectCode" required="false" />
                <lookupField attributeName="accountsReceivableSubObjectCode" required="false" />
                <lookupField attributeName="invoiceItemCode" required="false" />
                <lookupField attributeName="invoiceItemDescription" required="false" />
                <lookupField attributeName="invoiceItemNumber" required="false" />
                <lookupField attributeName="invoiceItemQuantity" required="false" />
                <lookupField attributeName="invoiceItemServiceDate" required="false" />
                <lookupField attributeName="invoiceItemTotalAmount" required="false" />
                <lookupField attributeName="invoiceItemUnitOfMeasureCode" required="false" />
                <lookupField attributeName="invoiceItemUnitPrice" required="false" />
                <lookupField attributeName="organizationReferenceId" required="false" />
                <lookupField attributeName="projectCode" required="false" />
                <lookupField attributeName="subAccountNumber" required="false" />
            </lookupFields>

            <resultFields>
                <field attributeName="accountNumber" />
                <field attributeName="accountsReceivableObjectCode" />
                <field attributeName="accountsReceivableSubObjectCode" />
                <field attributeName="chartOfAccountsCode" />
                <field attributeName="documentNumber" />
                <field attributeName="financialDocumentOverrideCode" />
                <field attributeName="financialDocumentPostingYear" />
                <field attributeName="financialObjectCode" />
                <field attributeName="financialSubObjectCode" />
                <field attributeName="invoiceItemCode" />
                <field attributeName="invoiceItemDescription" />
                <field attributeName="invoiceItemNumber" />
                <field attributeName="invoiceItemQuantity" />
                <field attributeName="invoiceItemServiceDate" />
                <field attributeName="invoiceItemTotalAmount" />
                <field attributeName="invoiceItemUnitOfMeasureCode" />
                <field attributeName="invoiceItemUnitPrice" />
                <field attributeName="organizationReferenceId" />
                <field attributeName="projectCode" />
                <field attributeName="subAccountNumber" />
            </resultFields>
        </lookup>

        <attributes>
            <attributeReference name="chartOfAccountsCode" sourceClassName="org.kuali.module.chart.bo.Chart" sourceAttributeName="chartOfAccountsCode"/>
            <attributeReference name="accountNumber" sourceClassName="org.kuali.module.chart.bo.Account" sourceAttributeName="accountNumber" />
            <attributeReference name="subAccountNumber" sourceClassName="org.kuali.module.chart.bo.SubAccount" sourceAttributeName="subAccountNumber">
            	<required>false</required>
            </attributeReference>
            <attributeReference name="financialObjectCode" sourceClassName="org.kuali.module.chart.bo.ObjectCode" sourceAttributeName="financialObjectCode" />
            <attributeReference name="financialSubObjectCode" sourceClassName="org.kuali.module.chart.bo.SubObjCd" sourceAttributeName="financialSubObjectCode">
            	<required>false</required>
            </attributeReference>
            <attributeReference name="projectCode" sourceClassName="org.kuali.module.chart.bo.ProjectCode" sourceAttributeName="code">
            	<required>false</required>
            </attributeReference>
            
            <attribute name="organizationReferenceId" forceUppercase="true">
                <label>Organization Reference Id</label>
                <shortLabel>Org Ref Id</shortLabel>

                <maxLength>8</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="10" />
                </control>

                <summary>organizationReferenceId</summary>
                <description>organizationReferenceId description...</description>
			</attribute>            

            <attributeReference name="accountsReceivableObjectCode" sourceClassName="org.kuali.module.chart.bo.ObjectCode" sourceAttributeName="financialObjectCode">
                <label>Accounts Receivable Object Code</label>
                <shortLabel>AR Object Code</shortLabel>
                <required>true</required>
                <summary>accountsReceivableObjectCode</summary>
                <description>accountsReceivableObjectCode description...</description>
			</attributeReference>

			<attributeReference name="accountsReceivableSubObjectCode" sourceClassName="org.kuali.module.chart.bo.SubObjCd" sourceAttributeName="financialSubObjectCode">
                <label>Accounts Receivable Sub Object Code</label>
                <shortLabel>AR Sub-Object Code</shortLabel>
                <required>false</required>
                <summary>accountsReceivableSubObjectCode</summary>
                <description>accountsReceivableSubObjectCode description...</description>
			</attributeReference>

            <attribute name="documentNumber" forceUppercase="true">
                <label>Document Number</label>
                <shortLabel>Document Number</shortLabel>

                <maxLength>14</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="16" />
                </control>

                <summary>documentNumber</summary>
                <description>documentNumber description...</description>
			</attribute>

            <attribute name="financialDocumentOverrideCode" forceUppercase="true">
                <label>Financial Document Override Code</label>
                <shortLabel>Override Code</shortLabel>

                <maxLength>1</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="3" />
                </control>

                <summary>financialDocumentOverrideCode</summary>
                <description>financialDocumentOverrideCode description...</description>
			</attribute>

            <attribute name="financialDocumentPostingYear" forceUppercase="true">
                <label>Financial Document Posting Year</label>
                <shortLabel>Posting Year</shortLabel>

                <maxLength>4</maxLength>
                <validationPattern><numeric /></validationPattern>

                <control>
                    <text size="6" />
                </control>

                <summary>financialDocumentPostingYear</summary>
                <description>financialDocumentPostingYear description...</description>
			</attribute>


            <attribute name="invoiceItemCode" forceUppercase="true">
                <label>Invoice Item Code</label>
                <shortLabel>Item Code</shortLabel>

                <maxLength>6</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="8" />
                </control>

                <summary>invoiceItemCode</summary>
                <description>invoiceItemCode description...</description>
			</attribute>

            <attribute name="invoiceItemDescription" forceUppercase="true">
                <label>Invoice Item Description</label>
                <shortLabel>Description</shortLabel>

                <maxLength>800</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="20" />
                </control>

                <summary>invoiceItemDescription</summary>
                <description>invoiceItemDescription description...</description>
			</attribute>

            <attribute name="invoiceItemNumber" forceUppercase="true">
                <label>Invoice Item Number</label>
                <shortLabel>Item Number</shortLabel>

                <maxLength>7</maxLength>
                <validationPattern><numeric /></validationPattern>

                <control>
                    <text size="9" />
                </control>

                <summary>invoiceItemNumber</summary>
                <description>invoiceItemNumber description...</description>
			</attribute>

            <attribute name="invoiceItemQuantity" forceUppercase="true">
                <label>Invoice Item Quantity</label>
                <shortLabel>Quantity</shortLabel>

                <maxLength>9</maxLength>
                <validationPattern><floatingPoint /></validationPattern>
                <required>true</required>

                <control>
                    <text size="11" />
                </control>

                <summary>invoiceItemQuantity</summary>
                <description>invoiceItemQuantity description...</description>
			</attribute>
			
			<attributeReference name="invoiceItemServiceDate"
                sourceClassName="org.kuali.core.bo.AttributeReferenceDummy"
                sourceAttributeName="genericDate" >
                <label>Invoice Item Service Date</label>
                <shortLabel>Service Date</shortLabel>

                <required>true</required>

                <summary><![CDATA[&nbsp;]]> </summary>
                <description>The date that this invoice was billed</description>
            </attributeReference>			


            <attribute name="invoiceItemTotalAmount" forceUppercase="true">
                <label>Invoice Item Total Amount</label>
                <shortLabel>Total Amount</shortLabel>

                <maxLength>19</maxLength>
                <validationPattern><floatingPoint /></validationPattern>

                <control>
                    <text size="21" />
                </control>

                <summary>invoiceItemTotalAmount</summary>
                <description>invoiceItemTotalAmount description...</description>
			</attribute>

            <attribute name="invoiceItemUnitOfMeasureCode" forceUppercase="true">
                <label>Invoice Item Unit Of Measure Code</label>
                <shortLabel>UOM</shortLabel>

                <maxLength>2</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="4" />
                </control>

                <summary>invoiceItemUnitOfMeasureCode</summary>
                <description>invoiceItemUnitOfMeasureCode description...</description>
			</attribute>

            <attribute name="invoiceItemUnitPrice" forceUppercase="true">
                <label>Invoice Item Unit Price</label>
                <shortLabel>Unit Price</shortLabel>

                <maxLength>19</maxLength>
                <validationPattern><floatingPoint /></validationPattern>
                <required>true</required>

                <control>
                    <text size="21" />
                </control>

                <summary>invoiceItemUnitPrice</summary>
                <description>invoiceItemUnitPrice description...</description>
			</attribute>

            <attributeReference name="versionNumber" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="versionNumber" />

        </attributes>
    </businessObject>
</dictionaryEntry>
