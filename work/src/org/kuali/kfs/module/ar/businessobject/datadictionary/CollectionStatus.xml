<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "../dataDictionary-1_0.dtd">
<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.module.ar.bo.CollectionStatus">
 
        <inquiry>
            <title>Collection Status Inquiry</title>
             
            <inquirySections>
                <inquirySection title="Header Text">
                    <inquiryFields>
                        <field attributeName="active" />
                        <field attributeName="customerCollectionStatusCode" />
                        <field attributeName="customerCollectionStatusName" />
                    </inquiryFields>
                </inquirySection>
            </inquirySections>  
        </inquiry>

        <lookup>
            <title>Collection Status Lookup</title>

            <instructions>Look up a Collection Status</instructions>

            <defaultSort sortAscending="true">
                <sortAttributes>
                    <sortAttribute attributeName="customerCollectionStatusCode" />
                </sortAttributes>
            </defaultSort>

            <lookupFields>
                <lookupField attributeName="active" required="false" />
                <lookupField attributeName="customerCollectionStatusCode" required="false" />
                <lookupField attributeName="customerCollectionStatusName" required="false" />
            </lookupFields>

            <resultFields>
                <field attributeName="active" />
                <field attributeName="customerCollectionStatusCode" />
                <field attributeName="customerCollectionStatusName" />
            </resultFields>
        </lookup>

        <attributes>

            <attribute name="active" forceUppercase="true">
                <label>Active</label>
                <shortLabel>Active</shortLabel>

                <maxLength>1</maxLength>

                <control>
                    <checkbox />
                </control>

                <summary>active</summary>
                <description>active description...</description>
			</attribute>

            <attribute name="customerCollectionStatusCode" forceUppercase="true">
                <label>Customer Collection Status Code</label>
                <shortLabel>Code</shortLabel>

                <maxLength>3</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="5" />
                </control>

                <summary>customerCollectionStatusCode</summary>
                <description>customerCollectionStatusCode description...</description>
			</attribute>

            <attribute name="customerCollectionStatusName" forceUppercase="true">
                <label>Customer Collection Status Name</label>
                <shortLabel>Name</shortLabel>

                <maxLength>40</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="42" />
                </control>

                <summary>customerCollectionStatusName</summary>
                <description>customerCollectionStatusName description...</description>
			</attribute>

            <attributeReference name="versionNumber" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="versionNumber" />

        </attributes>
    </businessObject>
</dictionaryEntry>
