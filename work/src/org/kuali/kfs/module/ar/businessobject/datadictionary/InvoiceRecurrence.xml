<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="InvoiceRecurrence" parent="InvoiceRecurrence-parentBean" />

  <bean id="InvoiceRecurrence-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.kfs.module.ar.businessobject.InvoiceRecurrence" />
    <property name="inquiryDefinition" >
      <ref bean="InvoiceRecurrence-inquiryDefinition" />
    </property>
    <property name="lookupDefinition" >
      <ref bean="InvoiceRecurrence-lookupDefinition" />
    </property>
    <property name="titleAttribute" value="invoiceNumber" />
    <property name="objectLabel" value="Invoice Recurrence" />
    <property name="attributes" >
      <list>
        <ref bean="InvoiceRecurrence-invoiceNumber" />
        <ref bean="InvoiceRecurrence-customerNumber" />
        <ref bean="InvoiceRecurrence-customer.customerName" />
        <ref bean="InvoiceRecurrence-documentRecurrenceBeginDate" />
        <ref bean="InvoiceRecurrence-documentRecurrenceEndDate" />
        <ref bean="InvoiceRecurrence-documentTotalRecurrenceNumber" />
        <ref bean="InvoiceRecurrence-documentRecurrenceIntervalCode" />
        <ref bean="InvoiceRecurrence-workgroupIdentifier" />
        <ref bean="InvoiceRecurrence-documentInitiatorUserIdentifier" />
        <ref bean="InvoiceRecurrence-documentInitiatorUserPersonUserIdentifier" />
        <ref bean="InvoiceRecurrence-documentInitiatorUser.personUserIdentifier" />
        <ref bean="InvoiceRecurrence-documentInitiatorUser.personName" />

        <ref bean="InvoiceRecurrence-documentLastCreateDate" />
        <ref bean="InvoiceRecurrence-active" />
        <ref bean="InvoiceRecurrence-versionNumber" />
      </list>
    </property>

    <property name="relationships" >
      <list>
        <bean parent="RelationshipDefinition">
          <property name="objectAttributeName" value="accountsReceivableDocumentHeader" />
          <property name="primitiveAttributes" >
            <list>
              <bean parent="PrimitiveAttributeDefinition"
                    p:sourceName="invoiceNumber"
                    p:targetName="documentNumber" />
            </list>
          </property>
          <property name="supportAttributes" >
            <list>
              <bean parent="SupportAttributeDefinition"
                    p:sourceName="customerNumber"
                    p:targetName="customerNumber" />
              <bean parent="SupportAttributeDefinition"
                    p:sourceName="customer.customerName"
                    p:targetName="customer.customerName" />
            </list>
          </property>
        </bean>
<!--
        <bean parent="RelationshipDefinition">
          <property name="objectAttributeName" value="customer" />
          <property name="primitiveAttributes" >
            <list>
              <bean parent="PrimitiveAttributeDefinition"
                    p:sourceName="customerNumber"
                    p:targetName="customerNumber" />
            </list>
          </property>
          <property name="supportAttributes" >
            <list>
              <bean parent="SupportAttributeDefinition"
                    p:sourceName="customer.customerName"
                    p:targetName="customerName" />
            </list>
          </property>
        </bean>
-->
        <bean parent="RelationshipDefinition">
          <property name="objectAttributeName" value="documentInitiatorUser" />
          <property name="primitiveAttributes" >
            <list>
              <bean parent="PrimitiveAttributeDefinition"
                    p:sourceName="documentInitiatorUserIdentifier"
                    p:targetName="personUniversalIdentifier" />
            </list>
          </property>
          <property name="supportAttributes" >
            <list>
              <bean parent="SupportAttributeDefinition"
                    p:identifier="true"
                    p:sourceName="documentInitiatorUser.personUserIdentifier"
                    p:targetName="personUserIdentifier" />
              <bean parent="SupportAttributeDefinition"
                    p:sourceName="documentInitiatorUser.personName"
                    p:targetName="personName" />
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->


  <bean id="InvoiceRecurrence-invoiceNumber" parent="InvoiceRecurrence-invoiceNumber-parentBean" />

  <bean id="InvoiceRecurrence-invoiceNumber-parentBean" abstract="true" parent="DocumentHeader-documentNumber">
    <property name="name" value="invoiceNumber" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Invoice Number" />
    <property name="shortLabel" value="InvNum" />
  </bean>
<!--
  <bean id="InvoiceRecurrence-referenceFinancialDocumentNumber" parent="InvoiceRecurrence-referenceFinancialDocumentNumber-parentBean" />

  <bean id="InvoiceRecurrence-referenceFinancialDocumentNumber-parentBean" abstract="true" parent="DocumentHeader-documentNumber">
    <property name="name" value="referenceFinancialDocumentNumber" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Reference Invoice Number" />
    <property name="shortLabel" value="RefInvNbr" />
    <property name="required" value="true" />
  </bean>
-->
  <bean id="InvoiceRecurrence-customerNumber" parent="InvoiceRecurrence-customerNumber-parentBean" />

  <bean id="InvoiceRecurrence-customerNumber-parentBean" abstract="true" parent="Customer-customerNumber">
    <property name="name" value="customerNumber" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Customer Number" />
    <property name="shortLabel" value="CustNum" />
    <property name="required" value="false" />
    <property name="maxLength" value="9" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="11" />
    </property>
    <property name="summary" value="customerNumber" />
    <property name="description" value="customerNumber description..." />
  </bean>
  <bean id="InvoiceRecurrence-customer.customerName" parent="InvoiceRecurrence-customer.customerName-parentBean" />

  <bean id="InvoiceRecurrence-customer.customerName-parentBean" abstract="true" parent="Customer-customerName">
    <property name="name" value="customer.customerName" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Customer Name" />
    <property name="shortLabel" value="Name" />
    <property name="required" value="false" />
    <property name="maxLength" value="60" />
	<property name="validationPattern">
	  <bean parent="AlphaValidationPattern"
		    p:allowWhitespace="true" />
	</property>
	<property name="control">
	  <bean parent="TextControlDefinition" 
	        p:size="62" />
	</property>
    <property name="summary" value="customer.customerName" />
    <property name="description" value="customer Name..." />
  </bean>
  <bean id="InvoiceRecurrence-documentRecurrenceBeginDate" parent="InvoiceRecurrence-documentRecurrenceBeginDate-parentBean" />

  <bean id="InvoiceRecurrence-documentRecurrenceBeginDate-parentBean" abstract="true" parent="GenericAttributes-genericDate">
    <property name="name" value="documentRecurrenceBeginDate" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Recurrence Begin Date" />
    <property name="shortLabel" value="BegDate" />
    <property name="required" value="true" />
    <property name="maxLength" value="10" />
    <property name="summary" value="documentRecurrenceBeginDate" />
    <property name="description" value="The date when the document recurrence begins" />
  </bean>
  <bean id="InvoiceRecurrence-documentRecurrenceEndDate" parent="InvoiceRecurrence-documentRecurrenceEndDate-parentBean" />

  <bean id="InvoiceRecurrence-documentRecurrenceEndDate-parentBean" abstract="true" parent="GenericAttributes-genericDate">
    <property name="name" value="documentRecurrenceEndDate" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Recurrence End Date" />
    <property name="shortLabel" value="EndDate" />
    <property name="maxLength" value="10" />
    <property name="summary" value="documentRecurrenceEndDate" />
    <property name="description" value="The date when the document recurrence ends" />
  </bean>
  <bean id="InvoiceRecurrence-documentTotalRecurrenceNumber" parent="InvoiceRecurrence-documentTotalRecurrenceNumber-parentBean" />

  <bean id="InvoiceRecurrence-documentTotalRecurrenceNumber-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="documentTotalRecurrenceNumber" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Total Number of Recurrences" />
    <property name="shortLabel" value="TotRecur" />
    <property name="maxLength" value="3" />
    <property name="validationPattern" >
      <bean parent="NumericValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="5" />
    </property>
    <property name="summary" value="documentTotalRecurrenceNumber" />
    <property name="description" value="Total number of recurrences a document will create" />
  </bean>
  <bean id="InvoiceRecurrence-documentRecurrenceIntervalCode" parent="InvoiceRecurrence-documentRecurrenceIntervalCode-parentBean" />

  <bean id="InvoiceRecurrence-documentRecurrenceIntervalCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="documentRecurrenceIntervalCode" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Recurrence Interval Code" />
    <property name="shortLabel" value="IntCd" />
    <property name="maxLength" value="2" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="SelectControlDefinition"
            p:valuesFinderClass="org.kuali.kfs.module.ar.businessobject.options.InvoiceRecurrenceIntervalCodeValuesFinder"
            p:includeKeyInLabel="true" />
    </property>
    <property name="summary" value="documentRecurrenceIntervalCode" />
    <property name="description" value="The document recurrence interval code defines the frequency of the recurrence." />
  </bean>
  <bean id="InvoiceRecurrence-workgroupIdentifier" parent="InvoiceRecurrence-workgroupIdentifier-parentBean" />

  <bean id="InvoiceRecurrence-workgroupIdentifier-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="workgroupIdentifier" />
    <property name="forceUppercase" value="false" />
    <property name="label" value="Workgroup Identifier" />
    <property name="shortLabel" value="Wrkgrp" />
    <property name="maxLength" value="70" />
    <property name="validationPattern" >
      <bean parent="AlphaNumericValidationPattern"
            p:allowUnderscore="true" />
    </property>
    <property name="required" value="true" />
    <property name="control" >
      <bean parent="WorkflowWorkgroupControlDefinition" />
    </property>
    <property name="summary" value="workgroupIdentifier" />
    <property name="description" value="The document recurrences will route to this workgroup" />
  </bean>
  <bean id="InvoiceRecurrence-documentInitiatorUserIdentifier" parent="InvoiceRecurrence-documentInitiatorUserIdentifier-parentBean" />

  <bean id="InvoiceRecurrence-documentInitiatorUserIdentifier-parentBean" abstract="true" parent="UniversalUser-personUniversalIdentifier">
    <property name="name" value="documentInitiatorUserIdentifier" />
    <property name="label" value="Invoice Initiator Unvl ID" />
    <property name="shortLabel" value="InvInitID" />
    <property name="required" value="true" />
    <property name="control" >
      <bean parent="HiddenControlDefinition" />
    </property>
    <property name="displayLabelAttribute" value="documentInitiatorUser.personUserIdentifier" />
    <property name="summary" value="documentInitiatorUserIdentifier" />
    <property name="description" value="The document recurrences will be initiated by this userid." />
  </bean>
  <bean id="InvoiceRecurrence-documentInitiatorUserPersonUserIdentifier" parent="InvoiceRecurrence-documentInitiatorUserPersonUserIdentifier-parentBean" />

  <bean id="InvoiceRecurrence-documentInitiatorUserPersonUserIdentifier-parentBean" abstract="true" parent="UniversalUser-personUserIdentifier">
    <property name="name" value="documentInitiatorUserPersonUserIdentifier" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Invoice Initiator Network ID" />
    <property name="shortLabel" value="InvInitID" />
    <property name="required" value="false" />
    <property name="control" >
      <bean parent="HiddenControlDefinition" />
    </property>
  </bean>
  <bean id="InvoiceRecurrence-documentInitiatorUser.personUserIdentifier" parent="InvoiceRecurrence-documentInitiatorUser.personUserIdentifier-parentBean" />

  <bean id="InvoiceRecurrence-documentInitiatorUser.personUserIdentifier-parentBean" abstract="true" parent="UniversalUser-personUserIdentifier">
    <property name="name" value="documentInitiatorUser.personUserIdentifier" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Invoice Initiator User ID" />
    <property name="shortLabel" value="InvInitID" />
    <property name="required" value="true" />
    <property name="control" >
      <bean parent="KualiUserControlDefinition"
            p:personNameAttributeName="documentInitiatorUser.personName"
            p:universalIdAttributeName="documentInitiatorUserIdentifier"
            p:userIdAttributeName="documentInitiatorUser.personUserIdentifier" />
    </property>
  </bean>
  <bean id="InvoiceRecurrence-documentInitiatorUser.personName" parent="InvoiceRecurrence-documentInitiatorUser.personName-parentBean" />

  <bean id="InvoiceRecurrence-documentInitiatorUser.personName-parentBean" abstract="true" parent="UniversalUser-personName">
    <property name="name" value="documentInitiatorUser.personName" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Invoice Initiator Name" />
    <property name="shortLabel" value="InvInitNm" />
    <property name="required" value="false" />
  </bean>
  <bean id="InvoiceRecurrence-documentLastCreateDate" parent="InvoiceRecurrence-documentLastCreateDate-parentBean" />

  <bean id="InvoiceRecurrence-documentLastCreateDate-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="documentLastCreateDate" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Last Create Date" />
    <property name="shortLabel" value="LstCrDt" />
    <property name="maxLength" value="7" />
    <property name="validationPattern" >
      <bean parent="DateValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="9" />
    </property>
    <property name="summary" value="documentLastCreateDate" />
    <property name="description" value="The date when the last document recurrence was created" />
  </bean>
  <bean id="InvoiceRecurrence-active" parent="InvoiceRecurrence-active-parentBean" />

  <bean id="InvoiceRecurrence-active-parentBean" abstract="true" parent="GenericAttributes-activeIndicator">
    <property name="name" value="active" />
  </bean>
  <bean id="InvoiceRecurrence-versionNumber" parent="InvoiceRecurrence-versionNumber-parentBean" />

  <bean id="InvoiceRecurrence-versionNumber-parentBean" abstract="true" parent="GenericAttributes-versionNumber">
  </bean>

<!-- Business Object Inquiry Definition -->


  <bean id="InvoiceRecurrence-inquiryDefinition" parent="InvoiceRecurrence-inquiryDefinition-parentBean" />

  <bean id="InvoiceRecurrence-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
    <property name="title" value="InvoiceRecurrence Inquiry" />
    <property name="inquirySections" >
      <list>
        <bean parent="InquirySectionDefinition">
          <property name="title" value="" />
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields" >
            <list>
              <bean parent="FieldDefinition"
                    p:attributeName="invoiceNumber" />
              <bean parent="FieldDefinition"
                    p:attributeName="customerNumber" />
              <bean parent="FieldDefinition"
                    p:attributeName="documentRecurrenceBeginDate" />
              <bean parent="FieldDefinition"
                    p:attributeName="documentRecurrenceEndDate" />
              <bean parent="FieldDefinition"
                    p:attributeName="documentTotalRecurrenceNumber" />
              <bean parent="FieldDefinition"
                    p:attributeName="documentRecurrenceIntervalCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="workgroupIdentifier" />
<!--
              <bean parent="FieldDefinition"
                    p:attributeName="documentInitiatorUserIdentifier" />
-->
              <bean parent="FieldDefinition"
                    p:attributeName="documentInitiatorUser.personUserIdentifier" />
              <bean parent="FieldDefinition"
                    p:attributeName="documentInitiatorUser.personName" />
              <bean parent="FieldDefinition"
                    p:attributeName="documentLastCreateDate" />
              <bean parent="FieldDefinition"
                    p:attributeName="active" />
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Business Object Lookup Definition -->


  <bean id="InvoiceRecurrence-lookupDefinition" parent="InvoiceRecurrence-lookupDefinition-parentBean" />

  <bean id="InvoiceRecurrence-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
    <property name="title" value="InvoiceRecurrence Lookup" />
    <property name="instructions" value="Look up a InvoiceRecurrence" />
    <property name="defaultSort" >
      <bean parent="SortDefinition">
      </bean>
    </property>
    <property name="lookupFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="invoiceNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="customerNumber" />
<!-- 
        <bean parent="FieldDefinition"
              p:attributeName="customer.customerName" />
-->
        <bean parent="FieldDefinition"
              p:attributeName="documentRecurrenceBeginDate" />
        <bean parent="FieldDefinition"
              p:attributeName="documentRecurrenceEndDate" />
        <bean parent="FieldDefinition"
              p:attributeName="documentTotalRecurrenceNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="documentRecurrenceIntervalCode" />
        <bean parent="FieldDefinition"
              p:attributeName="workgroupIdentifier" />
<!--
        <bean parent="FieldDefinition"
              p:attributeName="documentInitiatorUserIdentifier" />
-->
        <bean parent="FieldDefinition"
              p:attributeName="documentInitiatorUser.personUserIdentifier" />
        <bean parent="FieldDefinition"
              p:attributeName="documentInitiatorUser.personName" />
        <bean parent="FieldDefinition"
              p:attributeName="documentLastCreateDate" />
        <bean parent="FieldDefinition"
              p:attributeName="active" />
      </list>
    </property>
    <property name="resultFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="invoiceNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="customerNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="documentRecurrenceBeginDate" />
        <bean parent="FieldDefinition"
              p:attributeName="documentRecurrenceEndDate" />
        <bean parent="FieldDefinition"
              p:attributeName="documentTotalRecurrenceNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="documentRecurrenceIntervalCode" />
        <bean parent="FieldDefinition"
              p:attributeName="workgroupIdentifier" />
<!--
        <bean parent="FieldDefinition"
              p:attributeName="documentInitiatorUserIdentifier" />
-->
        <bean parent="FieldDefinition"
              p:attributeName="documentInitiatorUser.personUserIdentifier" />
        <bean parent="FieldDefinition"
              p:attributeName="documentInitiatorUser.personName" />
        <bean parent="FieldDefinition"
              p:attributeName="documentLastCreateDate" />
        <bean parent="FieldDefinition"
              p:attributeName="active" />
      </list>
    </property>
  </bean>
</beans>
