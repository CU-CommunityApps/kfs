<!-- Business Object Inquiry Definition -->


  <bean id="CustomerInvoiceItemCode-inquiryDefinition" parent="CustomerInvoiceItemCode-inquiryDefinition-parentBean" />

  <bean id="CustomerInvoiceItemCode-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
    <property name="title" value="Customer Invoice Item Code Inquiry" />
    <property name="inquirySections" >
      <list>
		<bean parent="InquirySectionDefinition">
          <property name="title" value="Invoice Item Code" />
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields" >
            <list>
			  <bean parent="FieldDefinition"
                    p:attributeName="chartOfAccountsCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="organizationCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="invoiceItemCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="invoiceItemDescription" />
              <bean parent="FieldDefinition"
                    p:attributeName="relatedStockNumber" />
              <bean parent="FieldDefinition"
                    p:attributeName="itemDefaultPrice" />
              <bean parent="FieldDefinition"
                    p:attributeName="itemDefaultQuantity" />
              <bean parent="FieldDefinition"
                    p:attributeName="defaultUnitOfMeasureCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="active" />  
              <bean parent="FieldDefinition"
                    p:attributeName="taxableIndicator" />                                           
            </list>
          </property>
        </bean>   
		<bean parent="InquirySectionDefinition">
          <property name="title" value="Item Code Default Account" />
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields" >
            <list>
              <bean parent="FieldDefinition"
                    p:attributeName="defaultInvoiceChartOfAccountsCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="defaultInvoiceAccountNumber" />
              <bean parent="FieldDefinition"
                    p:attributeName="defaultInvoiceSubAccountNumber" />                    
              <bean parent="FieldDefinition"
                    p:attributeName="defaultInvoiceFinancialObjectCode" />                    
              <bean parent="FieldDefinition"
                    p:attributeName="defaultInvoiceFinancialSubObjectCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="defaultInvoiceOrganizationReferenceIdentifier" />
              <bean parent="FieldDefinition"
                    p:attributeName="defaultInvoiceProjectCode" />            
            </list>
          </property>
        </bean>     
      </list>
    </property>
  </bean>

