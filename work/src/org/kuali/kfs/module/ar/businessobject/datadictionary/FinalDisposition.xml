  <bean id="FinalDisposition" parent="FinalDisposition-parentBean"/>

  <bean id="FinalDisposition-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.kfs.module.ar.businessobject.FinalDisposition"/>
    <property name="inquiryDefinition">
      <ref bean="FinalDisposition-inquiryDefinition"/>
    </property>
    <property name="lookupDefinition">
      <ref bean="FinalDisposition-lookupDefinition"/>
    </property>
    <property name="titleAttribute" value="dispositionCode"/>
    <property name="objectLabel" value="Final Disposition"/>
    <property name="attributes">
      <list>
        <ref bean="FinalDisposition-dispositionCode"/>
        <ref bean="FinalDisposition-dispositionDescription"/>
        <ref bean="FinalDisposition-active"/>
      </list>
    </property>
  </bean>

  <!-- Attribute Definitions -->
  
  <!--  Code -->
 	
  <bean id="FinalDisposition-dispositionCode" parent="FinalDisposition-dispositionCode-parentBean"/>

  <bean id="FinalDisposition-dispositionCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="dispositionCode"/>
    <property name="forceUppercase" value="true"/>
    <property name="label" value="Final Disposition Code"/>
    <property name="shortLabel" value="Code"/>
    <property name="maxLength" value="4"/>
    <property name="validationPattern">
      <ref bean="AnyCharacterValidation" />
    </property>
    <property name="required" value="true"/>
    <property name="control">
      <ref bean="FourCharacterTextControl" />
    </property>
  </bean>

  <!--  Description field -->
  <bean id="FinalDisposition-dispositionDescription" parent="FinalDisposition-dispositionDescription-parentBean"/>

  <bean id="FinalDisposition-dispositionDescription-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="dispositionDescription"/>
    <property name="label" value="Description"/>
    <property name="shortLabel" value="Type Desc"/>
    <property name="maxLength" value="25"/>
    <property name="validationPattern">
      <ref bean="AnyCharacterWithWhitespaceValidation" />
    </property>
    <property name="required" value="true"/>
    <property name="control">
      <bean parent="TextControlDefinition" p:size="32"/>
    </property>
  </bean>
  
  <!--  Active indicator field -->
  <bean id="FinalDisposition-active" parent="FinalDisposition-active-parentBean"/>

  <bean id="FinalDisposition-active-parentBean" abstract="true" parent="GenericAttributes-activeIndicator">
    <property name="name" value="active"/>
  </bean>
  
  <!-- Business Object Inquiry Definition -->

  <bean id="FinalDisposition-inquiryDefinition" parent="FinalDisposition-inquiryDefinition-parentBean"/>

  <bean id="FinalDisposition-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
    <property name="title" value="Final Disposition Inquiry"/>
    <property name="inquirySections">
      <list>
        <bean parent="InquirySectionDefinition">
          <property name="title" value="Final Disposition"/>
          <property name="numberOfColumns" value="1"/>
          <property name="inquiryFields">
            <list>
              <bean parent="FieldDefinition" p:attributeName="dispositionCode"/>
              <bean parent="FieldDefinition" p:attributeName="dispositionDescription"/>
              <bean parent="FieldDefinition" p:attributeName="active"/>
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

	<!-- Business Object Lookup Definition -->

  <bean id="FinalDisposition-lookupDefinition" parent="FinalDisposition-lookupDefinition-parentBean"/>

  <bean id="FinalDisposition-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
    <property name="title" value="Final Disposition Lookup"/>
    
    <property name="defaultSort">
      <bean parent="SortDefinition">
      </bean>
    </property>
    <property name="lookupFields">
      <list>
        <bean parent="FieldDefinition" p:attributeName="dispositionCode"/>
        <bean parent="FieldDefinition" p:attributeName="dispositionDescription"/>
        <bean parent="FieldDefinition" p:attributeName="active" p:defaultValue="Y"/>
      </list>
    </property>
    <property name="resultFields">
      <list>
        <bean parent="FieldDefinition" p:attributeName="dispositionCode"/>
        <bean parent="FieldDefinition" p:attributeName="dispositionDescription"/>
        <bean parent="FieldDefinition" p:attributeName="active"/>
      </list>
    </property>
  </bean>
</beans>
  