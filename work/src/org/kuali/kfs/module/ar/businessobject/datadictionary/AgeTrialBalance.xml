<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="AgeTrialBalance" parent="AgeTrialBalance-parentBean" />

  <bean id="AgeTrialBalance-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.kfs.module.ar.businessobject.AgeTrialBalance" />
    <property name="inquiryDefinition" >
      <ref bean="AgeTrialBalance-inquiryDefinition" />
    </property>
    <property name="lookupDefinition" >
      <ref bean="AgeTrialBalance-lookupDefinition" />
    </property>
    <property name="objectLabel" value="Age Trial Balance" />
    <property name="attributes" >
      <list>
        <ref bean="AgeTrialBalance-currentInvoiceAmount" />
        <ref bean="AgeTrialBalance-customerName" />
        <ref bean="AgeTrialBalance-customerNumber" />
        <ref bean="AgeTrialBalance-over30InvoiceAmount" />
        <ref bean="AgeTrialBalance-over60InvoiceAmount" />
        <ref bean="AgeTrialBalance-over90InvoiceAmount" />
        <ref bean="AgeTrialBalance-personUniversalIdentifier" />
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->


  <bean id="AgeTrialBalance-currentInvoiceAmount" parent="AgeTrialBalance-currentInvoiceAmount-parentBean" />

  <bean id="AgeTrialBalance-currentInvoiceAmount-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="currentInvoiceAmount" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Current Invoice Amount" />
    <property name="shortLabel" value="Amount" />
    <property name="maxLength" value="19" />
    <property name="validationPattern" >
      <bean parent="FloatingPointValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="21" />
    </property>
    <property name="summary" value="currentInvoiceAmount" />
    <property name="description" value="currentInvoiceAmount description..." />
  </bean>
  <bean id="AgeTrialBalance-customerName" parent="AgeTrialBalance-customerName-parentBean" />

  <bean id="AgeTrialBalance-customerName-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="customerName" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Customer Name" />
    <property name="shortLabel" value="Name" />
    <property name="maxLength" value="60" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="62" />
    </property>
    <property name="summary" value="customerName" />
    <property name="description" value="customerName description..." />
  </bean>
  <bean id="AgeTrialBalance-customerNumber" parent="AgeTrialBalance-customerNumber-parentBean" />

  <bean id="AgeTrialBalance-customerNumber-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="customerNumber" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Customer Number" />
    <property name="shortLabel" value="Number" />
    <property name="maxLength" value="9" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="11" />
    </property>
    <property name="summary" value="customerNumber" />
    <property name="description" value="customerNumber description..." />
  </bean>
  <bean id="AgeTrialBalance-over30InvoiceAmount" parent="AgeTrialBalance-over30InvoiceAmount-parentBean" />

  <bean id="AgeTrialBalance-over30InvoiceAmount-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="over30InvoiceAmount" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Over30 Invoice Amount" />
    <property name="shortLabel" value="Amount" />
    <property name="maxLength" value="19" />
    <property name="validationPattern" >
      <bean parent="FloatingPointValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="21" />
    </property>
    <property name="summary" value="over30InvoiceAmount" />
    <property name="description" value="over30InvoiceAmount description..." />
  </bean>
  <bean id="AgeTrialBalance-over60InvoiceAmount" parent="AgeTrialBalance-over60InvoiceAmount-parentBean" />

  <bean id="AgeTrialBalance-over60InvoiceAmount-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="over60InvoiceAmount" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Over60 Invoice Amount" />
    <property name="shortLabel" value="Amount" />
    <property name="maxLength" value="19" />
    <property name="validationPattern" >
      <bean parent="FloatingPointValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="21" />
    </property>
    <property name="summary" value="over60InvoiceAmount" />
    <property name="description" value="over60InvoiceAmount description..." />
  </bean>
  <bean id="AgeTrialBalance-over90InvoiceAmount" parent="AgeTrialBalance-over90InvoiceAmount-parentBean" />

  <bean id="AgeTrialBalance-over90InvoiceAmount-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="over90InvoiceAmount" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Over90 Invoice Amount" />
    <property name="shortLabel" value="Amount" />
    <property name="maxLength" value="19" />
    <property name="validationPattern" >
      <bean parent="FloatingPointValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="21" />
    </property>
    <property name="summary" value="over90InvoiceAmount" />
    <property name="description" value="over90InvoiceAmount description..." />
  </bean>
  <bean id="AgeTrialBalance-personUniversalIdentifier" parent="AgeTrialBalance-personUniversalIdentifier-parentBean" />

  <bean id="AgeTrialBalance-personUniversalIdentifier-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="personUniversalIdentifier" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Person Universal Identifier" />
    <property name="shortLabel" value="Identifier" />
    <property name="maxLength" value="10" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="12" />
    </property>
    <property name="summary" value="personUniversalIdentifier" />
    <property name="description" value="personUniversalIdentifier description..." />
  </bean>

<!-- Business Object Inquiry Definition -->


  <bean id="AgeTrialBalance-inquiryDefinition" parent="AgeTrialBalance-inquiryDefinition-parentBean" />

  <bean id="AgeTrialBalance-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
    <property name="title" value="Age Trial Balance Inquiry" />
    <property name="inquirySections" >
      <list>
        <bean parent="InquirySectionDefinition">
          <property name="title" value="" />
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields" >
            <list>
              <bean parent="FieldDefinition"
                    p:attributeName="currentInvoiceAmount" />
              <bean parent="FieldDefinition"
                    p:attributeName="customerName" />
              <bean parent="FieldDefinition"
                    p:attributeName="customerNumber" />
              <bean parent="FieldDefinition"
                    p:attributeName="over30InvoiceAmount" />
              <bean parent="FieldDefinition"
                    p:attributeName="over60InvoiceAmount" />
              <bean parent="FieldDefinition"
                    p:attributeName="over90InvoiceAmount" />
              <bean parent="FieldDefinition"
                    p:attributeName="personUniversalIdentifier" />
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Business Object Lookup Definition -->


  <bean id="AgeTrialBalance-lookupDefinition" parent="AgeTrialBalance-lookupDefinition-parentBean" />

  <bean id="AgeTrialBalance-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
    <property name="title" value="Age Trial Balance Lookup" />
    <property name="instructions" value="Look up an Age Trial Balance" />
    <property name="defaultSort" >
      <bean parent="SortDefinition">
      </bean>
    </property>
    <property name="lookupFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="currentInvoiceAmount" />
        <bean parent="FieldDefinition"
              p:attributeName="customerName" />
        <bean parent="FieldDefinition"
              p:attributeName="customerNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="over30InvoiceAmount" />
        <bean parent="FieldDefinition"
              p:attributeName="over60InvoiceAmount" />
        <bean parent="FieldDefinition"
              p:attributeName="over90InvoiceAmount" />
        <bean parent="FieldDefinition"
              p:attributeName="personUniversalIdentifier" />
      </list>
    </property>
    <property name="resultFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="currentInvoiceAmount" />
        <bean parent="FieldDefinition"
              p:attributeName="customerName" />
        <bean parent="FieldDefinition"
              p:attributeName="customerNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="over30InvoiceAmount" />
        <bean parent="FieldDefinition"
              p:attributeName="over60InvoiceAmount" />
        <bean parent="FieldDefinition"
              p:attributeName="over90InvoiceAmount" />
        <bean parent="FieldDefinition"
              p:attributeName="personUniversalIdentifier" />
      </list>
    </property>
  </bean>
</beans>
