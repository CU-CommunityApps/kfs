<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="NonAppliedDistributionMove" parent="NonAppliedDistributionMove-parentBean" />

  <bean id="NonAppliedDistributionMove-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.kfs.module.ar.businessobject.NonAppliedDistributionMove" />
    <property name="inquiryDefinition" >
      <ref bean="NonAppliedDistributionMove-inquiryDefinition" />
    </property>
    <property name="lookupDefinition" >
      <ref bean="NonAppliedDistributionMove-lookupDefinition" />
    </property>
    <property name="objectLabel" value="Non Applied Distribution Move" />
    <property name="attributes" >
      <list>
        <ref bean="NonAppliedDistributionMove-documentNumber" />
        <ref bean="NonAppliedDistributionMove-financialDocumentLineAmount" />
        <ref bean="NonAppliedDistributionMove-paidAppliedItemNumber" />
        <ref bean="NonAppliedDistributionMove-referenceFinancialDocumentNumber" />
        <ref bean="NonAppliedDistributionMove-universityFiscalPeriodCode" />
        <ref bean="NonAppliedDistributionMove-universityFiscalYear" />
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->


  <bean id="NonAppliedDistributionMove-documentNumber" parent="NonAppliedDistributionMove-documentNumber-parentBean" />

  <bean id="NonAppliedDistributionMove-documentNumber-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="documentNumber" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Document Number" />
    <property name="shortLabel" value="Number" />
    <property name="maxLength" value="14" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="16" />
    </property>
  </bean>
  <bean id="NonAppliedDistributionMove-financialDocumentLineAmount" parent="NonAppliedDistributionMove-financialDocumentLineAmount-parentBean" />

  <bean id="NonAppliedDistributionMove-financialDocumentLineAmount-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="financialDocumentLineAmount" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Financial Document Line Amount" />
    <property name="shortLabel" value="Amount" />
    <property name="maxLength" value="19" />
    <property name="validationPattern" >
      <bean parent="FloatingPointValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="21" />
    </property>
  </bean>
  <bean id="NonAppliedDistributionMove-paidAppliedItemNumber" parent="NonAppliedDistributionMove-paidAppliedItemNumber-parentBean" />

  <bean id="NonAppliedDistributionMove-paidAppliedItemNumber-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="paidAppliedItemNumber" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Paid Applied Item Number" />
    <property name="shortLabel" value="Number" />
    <property name="maxLength" value="7" />
    <property name="validationPattern" >
      <bean parent="NumericValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="9" />
    </property>
  </bean>
  <bean id="NonAppliedDistributionMove-referenceFinancialDocumentNumber" parent="NonAppliedDistributionMove-referenceFinancialDocumentNumber-parentBean" />

  <bean id="NonAppliedDistributionMove-referenceFinancialDocumentNumber-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="referenceFinancialDocumentNumber" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Reference Financial Document Number" />
    <property name="shortLabel" value="Number" />
    <property name="maxLength" value="14" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="16" />
    </property>
  </bean>
  <bean id="NonAppliedDistributionMove-universityFiscalPeriodCode" parent="NonAppliedDistributionMove-universityFiscalPeriodCode-parentBean" />

  <bean id="NonAppliedDistributionMove-universityFiscalPeriodCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="universityFiscalPeriodCode" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="University Fiscal Period Code" />
    <property name="shortLabel" value="Code" />
    <property name="maxLength" value="2" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="4" />
    </property>
  </bean>
  <bean id="NonAppliedDistributionMove-universityFiscalYear" parent="NonAppliedDistributionMove-universityFiscalYear-parentBean" />

  <bean id="NonAppliedDistributionMove-universityFiscalYear-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="universityFiscalYear" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="University Fiscal Year" />
    <property name="shortLabel" value="Year" />
    <property name="maxLength" value="4" />
    <property name="validationPattern" >
      <bean parent="NumericValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="6" />
    </property>
  </bean>

<!-- Business Object Inquiry Definition -->


  <bean id="NonAppliedDistributionMove-inquiryDefinition" parent="NonAppliedDistributionMove-inquiryDefinition-parentBean" />

  <bean id="NonAppliedDistributionMove-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
    <property name="title" value="Non-Applied Distribution Move Inquiry" />
    <property name="inquirySections" >
      <list>
        <bean parent="InquirySectionDefinition">
          <property name="title" value="" />
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields" >
            <list>
              <bean parent="FieldDefinition"
                    p:attributeName="documentNumber" />
              <bean parent="FieldDefinition"
                    p:attributeName="financialDocumentLineAmount" />
              <bean parent="FieldDefinition"
                    p:attributeName="paidAppliedItemNumber" />
              <bean parent="FieldDefinition"
                    p:attributeName="referenceFinancialDocumentNumber" />
              <bean parent="FieldDefinition"
                    p:attributeName="universityFiscalPeriodCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="universityFiscalYear" />
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Business Object Lookup Definition -->


  <bean id="NonAppliedDistributionMove-lookupDefinition" parent="NonAppliedDistributionMove-lookupDefinition-parentBean" />

  <bean id="NonAppliedDistributionMove-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
    <property name="title" value="Non-Applied Distribution Move Lookup" />
    <property name="instructions" value="Look up a Non-Applied Distribution Move" />
    <property name="defaultSort" >
      <bean parent="SortDefinition">
      </bean>
    </property>
    <property name="lookupFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="documentNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="financialDocumentLineAmount" />
        <bean parent="FieldDefinition"
              p:attributeName="paidAppliedItemNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="referenceFinancialDocumentNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="universityFiscalPeriodCode" />
        <bean parent="FieldDefinition"
              p:attributeName="universityFiscalYear" />
      </list>
    </property>
    <property name="resultFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="documentNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="financialDocumentLineAmount" />
        <bean parent="FieldDefinition"
              p:attributeName="paidAppliedItemNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="referenceFinancialDocumentNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="universityFiscalPeriodCode" />
        <bean parent="FieldDefinition"
              p:attributeName="universityFiscalYear" />
      </list>
    </property>
  </bean>
</beans>
