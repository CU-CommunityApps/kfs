  <bean id="CustomerNote" parent="CustomerNote-parentBean"/>

  <bean id="CustomerNote-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.kfs.module.ar.businessobject.CustomerNote"/>
    <property name="objectLabel" value="Customer Note"/>
    <property name="attributes">
      <list>
        <ref bean="CustomerNote-customerNumber"/>
        <ref bean="CustomerNote-customerNoteIdentifier"/>
        <ref bean="CustomerNote-noteText"/>
        <ref bean="CustomerNote-notePostedDate"/>
        <ref bean="CustomerNote-authorPrincipalId"/>
        <ref bean="CustomerNote-authorUniversal.name"/>
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->

  <bean id="CustomerNote-customerNumber" parent="CustomerNote-customerNumber-parentBean"/>

  <bean id="CustomerNote-customerNumber-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="customerNumber"/>
    <property name="forceUppercase" value="true"/>
    <property name="label" value="Customer Number"/>
    <property name="shortLabel" value="Number"/>
    <property name="maxLength" value="9"/>
    <property name="validationPattern">
      <ref bean="AnyCharacterValidation" />
    </property>
    <property name="control">
      <bean parent="TextControlDefinition" p:size="11"/>
    </property>
  </bean>
  
  <bean id="CustomerNote-customerNoteIdentifier" parent="CustomerNote-customerNoteIdentifier-parentBean"/>

  <bean id="CustomerNote-customerNoteIdentifier-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="customerNoteIdentifier"/>
    <property name="forceUppercase" value="true"/>
    <property name="label" value="Customer Note Identifier"/>
    <property name="shortLabel" value="Identifier"/>
    <property name="maxLength" value="7"/>
    <property name="validationPattern">
      <ref bean="NumericValidation" />
    </property>
    <property name="control">
      <ref bean="HiddenControl" />
    </property>
  </bean>
  
  <bean id="CustomerNote-noteText" parent="CustomerNote-noteText-parentBean"/>

  <bean id="CustomerNote-noteText-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="forceUppercase" value="false"/>
    <property name="shortLabel" value="Note Text"/>
    <property name="maxLength" value="800"/>
    <property name="validationPattern">
      <bean parent="AnyCharacterValidationPattern" p:allowWhitespace="true"/>
    </property>
    <property name="required" value="true"/>
    <property name="control">
      <bean parent="TextareaControlDefinition" p:cols="50" p:rows="3"/>
    </property>
    <property name="summary" value="&amp;nbsp;"/>
    <property name="name" value="noteText"/>
    <property name="label" value="Note Text"/>
    <property name="description" value="A free-form text field for the text of the Customer Note."/>
  </bean>
  
  <bean id="CustomerNote-notePostedDate" parent="CustomerNote-notePostedDate-parentBean"/>

  <bean id="CustomerNote-notePostedDate-parentBean" abstract="true" parent="GenericAttributes-genericDate">
    <property name="forceUppercase" value="true"/>
    <property name="shortLabel" value="Posted Date"/>
    <property name="maxLength" value="36"/>
    <property name="summary" value="&amp;nbsp;"/>
    <property name="name" value="notePostedDate"/>
    <property name="label" value="Posted Date"/>
    <property name="description" value="A free-form text field that identifies the date at which the Note is posted."/>
  </bean>
  
  <bean id="CustomerNote-authorPrincipalId" parent="CustomerNote-authorPrincipalId-parentBean"/>

  <bean id="CustomerNote-authorPrincipalId-parentBean" abstract="true" parent="AttributeReferenceDummy-genericSystemId">
    <property name="shortLabel" value="Author"/>
    <property name="required" value="true"/>
    <property name="name" value="authorPrincipalId"/>
    <property name="label" value="Author Principal Id"/>
    <property name="description" value="A free-form text field for the full name of the Author of the Note, expressed as &quot;Lastname, Firstname Initial&quot;"/>
  </bean>
  
  <bean id="CustomerNote-authorUniversal.name" parent="CustomerNote-authorUniversal.name-parentBean"/>

  <bean id="CustomerNote-authorUniversal.name-parentBean" abstract="true" parent="KimBaseBeans-principalName">
    <property name="shortLabel" value="Author"/>
    <property name="label" value="Author"/>
    <property name="name" value="authorUniversal.name"/>
    <property name="required" value="false"/>
  </bean>
  
</beans>