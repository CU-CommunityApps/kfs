<!-- Business Object Inquiry Definition -->
	<bean id="OrganizationOptions-inquiryDefinition" parent="OrganizationOptions-inquiryDefinition-parentBean" />
	<bean id="OrganizationOptions-inquiryDefinition-parentBean"
		abstract="true" parent="InquiryDefinition">
		<property name="title" value="Organization Options Inquiry" />
		<property name="inquirySections">
			<list>
				<bean parent="InquirySectionDefinition">
					<property name="title" value="Organization Options" />
					<property name="numberOfColumns" value="1" />
					<property name="inquiryFields">
						<list>
							<bean parent="FieldDefinition" 
								p:attributeName="chartOfAccountsCode" />
							<bean parent="FieldDefinition" 
								p:attributeName="organizationCode" />
							<bean parent="FieldDefinition" 
								p:attributeName="processingChartOfAccountCode" />
							<bean parent="FieldDefinition" 
								p:attributeName="processingOrganizationCode" />
						</list>
					</property>
				</bean>
				<bean parent="InquirySectionDefinition">
					<property name="title" value="Organization Invoice Information" />
					<property name="numberOfColumns" value="1" />
					<property name="inquiryFields">
						<list>
							<bean parent="FieldDefinition" 
								p:attributeName="organizationCheckPayableToName" />
							<bean parent="FieldDefinition" 
								p:attributeName="organizationPaymentTermsText" />
							<bean parent="FieldDefinition" 
								p:attributeName="organizationMessageText" />
							<bean parent="FieldDefinition" 
								p:attributeName="printInvoiceIndicator" />
						</list>
					</property>
				</bean>
				<bean parent="InquirySectionDefinition">
					<property name="title" value="Organization Remit To Address" />
					<property name="numberOfColumns" value="1" />
					<property name="inquiryFields">
						<list>
							<bean parent="FieldDefinition" 
								p:attributeName="organizationRemitToAddressName" />
							<bean parent="FieldDefinition" 
								p:attributeName="organizationRemitToLine1StreetAddress" />
							<bean parent="FieldDefinition" 
								p:attributeName="organizationRemitToLine2StreetAddress" />
							<bean parent="FieldDefinition" 
								p:attributeName="organizationRemitToCityName" />
							<bean parent="FieldDefinition" 
								p:attributeName="organizationRemitToStateCode" />
							<bean parent="FieldDefinition" 
								p:attributeName="organizationRemitToZipCode" />
						</list>
					</property>
				</bean>
				<bean parent="InquirySectionDefinition">
					<property name="title" value="Organization Phone" />
					<property name="numberOfColumns" value="1" />
					<property name="inquiryFields">
						<list>
							<bean parent="FieldDefinition" 
								p:attributeName="organizationPhoneNumber" />
							<bean parent="FieldDefinition" 
								p:attributeName="organizationFaxNumber" />
							<bean parent="FieldDefinition" 
								p:attributeName="organization800PhoneNumber" />
						</list>
					</property>
				</bean>
			</list>
		</property>
	</bean>

<!-- Business Object Lookup Definition -->


  <bean id="OrganizationOptions-lookupDefinition" parent="OrganizationOptions-lookupDefinition-parentBean" />

  <bean id="OrganizationOptions-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
    <property name="title" value="Organization Options Lookup" />
    <property name="instructions" value="Look up Organization Options" />
    <property name="defaultSort" >
      <bean parent="SortDefinition">
      </bean>
    </property>
    <property name="lookupFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="chartOfAccountsCode" />
        <bean parent="FieldDefinition"
              p:attributeName="organizationCode" />
        <bean parent="FieldDefinition"
              p:attributeName="processingChartOfAccountCode" />
        <bean parent="FieldDefinition"
              p:attributeName="processingOrganizationCode" />
      </list>
    </property>
    <property name="resultFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="organizationOptionsViewer" />
        <bean parent="FieldDefinition"
              p:attributeName="chartOfAccountsCode" />
        <bean parent="FieldDefinition"
              p:attributeName="organizationCode" />
        <bean parent="FieldDefinition"
              p:attributeName="processingChartOfAccountCode" />
      </list>
    </property>
  </bean>
