<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="NonAppliedDistribution" parent="NonAppliedDistribution-parentBean" />

  <bean id="NonAppliedDistribution-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.module.ar.bo.NonAppliedDistribution" />
    <property name="inquiryDefinition" >
      <ref bean="NonAppliedDistribution-inquiryDefinition" />
    </property>
    <property name="lookupDefinition" >
      <ref bean="NonAppliedDistribution-lookupDefinition" />
    </property>
    <property name="attributes" >
      <list>
        <ref bean="NonAppliedDistribution-documentNumber" />
        <ref bean="NonAppliedDistribution-financialDocumentLineAmount" />
        <ref bean="NonAppliedDistribution-paidAppliedItemNumber" />
        <ref bean="NonAppliedDistribution-referenceFinancialDocumentNumber" />
        <ref bean="NonAppliedDistribution-universityFiscalPeriodCode" />
        <ref bean="NonAppliedDistribution-universityFiscalYear" />
        <ref bean="NonAppliedDistribution-versionNumber" />
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->


  <bean id="NonAppliedDistribution-documentNumber" parent="NonAppliedDistribution-documentNumber-parentBean" />

  <bean id="NonAppliedDistribution-documentNumber-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="documentNumber" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Document Number" />
    <property name="shortLabel" value="Number" />
    <property name="maxLength" value="14" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="16" />
    </property>
    <property name="summary" value="documentNumber" />
    <property name="description" value="documentNumber description..." />
  </bean>
  <bean id="NonAppliedDistribution-financialDocumentLineAmount" parent="NonAppliedDistribution-financialDocumentLineAmount-parentBean" />

  <bean id="NonAppliedDistribution-financialDocumentLineAmount-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="financialDocumentLineAmount" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Financial Document Line Amount" />
    <property name="shortLabel" value="Amount" />
    <property name="maxLength" value="19" />
    <property name="validationPattern" >
      <bean parent="FloatingPointValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="21" />
    </property>
    <property name="summary" value="financialDocumentLineAmount" />
    <property name="description" value="financialDocumentLineAmount description..." />
  </bean>
  <bean id="NonAppliedDistribution-paidAppliedItemNumber" parent="NonAppliedDistribution-paidAppliedItemNumber-parentBean" />

  <bean id="NonAppliedDistribution-paidAppliedItemNumber-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="paidAppliedItemNumber" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Paid Applied Item Number" />
    <property name="shortLabel" value="Number" />
    <property name="maxLength" value="7" />
    <property name="validationPattern" >
      <bean parent="NumericValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="9" />
    </property>
    <property name="summary" value="paidAppliedItemNumber" />
    <property name="description" value="paidAppliedItemNumber description..." />
  </bean>
  <bean id="NonAppliedDistribution-referenceFinancialDocumentNumber" parent="NonAppliedDistribution-referenceFinancialDocumentNumber-parentBean" />

  <bean id="NonAppliedDistribution-referenceFinancialDocumentNumber-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="referenceFinancialDocumentNumber" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Reference Financial Document Number" />
    <property name="shortLabel" value="Number" />
    <property name="maxLength" value="14" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="16" />
    </property>
    <property name="summary" value="referenceFinancialDocumentNumber" />
    <property name="description" value="referenceFinancialDocumentNumber description..." />
  </bean>
  <bean id="NonAppliedDistribution-universityFiscalPeriodCode" parent="NonAppliedDistribution-universityFiscalPeriodCode-parentBean" />

  <bean id="NonAppliedDistribution-universityFiscalPeriodCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="universityFiscalPeriodCode" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="University Fiscal Period Code" />
    <property name="shortLabel" value="Code" />
    <property name="maxLength" value="2" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="4" />
    </property>
    <property name="summary" value="universityFiscalPeriodCode" />
    <property name="description" value="universityFiscalPeriodCode description..." />
  </bean>
  <bean id="NonAppliedDistribution-universityFiscalYear" parent="NonAppliedDistribution-universityFiscalYear-parentBean" />

  <bean id="NonAppliedDistribution-universityFiscalYear-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="universityFiscalYear" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="University Fiscal Year" />
    <property name="shortLabel" value="Year" />
    <property name="maxLength" value="4" />
    <property name="validationPattern" >
      <bean parent="NumericValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="6" />
    </property>
    <property name="summary" value="universityFiscalYear" />
    <property name="description" value="universityFiscalYear description..." />
  </bean>
  <bean id="NonAppliedDistribution-versionNumber" parent="NonAppliedDistribution-versionNumber-parentBean" />

  <bean id="NonAppliedDistribution-versionNumber-parentBean" abstract="true" parent="AttributeReferenceDummy-versionNumber">
  </bean>

<!-- Business Object Inquiry Definition -->


  <bean id="NonAppliedDistribution-inquiryDefinition" parent="NonAppliedDistribution-inquiryDefinition-parentBean" />

  <bean id="NonAppliedDistribution-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
    <property name="title" value="Non-Applied Distribution Inquiry" />
    <property name="inquirySections" >
      <list>
        <bean parent="InquirySectionDefinition">
          <property name="title" value="Header Text" />
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields" >
            <list>
              <bean parent="FieldDefinition"
                    p:attributeName="documentNumber" />
              <bean parent="FieldDefinition"
                    p:attributeName="financialDocumentLineAmount" />
              <bean parent="FieldDefinition"
                    p:attributeName="paidAppliedItemNumber" />
              <bean parent="FieldDefinition"
                    p:attributeName="referenceFinancialDocumentNumber" />
              <bean parent="FieldDefinition"
                    p:attributeName="universityFiscalPeriodCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="universityFiscalYear" />
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Business Object Lookup Definition -->


  <bean id="NonAppliedDistribution-lookupDefinition" parent="NonAppliedDistribution-lookupDefinition-parentBean" />

  <bean id="NonAppliedDistribution-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
    <property name="title" value="Non-Applied Distribution Lookup" />
    <property name="instructions" value="Look up a Non-Applied Distribution" />
    <property name="defaultSort" >
      <bean parent="SortDefinition">
      </bean>
    </property>
    <property name="lookupFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="documentNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="financialDocumentLineAmount" />
        <bean parent="FieldDefinition"
              p:attributeName="paidAppliedItemNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="referenceFinancialDocumentNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="universityFiscalPeriodCode" />
        <bean parent="FieldDefinition"
              p:attributeName="universityFiscalYear" />
      </list>
    </property>
    <property name="resultFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="documentNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="financialDocumentLineAmount" />
        <bean parent="FieldDefinition"
              p:attributeName="paidAppliedItemNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="referenceFinancialDocumentNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="universityFiscalPeriodCode" />
        <bean parent="FieldDefinition"
              p:attributeName="universityFiscalYear" />
      </list>
    </property>
  </bean>
</beans>
