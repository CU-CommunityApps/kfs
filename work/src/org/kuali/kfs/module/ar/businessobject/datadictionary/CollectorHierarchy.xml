  <bean id="CollectorHierarchy" parent="CollectorHierarchy-parentBean"/>

  <bean id="CollectorHierarchy-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.kfs.module.ar.businessobject.CollectorHierarchy"/>
    <property name="lookupDefinition">
			<ref bean="CollectorHierarchy-lookupDefinition" />
		</property>
    <property name="objectLabel" value="Collector Hierarchy"/>
    <property name="attributes">
      <list>
        <ref bean="CollectorHierarchy-principalId"/>
        <ref bean="CollectorHierarchy-collector.principalName"/>
        <ref bean="CollectorHierarchy-collector.name"/>
        <ref bean="CollectorHierarchy-userLookupRoleNamespaceCode"/>
        <ref bean="CollectorHierarchy-userLookupRoleName"/>
        <ref bean="CollectorHierarchy-active"/>
      </list>
    </property>
    <property name="collections">
			<list>
				<bean parent="CollectionDefinition" p:name="collectorInformations"
					p:label="Collector Information" p:shortLabel="Collector Information"
					p:elementLabel="Collector Information" />
			</list>
	</property>
    <property name="relationships">
      <list>
        <bean parent="RelationshipDefinition">
          <property name="objectAttributeName" value="collector"/>
          <property name="primitiveAttributes">
            <list>
              <bean parent="PrimitiveAttributeDefinition" p:sourceName="principalId" p:targetName="principalId"/>
            </list>
          </property>
          <property name="supportAttributes">
            <list>
              <bean parent="SupportAttributeDefinition" p:sourceName="collector.principalName" p:targetName="principalName"/>
              <bean parent="SupportAttributeDefinition" p:sourceName="collector.name" p:targetName="name"/>
              <bean parent="SupportAttributeDefinition" p:sourceName="userLookupRoleNamespaceCode" p:targetName="lookupRoleNamespaceCode"/>
              <bean parent="SupportAttributeDefinition" p:sourceName="userLookupRoleName" p:targetName="lookupRoleName"/>                    
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->

  <bean id="CollectorHierarchy-principalId" parent="CollectorHierarchy-principalId-parentBean"/>

  <bean id="CollectorHierarchy-principalId-parentBean" abstract="true" parent="PersonImpl-principalId">
    <property name="name" value="principalId"/>
    <property name="forceUppercase" value="true"/>
    <property name="label" value="Principal Id"/>
    <property name="shortLabel" value="Principal Id"/>
    <property name="maxLength" value="40"/>
    <property name="control">
      <ref bean="HiddenControl" />
    </property>
  </bean>
  
  <bean id="CollectorHierarchy-collector.principalName" parent="CollectorHierarchy-collector.principalName-parentBean"/>

  <bean id="CollectorHierarchy-collector.principalName-parentBean" abstract="true" parent="PersonImpl-principalName">
    <property name="name" value="collector.principalName"/>
    <property name="label" value="Collector Head"/>
    <property name="required" value="false"/>
  </bean>
  
  <bean id="CollectorHierarchy-collector.name" parent="CollectorHierarchy-collector.name-parentBean"/>

  <bean id="CollectorHierarchy-collector.name-parentBean" abstract="true" parent="PersonImpl-name">
    <property name="name" value="collector.name"/>
    <property name="label" value="Name"/>
    <property name="required" value="false"/>
    <property name="control">
      <ref bean="StandardNameTextControl" />
    </property>
  </bean>
  
  <bean id="CollectorHierarchy-active" parent="CollectorHierarchy-active-parentBean"/>

  <bean id="CollectorHierarchy-active-parentBean" abstract="true" parent="GenericAttributes-activeIndicator">
    <property name="name" value="active"/>
  </bean>
  
  <!--  for userrole namespace -->
  
  <bean id="CollectorHierarchy-userLookupRoleNamespaceCode" parent="CollectorHierarchy-userLookupRoleNamespaceCode-parentBean" />
	<bean id="CollectorHierarchy-userLookupRoleNamespaceCode-parentBean" parent="AttributeDefinition">
		<property name="name" value="userLookupRoleNamespaceCode" />
		<property name="label" value="Role Namespace Code" />
		<property name="required" value="false" />
		<property name="maxLength" value="40" />
		<property name="control">
			<ref bean="HiddenControl" />
		</property>
	</bean>
	<bean id="CollectorHierarchy-userLookupRoleName" parent="CollectorHierarchy-userLookupRoleName-parentBean" />
	<bean id="CollectorHierarchy-userLookupRoleName-parentBean" parent="AttributeDefinition">
		<property name="name" value="userLookupRoleName" />
		<property name="label" value="Role Name" />
		<property name="required" value="false" />
		<property name="maxLength" value="40" />
		<property name="control">
			<ref bean="HiddenControl" />
		</property>
	</bean>
  
  	<bean id="CollectorHierarchy-lookupDefinition" parent="CollectorHierarchy-lookupDefinition-parentBean" />

	<bean id="CollectorHierarchy-lookupDefinition-parentBean" abstract="true"
		parent="LookupDefinition">
		<property name="title" value="Collector Hierarchy Lookup" />
		<property name="lookupableID" value="collectorHierarchyLookupable" />

		<property name="defaultSort">
			<bean parent="SortDefinition">
			</bean>
		</property>
		<property name="lookupFields">
			<list>
				<bean parent="FieldDefinition" p:attributeName="collector.principalName" />
				<bean parent="FieldDefinition" p:attributeName="principalId" />
				<bean parent="FieldDefinition" p:attributeName="collector.name" p:noLookup="true"/>
				<bean parent="FieldDefinition" p:attributeName="userLookupRoleNamespaceCode" p:defaultValue="KFS-AR"/>
				<bean parent="FieldDefinition" p:attributeName="userLookupRoleName" p:defaultValue="CGB Collector"/>
				<bean parent="FieldDefinition" p:attributeName="active" p:defaultValue="Y"/>
			</list>
		</property>
		<property name="resultFields">
			<list>
				<bean parent="FieldDefinition" p:attributeName="collector.principalName" p:noLookup="true"/>
				<bean parent="FieldDefinition" p:attributeName="collector.name" />
				<bean parent="FieldDefinition" p:attributeName="active" />
			</list>
		</property>
	</bean>
  
</beans>

