<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="CustomerProcessingType" parent="CustomerProcessingType-parentBean" />

  <bean id="CustomerProcessingType-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.kfs.module.ar.businessobject.CustomerProcessingType" />
    <property name="inquiryDefinition" >
      <ref bean="CustomerProcessingType-inquiryDefinition" />
    </property>
    <property name="lookupDefinition" >
      <ref bean="CustomerProcessingType-lookupDefinition" />
    </property>
    <property name="attributes" >
      <list>
        <ref bean="CustomerProcessingType-active" />
        <ref bean="CustomerProcessingType-customerSpecialProcessingCode" />
        <ref bean="CustomerProcessingType-customerSpecialProcessingDescription" />
        <ref bean="CustomerProcessingType-versionNumber" />
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->


  <bean id="CustomerProcessingType-active" parent="CustomerProcessingType-active-parentBean" />

  <bean id="CustomerProcessingType-active-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="active" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Active" />
    <property name="shortLabel" value="Active" />
    <property name="maxLength" value="1" />
    <property name="control" >
      <bean parent="CheckboxControlDefinition" />
    </property>
    <property name="summary" value="active" />
    <property name="description" value="active description..." />
  </bean>
  <bean id="CustomerProcessingType-customerSpecialProcessingCode" parent="CustomerProcessingType-customerSpecialProcessingCode-parentBean" />

  <bean id="CustomerProcessingType-customerSpecialProcessingCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="customerSpecialProcessingCode" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Customer Special Processing Code" />
    <property name="shortLabel" value="Code" />
    <property name="maxLength" value="2" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="4" />
    </property>
    <property name="summary" value="customerSpecialProcessingCode" />
    <property name="description" value="customerSpecialProcessingCode description..." />
  </bean>
  <bean id="CustomerProcessingType-customerSpecialProcessingDescription" parent="CustomerProcessingType-customerSpecialProcessingDescription-parentBean" />

  <bean id="CustomerProcessingType-customerSpecialProcessingDescription-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="customerSpecialProcessingDescription" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Customer Special Processing Description" />
    <property name="shortLabel" value="Description" />
    <property name="maxLength" value="40" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="42" />
    </property>
    <property name="summary" value="customerSpecialProcessingDescription" />
    <property name="description" value="customerSpecialProcessingDescription description..." />
  </bean>
  <bean id="CustomerProcessingType-versionNumber" parent="CustomerProcessingType-versionNumber-parentBean" />

  <bean id="CustomerProcessingType-versionNumber-parentBean" abstract="true" parent="GenericAttributes-versionNumber">
  </bean>

<!-- Business Object Inquiry Definition -->


  <bean id="CustomerProcessingType-inquiryDefinition" parent="CustomerProcessingType-inquiryDefinition-parentBean" />

  <bean id="CustomerProcessingType-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
    <property name="title" value="Customer Processing Type Inquiry" />
    <property name="inquirySections" >
      <list>
        <bean parent="InquirySectionDefinition">
          <property name="title" value="" />
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields" >
            <list>
              <bean parent="FieldDefinition"
                    p:attributeName="active" />
              <bean parent="FieldDefinition"
                    p:attributeName="customerSpecialProcessingCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="customerSpecialProcessingDescription" />
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Business Object Lookup Definition -->


  <bean id="CustomerProcessingType-lookupDefinition" parent="CustomerProcessingType-lookupDefinition-parentBean" />

  <bean id="CustomerProcessingType-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
    <property name="title" value="Customer Processing Type Lookup" />
    <property name="instructions" value="Look up a Customer Processing Type" />
    <property name="defaultSort" >
      <bean parent="SortDefinition">
      </bean>
    </property>
    <property name="lookupFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="active" />
        <bean parent="FieldDefinition"
              p:attributeName="customerSpecialProcessingCode" />
        <bean parent="FieldDefinition"
              p:attributeName="customerSpecialProcessingDescription" />
      </list>
    </property>
    <property name="resultFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="active" />
        <bean parent="FieldDefinition"
              p:attributeName="customerSpecialProcessingCode" />
        <bean parent="FieldDefinition"
              p:attributeName="customerSpecialProcessingDescription" />
      </list>
    </property>
  </bean>
</beans>
