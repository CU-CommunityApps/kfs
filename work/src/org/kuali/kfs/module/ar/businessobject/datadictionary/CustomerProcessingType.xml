<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "../dataDictionary-1_0.dtd">
<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.module.ar.bo.CustomerProcessingType">
 
        <inquiry>
            <title>Customer Processing Type Inquiry</title>
             
            <inquirySections>
                <inquirySection title="Header Text">
                    <inquiryFields>
                        <field attributeName="active" />
                        <field attributeName="customerSpecialProcessingCode" />
                        <field attributeName="customerSpecialProcessingDescription" />
                    </inquiryFields>
                </inquirySection>
            </inquirySections>  
        </inquiry>

        <lookup>
            <title>Customer Processing Type Lookup</title>

            <instructions>Look up a Customer Processing Type</instructions>

            <defaultSort sortAscending="true">
                <sortAttributes>
                    <sortAttribute attributeName="customerSpecialProcessingCode" />
                </sortAttributes>
            </defaultSort>

            <lookupFields>
                <lookupField attributeName="active" required="false" />
                <lookupField attributeName="customerSpecialProcessingCode" required="false" />
                <lookupField attributeName="customerSpecialProcessingDescription" required="false" />
            </lookupFields>

            <resultFields>
                <field attributeName="active" />
                <field attributeName="customerSpecialProcessingCode" />
                <field attributeName="customerSpecialProcessingDescription" />
            </resultFields>
        </lookup>

        <attributes>

            <attribute name="active" forceUppercase="true">
                <label>Active</label>
                <shortLabel>Active</shortLabel>

                <maxLength>1</maxLength>

                <control>
                    <checkbox />
                </control>

                <summary>active</summary>
                <description>active description...</description>
			</attribute>

            <attribute name="customerSpecialProcessingCode" forceUppercase="true">
                <label>Customer Special Processing Code</label>
                <shortLabel>Code</shortLabel>

                <maxLength>2</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="4" />
                </control>

                <summary>customerSpecialProcessingCode</summary>
                <description>customerSpecialProcessingCode description...</description>
			</attribute>

            <attribute name="customerSpecialProcessingDescription" forceUppercase="true">
                <label>Customer Special Processing Description</label>
                <shortLabel>Description</shortLabel>

                <maxLength>40</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="42" />
                </control>

                <summary>customerSpecialProcessingDescription</summary>
                <description>customerSpecialProcessingDescription description...</description>
			</attribute>

            <attributeReference name="versionNumber" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="versionNumber" />

        </attributes>
    </businessObject>
</dictionaryEntry>
