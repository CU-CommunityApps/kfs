<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "../dataDictionary-1_0.dtd">
<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.module.ar.bo.PrintInvoiceOptions">
 
        <inquiry>
            <title>Print Invoice Options Inquiry</title>
             
            <inquirySections>
                <inquirySection title="Header Text">
                    <inquiryFields>
                        <field attributeName="active" />
                        <field attributeName="printInvoiceDescription" />
                        <field attributeName="printInvoiceIndicator" />
                    </inquiryFields>
                </inquirySection>
            </inquirySections>  
        </inquiry>

        <lookup>
            <title>Print Invoice Options Lookup</title>

            <instructions>Look up Print Invoice Options</instructions>

            <defaultSort sortAscending="true">
                <sortAttributes>
                    <sortAttribute attributeName="printInvoiceIndicator" />
                </sortAttributes>
            </defaultSort>

            <lookupFields>
                <lookupField attributeName="active" required="false" />
                <lookupField attributeName="printInvoiceDescription" required="false" />
                <lookupField attributeName="printInvoiceIndicator" required="false" />
            </lookupFields>

            <resultFields>
                <field attributeName="active" />
                <field attributeName="printInvoiceDescription" />
                <field attributeName="printInvoiceIndicator" />
            </resultFields>
        </lookup>

        <attributes>

            <attribute name="active" forceUppercase="true">
                <label>Active</label>
                <shortLabel>Active</shortLabel>

                <maxLength>1</maxLength>

                <control>
                    <checkbox />
                </control>

                <summary>active</summary>
                <description>active description...</description>
			</attribute>

            <attribute name="printInvoiceDescription" forceUppercase="true">
                <label>Print Invoice Description</label>
                <shortLabel>Description</shortLabel>

                <maxLength>40</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="42" />
                </control>

                <summary>printInvoiceDescription</summary>
                <description>printInvoiceDescription description...</description>
			</attribute>

            <attribute name="printInvoiceIndicator" forceUppercase="true">
                <label>Print Invoice Indicator</label>
                <shortLabel>Indicator</shortLabel>

                <maxLength>1</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="3" />
                </control>

                <summary>printInvoiceIndicator</summary>
                <description>printInvoiceIndicator description...</description>
			</attribute>

            <attributeReference name="versionNumber" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="versionNumber" />

        </attributes>
    </businessObject>
</dictionaryEntry>
