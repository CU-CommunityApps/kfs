<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="PrintInvoiceOptions" parent="PrintInvoiceOptions-parentBean" />

  <bean id="PrintInvoiceOptions-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.kfs.module.ar.businessobject.PrintInvoiceOptions" />
    <property name="inquiryDefinition" >
      <ref bean="PrintInvoiceOptions-inquiryDefinition" />
    </property>
    <property name="lookupDefinition" >
      <ref bean="PrintInvoiceOptions-lookupDefinition" />
    </property>
    <property name="objectLabel" value="Print Invoice Options" />
    <property name="attributes" >
      <list>
        <ref bean="PrintInvoiceOptions-active" />
        <ref bean="PrintInvoiceOptions-printInvoiceDescription" />
        <ref bean="PrintInvoiceOptions-printInvoiceIndicator" />
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->


  <bean id="PrintInvoiceOptions-active" parent="PrintInvoiceOptions-active-parentBean" />

  <bean id="PrintInvoiceOptions-active-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="active" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Active" />
    <property name="shortLabel" value="Active" />
    <property name="maxLength" value="1" />
    <property name="control" >
      <bean parent="CheckboxControlDefinition" />
    </property>
    <property name="summary" value="active" />
    <property name="description" value="active description..." />
  </bean>
  <bean id="PrintInvoiceOptions-printInvoiceDescription" parent="PrintInvoiceOptions-printInvoiceDescription-parentBean" />

  <bean id="PrintInvoiceOptions-printInvoiceDescription-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="printInvoiceDescription" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Print Invoice Description" />
    <property name="shortLabel" value="Description" />
    <property name="maxLength" value="40" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="42" />
    </property>
    <property name="summary" value="printInvoiceDescription" />
    <property name="description" value="printInvoiceDescription description..." />
  </bean>
  <bean id="PrintInvoiceOptions-printInvoiceIndicator" parent="PrintInvoiceOptions-printInvoiceIndicator-parentBean" />

  <bean id="PrintInvoiceOptions-printInvoiceIndicator-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="printInvoiceIndicator" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Print Invoice Indicator" />
    <property name="shortLabel" value="Indicator" />
    <property name="maxLength" value="1" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="3" />
    </property>
    <property name="summary" value="printInvoiceIndicator" />
    <property name="description" value="printInvoiceIndicator description..." />
  </bean>

<!-- Business Object Inquiry Definition -->


  <bean id="PrintInvoiceOptions-inquiryDefinition" parent="PrintInvoiceOptions-inquiryDefinition-parentBean" />

  <bean id="PrintInvoiceOptions-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
    <property name="title" value="Print Invoice Options Inquiry" />
    <property name="inquirySections" >
      <list>
        <bean parent="InquirySectionDefinition">
          <property name="title" value="" />
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields" >
            <list>
              <bean parent="FieldDefinition"
                    p:attributeName="active" />
              <bean parent="FieldDefinition"
                    p:attributeName="printInvoiceDescription" />
              <bean parent="FieldDefinition"
                    p:attributeName="printInvoiceIndicator" />
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Business Object Lookup Definition -->


  <bean id="PrintInvoiceOptions-lookupDefinition" parent="PrintInvoiceOptions-lookupDefinition-parentBean" />

  <bean id="PrintInvoiceOptions-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
    <property name="title" value="Print Invoice Options Lookup" />
    <property name="instructions" value="Look up Print Invoice Options" />
    <property name="defaultSort" >
      <bean parent="SortDefinition">
      </bean>
    </property>
    <property name="lookupFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="active" />
        <bean parent="FieldDefinition"
              p:attributeName="printInvoiceDescription" />
        <bean parent="FieldDefinition"
              p:attributeName="printInvoiceIndicator" />
      </list>
    </property>
    <property name="resultFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="active" />
        <bean parent="FieldDefinition"
              p:attributeName="printInvoiceDescription" />
        <bean parent="FieldDefinition"
              p:attributeName="printInvoiceIndicator" />
      </list>
    </property>
  </bean>
</beans>
