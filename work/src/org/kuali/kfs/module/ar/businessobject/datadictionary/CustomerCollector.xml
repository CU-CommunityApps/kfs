  <bean id="CustomerCollector" parent="CustomerCollector-parentBean"/>

  <bean id="CustomerCollector-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.kfs.module.ar.businessobject.CustomerCollector"/>
    <property name="objectLabel" value="Customer Collector"/>
    <property name="attributes">
      <list>
        <ref bean="CustomerCollector-customerNumber"/>
        <ref bean="CustomerCollector-principalId"/>
        <ref bean="CustomerCollector-collector.principalName"/>
        <ref bean="CustomerCollector-collector.name"/>
      </list>
    </property>
    <property name="relationships">
      <list>
        <bean parent="RelationshipDefinition">
          <property name="objectAttributeName" value="collector"/>
          <property name="primitiveAttributes">
            <list>
              <bean parent="PrimitiveAttributeDefinition" p:sourceName="principalId" p:targetName="principalId"/>
            </list>
          </property>
          <property name="supportAttributes">
            <list>
              <bean parent="SupportAttributeDefinition" p:sourceName="collector.principalName" p:targetName="principalName"/>
              <bean parent="SupportAttributeDefinition" p:sourceName="collector.name" p:targetName="name"/>
              <bean parent="SupportAttributeDefinition" p:sourceName="userLookupRoleNamespaceCode" p:targetName="lookupRoleNamespaceCode"/>
              <bean parent="SupportAttributeDefinition" p:sourceName="userLookupRoleName" p:targetName="lookupRoleName"/>                    
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->

  <bean id="CustomerCollector-customerNumber" parent="CustomerCollector-customerNumber-parentBean"/>

  <bean id="CustomerCollector-customerNumber-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="customerNumber"/>
    <property name="forceUppercase" value="true"/>
    <property name="label" value="Customer Number"/>
    <property name="shortLabel" value="Number"/>
    <property name="maxLength" value="9"/>
    <property name="validationPattern">
      <ref bean="AnyCharacterValidation" />
    </property>
    <property name="control">
      <bean parent="TextControlDefinition" p:size="11"/>
    </property>
  </bean>
  
  <bean id="CustomerCollector-principalId" parent="CustomerCollector-principalId-parentBean"/>

  <bean id="CustomerCollector-principalId-parentBean" abstract="true" parent="PersonImpl-principalId">
    <property name="name" value="principalId"/>
    <property name="forceUppercase" value="true"/>
    <property name="label" value="Principal Id"/>
    <property name="shortLabel" value="Principal Id"/>
    <property name="maxLength" value="40"/>
    <property name="control">
      <ref bean="HiddenControl" />
    </property>
  </bean>
  
  <bean id="CustomerCollector-collector.principalName" parent="CustomerCollector-collector.principalName-parentBean"/>

  <bean id="CustomerCollector-collector.principalName-parentBean" abstract="true" parent="PersonImpl-principalName">
    <property name="name" value="collector.principalName"/>
    <property name="label" value="Collector Principal Name"/>
    <property name="required" value="false"/>
  </bean>
  
  <bean id="CustomerCollector-collector.name" parent="CustomerCollector-collector.name-parentBean"/>

  <bean id="CustomerCollector-collector.name-parentBean" abstract="true" parent="PersonImpl-name">
    <property name="name" value="collector.name"/>
    <property name="label" value="Name"/>
    <property name="required" value="false"/>
    <property name="control">
      <ref bean="StandardNameTextControl" />
    </property>
  </bean>
  
</beans>
