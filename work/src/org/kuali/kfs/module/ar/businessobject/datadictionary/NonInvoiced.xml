                <shortLabel>Code</shortLabel>

                <maxLength>4</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="6" />
                </control>

                <summary>financialObjectCode</summary>
                <description>financialObjectCode description...</description>
			</attribute>

            <attribute name="financialSubObjectCode" forceUppercase="true">
                <label>Financial Sub Object Code</label>
