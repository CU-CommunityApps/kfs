<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "../dataDictionary-1_0.dtd">
<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.module.ar.bo.NonInvoiced">
 
        <inquiry>
            <title>Non-Invoiced Inquiry</title>
             
            <inquirySections>
                <inquirySection title="Header Text">
                    <inquiryFields>
                        <field attributeName="accountNumber" />
                        <field attributeName="chartOfAccountsCode" />
                        <field attributeName="documentNumber" />
                        <field attributeName="financialDocumentLineAmount" />
                        <field attributeName="financialDocumentLineNumber" />
                        <field attributeName="financialDocumentOverrideCode" />
                        <field attributeName="financialDocumentPostingYear" />
                        <field attributeName="financialObjectCode" />
                        <field attributeName="financialSubObjectCode" />
                        <field attributeName="projectCode" />
                        <field attributeName="subAccountNumber" />
                    </inquiryFields>
                </inquirySection>
            </inquirySections>  
        </inquiry>

        <lookup>
            <title>Non-Invoiced Lookup</title>

            <instructions>Look up a Non-Invoiced</instructions>

            <defaultSort sortAscending="true">
                <sortAttributes>
                    <sortAttribute attributeName="documentNumber" />
                    <sortAttribute attributeName="financialDocumentLineNumber" />
                </sortAttributes>
            </defaultSort>

            <lookupFields>
                <lookupField attributeName="accountNumber" required="false" />
                <lookupField attributeName="chartOfAccountsCode" required="false" />
                <lookupField attributeName="documentNumber" required="false" />
                <lookupField attributeName="financialDocumentLineAmount" required="false" />
                <lookupField attributeName="financialDocumentLineNumber" required="false" />
                <lookupField attributeName="financialDocumentOverrideCode" required="false" />
                <lookupField attributeName="financialDocumentPostingYear" required="false" />
                <lookupField attributeName="financialObjectCode" required="false" />
                <lookupField attributeName="financialSubObjectCode" required="false" />
                <lookupField attributeName="projectCode" required="false" />
                <lookupField attributeName="subAccountNumber" required="false" />
            </lookupFields>

            <resultFields>
                <field attributeName="accountNumber" />
                <field attributeName="chartOfAccountsCode" />
                <field attributeName="documentNumber" />
                <field attributeName="financialDocumentLineAmount" />
                <field attributeName="financialDocumentLineNumber" />
                <field attributeName="financialDocumentOverrideCode" />
                <field attributeName="financialDocumentPostingYear" />
                <field attributeName="financialObjectCode" />
                <field attributeName="financialSubObjectCode" />
                <field attributeName="projectCode" />
                <field attributeName="subAccountNumber" />
            </resultFields>
        </lookup>

        <attributes>

            <attribute name="accountNumber" forceUppercase="true">
                <label>Account Number</label>
                <shortLabel>Number</shortLabel>

                <maxLength>7</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="9" />
                </control>

                <summary>accountNumber</summary>
                <description>accountNumber description...</description>
			</attribute>

            <attribute name="chartOfAccountsCode" forceUppercase="true">
                <label>Chart Of Accounts Code</label>
                <shortLabel>Code</shortLabel>

                <maxLength>2</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="4" />
                </control>

                <summary>chartOfAccountsCode</summary>
                <description>chartOfAccountsCode description...</description>
			</attribute>

            <attribute name="documentNumber" forceUppercase="true">
