<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="CustomerAgingReportDetail" parent="CustomerAgingReportDetail-parentBean" />

  <bean id="CustomerAgingReportDetail-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.kfs.module.ar.businessobject.CustomerAgingReportDetail" />
    <!-- <property name="inquiryDefinition" >
      <ref bean="AccountBalanceByConsolidation-inquiryDefinition" />
    </property> -->
    <property name="lookupDefinition" >
      <ref bean="CustomerAgingReportDetail-lookupDefinition" />
    </property>
    <property name="attributes" >
      <list>
    <ref bean="CustomerAgingReportDetail-chartOfAccountsCode" />
	<ref bean="CustomerAgingReportDetail-organizationCode" />
	<ref bean="CustomerAgingReportDetail-accountNumber" />          
    <ref bean="CustomerAgingReportDetail-customerName" />
	<ref bean="CustomerAgingReportDetail-customerNumber" />
	<ref bean="CustomerAgingReportDetail-unpaidBalance0to30" />
	<ref bean="CustomerAgingReportDetail-unpaidBalance31to60" />
	<ref bean="CustomerAgingReportDetail-unpaidBalance61to90" />
    <ref bean="CustomerAgingReportDetail-unpaidBalance91toSYSPR" />
    <ref bean="CustomerAgingReportDetail-unpaidBalanceSYSPRplus1orMore" />
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->
<bean id="CustomerAgingReportDetail-chartOfAccountsCode" parent="CustomerAgingReportDetail-chartOfAccountsCode-parentBean" />
<bean id="CustomerAgingReportDetail-chartOfAccountsCode-parentBean" abstract="true" parent="Chart-chartOfAccountsCode"></bean>

<bean id="CustomerAgingReportDetail-organizationCode" parent="CustomerAgingReportDetail-organizationCode-parentBean" />
<bean id="CustomerAgingReportDetail-organizationCode-parentBean" abstract="true" parent="Org-organizationCode"></bean>

<bean id="CustomerAgingReportDetail-accountNumber" parent="CustomerAgingReportDetail-accountNumber-parentBean" />
<bean id="CustomerAgingReportDetail-accountNumber-parentBean" abstract="true" parent="Account-accountNumber"></bean>

<bean id="CustomerAgingReportDetail-customerName" parent="CustomerAgingReportDetail-customerName-parentBean" />
<bean id="CustomerAgingReportDetail-customerName-parentBean" abstract="true" parent="Customer-customerName"></bean>

<bean id="CustomerAgingReportDetail-customerNumber" parent="CustomerAgingReportDetail-customerNumber-parentBean" />
<bean id="CustomerAgingReportDetail-customerNumber-parentBean" abstract="true" parent="Customer-customerNumber"></bean>

<bean id="CustomerAgingReportDetail-unpaidBalance0to30" parent="CustomerAgingReportDetail-unpaidBalance0to30-parentBean" />
<bean id="CustomerAgingReportDetail-unpaidBalance0to30-parentBean" abstract="true" parent="GenericAttributes-genericAmount">
    <property name="name" value="unpaidBalance0to30" />
    <property name="label" value="0-30 days" />
</bean>

<bean id="CustomerAgingReportDetail-unpaidBalance31to60" parent="CustomerAgingReportDetail-unpaidBalance31to60-parentBean" />
<bean id="CustomerAgingReportDetail-unpaidBalance31to60-parentBean" abstract="true" parent="GenericAttributes-genericAmount">
    <property name="name" value="unpaidBalance31to60" />
    <property name="label" value="31-60 days" />
</bean>

<bean id="CustomerAgingReportDetail-unpaidBalance61to90" parent="CustomerAgingReportDetail-unpaidBalance61to90-parentBean" />
<bean id="CustomerAgingReportDetail-unpaidBalance61to90-parentBean" abstract="true" parent="GenericAttributes-genericAmount">
    <property name="name" value="unpaidBalance61to90" />
    <property name="label" value="61-90 days" />
</bean>

<bean id="CustomerAgingReportDetail-unpaidBalance91toSYSPR" parent="CustomerAgingReportDetail-unpaidBalance91toSYSPR-parentBean" />
<bean id="CustomerAgingReportDetail-unpaidBalance91toSYSPR-parentBean" abstract="true" parent="GenericAttributes-genericAmount">
    <property name="name" value="unpaidBalance91toSYSPR" />
    <property name="label" value="91-??? days" />
</bean>

<bean id="CustomerAgingReportDetail-unpaidBalanceSYSPRplus1orMore" parent="CustomerAgingReportDetail-unpaidBalanceSYSPRplus1orMore-parentBean" />
<bean id="CustomerAgingReportDetail-unpaidBalanceSYSPRplus1orMore-parentBean" abstract="true" parent="GenericAttributes-genericAmount">
    <property name="name" value="unpaidBalanceSYSPRplus1orMore" />
    <property name="label" value="???+ days" />
</bean>


<!-- Business Object Inquiry Definition -->


  <bean id="CustomerAgingReportDetail-inquiryDefinition" parent="CustomerAgingReportDetail-inquiryDefinition-parentBean" />

  <bean id="CustomerAgingReportDetail-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
    <property name="title" value="Balances By Consolidation" />
    <property name="inquirySections" >
      <list>
        <bean parent="InquirySectionDefinition">
          <property name="title" value="" />
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields" >
            <list>
              <bean parent="FieldDefinition"
                    p:attributeName="customerName" />
              <bean parent="FieldDefinition"
                    p:attributeName="customerNumber" />
              <bean parent="FieldDefinition"
                    p:attributeName="unpaidBalance0to30" />
              <bean parent="FieldDefinition"
                    p:attributeName="unpaidBalance31to60" />
              <bean parent="FieldDefinition"
                    p:attributeName="unpaidBalance61to90" />
              <bean parent="FieldDefinition"
                    p:attributeName="unpaidBalance91toSYSPR" />
              <bean parent="FieldDefinition"
                    p:attributeName="unpaidBalanceSYSPRplus1orMore" />
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Business Object Lookup Definition -->


  <bean id="CustomerAgingReportDetail-lookupDefinition" parent="CustomerAgingReportDetail-lookupDefinition-parentBean" />

  <bean id="CustomerAgingReportDetail-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
    <property name="title" value="Customer Aging Report" />
    <property name="lookupableID" value="arCustomerAgingReportLookupable" />
    <property name="instructions" value="Lookup open invoices and credit memos for all customers from same Account or Org." />
    <!-- <property name="defaultSort" >
      <bean parent="SortDefinition">
        <property name="attributeNames" >
          <list>
            <value>financialObject.financialObjectLevel.finatncialConsolidationObject.finConsolidationObjectCode</value>
          </list>
        </property>
      </bean>
    </property> -->
    <property name="lookupFields" >
      <list>
        <bean parent="FieldDefinition"
              p:required="true"
              p:attributeName="chartOfAccountsCode" 	
              p:defaultValueFinderClass="org.kuali.kfs.module.ar.businessobject.defaultvalue.ChartOrganizationAccountingDefaultValueFinder" />
        <bean parent="FieldDefinition"
              p:required="true"
              p:attributeName="organizationCode"/>
        <bean parent="FieldDefinition"
              p:required="true"
              p:attributeName="accountNumber"
              p:defaultValueFinderClass="org.kuali.kfs.module.ar.businessobject.defaultvalue.AccountOrganizationAccountingDefaultValueFinder" />
      </list>
    </property>
    <property name="resultFields" >
      <list>
	    <bean parent="FieldDefinition"
              p:attributeName="customerName" />
          <bean parent="FieldDefinition"
                p:attributeName="customerNumber" />
          <bean parent="FieldDefinition"
                p:attributeName="unpaidBalance0to30" />
          <bean parent="FieldDefinition"
                p:attributeName="unpaidBalance31to60" />
          <bean parent="FieldDefinition"
                p:attributeName="unpaidBalance61to90" />
          <bean parent="FieldDefinition"
                p:attributeName="unpaidBalance91toSYSPR" />
          <bean parent="FieldDefinition"
                p:attributeName="unpaidBalanceSYSPRplus1orMore" />
      </list>
    </property>
  </bean>
</beans>
