  <bean id="EndowmentTransactionLine" parent="EndowmentTransactionLine-parentBean"/>

  <bean id="EndowmentTransactionLine-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.kfs.module.endow.businessobject.EndowmentTransactionLine"/>
    <property name="inquiryDefinition">
      <ref bean="EndowmentTransactionLine-inquiryDefinition"/>
    </property>
    <property name="lookupDefinition">
      <ref bean="EndowmentTransactionLine-lookupDefinition"/>
    </property>
    <property name="objectLabel" value="Transaction Line"/>
    <property name="attributes">
      <list>
        <ref bean="EndowmentTransactionLine-documentNumber"/>
        <ref bean="EndowmentTransactionLine-transactionLineTypeCode"/>
        <ref bean="EndowmentTransactionLine-transactionLineNumber"/>
        <ref bean="EndowmentTransactionLine-kemid"/>
        <ref bean="EndowmentTransactionLine-etranCode"/>
        <ref bean="EndowmentTransactionLine-transactionLineDescription"/>
        <ref bean="EndowmentTransactionLine-transactionIncomePrincipalIndicatorCode"/>
        <ref bean="EndowmentTransactionLine-transactionAmount"/>
        <ref bean="EndowmentTransactionLine-corpusIndicator"/>
        <ref bean="EndowmentTransactionLine-linePosted"/>
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->
  <bean id="EndowmentTransactionLine-documentNumber" parent="EndowmentTransactionLine-documentNumber-parentBean"/>

  <bean id="EndowmentTransactionLine-documentNumber-parentBean" abstract="true" parent="DocumentHeader-documentNumber">
    <property name="shortLabel" value="Application Doc #"/>
  </bean>
  <bean id="EndowmentTransactionLine-transactionAmount" parent="EndowmentTransactionLine-transactionAmount-parentBean"/>

  <bean id="EndowmentTransactionLine-transactionAmount-parentBean" abstract="true" parent="GenericAttributes-genericAmount">
    <property name="name" value="transactionAmount"/>
    <property name="label" value="Transaction Amount"/>
    <property name="shortLabel" value="Amount"/>
    <property name="required" value="true"/>
  </bean>
  <bean id="EndowmentTransactionLine-transactionLineTypeCode" parent="EndowmentTransactionLine-transactionLineTypeCode-parentBean"/>

  <bean id="EndowmentTransactionLine-transactionLineTypeCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="transactionLineTypeCode"/>
    <property name="forceUppercase" value="false"/>
    <property name="label" value="Transaction Line Type Code"/>
    <property name="shortLabel" value="Type"/>
    <property name="maxLength" value="1"/>
    <property name="control">
      <bean parent="TextControlDefinition" p:size="2"/>
    </property>
  </bean>
  <bean id="EndowmentTransactionLine-transactionLineNumber" parent="EndowmentTransactionLine-transactionLineNumber-parentBean"/>

  <bean id="EndowmentTransactionLine-transactionLineNumber-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="transactionLineNumber"/>
    <property name="label" value="Transaction Line Number"/>
    <property name="shortLabel" value="Number"/>
    <property name="maxLength" value="7"/>
    <property name="control">
      <bean parent="TextControlDefinition" p:size="8"/>
    </property>
  </bean>  
  <bean id="EndowmentTransactionLine-kemid" parent="EndowmentTransactionLine-kemid-parentBean"/>

  <bean id="EndowmentTransactionLine-kemid-parentBean" abstract="true" parent="KEMID-kemid">
    <property name="name" value="kemid"/>
    <property name="label" value="Kemid"/>
  </bean>
  <bean id="EndowmentTransactionLine-etranCode" parent="EndowmentTransactionLine-etranCode-parentBean"/>

  <bean id="EndowmentTransactionLine-etranCode-parentBean" abstract="true" parent="EndowmentTransactionCode-code">
    <property name="name" value="etranCode"/>
    <property name="label" value="Endowment Transaction Code"/>
  </bean>  
  <bean id="EndowmentTransactionLine-transactionLineDescription" parent="EndowmentTransactionLine-transactionLineDescription-parentBean"/>

  <bean id="EndowmentTransactionLine-transactionLineDescription-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="transactionLineDescription"/>
    <property name="label" value="Transaction Line Description"/>
    <property name="shortLabel" value="Description"/>
    <property name="maxLength" value="40"/>
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern"
            p:allowWhitespace="true" />
    </property>
    <property name="control">
      <bean parent="TextControlDefinition" p:size="42"/>
    </property>
  </bean>
  <bean id="EndowmentTransactionLine-transactionIncomePrincipalIndicatorCode" parent="EndowmentTransactionLine-transactionIncomePrincipalIndicatorCode-parentBean"/>

  <bean id="EndowmentTransactionLine-transactionIncomePrincipalIndicatorCode-parentBean" abstract="true" parent="IncomePrincipalIndicator-code">
    <property name="name" value="transactionIncomePrincipalIndicatorCode"/>
    <property name="label" value="Income/Principal Indicator"/>
  </bean>  
  <bean id="EndowmentTransactionLine-corpusIndicator" parent="EndowmentTransactionLine-corpusIndicator-parentBean"/>

  <bean id="EndowmentTransactionLine-corpusIndicator-parentBean" abstract="true" parent="GenericAttributes-genericBoolean">
    <property name="name" value="corpusIndicator"/>
    <property name="label" value="Corpus Indicator"/>
    <property name="shortLabel" value="Corpus Indicator"/>
  </bean>
  <bean id="EndowmentTransactionLine-linePosted" parent="EndowmentTransactionLine-linePosted-parentBean"/>

  <bean id="EndowmentTransactionLine-linePosted-parentBean" abstract="true" parent="GenericAttributes-genericBoolean">
    <property name="name" value="linePosted"/>
    <property name="label" value="Line Posted"/>
    <property name="shortLabel" value="Line Posted"/>
  </bean>

<!-- Business Object Inquiry Definition -->


  <bean id="EndowmentTransactionLine-inquiryDefinition" parent="EndowmentTransactionLine-inquiryDefinition-parentBean"/>

  <bean id="EndowmentTransactionLine-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
    <property name="title" value="Endowment Transaction Line Inquiry"/>
    <property name="inquirySections">
      <list>
        <bean parent="InquirySectionDefinition">
          <property name="title" value=""/>
          <property name="numberOfColumns" value="1"/>
          <property name="inquiryFields">
            <list>
              <bean parent="FieldDefinition" p:attributeName="transactionLineTypeCode"/>
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Business Object Lookup Definition -->


  <bean id="EndowmentTransactionLine-lookupDefinition" parent="EndowmentTransactionLine-lookupDefinition-parentBean"/>

  <bean id="EndowmentTransactionLine-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
    <property name="title" value="Endowment Transaction Line Lookup"/>
    
    <property name="defaultSort">
      <bean parent="SortDefinition">
      </bean>
    </property>
    <property name="lookupFields">
      <list>
        <bean parent="FieldDefinition" p:attributeName="transactionLineTypeCode"/>
      </list>
    </property>
    <property name="resultFields">
      <list>
        <bean parent="FieldDefinition" p:attributeName="transactionLineTypeCode"/>
      </list>
    </property>
  </bean>
</beans>
