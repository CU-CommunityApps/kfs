<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xmlns:dd="http://rice.kuali.org/dd"
    xsi:schemaLocation="
http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd
http://rice.kuali.org/dd http://rice.kuali.org/dd/dd.xsd">

  <bean id="KEMIDCurrentAvailableBalance" parent="KEMIDCurrentAvailableBalance-parentBean" />

  <bean id="KEMIDCurrentAvailableBalance-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.kfs.module.endow.businessobject.KEMIDCurrentAvailableBalance" />
    <property name="inquiryDefinition" >
      <ref bean="KEMIDCurrentAvailableBalance-inquiryDefinition" />
    </property>
    <property name="lookupDefinition" >
      <ref bean="KEMIDCurrentAvailableBalance-lookupDefinition" />
    </property>
    <property name="objectLabel" value="Current Balances" />
    <property name="attributes" >
      <list>
        <ref bean="KEMIDCurrentAvailableBalance-kemid" />
        <ref bean="KEMIDCurrentAvailableBalance-availableIncomeCash" />
        <ref bean="KEMIDCurrentAvailableBalance-availablePrincipalCash" />
        <ref bean="KEMIDCurrentAvailableBalance-availableTotalCash" />
        <ref bean="KEMIDCurrentAvailableBalance-kemidObj.shortTitle" />
        <ref bean="KEMIDCurrentAvailableBalance-kemidObj.purposeCode" />
        <ref bean="KEMIDCurrentAvailableBalance-kemidObj.active" />
        <ref bean="KEMIDCurrentAvailableBalance-balanceDate" />
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->


  <bean id="KEMIDCurrentAvailableBalance-kemid" parent="KEMIDCurrentAvailableBalance-kemid-parentBean" />

  <bean id="KEMIDCurrentAvailableBalance-kemid-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="kemid" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="KEMID" />
    <property name="shortLabel" value="KEMID" />
    <property name="maxLength" value="10" />
    <property name="required" value="true" />
    <property name="validationPattern">
      <bean parent="AlphaNumericValidationPattern"/>
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="11" />
    </property>
  </bean>
 
  <bean id="KEMIDCurrentAvailableBalance-availableIncomeCash" parent="KEMIDCurrentAvailableBalance-availableIncomeCash-parentBean" />

  <bean id="KEMIDCurrentAvailableBalance-availableIncomeCash-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="availableIncomeCash" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Income Available Cash" />
    <property name="shortLabel" value="Income" />
    <property name="maxLength" value="21" />
    <property name="required" value="true" />
    <property name="validationPattern">
      <bean parent="FixedPointValidationPattern"
            p:precision="19"
            p:scale="2" 
            />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="22" />
    </property>
  </bean>
  <bean id="KEMIDCurrentAvailableBalance-availablePrincipalCash" parent="KEMIDCurrentAvailableBalance-availablePrincipalCash-parentBean" />

  <bean id="KEMIDCurrentAvailableBalance-availablePrincipalCash-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="availablePrincipalCash" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Principal Available Cash" />
    <property name="shortLabel" value="Principal" />
    <property name="maxLength" value="21" />
    <property name="required" value="true" />
    <property name="validationPattern">
      <bean parent="FixedPointValidationPattern"
            p:precision="19"
            p:scale="2" 
            />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="22" />
    </property>
  </bean>
  <bean id="KEMIDCurrentAvailableBalance-availableTotalCash" parent="KEMIDCurrentAvailableBalance-availableTotalCash-parentBean" />

  <bean id="KEMIDCurrentAvailableBalance-availableTotalCash-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="availableTotalCash" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Total Available Cash" />
    <property name="shortLabel" value="Total" />
    <property name="maxLength" value="21" />
    <property name="required" value="true" />
    <property name="validationPattern">
      <bean parent="FixedPointValidationPattern"
            p:precision="19"
            p:scale="2" 
            />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="22" />
    </property>
  </bean>          
  <bean id="KEMIDCurrentAvailableBalance-kemidObj.purposeCode" parent="KEMIDCurrentAvailableBalance-kemidObj.purposeCode-parentBean" />

  <bean id="KEMIDCurrentAvailableBalance-kemidObj.purposeCode-parentBean" abstract="true" parent="KEMID-purposeCode">
    <property name="name" value="kemidObj.purposeCode" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Purpose" />
  </bean> 
  <bean id="KEMIDCurrentAvailableBalance-kemidObj.shortTitle" parent="KEMIDCurrentAvailableBalance-kemidObj.shortTitle-parentBean" />

  <bean id="KEMIDCurrentAvailableBalance-kemidObj.shortTitle-parentBean" abstract="true" parent="KEMID-shortTitle">
    <property name="name" value="kemidObj.shortTitle" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Short Title" />
  </bean> 
  <bean id="KEMIDCurrentAvailableBalance-balanceDate" parent="KEMIDCurrentAvailableBalance-balanceDate-parentBean" />

  <bean id="KEMIDCurrentAvailableBalance-balanceDate-parentBean" abstract="true" parent="GenericAttributes-genericDate">
    <property name="name" value="balanceDate" />
    <property name="label" value="Balance Date" />
    <property name="control" >
      <bean parent="SelectControlDefinition"
            p:valuesFinderClass="org.kuali.kfs.module.endow.businessobject.options.BalanceDateValuesFinder"
            p:includeKeyInLabel="false" />
    </property>
  </bean>
  <bean id="KEMIDCurrentAvailableBalance-kemidObj.active" parent="KEMIDCurrentAvailableBalance-kemidObj.active-parentBean" />

  <bean id="KEMIDCurrentAvailableBalance-kemidObj.active-parentBean" abstract="true" parent="KEMID-active">
    <property name="name" value="kemidObj.active" />
    <property name="label" value="Closed Indicator" />
  </bean>   
  
<!-- Business Object Inquiry Definition -->


  <bean id="KEMIDCurrentAvailableBalance-inquiryDefinition" parent="KEMIDCurrentAvailableBalance-inquiryDefinition-parentBean" />

  <bean id="KEMIDCurrentAvailableBalance-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
    <property name="title" value="KEMID Current Available Balance Inquiry" />
    <property name="inquirySections" >
      <list>
        <bean parent="InquirySectionDefinition">
          <property name="title" value="" />
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields" >
            <list>
              <bean parent="FieldDefinition"
                    p:attributeName="kemid" />
              <bean parent="FieldDefinition"
                    p:attributeName="availableIncomeCash" />
              <bean parent="FieldDefinition"
                    p:attributeName="availablePrincipalCash" />
              <bean parent="FieldDefinition"
                    p:attributeName="availableTotalCash" />                   
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Business Object Lookup Definition -->


  <bean id="KEMIDCurrentAvailableBalance-lookupDefinition" parent="KEMIDCurrentAvailableBalance-lookupDefinition-parentBean" />

  <bean id="KEMIDCurrentAvailableBalance-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
    <property name="title" value="KEMID Current Available Balances Lookup" />
    
    <property name="defaultSort" >
      <bean parent="SortDefinition">
        <property name="attributeNames" >
          <list>
            <value>kemid</value>
          </list>
        </property>
      </bean>
    </property>
    <property name="lookupFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="kemid" />
        <bean parent="FieldDefinition"
              p:attributeName="kemidObj.purposeCode" /> 
        <bean parent="FieldDefinition"
              p:attributeName="kemidObj.active" />              
        <bean parent="FieldDefinition"
              p:attributeName="balanceDate" />                                     
      </list>
    </property>
    <property name="resultFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="kemid" />
        <bean parent="FieldDefinition"
              p:attributeName="kemidObj.shortTitle" />              
        <bean parent="FieldDefinition"
              p:attributeName="kemidObj.purposeCode" />
        <bean parent="FieldDefinition"
              p:attributeName="availableIncomeCash" />
        <bean parent="FieldDefinition"
              p:attributeName="availablePrincipalCash" />
        <bean parent="FieldDefinition"
              p:attributeName="availableTotalCash" />                                          
        <bean parent="FieldDefinition"
              p:attributeName="kemidObj.active" />                                                                 
      </list>
    </property>
  </bean>
</beans>
