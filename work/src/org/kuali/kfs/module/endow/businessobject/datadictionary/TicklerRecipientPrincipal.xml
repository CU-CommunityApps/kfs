<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" 
		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
		xmlns:p="http://www.springframework.org/schema/p" 
		xmlns:dd="http://rice.kuali.org/dd" 
		xsi:schemaLocation="http://www.springframework.org/schema/beans         
			http://www.springframework.org/schema/beans/spring-beans-2.0.xsd         
			http://rice.kuali.org/dd         
			http://rice.kuali.org/dd/dd.xsd">

	<bean id="TicklerRecipientPrincipal" parent="TicklerRecipientPrincipal-parentBean" />

  	<bean id="TicklerRecipientPrincipal-parentBean" abstract="true" parent="BusinessObjectEntry">
    	<property name="businessObjectClass" value="org.kuali.kfs.module.endow.businessobject.TicklerRecipientPrincipal" />
    	<property name="inquiryDefinition">
      		<ref bean="TicklerRecipientPrincipal-inquiryDefinition" />
    	</property>
    	<property name="lookupDefinition">
      		<ref bean="TicklerRecipientPrincipal-lookupDefinition" />
    	</property>
    	<property name="titleAttribute" value="number" />
    	<property name="objectLabel" value="Tickler Recipient Principal" />
    	<property name="attributes">
      		<list>
        		<ref bean="TicklerRecipientPrincipal-principalId" />
		        <ref bean="TicklerRecipientPrincipal-number" />
		        <ref bean="TicklerRecipientPrincipal-active"/>
        		<ref bean="TicklerRecipientPrincipal-newCollectionRecord"/>
        		<ref bean="TicklerRecipientPrincipal-contact.principalName"/>
        		<ref bean="TicklerRecipientPrincipal-contectNameAndID"/> 
      		</list>
    	</property>
    	<property name="relationships">
	      <list>
	        <bean parent="RelationshipDefinition">
	          <property name="objectAttributeName" value="contact"/>
	          <property name="primitiveAttributes">
	            <list>
	              <bean parent="PrimitiveAttributeDefinition" p:sourceName="principalId" p:targetName="principalId"/>
	            </list>
	          </property>
			  <property name="supportAttributes">
	            <list>
	              <bean parent="SupportAttributeDefinition" p:identifier="true" p:sourceName="contact.principalName" p:targetName="principalName"/>
	              <bean parent="SupportAttributeDefinition" p:sourceName="contact.name" p:targetName="name"/>
	            </list>
	          </property>
	        </bean>
	      </list>
	    </property>
	</bean>

<!-- Attribute Definitions -->

	<!--  Principal ID -->
	  <bean id="TicklerRecipientPrincipal-principalId" parent="TicklerRecipientPrincipal-principalId-parentBean"/>
	
	  <bean id="TicklerRecipientPrincipal-principalId-parentBean" abstract="true" parent="PersonImpl-principalName">
	    <property name="label" value="Person"/>
	    <property name="shortLabel" value="Person"/>
	    <property name="maxLength" value="30"/>
	    <property name="control">
	      <bean parent="TextControlDefinition" p:size="32"/>
	    </property>
	    <property name="summary" value="&amp;nbsp;"/>
	    <property name="name" value="principalId"/>
	    <property name="description" value="The username of an individual user who receives the document ad hoc for the Action Requested"/>
	  </bean>

 	
	  <bean id="TicklerRecipientPrincipal-contectNameAndID" parent="TicklerRecipientPrincipal-contectNameAndID-parentBean"/>
	
	  <bean id="TicklerRecipientPrincipal-contectNameAndID-parentBean" abstract="true" parent="PersonImpl-principalName">
	    <property name="name" value="contectNameAndID"/>
	    <property name="label" value="Person"/>
	    <property name="shortLabel" value="Person"/>
	    <property name="required" value="false"/>
	  </bean>
 	

  <bean id="TicklerRecipientPrincipal-contact.principalName" parent="TicklerRecipientPrincipal-contact.principalName-parentBean"/>

  <dd:boAttributeRef id="TicklerRecipientPrincipal-contact.principalName-parentBean" parent="PersonImpl-principalName" abstract="true" 
  					attributeName="contact.principalName" 
  					label="Person" 
  					shortLabel="Person" 
  					required="false">
      <dd:user universalIdAttribute="principalId" userObjectAttribute="contact"/> 
  </dd:boAttributeRef>

	  
	<!-- Tickler Number -->
	<bean id="TicklerRecipientPrincipal-number" parent="TicklerRecipientPrincipal-number-parentBean" />

 	<bean id="TicklerRecipientPrincipal-number-parentBean" abstract="true" parent="Tickler-number-parentBean" />

	<!--  Active -->  
  	<bean id="TicklerRecipientPrincipal-active" parent="TicklerRecipientPrincipal-active-parentBean" />
		        
  	<bean id="TicklerRecipientPrincipal-active-parentBean" abstract="true" parent="GenericAttributes-genericBoolean">
  		<property name="name" value="active" />
    	<property name="label" value="Active Indicator" />
  	</bean>
	<bean id="TicklerRecipientPrincipal-newCollectionRecord" parent="TicklerRecipientPrincipal-newCollectionRecord-parentBean"/>

  	<bean id="TicklerRecipientPrincipal-newCollectionRecord-parentBean" abstract="true" parent="GenericAttributes-newCollectionRecord">
  	</bean>
  
  
<!-- Business Object Inquiry Definition -->

	<bean id="TicklerRecipientPrincipal-inquiryDefinition" parent="TicklerRecipientPrincipal-inquiryDefinition-parentBean" />

	<bean id="TicklerRecipientPrincipal-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
    	<property name="title" value="Tickler Recipient Principal Inquiry" />
    	<property name="inquirySections">
      		<list>
        		<bean parent="InquirySectionDefinition">
          			<property name="title" value="Tickler Security" />
          			<property name="numberOfColumns" value="1" />
          			<property name="inquiryFields">
            			<list>
			            	<bean parent="FieldDefinition" 
			              		  p:attributeName="principalId" />
			              	<bean parent="FieldDefinition" 
			              		  p:attributeName="number" />
			              	<bean parent="FieldDefinition" 
			              		  p:attributeName="active" />
            			</list>
          			</property>
        		</bean>
      		</list>
		</property>
	</bean>

<!-- Business Object Lookup Definition -->

	<bean id="TicklerRecipientPrincipal-lookupDefinition" parent="TicklerRecipientPrincipal-lookupDefinition-parentBean" />

  	<bean id="TicklerRecipientPrincipal-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
    	<property name="title" value="Tickler Recipient Principal Inquiry" />    
    	<property name="defaultSort">
	      	<bean parent="SortDefinition" />
    	</property>
    	<property name="lookupFields">
      		<list>
            	<bean parent="FieldDefinition" 
              		  p:attributeName="contact.principalName" />
              	<bean parent="FieldDefinition" 
              		  p:attributeName="number" />
              	<bean parent="FieldDefinition" 
              		  p:attributeName="active" />
      		</list>
    	</property>    	
    	<property name="resultFields">
        	<list>
            	<bean parent="FieldDefinition" 
              		  p:attributeName="contact.principalName" />
              	<bean parent="FieldDefinition" 
              		  p:attributeName="number" />
              	<bean parent="FieldDefinition" 
              		  p:attributeName="active" />
      		</list>
    	</property>
	</bean>
	
</beans>