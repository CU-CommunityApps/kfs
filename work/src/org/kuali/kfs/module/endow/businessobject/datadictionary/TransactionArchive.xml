  <bean id="TransactionArchive-kemid-parentBean" abstract="true" parent="KEMID-kemid">
  </bean>
  
  <bean id="TransactionArchive-kemidObj.shortTitle" parent="TransactionArchive-kemidObj.shortTitle-parentBean" />
  <bean id="TransactionArchive-kemidObj.shortTitle-parentBean" abstract="true" parent="KEMID-shortTitle">
    <property name="name" value="kemidObj.shortTitle" />
