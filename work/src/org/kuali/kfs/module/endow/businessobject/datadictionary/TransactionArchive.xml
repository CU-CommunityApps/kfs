<!-- Business Object Inquiry Definition -->

  <bean id="TransactionArchive-inquiryDefinition" parent="TransactionArchive-inquiryDefinition-parentBean" />
  <bean id="TransactionArchive-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
    <property name="title" value="Endowment Transaction Archive Inquiry" />
    <property name="inquirySections" >
      <list>
        <bean parent="InquirySectionDefinition">
          <property name="title" value="Transaction Archive Detail" />
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields" >
            <list>
              <bean parent="FieldDefinition"
                    p:attributeName="documentNumber" />
              <bean parent="FieldDefinition"
                    p:attributeName="lineNumber" />
              <bean parent="FieldDefinition"
                    p:attributeName="lineTypeCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="typeCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="subTypeCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="srcTypeCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="kemid" />
              <bean parent="FieldDefinition"
                    p:attributeName="etranObj.name" />
              <bean parent="FieldDefinition"
                    p:attributeName="postedDate" />
              <bean parent="FieldDefinition"
                    p:attributeName="incomePrincipalIndicatorCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="incomeCashAmount" />
              <bean parent="FieldDefinition"
                    p:attributeName="principalCashAmount" />
              <bean parent="FieldDefinition"
                    p:attributeName="description" />
              <bean parent="FieldDefinition"
                    p:attributeName="lineDescription" />
              <bean parent="FieldDefinition"
                    p:attributeName="corpusIndicator" />
              <bean parent="FieldDefinition"
                    p:attributeName="corpusAmount" />           
            </list>
          </property>
        </bean>
        <bean parent="InquirySectionDefinition">
          <property name="title" value="Transaction Security" />
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields" >
            <list>
              <bean parent="FieldDefinition"
                    p:attributeName="archiveSecurity.securityId" />
              <bean parent="FieldDefinition"
                    p:attributeName="archiveSecurity.registrationCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="archiveSecurity.etranCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="archiveSecurity.unitsHeld"  />
              <bean parent="FieldDefinition"
                    p:attributeName="archiveSecurity.unitValue" />
              <bean parent="FieldDefinition"
                    p:attributeName="archiveSecurity.holdingCost" />
              <bean parent="FieldDefinition"
                    p:attributeName="archiveSecurity.longTermGainLoss" />
              <bean parent="FieldDefinition"
                    p:attributeName="archiveSecurity.shortTermGainLoss" />                          
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Business Object Lookup Definition -->

  <bean id="TransactionArchive-lookupDefinition" parent="TransactionArchive-lookupDefinition-parentBean" />
  <bean id="TransactionArchive-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
    <property name="title" value="Endowment Transaction Archive Lookup" />
    <property name="defaultSort" >
      <bean parent="SortDefinition">
        <property name="attributeNames" >
          <list>
            <value>documentNumber</value>
          </list>
        </property>
      </bean>
    </property>
    <property name="lookupFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="kemid" />
        <bean parent="FieldDefinition"
              p:attributeName="typeCode" />
        <bean parent="FieldDefinition"
              p:attributeName="etranCode" />
        <bean parent="FieldDefinition"
              p:attributeName="archiveSecurity.securityId" />
        <bean parent="FieldDefinition"
              p:attributeName="postedDate" />
        <bean parent="FieldDefinition"
              p:attributeName="greaterAmount" />
        <bean parent="FieldDefinition"
              p:attributeName="lessAmount" />
      </list>
    </property>
    <property name="resultFields">
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="transactionArchiveViewer" />
        <bean parent="FieldDefinition"
              p:attributeName="documentNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="kemid" />
        <bean parent="FieldDefinition"
              p:attributeName="typeCode" />
        <bean parent="FieldDefinition"
              p:attributeName="postedDate" />
        <bean parent="FieldDefinition"
              p:attributeName="incomeCashAmount" />
        <bean parent="FieldDefinition"
              p:attributeName="principalCashAmount" />
        <bean parent="FieldDefinition"
              p:attributeName="etranCode" />
        <bean parent="FieldDefinition"
              p:attributeName="archiveSecurity.securityId" />
        <bean parent="FieldDefinition"
              p:attributeName="archiveSecurity.unitsHeld" 
              p:forceInquiry="false" />
        <bean parent="FieldDefinition"
              p:attributeName="archiveSecurity.holdingCost"
              p:forceInquiry="false" />
      </list>
    </property>
  </bean>
</beans>
