<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="EndowmentSourceTransactionSecurity" parent="EndowmentSourceTransactionSecurity-parentBean" />

  <bean id="EndowmentSourceTransactionSecurity-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.kfs.module.endow.businessobject.EndowmentSourceTransactionSecurity" />
    <property name="inquiryDefinition" >
      <ref bean="EndowmentSourceTransactionSecurity-inquiryDefinition" />
    </property>
    <property name="titleAttribute" value="securityId" />
    <property name="objectLabel" value="Transaction Security" />
    <property name="attributes" >
      <list>
        <ref bean="EndowmentSourceTransactionSecurity-securityId" />
        <ref bean="EndowmentSourceTransactionSecurity-registrationCode" />
        <ref bean="EndowmentSourceTransactionSecurity-securityLineTypeCode" />
      </list>
    </property>
  </bean>
  
  <!-- Attribute Definitions -->


  <bean id="EndowmentSourceTransactionSecurity-Security-id" parent="EndowmentSourceTransactionSecurity-securityId-parentBean" />

  <bean id="EndowmentSourceTransactionSecurity-securityId-parentBean" abstract="true" parent="Security-id">
    <property name="name" value="Security-id" />
  </bean>
  
   <bean id="EndowmentSourceTransactionSecurity-Security-securityClassCode" parent="EndowmentSourceTransactionSecurity-securityClassCode-parentBean" />

  <bean id="EndowmentSourceTransactionSecurity-securityClassCode-parentBean" abstract="true" parent="ClassCode-code">
    <property name="name" value="securityClassCode" />
  </bean>
  
  <bean id="EndowmentSourceTransactionSecurity-Security-taxLotIndicator" parent="EndowmentSourceTransactionSecurity-taxLotIndicator-parentBean" />

  <bean id="EndowmentSourceTransactionSecurity-taxLotIndicator-parentBean" abstract="true" parent="ClassCode-taxLotIndicator">
    <property name="name" value="taxLotIndicator" />
  </bean>
  
    <bean id="EndowmentSourceTransactionSecurity-Security-id" parent="EndowmentSourceTransactionSecurity-securityId-parentBean" />

  <bean id="EndowmentSourceTransactionSecurity-securityId-parentBean" abstract="true" parent="Security-id">
    <property name="name" value="Security-id" />
  </bean>  
  
    <bean id="EndowmentSourceTransactionSecurity-Security-id" parent="EndowmentSourceTransactionSecurity-securityId-parentBean" />

  <bean id="EndowmentSourceTransactionSecurity-securityId-parentBean" abstract="true" parent="Security-id">
    <property name="name" value="Security-id" />
  </bean> 
  
    <bean id="EndowmentSourceTransactionSecurity-active" parent="EndowmentSourceTransactionSecurity-active-parentBean" />

  <bean id="EndowmentSourceTransactionSecurity-active-parentBean" abstract="true" parent="GenericAttributes-activeIndicator">
    <property name="name" value="active" />
  </bean>
  
</beans>  