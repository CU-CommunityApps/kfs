<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" 
		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
		xmlns:p="http://www.springframework.org/schema/p" 
		xmlns:dd="http://rice.kuali.org/dd" 
		xsi:schemaLocation="http://www.springframework.org/schema/beans         
			http://www.springframework.org/schema/beans/spring-beans-2.0.xsd         
			http://rice.kuali.org/dd         
			http://rice.kuali.org/dd/dd.xsd">

	<bean id="KemidBenefittingOrganization" parent="KemidBenefittingOrganization-parentBean" />

  	<bean id="KemidBenefittingOrganization-parentBean" abstract="true" parent="BusinessObjectEntry">
    	<property name="businessObjectClass" value="org.kuali.kfs.module.endow.businessobject.KemidBenefittingOrganization" />
    	<property name="inquiryDefinition">
      		<ref bean="KemidBenefittingOrganization-inquiryDefinition" />
    	</property>
    	<property name="lookupDefinition">
      		<ref bean="KemidBenefittingOrganization-lookupDefinition" />
    	</property>
    	<property name="titleAttribute" value="benefittingOrgSeqNumber" />
    	<property name="objectLabel" value="Benefitting Organization" />
    	<property name="attributes">
      		<list>
        		<ref bean="KemidBenefittingOrganization-kemid" />
        		<ref bean="KemidBenefittingOrganization-benefittingOrgSeqNumber" />
		        <ref bean="KemidBenefittingOrganization-benefittingOrgCode" />
		        <ref bean="KemidBenefittingOrganization-benefittingChartCode" />
		        <ref bean="KemidBenefittingOrganization-benefitPrecent"/>
		        <ref bean="KemidBenefittingOrganization-startDate"/>
		        <ref bean="KemidBenefittingOrganization-lastChangeDate"/>
		        <ref bean="KemidBenefittingOrganization-active"/>
        		<ref bean="KemidBenefittingOrganization-newCollectionRecord"/>	   
      		</list>
    	</property>
	</bean>

<!-- Attribute Definitions -->

	<bean id="KemidBenefittingOrganization-kemid" parent="KemidBenefittingOrganization-kemid-parentBean" />

 	<bean id="KemidBenefittingOrganization-kemid-parentBean" abstract="true" parent="KEMID-kemid">
    	<property name="name" value="kemid" />
    	<property name="required" value="true" />
  	</bean>
	<bean id="KemidBenefittingOrganization-benefittingOrgSeqNumber" parent="KemidBenefittingOrganization-benefittingOrgSeqNumber-parentBean" />

 	<bean id="KemidBenefittingOrganization-benefittingOrgSeqNumber-parentBean" abstract="true" parent="AttributeDefinition">
    	<property name="name" value="benefittingOrgSeqNumber" />
    	<property name="label" value="Benefitting Organization Sequence Number" />
    	<property name="shortLabel" value="Benefitting Org Seq Number" />
    	<property name="maxLength" value="3" />
    	<property name="validationPattern">
      		<bean parent="NumericValidationPattern" />
    	</property>
    	<property name="required" value="true" />
    	<property name="control">
      		<bean parent="TextControlDefinition" 
      			  p:size="4" />
    	</property>
  	</bean>
	<bean id="KemidBenefittingOrganization-benefittingOrgCode" parent="KemidBenefittingOrganization-benefittingOrgCode-parentBean" />

 	<bean id="KemidBenefittingOrganization-benefittingOrgCode-parentBean" abstract="true" parent="Organization-organizationCode">
    	<property name="name" value="benefittingOrgCode" />
    	<property name="label" value="Benefitting Organization" />
  	</bean>
	<bean id="KemidBenefittingOrganization-benefittingChartCode" parent="KemidBenefittingOrganization-benefittingChartCode-parentBean" />

 	<bean id="KemidBenefittingOrganization-benefittingChartCode-parentBean" abstract="true" parent="Chart-chartOfAccountsCode">
    	<property name="name" value="benefittingChartCode" />
    	<property name="label" value="Benefitting Chart" />
  	</bean>  	
	<bean id="KemidBenefittingOrganization-benefitPrecent" parent="KemidBenefittingOrganization-benefitPrecent-parentBean" />

 	<bean id="KemidBenefittingOrganization-benefitPrecent-parentBean" abstract="true" parent="AttributeDefinition">
    	<property name="name" value="benefitPrecent" />
    	<property name="label" value="Benefit Percent" />
    	<property name="shortLabel" value="Benefit Percent" />
    	<property name="maxLength" value="6" />
    	<property name="validationPattern">
            <bean parent="FixedPointValidationPattern"
                  p:precision="3"
                  p:scale="2" 
                  p:allowNegative="true"/>
        </property>
        <property name="control">
            <bean parent="TextControlDefinition" p:size="8"/>
        </property>
  	</bean>
  	<bean id="KemidBenefittingOrganization-startDate" parent="KemidBenefittingOrganization-startDate-parentBean" />
		        
  	<bean id="KemidBenefittingOrganization-startDate-parentBean" abstract="true" parent="GenericAttributes-genericDate">
  		<property name="name" value="startDate" />
    	<property name="label" value="Start Date" />
  	</bean>
  	<bean id="KemidBenefittingOrganization-lastChangeDate" parent="KemidBenefittingOrganization-lastChangeDate-parentBean" />
		        
  	<bean id="KemidBenefittingOrganization-lastChangeDate-parentBean" abstract="true" parent="GenericAttributes-genericDate">
  		<property name="name" value="lastChangeDate" />
    	<property name="label" value="Last Change Date" />
  	</bean>  	
  	<bean id="KemidBenefittingOrganization-active" parent="KemidBenefittingOrganization-active-parentBean" />
		        
  	<bean id="KemidBenefittingOrganization-active-parentBean" abstract="true" parent="GenericAttributes-genericBoolean">
  		<property name="name" value="active" />
    	<property name="label" value="Active" />
  	</bean>
	<bean id="KemidBenefittingOrganization-newCollectionRecord" parent="KemidBenefittingOrganization-newCollectionRecord-parentBean"/>

  	<bean id="KemidBenefittingOrganization-newCollectionRecord-parentBean" abstract="true" parent="GenericAttributes-newCollectionRecord">
  	</bean>
  
<!-- Business Object Inquiry Definition -->

	<bean id="KemidBenefittingOrganization-inquiryDefinition" parent="KemidBenefittingOrganization-inquiryDefinition-parentBean" />

	<bean id="KemidBenefittingOrganization-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
    	<property name="title" value="Benefitting Organization Inquiry" />
    	<property name="inquirySections">
      		<list>
        		<bean parent="InquirySectionDefinition">
          			<property name="title" value="" />
          			<property name="numberOfColumns" value="1" />
          			<property name="inquiryFields">
            			<list>
              				<bean parent="FieldDefinition" 
              				      p:attributeName="kemid" />
              				<bean parent="FieldDefinition" 
              				      p:attributeName="benefittingChartCode" />               				
              				<bean parent="FieldDefinition" 
              				      p:attributeName="benefittingOrgCode" />             				      
              				<bean parent="FieldDefinition" 
              				      p:attributeName="benefitPrecent" />
              				<bean parent="FieldDefinition" 
              				      p:attributeName="startDate" />
              				<bean parent="FieldDefinition" 
              				      p:attributeName="lastChangeDate" />         				      
              				<bean parent="FieldDefinition" 
              				      p:attributeName="active" />            				      
            			</list>
          			</property>
        		</bean>
      		</list>
		</property>
	</bean>

<!-- Business Object Lookup Definition -->

	<bean id="KemidBenefittingOrganization-lookupDefinition" parent="KemidBenefittingOrganization-lookupDefinition-parentBean" />

  	<bean id="KemidBenefittingOrganization-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
    	<property name="title" value="Benefitting Organization Lookup" />    
    	<property name="defaultSort">
	      	<bean parent="SortDefinition" />
    	</property>
    	<property name="lookupFields">
      		<list>
            	<bean parent="FieldDefinition" 
              		  p:attributeName="kemid" />
              	<bean parent="FieldDefinition" 
              		  p:attributeName="benefittingOrgCode" />         		  
              	<bean parent="FieldDefinition" 
              		  p:attributeName="active" />
      		</list>
    	</property>    	
    	<property name="resultFields">
        	<list>
            	<bean parent="FieldDefinition" 
              		  p:attributeName="kemid" />
              	<bean parent="FieldDefinition" 
              		  p:attributeName="benefittingOrgCode" />
              	<bean parent="FieldDefinition" 
              		  p:attributeName="active" />
      		</list>
    	</property>
	</bean>
	
</beans>