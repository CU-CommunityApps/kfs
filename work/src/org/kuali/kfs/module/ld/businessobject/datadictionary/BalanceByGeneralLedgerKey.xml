<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "../dataDictionary-1_0.dtd">
<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.module.labor.bo.BalanceByGeneralLedgerKey">


        <inquiry>
            <title>Balance By General Ledger Key Inquiry</title>
             
            <inquirySections>
                <inquirySection title="Header Text">
                    <inquiryFields>
                        <field attributeName="accountLineAnnualBalanceAmount" />
                        <field attributeName="beginningAndContractsAndGrantsAmount" />
                        <field attributeName="emplid" />
                        <field attributeName="financialObjectCode" />
                        <field attributeName="financialSubObjectCode" />
                        <field attributeName="july1BudgetAmount" />
                        <field attributeName="personUniversalIdentifier" />
                        <field attributeName="positionNumber" />
                        <field attributeName="subAccountNumber" />
                    </inquiryFields>
                </inquirySection>
            </inquirySections>  
        </inquiry>

        <lookup>
           <lookupableID>CurrentYearsFundsLookupable</lookupableID>
            <title>Balance By General Ledger Key Lookup</title>

            <instructions>Look up a Balance By General Ledger Key</instructions>

            <defaultSort sortAscending="true">
                <sortAttributes>
                    <sortAttribute attributeName="subAccountNumber" />
                    <sortAttribute attributeName="financialObjectCode" />
                    <sortAttribute attributeName="financialSubObjectCode" />
                </sortAttributes>
            </defaultSort>

            <lookupFields>
                <lookupField attributeName="accountLineAnnualBalanceAmount" required="false" />
                <lookupField attributeName="beginningAndContractsAndGrantsAmount" required="false" />
                <lookupField attributeName="emplid" required="false" />
                <lookupField attributeName="financialObjectCode" required="false" />
                <lookupField attributeName="financialSubObjectCode" required="false" />
                <lookupField attributeName="july1BudgetAmount" required="false" />
                <lookupField attributeName="personUniversalIdentifier" required="false" />
                <lookupField attributeName="positionNumber" required="false" />
                <lookupField attributeName="subAccountNumber" required="false" />
            </lookupFields>

            <resultFields>
                <field attributeName="accountLineAnnualBalanceAmount" />
                <field attributeName="beginningAndContractsAndGrantsAmount" />
                <field attributeName="emplid" />
                <field attributeName="financialObjectCode" />
                <field attributeName="financialSubObjectCode" />
                <field attributeName="july1BudgetAmount" />
                <field attributeName="personUniversalIdentifier" />
                <field attributeName="positionNumber" />
                <field attributeName="subAccountNumber" />
            </resultFields>
        </lookup>

        <attributes>

            <attribute name="accountLineAnnualBalanceAmount" forceUppercase="true">
                <label>Account Line Annual Balance Amount</label>
                <shortLabel>Amount</shortLabel>

                <maxLength>19</maxLength>
                <validationPattern><floatingPoint /></validationPattern>

                <control>
                    <text size="21" />
                </control>
			</attribute>

            <attribute name="beginningAndContractsAndGrantsAmount" forceUppercase="true">
                <label>Beginning And Contracts And Grants Amount</label>
                <shortLabel>Amount</shortLabel>

                <maxLength>19</maxLength>
                <validationPattern><floatingPoint /></validationPattern>

                <control>
                    <text size="21" />
                </control>
			</attribute>

            <attribute name="emplid" forceUppercase="true">
                <label>Emplid</label>
                <shortLabel>Emplid</shortLabel>

                <maxLength>11</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="13" />
                </control>
			</attribute>

            <attribute name="financialObjectCode" forceUppercase="true">
                <label>Financial Object Code</label>
                <shortLabel>Code</shortLabel>

                <maxLength>4</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="6" />
                </control>
			</attribute>

            <attribute name="financialSubObjectCode" forceUppercase="true">
                <label>Financial Sub Object Code</label>
                <shortLabel>Code</shortLabel>

                <maxLength>3</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="5" />
                </control>
			</attribute>

            <attribute name="july1BudgetAmount" forceUppercase="true">
                <label>July1 Budget Amount</label>
                <shortLabel>Amount</shortLabel>

                <maxLength>19</maxLength>
                <validationPattern><floatingPoint /></validationPattern>

                <control>
                    <text size="21" />
                </control>
			</attribute>

            <attribute name="personUniversalIdentifier" forceUppercase="true">
                <label>Person Universal Identifier</label>
                <shortLabel>Identifier</shortLabel>

                <maxLength>10</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="12" />
                </control>
			</attribute>

            <attribute name="positionNumber" forceUppercase="true">
                <label>Position Number</label>
                <shortLabel>Number</shortLabel>

                <maxLength>8</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="10" />
                </control>
			</attribute>

            <attribute name="subAccountNumber" forceUppercase="true">
                <label>Sub Account Number</label>
                <shortLabel>Number</shortLabel>

                <maxLength>5</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="7" />
                </control>
			</attribute>

            <attributeReference name="versionNumber" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="versionNumber" />

        </attributes>
    </businessObject>
</dictionaryEntry>
