<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "../dataDictionary-1_0.dtd">
<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.module.labor.bo.PositionObjectGroup">
        <customAttributeEnabled>false</customAttributeEnabled>

        <inquiry>
            <title>Position Object Group Inquiry</title>

            <inquiryFields>
                <field attributeName="positionObjectGroupCode" />
                <field attributeName="positionObjectGroupName" />
                <field attributeName="rowActiveIndicator" />
            </inquiryFields>
        </inquiry>

        <lookup>
            <title>Position Object Group Lookup</title>

			<menubar><![CDATA[<a href="maintenance.do?methodToCall=start&businessObjectClassName=org.kuali.module.labor.bo.PositionObjectGroup">Create New</a>]]></menubar>

            <instructions>Look up a Position Object Group</instructions>

            <defaultSort sortAscending="true">
                <sortAttributes>
                    <sortAttribute attributeName="positionObjectGroupCode" />
                </sortAttributes>
            </defaultSort>

            <lookupFields>
                <lookupField attributeName="positionObjectGroupCode" required="false" />
                <lookupField attributeName="positionObjectGroupName" required="false" />
                <lookupField attributeName="rowActiveIndicator" required="false" />
            </lookupFields>

            <resultFields>
                <field attributeName="positionObjectGroupCode" />
                <field attributeName="positionObjectGroupName" />
                <field attributeName="rowActiveIndicator" />
            </resultFields>
        </lookup>

        <attributes>

            <attribute name="positionObjectGroupCode" forceUppercase="true">
                <label>Position Object Group Code</label>
                <shortLabel>Code</shortLabel>

                <maxLength>2</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="4" />
                </control>

                <summary>positionObjectGroupCode</summary>
                <description>positionObjectGroupCode description...</description>
			</attribute>

            <attribute name="positionObjectGroupName" forceUppercase="true">
                <label>Position Object Group Name</label>
                <shortLabel>Name</shortLabel>

                <maxLength>40</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="42" />
                </control>

                <summary>positionObjectGroupName</summary>
                <description>positionObjectGroupName description...</description>
			</attribute>

            <attribute name="rowActiveIndicator" forceUppercase="true">
                <label>Row Active Indicator</label>
                <shortLabel>Indicator</shortLabel>

                <maxLength>1</maxLength>

                <control>
                    <checkbox />
                </control>

                <summary>rowActiveIndicator</summary>
                <description>rowActiveIndicator description...</description>
			</attribute>

            <attributeReference name="versionNumber" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="versionNumber" />

        </attributes>
    </businessObject>
</dictionaryEntry>
