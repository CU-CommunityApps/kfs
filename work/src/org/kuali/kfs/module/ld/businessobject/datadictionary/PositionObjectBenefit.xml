<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="PositionObjectBenefit" parent="PositionObjectBenefit-parentBean" />

  <bean id="PositionObjectBenefit-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.module.labor.bo.PositionObjectBenefit" />
    <property name="inquiryDefinition" >
      <ref bean="PositionObjectBenefit-inquiryDefinition" />
    </property>
    <property name="lookupDefinition" >
      <ref bean="PositionObjectBenefit-lookupDefinition" />
    </property>
    <property name="attributes" >
      <list>
        <ref bean="PositionObjectBenefit-universityFiscalYear" />
        <ref bean="PositionObjectBenefit-chartOfAccountsCode" />
        <ref bean="PositionObjectBenefit-financialObjectCode" />
        <ref bean="PositionObjectBenefit-financialObjectBenefitsTypeCode" />
        <ref bean="PositionObjectBenefit-versionNumber" />
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->


  <bean id="PositionObjectBenefit-universityFiscalYear" parent="PositionObjectBenefit-universityFiscalYear-parentBean" />

  <bean id="PositionObjectBenefit-universityFiscalYear-parentBean" abstract="true" parent="GenericAttributes-genericFiscalYear">
    <property name="name" value="universityFiscalYear" />
    <property name="required" value="true" />
  </bean>
  <bean id="PositionObjectBenefit-chartOfAccountsCode" parent="PositionObjectBenefit-chartOfAccountsCode-parentBean" />

  <bean id="PositionObjectBenefit-chartOfAccountsCode-parentBean" abstract="true" parent="Chart-chartOfAccountsCode">
  </bean>
  <bean id="PositionObjectBenefit-financialObjectCode" parent="PositionObjectBenefit-financialObjectCode-parentBean" />

  <bean id="PositionObjectBenefit-financialObjectCode-parentBean" abstract="true" parent="ObjectCode-financialObjectCode">
  </bean>
  <bean id="PositionObjectBenefit-financialObjectBenefitsTypeCode" parent="PositionObjectBenefit-financialObjectBenefitsTypeCode-parentBean" />

  <bean id="PositionObjectBenefit-financialObjectBenefitsTypeCode-parentBean" abstract="true" parent="BenefitsType-positionBenefitTypeCode">
    <property name="name" value="financialObjectBenefitsTypeCode" />
  </bean>
  <bean id="PositionObjectBenefit-versionNumber" parent="PositionObjectBenefit-versionNumber-parentBean" />

  <bean id="PositionObjectBenefit-versionNumber-parentBean" abstract="true" parent="GenericAttributes-versionNumber">
  </bean>

<!-- Business Object Inquiry Definition -->


  <bean id="PositionObjectBenefit-inquiryDefinition" parent="PositionObjectBenefit-inquiryDefinition-parentBean" />

  <bean id="PositionObjectBenefit-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
    <property name="title" value="Labor Object Code Benefits Inquiry" />
    <property name="inquirySections" >
      <list>
        <bean parent="InquirySectionDefinition">
          <property name="title" value="Header Text" />
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields" >
            <list>
              <bean parent="FieldDefinition"
                    p:attributeName="universityFiscalYear" />
              <bean parent="FieldDefinition"
                    p:attributeName="chartOfAccountsCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="financialObjectCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="financialObjectBenefitsTypeCode" />
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Business Object Lookup Definition -->


  <bean id="PositionObjectBenefit-lookupDefinition" parent="PositionObjectBenefit-lookupDefinition-parentBean" />

  <bean id="PositionObjectBenefit-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
    <property name="title" value="Labor Object Code Benefits Lookup" />
    <property name="lookupableID" value="positionObjectBenefitLookupable" />
    <property name="instructions" value="Lookup a Labor Object Code Benefit" />
    <property name="defaultSort" >
      <bean parent="SortDefinition">
      </bean>
    </property>
    <property name="lookupFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="universityFiscalYear" />
        <bean parent="FieldDefinition"
              p:attributeName="chartOfAccountsCode" />
        <bean parent="FieldDefinition"
              p:attributeName="financialObjectCode" />
        <bean parent="FieldDefinition"
              p:attributeName="financialObjectBenefitsTypeCode" />
      </list>
    </property>
    <property name="resultFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="universityFiscalYear" />
        <bean parent="FieldDefinition"
              p:attributeName="chartOfAccountsCode" />
        <bean parent="FieldDefinition"
              p:attributeName="financialObjectCode" />
        <bean parent="FieldDefinition"
              p:attributeName="financialObjectBenefitsTypeCode" />
      </list>
    </property>
  </bean>
</beans>
