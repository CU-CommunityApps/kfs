                <maxLength>5</maxLength>
                <validationPattern><numeric /></validationPattern>

                <control>
                    <text size="7" />
                </control>

                <summary>transactionLedgerEntrySequenceNumber</summary>
                <description>transactionLedgerEntrySequenceNumber description...</description>
