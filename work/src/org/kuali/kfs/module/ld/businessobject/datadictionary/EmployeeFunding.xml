<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "../dataDictionary-1_0.dtd">
<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.module.labor.bo.EmployeeFunding">


        <inquiry>
            <title>Employee Funding Balance Inquiry</title>

            <inquirySections><inquirySection title="Header Text"><inquiryFields>
				<field attributeName="month1Amount" />
            </inquiryFields></inquirySection></inquirySections>
        </inquiry>

        <lookup>
	        <lookupableID>EmployeeFundingLookupable</lookupableID>	        
            <title>Employee Funding Balance Inquiry</title>

            <instructions>Employee Funding Look up</instructions>

            <defaultSort sortAscending="true">
                <sortAttributes>
                    <sortAttribute attributeName="chartOfAccountsCode" />
                    <sortAttribute attributeName="accountNumber" />
                    <sortAttribute attributeName="financialObjectCode" />
                    <sortAttribute attributeName="financialSubObjectCode" />
                </sortAttributes>
            </defaultSort>
            
            <lookupFields>
			    <lookupField attributeName="universityFiscalYear" required="true">
                    <defaultValueFinderClass>org.kuali.kfs.lookup.valuefinder.FiscalYearFinder</defaultValueFinderClass>
                </lookupField>
                <lookupField attributeName="emplid" required="true"/>
                
                <lookupField attributeName="chartOfAccountsCode" />
                <lookupField attributeName="accountNumber" />
                <lookupField attributeName="subAccountNumber" />
                <lookupField attributeName="financialObjectCode" />
                <lookupField attributeName="financialSubObjectCode" />
                
                <lookupField attributeName="dummyBusinessObject.pendingEntryOption">
                    <defaultValueFinderClass>org.kuali.module.gl.web.optionfinder.GLPendingEntryOptionFinder</defaultValueFinderClass>
                </lookupField>
            </lookupFields>

            <resultFields>				
				<field attributeName="personName" />
				<field attributeName="emplid" />
				<field attributeName="universityFiscalYear"/> 
                <field attributeName="chartOfAccountsCode" />
                <field attributeName="accountNumber" />
                <field attributeName="subAccountNumber" />
                <field attributeName="financialObjectCode" />
                <field attributeName="financialSubObjectCode" />
				<field attributeName="positionNumber" />
				<field attributeName="csfDeleteCode" />				
				<field attributeName="csfFundingStatusCode" />
				<field attributeName="csfFullTimeEmploymentQuantity" />
				<field attributeName="csfAmount" />	
				<field attributeName="currentAmount" />				
				<field attributeName="outstandingEncumbrance" />
				<field attributeName="totalAmount" />
												
            </resultFields>
        </lookup>

        <attributes>
            <attributeReference name="versionNumber" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="versionNumber" />
            <attributeReference name="emplid" sourceClassName="org.kuali.core.bo.user.UniversalUser" sourceAttributeName="personPayrollIdentifier"/>
            <attributeReference name="personName" sourceClassName="org.kuali.core.bo.user.UniversalUser" sourceAttributeName="personName" />
            <attributeReference name="positionNumber" sourceClassName="org.kuali.module.labor.bo.PositionData" sourceAttributeName="positionNumber"/>

            <attributeReference name="universityFiscalYear" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="genericFiscalYear"/>
            <attributeReference name="subAccountNumber" sourceClassName="org.kuali.module.chart.bo.SubAccount" sourceAttributeName="subAccountNumber" />          
			<attributeReference name="accountNumber" sourceClassName="org.kuali.module.chart.bo.Account" sourceAttributeName="accountNumber" />
			<attributeReference name="chartOfAccountsCode" sourceClassName="org.kuali.module.chart.bo.Chart" sourceAttributeName="chartOfAccountsCode" />
			<attributeReference name="financialBalanceTypeCode" sourceClassName="org.kuali.module.chart.bo.codes.BalanceTyp" sourceAttributeName="code">
				<control>
					<hidden />
				</control>
			</attributeReference>
			
			<attributeReference name="financialObjectCode" sourceClassName="org.kuali.module.chart.bo.ObjectCode" sourceAttributeName="financialObjectCode" />
			<attributeReference name="financialObjectTypeCode" sourceClassName="org.kuali.module.chart.bo.ObjectType" sourceAttributeName="code" />
			<attributeReference name="financialSubObjectCode" sourceClassName="org.kuali.module.chart.bo.SubObjCd" sourceAttributeName="financialSubObjectCode" />

            <attributeReference name="outstandingEncumbrance" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="genericAmount">
                <label>Encumbance Amount</label>
                <shortLabel>Encumb</shortLabel>
                <formatterClass>org.kuali.core.web.format.CurrencyFormatter</formatterClass>
            </attributeReference>
			
		    <attributeReference name="currentAmount" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="genericAmount">
                <label>Current Amount</label>
                <shortLabel>Current</shortLabel>
                <formatterClass>org.kuali.core.web.format.CurrencyFormatter</formatterClass>
            </attributeReference>

			<attributeReference name="totalAmount" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="genericAmount">
                <label>Total Amount</label>
                <shortLabel>Total</shortLabel>
                <formatterClass>org.kuali.core.web.format.CurrencyFormatter</formatterClass>
            </attributeReference>	

            <attributeReference name="month1Amount" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="genericAmount">
                <label>YTD Actual</label>
                <formatterClass>org.kuali.core.web.format.CurrencyFormatter</formatterClass>
            </attributeReference>		
        
	        <attributeReference name="csfAmount" sourceClassName="org.kuali.module.budget.bo.CalculatedSalaryFoundationTracker" sourceAttributeName="csfAmount"/>	
	        <attributeReference name="csfDeleteCode" sourceClassName="org.kuali.module.budget.bo.CalculatedSalaryFoundationTracker" sourceAttributeName="csfDeleteCode"/>	
	        <attributeReference name="csfTimePercent" sourceClassName="org.kuali.module.budget.bo.CalculatedSalaryFoundationTracker" sourceAttributeName="csfTimePercent"/>
	        <attributeReference name="csfFundingStatusCode" sourceClassName="org.kuali.module.budget.bo.CalculatedSalaryFoundationTracker" sourceAttributeName="csfFundingStatusCode"/>
	        <attributeReference name="csfFullTimeEmploymentQuantity" sourceClassName="org.kuali.module.budget.bo.CalculatedSalaryFoundationTracker" sourceAttributeName="csfFullTimeEmploymentQuantity"/>
	        	        
            <attributeReference name="dummyBusinessObject.pendingEntryOption" sourceClassName="org.kuali.module.gl.bo.TransientBalanceInquiryAttributes" sourceAttributeName="pendingEntryOption" />
		</attributes>
		
		<relationships>
            <relationship objectAttributeName="ledgerPerson" >
                <primitiveAttribute sourceName="ledgerPerson.personUniversalIdentifier" targetName="personUniversalIdentifier" />
				<supportAttribute sourceName="emplid" targetName="personPayrollIdentifier" identifier="true" />
	        </relationship>
        </relationships>
    </businessObject>
</dictionaryEntry>