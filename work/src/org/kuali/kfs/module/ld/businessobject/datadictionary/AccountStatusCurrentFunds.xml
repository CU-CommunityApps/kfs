<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "../dataDictionary-1_0.dtd">
<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.module.labor.bo.AccountStatusCurrentFunds">


        <inquiry>
            <title>Account Status (Current Funds) Balance Inquiry</title>

            <inquirySections><inquirySection title="Header Text"><inquiryFields>
                <field attributeName="accountNumber" />
                <field attributeName="chartOfAccountsCode" />
                <field attributeName="financialBalanceTypeCode" />
                <field attributeName="financialObjectCode" />
                <field attributeName="financialObjectTypeCode" />
                <field attributeName="financialSubObjectCode" />
                <field attributeName="subAccountNumber" />
                <field attributeName="universityFiscalYear" />
            </inquiryFields></inquirySection></inquirySections>
        </inquiry>

        <lookup>
	        <lookupableID>CurrentFundsLookupable</lookupableID>	        
            <title>Account Status (Current Funds) Balance Inquiry</title>

            <instructions>Look up a Account Current Funds Ledger Balance</instructions>

            <defaultSort sortAscending="true">
                <sortAttributes>
                    <sortAttribute attributeName="universityFiscalYear" />
                    <sortAttribute attributeName="chartOfAccountsCode" />
                    <sortAttribute attributeName="accountNumber" />
                </sortAttributes>
            </defaultSort>

            <lookupFields>
			    <lookupField attributeName="universityFiscalYear" required="true">
                    <defaultValueFinderClass>org.kuali.kfs.lookup.valuefinder.FiscalYearFinder</defaultValueFinderClass>
                </lookupField>
                <lookupField attributeName="chartOfAccountsCode" required="false" />
                <lookupField attributeName="accountNumber" required="true" />
                <lookupField attributeName="financialObjectCode" required="false" />
            </lookupFields>

            <resultFields>
                <field attributeName="subAccountNumber" />
                <field attributeName="financialSubObjectCode" />
				<field attributeName="personName" />
				<field attributeName="positionNumber" />				
				<field attributeName="ytdActualAmount" />
				<field attributeName="outstandingEncum" />
            </resultFields>
        </lookup>

        <attributes>
            <attribute name="positionNumber" forceUppercase="true">
                <label>Position</label>
                <shortLabel>Number</shortLabel>

                <maxLength>8</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="10" />
                </control>

                <summary>positionNumber</summary>
                <description>positionNumber description...</description>
			</attribute>

            <attributeReference name="universityFiscalYear" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="genericFiscalYear">
                <label>Fiscal Year</label>
                <shortLabel>Fiscal Year</shortLabel>
                <description>Identifies the Fiscal Year to which the balances apply.</description>
            </attributeReference>

            <attributeReference name="subAccountNumber" sourceClassName="org.kuali.module.chart.bo.SubAccount" sourceAttributeName="subAccountNumber" />          

			<attributeReference name="accountNumber" sourceClassName="org.kuali.module.chart.bo.Account" sourceAttributeName="accountNumber" />
			<attributeReference name="chartOfAccountsCode" sourceClassName="org.kuali.module.chart.bo.Chart" sourceAttributeName="chartOfAccountsCode" />
			<attributeReference name="financialBalanceTypeCode" sourceClassName="org.kuali.module.chart.bo.codes.BalanceTyp" sourceAttributeName="code">
				<control>
					<hidden />
				</control>
			</attributeReference>
			<attributeReference name="financialObjectCode" sourceClassName="org.kuali.module.chart.bo.ObjectCode" sourceAttributeName="financialObjectCode" />
			<attributeReference name="financialObjectTypeCode" sourceClassName="org.kuali.module.chart.bo.ObjectType" sourceAttributeName="code" />
			<attributeReference name="financialSubObjectCode" sourceClassName="org.kuali.module.chart.bo.SubObjCd" sourceAttributeName="financialSubObjectCode" />

            <attributeReference name="financialBeginningBalanceLineAmount" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="genericAmount">
                <label>Beginning Balance Line Amount</label>
                <shortLabel>Beginning Balance Line Amount</shortLabel>
                <summary>BeginningBalanceLineAmount</summary>
                <description>beginningBalanceLineAmount description...</description>
                <formatterClass>org.kuali.core.web.format.CurrencyFormatter</formatterClass>
            </attributeReference>
			
            <attributeReference name="ytdActualAmount" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="genericAmount">
                <label>YTD Actual</label>
                <shortLabel>YTD Actual</shortLabel>
                <summary>ytdBalanceAmount</summary>
                <description>ytdBalanceAmount description...</description>
                <formatterClass>org.kuali.core.web.format.CurrencyFormatter</formatterClass>
            </attributeReference>

            <attributeReference name="outstandingEncum" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="genericAmount">
                <label>Outstanding Encum</label>
                <shortLabel>Outstanding Encum</shortLabel>
                <summary>Outstanding Encumberances</summary>
                <description>OutstandingEncum description...</description>
                <formatterClass>org.kuali.core.web.format.CurrencyFormatter</formatterClass>
            </attributeReference>
			
            <attributeReference name="versionNumber" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="versionNumber" />

            <attributeReference name="dummyBusinessObject.genericAmount" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="genericAmount">
                <label>Year End Close</label>
                <shortLabel>Year End Close</shortLabel>
                <summary>Year End Close</summary>
                <description>Year End Close Amount...</description>
                <formatterClass>org.kuali.core.web.format.CurrencyFormatter</formatterClass>
            </attributeReference>

            <attribute name="emplid" forceUppercase="true">
                <label>Emplid</label>
                <shortLabel>Emplid</shortLabel>

                <maxLength>11</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="13" />
                </control>

                <summary>emplid</summary>
                <description>emplid description...</description>
			</attribute>

			<attribute name="personName" forceUppercase="true">
				<label>Person</label>
				<shortLabel>Name</shortLabel>

				<maxLength>80</maxLength>

				<control>
					<hidden/>
					<!-- enable if you want to be able to set the display name separate from the individual name fields
					<text size="40" />
					-->
				</control>

				<summary>Person</summary>
				<description>Name</description>

			</attribute>

        </attributes>
    </businessObject>
</dictionaryEntry>
