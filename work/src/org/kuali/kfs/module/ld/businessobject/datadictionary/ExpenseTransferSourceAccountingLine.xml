<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "../../../core/datadictionary/dataDictionary-1_0.dtd"
    [<!ENTITY accountingLineBaseAttributes SYSTEM "../../../kfs/entities/AccountingLineBaseAttributes.xml">]>
<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.module.labor.bo.ExpenseTransferSourceAccountingLine">
        <customAttributeEnabled>false</customAttributeEnabled>

        <attributes>

            <attribute name="emplid" forceUppercase="true">
                <label>Emplid</label>
                <shortLabel>Emplid</shortLabel>

                <maxLength>11</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="13" />
                </control>

                <summary>emplid</summary>
                <description>emplid description...</description>
			</attribute>

            <attribute name="payrollEndDateFiscalPeriodCode" forceUppercase="true">
                <label>Payroll End Date Fiscal Period Code</label>
                <shortLabel>Pay Per</shortLabel>

                <maxLength>2</maxLength>
                <validationPattern><anyCharacter /></validationPattern>
				<required>true</required>
                <control>
                    <text size="4" />
                </control>

                <summary>payrollEndDateFiscalPeriodCode</summary>
                <description>payrollEndDateFiscalPeriodCode description...</description>
			</attribute>

            <attribute name="payrollEndDateFiscalYear" forceUppercase="true">
                <label>Payroll End Date Fiscal Year</label>
                <shortLabel>Pay FY</shortLabel>

                <maxLength>4</maxLength>
                <validationPattern><numeric /></validationPattern>
				<required>true</required>
                <control>
                    <text size="6" />
                </control>

                <summary>payrollEndDateFiscalYear</summary>
                <description>payrollEndDateFiscalYear description...</description>
			</attribute>

            <attribute name="payrollTotalHours" forceUppercase="true">
                <label>Payroll Total Hours</label>
                <shortLabel>Pay Hrs</shortLabel>

                <maxLength>9</maxLength>
                <validationPattern><floatingPoint /></validationPattern>

                <control>
                    <text size="11" />
                </control>

                <summary>payrollTotalHours</summary>
                <description>payrollTotalHours description...</description>
			</attribute>

            <attribute name="positionNumber" forceUppercase="true">
                <label>Position Number</label>
                <shortLabel>Position</shortLabel>

                <maxLength>8</maxLength>
                <validationPattern><anyCharacter /></validationPattern>
				<required>true</required>
                <control>
                    <text size="10" />
                </control>

                <summary>positionNumber</summary>
                <description>positionNumber description...</description>
			</attribute>

            &accountingLineBaseAttributes;

        </attributes>
    </businessObject>
</dictionaryEntry>
