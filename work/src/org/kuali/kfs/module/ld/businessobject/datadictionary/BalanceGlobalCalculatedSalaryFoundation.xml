<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "../dataDictionary-1_0.dtd">
<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.module.labor.bo.BalanceGlobalCalculatedSalaryFoundation">
        <customAttributeEnabled>false</customAttributeEnabled>

        <inquiry>
            <title>Balance Global Calculated Salary Foundation Inquiry</title>
             
            <inquirySections>
                <inquirySection title="Header Text">
                    <inquiryFields>
                        <field attributeName="budgetAmount" />
                        <field attributeName="calculatedSalaryFoundationAmount" />
                        <field attributeName="financialObjectCode" />
                        <field attributeName="financialSubObjectCode" />
                        <field attributeName="personUniversalIdentifier" />
                        <field attributeName="subAccountNumber" />
                    </inquiryFields>
                </inquirySection>
            </inquirySections>  
        </inquiry>

        <lookup>
            <title>Balance Global Calculated Salary Foundation Lookup</title>

            <instructions>Look up a Balance Global Calculated Salary Foundation</instructions>

            <defaultSort sortAscending="true">
                <sortAttributes>
                    <sortAttribute attributeName="subAccountNumber" />
                    <sortAttribute attributeName="financialObjectCode" />
                    <sortAttribute attributeName="financialSubObjectCode" />
                </sortAttributes>
            </defaultSort>

            <lookupFields>
                <lookupField attributeName="budgetAmount" required="false" />
                <lookupField attributeName="calculatedSalaryFoundationAmount" required="false" />
                <lookupField attributeName="financialObjectCode" required="false" />
                <lookupField attributeName="financialSubObjectCode" required="false" />
                <lookupField attributeName="personUniversalIdentifier" required="false" />
                <lookupField attributeName="subAccountNumber" required="false" />
            </lookupFields>

            <resultFields>
                <field attributeName="budgetAmount" />
                <field attributeName="calculatedSalaryFoundationAmount" />
                <field attributeName="financialObjectCode" />
                <field attributeName="financialSubObjectCode" />
                <field attributeName="personUniversalIdentifier" />
                <field attributeName="subAccountNumber" />
            </resultFields>
        </lookup>

        <attributes>

            <attribute name="budgetAmount" forceUppercase="true">
                <label>Budget Amount</label>
                <shortLabel>Amount</shortLabel>

                <maxLength>19</maxLength>
                <validationPattern><floatingPoint /></validationPattern>

                <control>
                    <text size="21" />
                </control>

                <summary>budgetAmount</summary>
                <description>budgetAmount description...</description>
			</attribute>

            <attribute name="calculatedSalaryFoundationAmount" forceUppercase="true">
                <label>Calculated Salary Foundation Amount</label>
                <shortLabel>Amount</shortLabel>

                <maxLength>19</maxLength>
                <validationPattern><floatingPoint /></validationPattern>

                <control>
                    <text size="21" />
                </control>

                <summary>calculatedSalaryFoundationAmount</summary>
                <description>calculatedSalaryFoundationAmount description...</description>
			</attribute>

            <attribute name="financialObjectCode" forceUppercase="true">
                <label>Financial Object Code</label>
                <shortLabel>Code</shortLabel>

                <maxLength>4</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="6" />
                </control>

                <summary>financialObjectCode</summary>
                <description>financialObjectCode description...</description>
			</attribute>

            <attribute name="financialSubObjectCode" forceUppercase="true">
                <label>Financial Sub Object Code</label>
                <shortLabel>Code</shortLabel>

                <maxLength>3</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="5" />
                </control>

                <summary>financialSubObjectCode</summary>
                <description>financialSubObjectCode description...</description>
			</attribute>

            <attribute name="personUniversalIdentifier" forceUppercase="true">
