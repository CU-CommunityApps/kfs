<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="BalanceGlobalCalculatedSalaryFoundation" parent="BalanceGlobalCalculatedSalaryFoundation-parentBean" />

  <bean id="BalanceGlobalCalculatedSalaryFoundation-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.kfs.module.ld.businessobject.BalanceGlobalCalculatedSalaryFoundation" />
    <property name="objectLabel" value="Labor Balance Global Calculated Salary Foundation" />
    
    <property name="inquiryDefinition" >
      <ref bean="BalanceGlobalCalculatedSalaryFoundation-inquiryDefinition" />
    </property>
    <property name="lookupDefinition" >
      <ref bean="BalanceGlobalCalculatedSalaryFoundation-lookupDefinition" />
    </property>
    <property name="attributes" >
      <list>
        <ref bean="BalanceGlobalCalculatedSalaryFoundation-budgetAmount" />
        <ref bean="BalanceGlobalCalculatedSalaryFoundation-calculatedSalaryFoundationAmount" />
        <ref bean="BalanceGlobalCalculatedSalaryFoundation-financialObjectCode" />
        <ref bean="BalanceGlobalCalculatedSalaryFoundation-financialSubObjectCode" />
        <ref bean="BalanceGlobalCalculatedSalaryFoundation-personUniversalIdentifier" />
        <ref bean="BalanceGlobalCalculatedSalaryFoundation-subAccountNumber" />
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->


  <bean id="BalanceGlobalCalculatedSalaryFoundation-budgetAmount" parent="BalanceGlobalCalculatedSalaryFoundation-budgetAmount-parentBean" />

  <bean id="BalanceGlobalCalculatedSalaryFoundation-budgetAmount-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="budgetAmount" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Budget Amount" />
    <property name="shortLabel" value="Amount" />
    <property name="maxLength" value="19" />
    <property name="validationPattern" >
      <bean parent="FloatingPointValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="21" />
    </property>
  </bean>
  <bean id="BalanceGlobalCalculatedSalaryFoundation-calculatedSalaryFoundationAmount" parent="BalanceGlobalCalculatedSalaryFoundation-calculatedSalaryFoundationAmount-parentBean" />

  <bean id="BalanceGlobalCalculatedSalaryFoundation-calculatedSalaryFoundationAmount-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="calculatedSalaryFoundationAmount" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Calculated Salary Foundation Amount" />
    <property name="shortLabel" value="Amount" />
    <property name="maxLength" value="19" />
    <property name="validationPattern" >
      <bean parent="FloatingPointValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="21" />
    </property>
  </bean>
  <bean id="BalanceGlobalCalculatedSalaryFoundation-financialObjectCode" parent="BalanceGlobalCalculatedSalaryFoundation-financialObjectCode-parentBean" />

  <bean id="BalanceGlobalCalculatedSalaryFoundation-financialObjectCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="financialObjectCode" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Financial Object Code" />
    <property name="shortLabel" value="Code" />
    <property name="maxLength" value="4" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="6" />
    </property>
  </bean>
  <bean id="BalanceGlobalCalculatedSalaryFoundation-financialSubObjectCode" parent="BalanceGlobalCalculatedSalaryFoundation-financialSubObjectCode-parentBean" />

  <bean id="BalanceGlobalCalculatedSalaryFoundation-financialSubObjectCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="financialSubObjectCode" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Financial Sub Object Code" />
    <property name="shortLabel" value="Code" />
    <property name="maxLength" value="3" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="5" />
    </property>
  </bean>
  <bean id="BalanceGlobalCalculatedSalaryFoundation-personUniversalIdentifier" parent="BalanceGlobalCalculatedSalaryFoundation-personUniversalIdentifier-parentBean" />

  <bean id="BalanceGlobalCalculatedSalaryFoundation-personUniversalIdentifier-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="personUniversalIdentifier" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Person Universal Identifier" />
    <property name="shortLabel" value="Identifier" />
    <property name="maxLength" value="10" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="12" />
    </property>
  </bean>
  <bean id="BalanceGlobalCalculatedSalaryFoundation-subAccountNumber" parent="BalanceGlobalCalculatedSalaryFoundation-subAccountNumber-parentBean" />

  <bean id="BalanceGlobalCalculatedSalaryFoundation-subAccountNumber-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="subAccountNumber" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Sub Account Number" />
    <property name="shortLabel" value="Number" />
    <property name="maxLength" value="5" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="7" />
    </property>
  </bean>

<!-- Business Object Inquiry Definition -->


  <bean id="BalanceGlobalCalculatedSalaryFoundation-inquiryDefinition" parent="BalanceGlobalCalculatedSalaryFoundation-inquiryDefinition-parentBean" />

  <bean id="BalanceGlobalCalculatedSalaryFoundation-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
    <property name="title" value="Balance Global Calculated Salary Foundation Inquiry" />
    <property name="inquirySections" >
      <list>
        <bean parent="InquirySectionDefinition">
          <property name="title" value="" />
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields" >
            <list>
              <bean parent="FieldDefinition"
                    p:attributeName="budgetAmount" />
              <bean parent="FieldDefinition"
                    p:attributeName="calculatedSalaryFoundationAmount" />
              <bean parent="FieldDefinition"
                    p:attributeName="financialObjectCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="financialSubObjectCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="personUniversalIdentifier" />
              <bean parent="FieldDefinition"
                    p:attributeName="subAccountNumber" />
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Business Object Lookup Definition -->


  <bean id="BalanceGlobalCalculatedSalaryFoundation-lookupDefinition" parent="BalanceGlobalCalculatedSalaryFoundation-lookupDefinition-parentBean" />

  <bean id="BalanceGlobalCalculatedSalaryFoundation-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
    <property name="title" value="Account Status (Base Funds)" />
    <property name="instructions" value="Look up a Balance Global Calculated Salary Foundation" />
    <property name="defaultSort" >
      <bean parent="SortDefinition">
      </bean>
    </property>
    <property name="lookupFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="budgetAmount" />
        <bean parent="FieldDefinition"
              p:attributeName="calculatedSalaryFoundationAmount" />
        <bean parent="FieldDefinition"
              p:attributeName="financialObjectCode" />
        <bean parent="FieldDefinition"
              p:attributeName="financialSubObjectCode" />
        <bean parent="FieldDefinition"
              p:attributeName="personUniversalIdentifier" />
        <bean parent="FieldDefinition"
              p:attributeName="subAccountNumber" />
      </list>
    </property>
    <property name="resultFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="budgetAmount" />
        <bean parent="FieldDefinition"
              p:attributeName="calculatedSalaryFoundationAmount" />
        <bean parent="FieldDefinition"
              p:attributeName="financialObjectCode" />
        <bean parent="FieldDefinition"
              p:attributeName="financialSubObjectCode" />
        <bean parent="FieldDefinition"
              p:attributeName="personUniversalIdentifier" />
        <bean parent="FieldDefinition"
              p:attributeName="subAccountNumber" />
      </list>
    </property>
  </bean>
</beans>
