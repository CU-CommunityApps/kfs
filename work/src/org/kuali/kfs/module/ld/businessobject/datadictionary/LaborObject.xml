<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "../dataDictionary-1_0.dtd">
<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.module.labor.bo.LaborObject">


        <inquiry>
            <title>Labor Object Code Inquiry</title>

            <inquirySections><inquirySection title="Header Text"><inquiryFields>
                <field attributeName="chartOfAccountsCode" />
                <field attributeName="detailPositionRequiredIndicator" />
                <field attributeName="financialObjectCode" />
                <field attributeName="financialObjectFringeOrSalaryCode" />
                <field attributeName="financialObjectHoursRequiredIndicator" />
                <field attributeName="financialObjectPayTypeCode" />
                <field attributeName="positionObjectGroupCode" />
                <field attributeName="universityFiscalYear" />
            </inquiryFields></inquirySection></inquirySections>
        </inquiry>

        <lookup>
            <title>Labor Object Code Lookup</title>

            <instructions>Look up a Labor Object</instructions>

            <defaultSort sortAscending="true">
                <sortAttributes>
                    <sortAttribute attributeName="universityFiscalYear" />
                    <sortAttribute attributeName="chartOfAccountsCode" />
                    <sortAttribute attributeName="financialObjectCode" />
                </sortAttributes>
            </defaultSort>

            <lookupFields>
                <lookupField attributeName="universityFiscalYear" required="false" />
                <lookupField attributeName="chartOfAccountsCode" required="false" />
                <lookupField attributeName="financialObjectCode" required="false" />
                <lookupField attributeName="financialObjectFringeOrSalaryCode" required="false" />
                <lookupField attributeName="financialObjectPayTypeCode" required="false" />
                <lookupField attributeName="positionObjectGroupCode" required="false" />
                <lookupField attributeName="detailPositionRequiredIndicator" required="false" />
                <lookupField attributeName="financialObjectHoursRequiredIndicator" required="false" />
            </lookupFields>

            <resultFields>
                <field attributeName="chartOfAccountsCode" />
                <field attributeName="detailPositionRequiredIndicator" />
                <field attributeName="financialObjectCode" />
                <field attributeName="financialObjectFringeOrSalaryCode" />
                <field attributeName="financialObjectHoursRequiredIndicator" />
                <field attributeName="financialObjectPayTypeCode" />
                <field attributeName="positionObjectGroupCode" />
                <field attributeName="universityFiscalYear" />
            </resultFields>
        </lookup>

        <attributes>
			<attributeReference name="chartOfAccountsCode" sourceClassName="org.kuali.module.chart.bo.Chart" sourceAttributeName="chartOfAccountsCode">
			   <label>Chart Of Accounts Code</label>
			</attributeReference>

            <attribute name="detailPositionRequiredIndicator" forceUppercase="true">
                <label>Detail Position Required Indicator</label>
                <shortLabel>Indicator</shortLabel>

                <maxLength>1</maxLength>

                <control>
                    <checkbox />
                </control>

                <summary>detailPositionRequiredIndicator</summary>
                <description>detailPositionRequiredIndicator description...</description>
			</attribute>

			<attributeReference name="financialObjectCode" sourceClassName="org.kuali.module.chart.bo.ObjectCode" sourceAttributeName="financialObjectCode">
                <label>Financial Object Code</label>
                <shortLabel>Code</shortLabel>
            </attributeReference>

			<attribute name="financialObjectFringeOrSalaryCode" forceUppercase="true">
                <label>Financial Object Fringe Or Salary Code</label>
                <shortLabel>Code</shortLabel>

                <maxLength>1</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="3" />
                </control>

                <summary>financialObjectFringeOrSalaryCode</summary>
                <description>financialObjectFringeOrSalaryCode description...</description>
			</attribute>

            <attribute name="financialObjectHoursRequiredIndicator" forceUppercase="true">
                <label>Financial Object Hours Required Indicator</label>
                <shortLabel>Indicator</shortLabel>

                <maxLength>1</maxLength>

                <control>
                    <checkbox />
                </control>

                <summary>financialObjectHoursRequiredIndicator</summary>
                <description>financialObjectHoursRequiredIndicator description...</description>
			</attribute>

            <attribute name="financialObjectPayTypeCode" forceUppercase="true">
                <label>Financial Object Pay Type Code</label>
                <shortLabel>Code</shortLabel>

                <maxLength>1</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="3" />
                </control>

                <summary>financialObjectPayTypeCode</summary>
                <description>financialObjectPayTypeCode description...</description>
			</attribute>

            <attributeReference name="positionObjectGroupCode" sourceClassName="org.kuali.module.labor.bo.PositionObjectGroup" sourceAttributeName="positionObjectGroupCode"/>			
            <attributeReference name="universityFiscalYear" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="genericFiscalYear"/>									
            <attributeReference name="versionNumber" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="versionNumber" />
        </attributes>
    </businessObject>
</dictionaryEntry>
