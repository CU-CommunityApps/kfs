<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "../dataDictionary-1_0.dtd">
<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.module.labor.bo.July1PositionFunding">

        <inquiry>
            <title>July 1 Position Funding Inquiry</title>
             
            <inquirySections>
                <inquirySection title="Header Text">
                    <inquiryFields>
                        <field attributeName="accountNumber" />
                        <field attributeName="chartOfAccountsCode" />
                        <field attributeName="emplid" />
                        <field attributeName="financialObjectCode" />
                        <field attributeName="financialSubObjectCode" />
                        <field attributeName="july1BudgetAmount" />
                        <field attributeName="july1BudgetFteQuantity" />
                        <field attributeName="july1BudgetTimePercent" />
                        <field attributeName="positionNumber" />
                        <field attributeName="subAccountNumber" />
                        <field attributeName="universityFiscalYear" />
                    </inquiryFields>
                </inquirySection>
            </inquirySections>  
        </inquiry>

        <lookup>
			<lookupableID>july1PositionFundingLookupable</lookupableID>	        
            <title>July 1 Position Funding Lookup</title>

            <instructions>Look up July 1 Position Funding</instructions>

            <defaultSort sortAscending="true">
                <sortAttributes>
                    <sortAttribute attributeName="universityFiscalYear" />
                    <sortAttribute attributeName="chartOfAccountsCode" />
                    <sortAttribute attributeName="accountNumber" />
                    <sortAttribute attributeName="positionNumber" />
                </sortAttributes>
            </defaultSort>

            <lookupFields>
			    <lookupField attributeName="universityFiscalYear" required="true">
                    <defaultValueFinderClass>org.kuali.kfs.lookup.valuefinder.FiscalYearFinder</defaultValueFinderClass>
                </lookupField>
                <lookupField attributeName="chartOfAccountsCode" required="true" />                
                <lookupField attributeName="accountNumber" required="true" />
                <lookupField attributeName="subAccountNumber" required="false" />
               
            </lookupFields>

            <resultFields>
                <field attributeName="universityFiscalYear" />
                <field attributeName="chartOfAccountsCode" />                
                <field attributeName="accountNumber" />
                <field attributeName="subAccountNumber" />
                <field attributeName="financialObjectCode" />
                <field attributeName="financialSubObjectCode" />                                
                <field attributeName="personName" />
				<field attributeName="emplid" />                
				<field attributeName="positionNumber" />                
                <field attributeName="july1BudgetAmount" />
                <field attributeName="july1BudgetFteQuantity" />
                <field attributeName="july1BudgetTimePercent" />                                
            </resultFields>
        </lookup>

        <attributes>

            <attributeReference name="universityFiscalYear" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="genericFiscalYear">
                <label>Fiscal Year</label>
                <shortLabel>Fiscal Year</shortLabel>
                <description>Identifies the Fiscal Year to which the balances apply.</description>
            </attributeReference>
            <attributeReference name="chartOfAccountsCode" sourceClassName="org.kuali.module.chart.bo.Chart" sourceAttributeName="chartOfAccountsCode" />
			<attributeReference name="accountNumber" sourceClassName="org.kuali.module.chart.bo.Account" sourceAttributeName="accountNumber" />
			<attributeReference name="subAccountNumber" sourceClassName="org.kuali.module.chart.bo.SubAccount" sourceAttributeName="subAccountNumber" />          
			<attributeReference name="financialObjectCode" sourceClassName="org.kuali.module.chart.bo.ObjectCode" sourceAttributeName="financialObjectCode" />			
            <attributeReference name="financialSubObjectCode" sourceClassName="org.kuali.module.chart.bo.SubObjCd" sourceAttributeName="financialSubObjectCode" />
            
            <attributeReference name="emplid" sourceClassName="org.kuali.core.bo.user.UniversalUser" sourceAttributeName="personPayrollIdentifier"/>
            <attributeReference name="personName" sourceClassName="org.kuali.core.bo.user.UniversalUser" sourceAttributeName="personName" />
            <attributeReference name="positionNumber" sourceClassName="org.kuali.module.labor.bo.PositionData" sourceAttributeName="positionNumber"/>
            <attributeReference name="versionNumber" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="versionNumber" />
            
            <attributeReference name="july1BudgetAmount" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="genericAmount">
                <label>July1 Budget Amount</label>
                <shortLabel>Amount</shortLabel>
                <summary>july1BudgetAmount</summary>
                <description>july1BudgetAmount description...</description>
                <formatterClass>org.kuali.core.web.format.CurrencyFormatter</formatterClass>
            </attributeReference>
            
            <attributeReference name="july1BudgetFteQuantity" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="genericAmount">
                <label>July1 Budget FTE Quantity</label>
                <shortLabel>Quantity</shortLabel>
                <summary>july1BudgetFteQuantity</summary>
                <description>july1BudgetFteQuantity description...</description>
            </attributeReference>

            <attribute name="july1BudgetTimePercent" forceUppercase="true">
                <label>July1 Budget Time Percent</label>
                <shortLabel>Percent</shortLabel>

                <maxLength>9</maxLength>
                <validationPattern><floatingPoint /></validationPattern>

                <control>
                    <text size="11" />
                </control>

                <summary>july1BudgetTimePercent</summary>
                <description>july1BudgetTimePercent description...</description>
			</attribute>

            <attributeReference name="dummyBusinessObject.genericAmount" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="genericAmount">
                <label>Year End Close</label>
                <shortLabel>Year End Close</shortLabel>
                <summary>Year End Close</summary>
                <description>Year End Close Amount...</description>
                <formatterClass>org.kuali.core.web.format.CurrencyFormatter</formatterClass>
            </attributeReference>

        </attributes>
    </businessObject>
</dictionaryEntry>
