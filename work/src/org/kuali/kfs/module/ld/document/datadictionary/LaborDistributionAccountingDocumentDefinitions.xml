<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

	<bean id="labor-BenefitExpenseTransferDocument-accountingInformation"
	parent="labor-BenefitExpenseTransferDocument-accountingInformation-parent" />

	<bean parent="AccountingLineView-line"
		id="labor-BenefitExpenseTransferDocument-accountingInformation-parent" abstract="true">
		<property name="elementName" value="accountingInformation" />
		<property name="fields">
			<list>
				<bean parent="AccountingLineView-field"
					p:name="postingYear" p:hidden="true" />
				<bean parent="AccountingLineView-field"
					p:name="overrideCode" p:hidden="true" />
				<bean parent="AccountingLineView-field"
					p:name="sequenceNumber" p:hidden="true" />
				<bean parent="AccountingLineView-field"
					p:name="versionNumber" p:hidden="true" />
				<bean parent="AccountingLineView-field"
					p:name="documentNumber" p:hidden="true" />
				<bean parent="AccountingLineView-field"
					p:name="chartOfAccountsCode" 
					p:useShortLabel="true"
					p:required="true"
					p:webUILeaveFieldFunction="loadChartInfo"
					p:dynamicLabelProperty="chart.finChartOfAccountDescription" />
				<bean parent="AccountingLineView-field"
					p:name="accountNumber" p:required="true"
					p:webUILeaveFieldFunction="loadAccountInfo"
					p:dynamicLabelProperty="account.accountName">
					<property name="overrideFields">
						<list>
							<bean
								parent="AccountingLineView-overrideField"
								p:name="accountExpiredOverride" />
							<bean
								parent="AccountingLineView-overrideField"
								p:name="nonFringeAccountOverride" />
						</list>
					</property>
				</bean>
				<bean parent="AccountingLineView-field"
					p:name="subAccountNumber"
					p:useShortLabel="true"
					p:webUILeaveFieldFunction="loadSubAccountInfo"
					p:dynamicLabelProperty="subAccount.subAccountName" />
				<bean parent="AccountingLineView-field"
					p:name="financialObjectCode" 
					p:useShortLabel="true"
					p:required="true"
					p:dynamicNameLabelGeneratorBeanName="objectCodeDynamicNameLabelGenerator">
					<property name="overrideFields">
						<list>
							<bean
								parent="AccountingLineView-overrideField"
								p:name="objectBudgetOverride" />
						</list>
					</property>
				</bean>
				<bean parent="AccountingLineView-field"
					p:name="financialSubObjectCode"
					p:useShortLabel="true"
					p:webUILeaveFieldFunction="loadSubObjectInfo"
					p:dynamicLabelProperty="subObjectCode.financialSubObjectCodeName" />
				<bean parent="AccountingLineView-field"
					p:name="projectCode" 
					p:useShortLabel="true"
					p:webUILeaveFieldFunction="loadProjectInfo"
					p:dynamicLabelProperty="project.projectDescription" />
				<bean parent="AccountingLineView-field"
					p:name="organizationReferenceId"
					p:useShortLabel="true" />
				<bean parent="AccountingLineView-field"
					p:name="positionNumber" 
					p:hidden="true" 
					p:useShortLabel="true" />
				<bean parent="AccountingLineView-field"
					p:name="payrollEndDateFiscalYear" 
					p:required="true" 
					p:useShortLabel="true"/>
				<bean parent="AccountingLineView-field"
					p:name="payrollEndDateFiscalPeriodCode" 
					p:useShortLabel="true"/>
			</list>
		</property>
	</bean>

	<bean id="labor-BenefitExpenseTransferDocument-accountingLineView"
		parent="labor-BenefitExpenseTransferDocument-accountingLineView-parent" />

	<bean id="labor-BenefitExpenseTransferDocument-accountingLineView-parent"
		parent="AccountingLineView" abstract="true">
		<property name="elements">
			<list>
				<bean parent="AccountingLineView-sequenceNumber" />
				<bean parent="AccountingLineView-lines">
					<property name="lines">
						<list>
							<ref bean="labor-BenefitExpenseTransferDocument-accountingInformation" />
						</list>
					</property>
				</bean>
				<bean parent="AccountingLineView-field" 
					p:useShortLabel="true"
					p:name="amount"
					p:required="true" />
				<bean parent="AccountingLineView-actions" />	
			</list>
		</property>
	</bean>
    	    	
    <bean id="labor-BenefitExpenseTransferDocument-AccountingLineGroup" abstract="true" class="org.kuali.kfs.sys.document.datadictionary.AccountingLineGroupDefinition">
		<property name="accountingLineAuthorizerClass" value="org.kuali.kfs.module.ld.document.authorization.BenefitExpenseTransferAccountingLineAuthorizer" />
    </bean>
    
 	<bean id="ExpenseTransferDocument-sourceGroupActions" class="org.springframework.beans.factory.config.ListFactoryBean">
		<property name="sourceList">
			<list>
				<bean parent="AccountingLineView-action" p:actionMethod="copyAllAccountingLines" p:actionLabel="copy all" p:imageName="tinybutton-copyall.gif"/>
				<bean parent="AccountingLineView-action" p:actionMethod="deleteAllSourceAccountingLines" p:actionLabel="delete all" p:imageName="tinybutton-deleteall.gif"/>
			</list>
		</property>
	</bean>
	
	<bean id="ExpenseTransferDocument-targetGroupActions" class="org.springframework.beans.factory.config.ListFactoryBean">
		<property name="sourceList">
			<list>
				<bean parent="AccountingLineView-action" p:actionMethod="deleteAllTargetAccountingLines" p:actionLabel="delete all" p:imageName="tinybutton-deleteall.gif"/>
			</list>
		</property>
	</bean>      
 
</beans>