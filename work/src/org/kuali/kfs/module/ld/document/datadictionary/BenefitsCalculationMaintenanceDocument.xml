<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="BenefitsCalculationMaintenanceDocument" parent="BenefitsCalculationMaintenanceDocument-parentBean" />

  <bean id="BenefitsCalculationMaintenanceDocument-parentBean" abstract="true" parent="MaintenanceDocumentEntry">
    <property name="businessObjectClass" value="org.kuali.kfs.module.ld.businessobject.BenefitsCalculation" />
    <property name="maintainableClass" value="org.kuali.rice.kns.maintenance.KualiMaintainableImpl" />
    <property name="maintainableSections" >
      <list>
        <ref bean="BenefitsCalculationMaintenanceDocument-EditLaborCalculation" />
      </list>
    </property>
    <property name="defaultExistenceChecks" >
      <list>
        <bean parent="ReferenceDefinition"
              p:attributeName="universityFiscal"
              p:attributeToHighlightOnFail="universityFiscalYear" />
        <bean parent="ReferenceDefinition"
              p:attributeName="chartOfAccounts"
              p:attributeToHighlightOnFail="chartOfAccountsCode" />
        <bean parent="ReferenceDefinition"
              p:attributeName="positionFringeBenefitObject"
              p:attributeToHighlightOnFail="positionFringeBenefitObjectCode" />
        <bean parent="ReferenceDefinition"
              p:attributeName="positionBenefitType"
              p:attributeToHighlightOnFail="positionBenefitTypeCode" />
      </list>
    </property>
    <property name="lockingKeys" >
      <list>
        <value>universityFiscalYear</value>
        <value>chartOfAccountsCode</value>
        <value>positionBenefitTypeCode</value>
      </list>
    </property>
    <property name="label" value="Labor Benefits Calculation" />
    <property name="shortLabel" value="Labor Benefits Calculation" />
    <property name="summary" value="Labor Calculation Maintenance Document" />
    <property name="description" value="Document used to create or update Labor calculations." />
    <property name="helpDefinition" >
      <bean parent="HelpDefinition" p:parameterClass="org.kuali.kfs.module.ld.businessobject.BenefitsCalculation" p:parameterName="LABOR_BENEFITS_CALCULATION" />
    </property>
    <property name="documentTypeName" value="BenefitsCalculationMaintenanceDocument" />
    <property name="businessRulesClass" value="org.kuali.kfs.module.ld.document.validation.impl.BenefitsCalculationDocumentRule" />
    <property name="documentAuthorizerClass" value="org.kuali.kfs.sys.document.authorization.FinancialSystemMaintenanceDocumentAuthorizerBase" />
    <property name="documentTypeCode" value="BCAL" />
    <property name="workflowProperties" >
      <ref bean="BenefitsCalculationMaintenanceDocument-workflowProperties" />
    </property>
    <property name="authorizations" >
      <list>
        <bean parent="AuthorizationDefinition">
          <property name="action" value="initiate" />
          <property name="authorizedGroups" >
            <set>
              <value>SY_MAINTENANCE_USERS</value>
            </set>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Maintenance Section Definitions -->


  <bean id="BenefitsCalculationMaintenanceDocument-EditLaborCalculation" parent="BenefitsCalculationMaintenanceDocument-EditLaborCalculation-parentBean" />

  <bean id="BenefitsCalculationMaintenanceDocument-EditLaborCalculation-parentBean" abstract="true" parent="MaintainableSectionDefinition">
    <property name="id" value="Edit Labor Calculation" />
    <property name="title" value="Edit Labor Calculation" />
    <property name="maintainableItems" >
      <list>
        <bean parent="MaintainableFieldDefinition"
              p:name="universityFiscalYear"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="chartOfAccountsCode"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="positionBenefitTypeCode"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="positionFringeBenefitObjectCode"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="positionFringeBenefitPercent"
              p:required="true" />
      </list>
    </property>
  </bean>

<!-- Exported Workflow Properties -->


  <bean id="BenefitsCalculationMaintenanceDocument-workflowProperties" parent="BenefitsCalculationMaintenanceDocument-workflowProperties-parentBean" />

  <bean id="BenefitsCalculationMaintenanceDocument-workflowProperties-parentBean" abstract="true" parent="WorkflowProperties">
  </bean>
</beans>
