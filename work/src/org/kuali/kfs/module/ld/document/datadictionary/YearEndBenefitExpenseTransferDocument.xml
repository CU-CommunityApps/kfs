<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="YearEndBenefitExpenseTransferDocument" parent="YearEndBenefitExpenseTransferDocument-parentBean" />

  <bean id="YearEndBenefitExpenseTransferDocument-parentBean" abstract="true" parent="TransactionalDocumentEntry">
    <property name="allowsErrorCorrection" value="true" />
    <property name="label" value="Year End Benefit Expense Transfer" />
    <property name="shortLabel" value="Year End Benefit Expense Transfer" />
    <property name="summary" value="&amp;nbsp;" />
    <property name="description" value="Year End Benefit Expense Transfer Document" />
    <property name="documentTypeName" value="YearEndBenefitExpenseTransferDocument" />
    <property name="documentClass" value="org.kuali.kfs.module.ld.document.YearEndBenefitExpenseTransferDocument" />
    <property name="businessRulesClass" value="org.kuali.kfs.module.ld.document.validation.impl.YearEndBenefitExpenseTransferDocumentRule" />
    <property name="documentAuthorizerClass" value="org.kuali.kfs.module.ld.document.authorization.LaborExpenseDocumentAuthorizerBase" />
    <property name="documentTypeCode" value="YEBT" />
    <property name="allowsCopy" value="true" />
    <property name="workflowProperties" >
      <ref bean="YearEndBenefitExpenseTransferDocument-workflowProperties" />
    </property>
    <property name="authorizations" >
      <list>
        <bean parent="AuthorizationDefinition">
          <property name="action" value="initiate" />
          <property name="authorizedGroups" >
            <set>
              <value>kualiUniversalGroup</value>
            </set>
          </property>
        </bean>
      </list>
    </property>
    <property name="validationMap" ref="YearEndBenefitExpenseTransferDocument-validations" />
    <property name="accountingLineGroups">
    	<map>
    		<entry>
    			<key><value>source</value></key>
    			<ref bean="YearEndBenefitExpenseTransferDocument-sourceAccountingLineGroup" parent="AccountingLineGroup" />
    		</entry>
    		<entry>
    			<key><value>target</value></key>
    			<ref bean="YearEndBenefitExpenseTransferDocument-targetAccountingLineGroup" parent="AccountingLineGroup" />
    		</entry>    		
    	</map>
    </property> 
    
  </bean>

<!-- Exported Workflow Properties -->


  <bean id="YearEndBenefitExpenseTransferDocument-workflowProperties" parent="YearEndBenefitExpenseTransferDocument-workflowProperties-parentBean" />

  <bean id="YearEndBenefitExpenseTransferDocument-workflowProperties-parentBean" abstract="true" parent="WorkflowProperties">
    <property name="workflowPropertyGroups" >
      <list>
        <bean parent="WorkflowPropertyGroup">
          <property name="workflowProperties" >
            <list>
              <bean parent="WorkflowProperty"
                    p:path="documentHeader" />
              <bean parent="WorkflowProperty"
                    p:path="sourceAccountingLines" />
              <bean parent="WorkflowProperty"
                    p:path="targetAccountingLines" />
              <bean parent="WorkflowProperty"
                    p:path="sourceAccountingLines.account" />
              <bean parent="WorkflowProperty"
                    p:path="targetAccountingLines.account" />
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

  <!-- Validations -->
  
  <bean id="YearEndBenefitExpenseTransferDocument-validations" parent="YearEndBenefitExpenseTransferDocument-validations-parentBean" />
  
  <bean id="YearEndBenefitExpenseTransferDocument-validations-parentBean" class="org.springframework.beans.factory.config.MapFactoryBean">
  	<property name="sourceMap">
		<map>
			<entry>
				<key><value>org.kuali.kfs.sys.document.validation.event.AttributedAddAccountingLineEvent</value></key>
				<value>BenefitExpenseTransferDocument-addAccountingLineValidation</value>
			</entry>
						
			<entry>
				<key><value>org.kuali.kfs.sys.document.validation.event.AttributedRouteDocumentEvent</value></key>
				<value>BenefitExpenseTransferDocument-routeDocumentValidation</value>
			</entry>
    	</map>
    </property>
  </bean>  

  <!-- accounting line groups -->
  <bean id="YearEndBenefitExpenseTransferDocument-sourceAccountingLineGroup" parent="BenefitExpenseTransferDocument-sourceAccountingLineGroup-parentBean" />
  <bean id="YearEndBenefitExpenseTransferDocument-targetAccountingLineGroup" parent="BenefitExpenseTransferDocument-targetAccountingLineGroup-parentBean" />
  
</beans>

