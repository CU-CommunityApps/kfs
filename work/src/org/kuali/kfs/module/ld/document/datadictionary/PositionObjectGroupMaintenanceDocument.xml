<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="PositionObjectGroupMaintenanceDocument" parent="PositionObjectGroupMaintenanceDocument-parentBean" />

  <bean id="PositionObjectGroupMaintenanceDocument-parentBean" abstract="true" parent="MaintenanceDocumentEntry">
    <property name="businessObjectClass" value="org.kuali.kfs.module.ld.businessobject.PositionObjectGroup" />
    <property name="maintainableClass" value="org.kuali.core.maintenance.KualiMaintainableImpl" />
    <property name="maintainableSections" >
      <list>
        <ref bean="PositionObjectGroupMaintenanceDocument-EditPositionObjectGroup" />
      </list>
    </property>
    <property name="lockingKeys" >
      <list>
        <value>positionObjectGroupCode</value>
      </list>
    </property>
    <property name="label" value="Labor Position Object Group Code Maintenance Document" />
    <property name="shortLabel" value="Position Object Group Code Document" />
    <property name="summary" value="Position Object Group Code Document" />
    <property name="description" value="Document used to create or update Position Object Group Objects" />
    <property name="documentTypeName" value="PositionObjectGroupMaintenanceDocument" />
    <property name="documentAuthorizerClass" value="org.kuali.kfs.sys.document.web.struts.FinancialSystemMaintenanceDocumentAuthorizerBase" />
    <property name="documentTypeCode" value="POGR" />
    <property name="workflowProperties" >
      <ref bean="PositionObjectGroupMaintenanceDocument-workflowProperties" />
    </property>
    <property name="authorizations" >
      <list>
        <bean parent="AuthorizationDefinition">
          <property name="action" value="initiate" />
          <property name="authorizedGroups" >
            <set>
              <value>SY_MAINTENANCE_USERS</value>
            </set>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Maintenance Section Definitions -->


  <bean id="PositionObjectGroupMaintenanceDocument-EditPositionObjectGroup" parent="PositionObjectGroupMaintenanceDocument-EditPositionObjectGroup-parentBean" />

  <bean id="PositionObjectGroupMaintenanceDocument-EditPositionObjectGroup-parentBean" abstract="true" parent="MaintainableSectionDefinition">
    <property name="id" value="Edit Position Object Group" />
    <property name="title" value="Edit Position Object Group" />
    <property name="maintainableItems" >
      <list>
        <bean parent="MaintainableFieldDefinition"
              p:name="positionObjectGroupCode"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="positionObjectGroupName"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="rowActiveIndicator"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="versionNumber" />
      </list>
    </property>
  </bean>

<!-- Exported Workflow Properties -->


  <bean id="PositionObjectGroupMaintenanceDocument-workflowProperties" parent="PositionObjectGroupMaintenanceDocument-workflowProperties-parentBean" />

  <bean id="PositionObjectGroupMaintenanceDocument-workflowProperties-parentBean" abstract="true" parent="WorkflowProperties">
  </bean>
</beans>
