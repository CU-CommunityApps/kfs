<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="BenefitsTypeMaintenanceDocument" parent="BenefitsTypeMaintenanceDocument-parentBean" />

  <bean id="BenefitsTypeMaintenanceDocument-parentBean" abstract="true" parent="MaintenanceDocumentEntry">
    <property name="businessObjectClass" value="org.kuali.module.labor.bo.BenefitsType" />
    <property name="maintainableClass" value="org.kuali.core.maintenance.KualiMaintainableImpl" />
    <property name="maintainableSections" >
      <list>
        <ref bean="BenefitsTypeMaintenanceDocument-EditBenefitCode" />
      </list>
    </property>
    <property name="lockingKeys" >
      <list>
        <value>positionBenefitTypeCode</value>
      </list>
    </property>
    <property name="label" value="Labor Benefits Type" />
    <property name="shortLabel" value="Labor Benefits Type" />
    <property name="summary" value="Benefits Type Maintenance Document" />
    <property name="description" value="Document used to create or update Benefit type codes." />
    <property name="helpDefinition" >
      <bean parent="HelpDefinition"
            p:parameterNamespace="KFS-LD"
            p:parameterName="LABOR_BENEFITS_TYPE"
            p:parameterDetailType="BenefitsType" />
    </property>
    <property name="documentTypeName" value="BenefitsTypeMaintenanceDocument" />
    <property name="documentAuthorizerClass" value="org.kuali.core.document.authorization.MaintenanceDocumentAuthorizerBase" />
    <property name="documentTypeCode" value="BENT" />
    <property name="workflowProperties" >
      <ref bean="BenefitsTypeMaintenanceDocument-workflowProperties" />
    </property>
    <property name="authorizations" >
      <list>
        <bean parent="AuthorizationDefinition">
          <property name="action" value="initiate" />
          <property name="authorizedGroups" >
            <set>
              <value>SY_MAINTAINENANCE_USERS</value>
            </set>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Maintenance Section Definitions -->


  <bean id="BenefitsTypeMaintenanceDocument-EditBenefitCode" parent="BenefitsTypeMaintenanceDocument-EditBenefitCode-parentBean" />

  <bean id="BenefitsTypeMaintenanceDocument-EditBenefitCode-parentBean" abstract="true" parent="MaintainableSectionDefinition">
    <property name="id" value="Edit Benefit Code" />
    <property name="title" value="Edit Benefit Code" />
    <property name="maintainableItems" >
      <list>
        <bean parent="MaintainableFieldDefinition"
              p:name="positionBenefitTypeCode"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="positionBenefitTypeDescription"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="positionBenefitRetirementIndicator" />
        <bean parent="MaintainableFieldDefinition"
              p:name="versionNumber" />
      </list>
    </property>
  </bean>

<!-- Exported Workflow Properties -->


  <bean id="BenefitsTypeMaintenanceDocument-workflowProperties" parent="BenefitsTypeMaintenanceDocument-workflowProperties-parentBean" />

  <bean id="BenefitsTypeMaintenanceDocument-workflowProperties-parentBean" abstract="true" parent="WorkflowProperties">
  </bean>
</beans>
