<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="ContractsAndGrantsRoleCode" parent="ContractsAndGrantsRoleCode-parentBean" />

  <bean id="ContractsAndGrantsRoleCode-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.kfs.module.cg.businessobject.ContractsAndGrantsRoleCode" />
    <property name="inquiryDefinition" >
      <ref bean="ContractsAndGrantsRoleCode-inquiryDefinition" />
    </property>
    <property name="lookupDefinition" >
      <ref bean="ContractsAndGrantsRoleCode-lookupDefinition" />
    </property>
    <property name="objectLabel" value="Person Role" />
    <property name="attributes" >
      <list>
        <ref bean="ContractsAndGrantsRoleCode-personRoleCode" />
        <ref bean="ContractsAndGrantsRoleCode-personRoleDescription" />
        <ref bean="ContractsAndGrantsRoleCode-personRoleSortNumber" />
        <ref bean="ContractsAndGrantsRoleCode-active" />
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->


  <bean id="ContractsAndGrantsRoleCode-personRoleCode" parent="ContractsAndGrantsRoleCode-personRoleCode-parentBean" />

  <bean id="ContractsAndGrantsRoleCode-personRoleCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="personRoleCode" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Person Role Code" />
    <property name="shortLabel" value="Code" />
    <property name="maxLength" value="1" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="3" />
    </property>
    
    
  </bean>
  <bean id="ContractsAndGrantsRoleCode-personRoleDescription" parent="ContractsAndGrantsRoleCode-personRoleDescription-parentBean" />

  <bean id="ContractsAndGrantsRoleCode-personRoleDescription-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="personRoleDescription" />
    <property name="label" value="Person Role Description" />
    <property name="shortLabel" value="Description" />
    <property name="maxLength" value="40" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern"
            p:allowWhitespace="true" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="42" />
    </property>
    
    
  </bean>
  <bean id="ContractsAndGrantsRoleCode-personRoleSortNumber" parent="ContractsAndGrantsRoleCode-personRoleSortNumber-parentBean" />

  <bean id="ContractsAndGrantsRoleCode-personRoleSortNumber-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="personRoleSortNumber" />
    <property name="label" value="Person Role Sort Number" />
    <property name="shortLabel" value="Number" />
    <property name="maxLength" value="2" />
    <property name="validationPattern" >
      <bean parent="NumericValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="2" />
    </property>
    
    
  </bean>
  <bean id="ContractsAndGrantsRoleCode-active" parent="ContractsAndGrantsRoleCode-active-parentBean" />

  <bean id="ContractsAndGrantsRoleCode-active-parentBean" abstract="true" parent="GenericAttributes-activeIndicator">
    <property name="name" value="active" />
  </bean>

<!-- Business Object Inquiry Definition -->


  <bean id="ContractsAndGrantsRoleCode-inquiryDefinition" parent="ContractsAndGrantsRoleCode-inquiryDefinition-parentBean" />

  <bean id="ContractsAndGrantsRoleCode-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
    <property name="title" value="Person Role Inquiry" />
    <property name="inquirySections" >
      <list>
        <bean parent="InquirySectionDefinition">
          <property name="title" value="" />
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields" >
            <list>
              <bean parent="FieldDefinition"
                    p:attributeName="personRoleCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="personRoleDescription" />
              <bean parent="FieldDefinition"
                    p:attributeName="personRoleSortNumber" />
              <bean parent="FieldDefinition"
                    p:attributeName="active" />
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Business Object Lookup Definition -->


  <bean id="ContractsAndGrantsRoleCode-lookupDefinition" parent="ContractsAndGrantsRoleCode-lookupDefinition-parentBean" />

  <bean id="ContractsAndGrantsRoleCode-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
    <property name="title" value="Contracts And Grants Role Code Lookup" />
    <property name="instructions" value="Look up a Contracts And Grants Role Code" />
    <property name="defaultSort" >
      <bean parent="SortDefinition">
      </bean>
    </property>
    <property name="lookupFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="personRoleCode" />
        <bean parent="FieldDefinition"
              p:attributeName="personRoleDescription" />
        <bean parent="FieldDefinition"
              p:defaultValue="Y"
              p:attributeName="active" />
      </list>
    </property>
    <property name="resultFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="personRoleCode" />
        <bean parent="FieldDefinition"
              p:attributeName="personRoleDescription" />
        <bean parent="FieldDefinition"
              p:attributeName="personRoleSortNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="active" />
      </list>
    </property>
  </bean>
</beans>
