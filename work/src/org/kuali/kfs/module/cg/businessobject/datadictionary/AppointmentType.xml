                <validationPattern>
                    <floatingPoint />
                </validationPattern>

                <control>
                    <text size="10" />
                </control>

                <summary>Fringe Rate</summary>
                <description>Fringe Rate</description>
            </attribute>

            <attribute name="costShareFringeRateAmount" forceUppercase="false">
                <label>Cost Share Fringe Rate</label>
                <shortLabel>Cost Share</shortLabel>

                <maxLength>8</maxLength>
                <validationPattern>
