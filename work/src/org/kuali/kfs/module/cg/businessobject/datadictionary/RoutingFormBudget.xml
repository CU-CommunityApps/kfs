<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "../dataDictionary-1_0.dtd">
<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.module.kra.routingform.bo.RoutingFormBudget">
        <customAttributeEnabled>false</customAttributeEnabled>

        <inquiry>
            <title>Routing Form Budget Inquiry</title>

            <inquirySections><inquirySection title="Header Text"><inquiryFields>
                <field attributeName="routingFormBudgetDirectAmount" />
                <field attributeName="routingFormBudgetEndDate" />
                <field attributeName="routingFormBudgetIndirectCostAmount" />
                <field attributeName="routingFormBudgetIndirectCostDescription" />
                <field attributeName="routingFormBudgetMaximumPeriodNumber" />
                <field attributeName="routingFormBudgetMinimumPeriodNumber" />
                <field attributeName="routingFormBudgetStartDate" />
                <field attributeName="documentNumber" />
            </inquiryFields></inquirySection></inquirySections>
        </inquiry>

        <lookup>
            <title>Routing Form Budget Lookup</title>

            <instructions>Look up a Routing Form Budget</instructions>

            <defaultSort sortAscending="true">
                <sortAttributes>
                    <sortAttribute attributeName="documentNumber" />
                </sortAttributes>
            </defaultSort>

            <lookupFields>
                <lookupField attributeName="routingFormBudgetDirectAmount" required="false" />
                <lookupField attributeName="routingFormBudgetEndDate" required="false" />
                <lookupField attributeName="routingFormBudgetIndirectCostAmount" required="false" />
                <lookupField attributeName="routingFormBudgetIndirectCostDescription" required="false" />
                <lookupField attributeName="routingFormBudgetMaximumPeriodNumber" required="false" />
                <lookupField attributeName="routingFormBudgetMinimumPeriodNumber" required="false" />
                <lookupField attributeName="routingFormBudgetStartDate" required="false" />
                <lookupField attributeName="documentNumber" required="false" />
            </lookupFields>

            <resultFields>
                <field attributeName="routingFormBudgetDirectAmount" />
                <field attributeName="routingFormBudgetEndDate" />
                <field attributeName="routingFormBudgetIndirectCostAmount" />
                <field attributeName="routingFormBudgetIndirectCostDescription" />
                <field attributeName="routingFormBudgetMaximumPeriodNumber" />
                <field attributeName="routingFormBudgetMinimumPeriodNumber" />
                <field attributeName="routingFormBudgetStartDate" />
                <field attributeName="documentNumber" />
            </resultFields>
        </lookup>

        <attributes>

            <attribute name="routingFormBudgetDirectAmount" forceUppercase="true">
                <label>Routing Form Budget Direct Amount</label>
                <shortLabel>Direct Costs</shortLabel>

                <maxLength>19</maxLength>
                <validationPattern><floatingPoint /></validationPattern>

                <control>
                    <text size="21" />
                </control>

                <summary>routingFormBudgetDirectAmount</summary>
                <description>routingFormBudgetDirectAmount description...</description>
			</attribute>

            <attribute name="routingFormBudgetEndDate" forceUppercase="true">
                <label>Routing Form Budget End Date</label>
                <shortLabel>End Date</shortLabel>

                <maxLength>10</maxLength>
                <validationPattern><date /></validationPattern>

                <control>
                    <text size="10" />
                </control>

                <summary>routingFormBudgetEndDate</summary>
                <description>routingFormBudgetEndDate description...</description>
			</attribute>

            <attribute name="routingFormBudgetIndirectCostAmount" forceUppercase="true">
                <label>Routing Form Budget Indirect Cost Amount</label>
                <shortLabel>Indirect Costs</shortLabel>

                <maxLength>19</maxLength>
                <validationPattern><floatingPoint /></validationPattern>

                <control>
                    <text size="21" />
                </control>

                <summary>routingFormBudgetIndirectCostAmount</summary>
                <description>routingFormBudgetIndirectCostAmount description...</description>
			</attribute>

            <attribute name="routingFormBudgetIndirectCostDescription" forceUppercase="true">
                <label>Routing Form Budget Indirect Cost Description</label>
                <shortLabel>Description</shortLabel>

                <maxLength>1000</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <textarea rows="5" cols="80"/>
                </control>

                <summary>routingFormBudgetIndirectCostDescription</summary>
                <description>routingFormBudgetIndirectCostDescription description...</description>
			</attribute>

            <attribute name="routingFormBudgetMaximumPeriodNumber" forceUppercase="true">
                <label>Routing Form Budget Maximum Period Number</label>
                <shortLabel>Number</shortLabel>

                <maxLength>2</maxLength>
                <validationPattern><numeric /></validationPattern>

                <control>
                    <text size="4" />
                </control>

                <summary>routingFormBudgetMaximumPeriodNumber</summary>
                <description>routingFormBudgetMaximumPeriodNumber description...</description>
			</attribute>

            <attribute name="routingFormBudgetMinimumPeriodNumber" forceUppercase="true">
                <label>Routing Form Budget Minimum Period Number</label>
                <shortLabel>Number</shortLabel>

                <maxLength>2</maxLength>
                <validationPattern><numeric /></validationPattern>

                <control>
                    <text size="4" />
                </control>

                <summary>routingFormBudgetMinimumPeriodNumber</summary>
                <description>routingFormBudgetMinimumPeriodNumber description...</description>
			</attribute>

            <attribute name="routingFormBudgetStartDate" forceUppercase="true">
                <label>Routing Form Budget Start Date</label>
                <shortLabel>Start Date</shortLabel>

                <maxLength>10</maxLength>
                <validationPattern><date /></validationPattern>

                <control>
                    <text size="10" />
                </control>

                <summary>routingFormBudgetStartDate</summary>
                <description>routingFormBudgetStartDate description...</description>
			</attribute>

            <attribute name="documentNumber" forceUppercase="true">
                <label>Document Number</label>
                <shortLabel>Number</shortLabel>

                <maxLength>14</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="16" />
                </control>

                <summary>documentNumber</summary>
                <description>documentNumber description...</description>
			</attribute>

            <attributeReference name="versionNumber" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="versionNumber" />

        </attributes>
    </businessObject>
</dictionaryEntry>
