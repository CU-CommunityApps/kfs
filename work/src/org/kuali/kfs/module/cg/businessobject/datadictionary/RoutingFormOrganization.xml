<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="RoutingFormOrganization" parent="RoutingFormOrganization-parentBean" />

  <bean id="RoutingFormOrganization-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.kfs.module.cg.businessobject.RoutingFormOrganization" />
    <property name="inquiryDefinition" >
      <ref bean="RoutingFormOrganization-inquiryDefinition" />
    </property>
    <property name="lookupDefinition" >
      <ref bean="RoutingFormOrganization-lookupDefinition" />
    </property>
    <property name="objectLabel" value="Routing Form Organization" />
    <property name="attributes" >
      <list>
        <ref bean="RoutingFormOrganization-budgetIndicator" />
        <ref bean="RoutingFormOrganization-chartOfAccountsCode" />
        <ref bean="RoutingFormOrganization-organizationCode" />
        <ref bean="RoutingFormOrganization-routingFormPrimaryOrganizationIndicator" />
        <ref bean="RoutingFormOrganization-documentNumber" />
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->


  <bean id="RoutingFormOrganization-budgetIndicator" parent="RoutingFormOrganization-budgetIndicator-parentBean" />

  <bean id="RoutingFormOrganization-budgetIndicator-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="budgetIndicator" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Budget Indicator" />
    <property name="shortLabel" value="Indicator" />
    <property name="maxLength" value="1" />
    <property name="control" >
      <bean parent="CheckboxControlDefinition" />
    </property>
    <property name="summary" value="budgetIndicator" />
    <property name="description" value="budgetIndicator description..." />
  </bean>
  <bean id="RoutingFormOrganization-chartOfAccountsCode" parent="RoutingFormOrganization-chartOfAccountsCode-parentBean" />

  <bean id="RoutingFormOrganization-chartOfAccountsCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="chartOfAccountsCode" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Chart Of Accounts Code" />
    <property name="shortLabel" value="Chart" />
    <property name="maxLength" value="2" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="SelectControlDefinition"
            p:valuesFinderClass="org.kuali.kfs.coa.businessobject.options.SimpleChartValuesFinder"
            p:includeKeyInLabel="false" />
    </property>
    <property name="summary" value="chartOfAccountsCode" />
    <property name="description" value="chartOfAccountsCode description..." />
  </bean>
  <bean id="RoutingFormOrganization-organizationCode" parent="RoutingFormOrganization-organizationCode-parentBean" />

  <bean id="RoutingFormOrganization-organizationCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="organizationCode" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Organization Code" />
    <property name="shortLabel" value="Code" />
    <property name="maxLength" value="4" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="6" />
    </property>
    <property name="summary" value="organizationCode" />
    <property name="description" value="organizationCode description..." />
  </bean>
  <bean id="RoutingFormOrganization-routingFormPrimaryOrganizationIndicator" parent="RoutingFormOrganization-routingFormPrimaryOrganizationIndicator-parentBean" />

  <bean id="RoutingFormOrganization-routingFormPrimaryOrganizationIndicator-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="routingFormPrimaryOrganizationIndicator" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Routing Form Primary Organization Indicator" />
    <property name="shortLabel" value="Indicator" />
    <property name="maxLength" value="1" />
    <property name="control" >
      <bean parent="CheckboxControlDefinition" />
    </property>
    <property name="summary" value="routingFormPrimaryOrganizationIndicator" />
    <property name="description" value="routingFormPrimaryOrganizationIndicator description..." />
  </bean>
  <bean id="RoutingFormOrganization-documentNumber" parent="RoutingFormOrganization-documentNumber-parentBean" />

  <bean id="RoutingFormOrganization-documentNumber-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="documentNumber" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Document Number" />
    <property name="shortLabel" value="Number" />
    <property name="maxLength" value="14" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="16" />
    </property>
    <property name="summary" value="documentNumber" />
    <property name="description" value="documentNumber description..." />
  </bean>

<!-- Business Object Inquiry Definition -->


  <bean id="RoutingFormOrganization-inquiryDefinition" parent="RoutingFormOrganization-inquiryDefinition-parentBean" />

  <bean id="RoutingFormOrganization-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
    <property name="title" value="Routing Form Organization Inquiry" />
    <property name="inquirySections" >
      <list>
        <bean parent="InquirySectionDefinition">
          <property name="title" value="" />
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields" >
            <list>
              <bean parent="FieldDefinition"
                    p:attributeName="budgetIndicator" />
              <bean parent="FieldDefinition"
                    p:attributeName="chartOfAccountsCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="organizationCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="routingFormPrimaryOrganizationIndicator" />
              <bean parent="FieldDefinition"
                    p:attributeName="documentNumber" />
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Business Object Lookup Definition -->


  <bean id="RoutingFormOrganization-lookupDefinition" parent="RoutingFormOrganization-lookupDefinition-parentBean" />

  <bean id="RoutingFormOrganization-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
    <property name="title" value="Routing Form Organization Lookup" />
    <property name="instructions" value="Look up a Routing Form Organization" />
    <property name="defaultSort" >
      <bean parent="SortDefinition">
      </bean>
    </property>
    <property name="lookupFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="budgetIndicator" />
        <bean parent="FieldDefinition"
              p:attributeName="chartOfAccountsCode" />
        <bean parent="FieldDefinition"
              p:attributeName="organizationCode" />
        <bean parent="FieldDefinition"
              p:attributeName="routingFormPrimaryOrganizationIndicator" />
        <bean parent="FieldDefinition"
              p:attributeName="documentNumber" />
      </list>
    </property>
    <property name="resultFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="budgetIndicator" />
        <bean parent="FieldDefinition"
              p:attributeName="chartOfAccountsCode" />
        <bean parent="FieldDefinition"
              p:attributeName="organizationCode" />
        <bean parent="FieldDefinition"
              p:attributeName="routingFormPrimaryOrganizationIndicator" />
        <bean parent="FieldDefinition"
              p:attributeName="documentNumber" />
      </list>
    </property>
  </bean>
</beans>
