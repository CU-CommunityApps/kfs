<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="ResearchRiskType" parent="ResearchRiskType-parentBean" />

  <bean id="ResearchRiskType-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.kfs.module.cg.businessobject.ResearchRiskType" />
    <property name="inquiryDefinition" >
      <ref bean="ResearchRiskType-inquiryDefinition" />
    </property>
    <property name="lookupDefinition" >
      <ref bean="ResearchRiskType-lookupDefinition" />
    </property>
    <property name="objectLabel" value="Research Risk Type" />
    <property name="attributes" >
      <list>
        <ref bean="ResearchRiskType-dataObjectMaintenanceCodeActiveIndicator" />
        <ref bean="ResearchRiskType-researchRiskTypeCode" />
        <ref bean="ResearchRiskType-controlAttributeTypeCode" />
        <ref bean="ResearchRiskType-researchRiskTypeDescription" />
        <ref bean="ResearchRiskType-researchRiskTypeSortNumber" />
        <ref bean="ResearchRiskType-researchRiskTypeNotificationGroupText" />
        <ref bean="ResearchRiskType-researchRiskTypeNotificationValue" />
        <ref bean="ResearchRiskType-versionNumber" />
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->


  <bean id="ResearchRiskType-dataObjectMaintenanceCodeActiveIndicator" parent="ResearchRiskType-dataObjectMaintenanceCodeActiveIndicator-parentBean" />

  <bean id="ResearchRiskType-dataObjectMaintenanceCodeActiveIndicator-parentBean" abstract="true" parent="GenericAttributes-activeIndicator">
    <property name="name" value="dataObjectMaintenanceCodeActiveIndicator" />
  </bean>
  <bean id="ResearchRiskType-researchRiskTypeCode" parent="ResearchRiskType-researchRiskTypeCode-parentBean" />

  <bean id="ResearchRiskType-researchRiskTypeCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="researchRiskTypeCode" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Research Risk Type Code" />
    <property name="shortLabel" value="Code" />
    <property name="maxLength" value="2" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="4" />
    </property>
    <property name="summary" value="researchRiskTypeCode" />
    <property name="description" value="researchRiskTypeCode description..." />
  </bean>
  <bean id="ResearchRiskType-controlAttributeTypeCode" parent="ResearchRiskType-controlAttributeTypeCode-parentBean" />

  <bean id="ResearchRiskType-controlAttributeTypeCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="controlAttributeTypeCode" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Control Attribute Type Code" />
    <property name="shortLabel" value="Code" />
    <property name="maxLength" value="1" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="SelectControlDefinition"
            p:valuesFinderClass="org.kuali.kfs.module.cg.businessobject.options.ResearchControlAttributeTypeCodeValuesFinder"
            p:includeKeyInLabel="false" />
    </property>
    <property name="summary" value="controlAttributeTypeCode" />
    <property name="description" value="Specifies how the research risk is displayed. A=all columns; S=some columns; D=description textarea" />
  </bean>
  <bean id="ResearchRiskType-researchRiskTypeDescription" parent="ResearchRiskType-researchRiskTypeDescription-parentBean" />

  <bean id="ResearchRiskType-researchRiskTypeDescription-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="researchRiskTypeDescription" />
    <property name="forceUppercase" value="false" />
    <property name="label" value="Research Risk Type Description" />
    <property name="shortLabel" value="Description" />
    <property name="maxLength" value="30" />
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="32" />
    </property>
    <property name="summary" value="researchRiskTypeDescription" />
    <property name="description" value="researchRiskTypeDescription description..." />
  </bean>
  <bean id="ResearchRiskType-researchRiskTypeSortNumber" parent="ResearchRiskType-researchRiskTypeSortNumber-parentBean" />

  <bean id="ResearchRiskType-researchRiskTypeSortNumber-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="researchRiskTypeSortNumber" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Sort Number" />
    <property name="shortLabel" value="Number" />
    <property name="maxLength" value="2" />
    <property name="validationPattern" >
      <bean parent="NumericValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="4" />
    </property>
    <property name="summary" value="sortNumber" />
    <property name="description" value="sortNumber description..." />
  </bean>
  <bean id="ResearchRiskType-researchRiskTypeNotificationGroupText" parent="ResearchRiskType-researchRiskTypeNotificationGroupText-parentBean" />

  <bean id="ResearchRiskType-researchRiskTypeNotificationGroupText-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="researchRiskTypeNotificationGroupText" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Research Risk Type Notification Group" />
    <property name="shortLabel" value="Not. Group" />
    <property name="maxLength" value="70" />
    <property name="required" value="false" />
    <property name="control" >
      <bean parent="WorkflowWorkgroupControlDefinition" />
    </property>
    <property name="summary" value="researchRiskTypeNotificationGroupText" />
    <property name="description" value="Workgroup to notify if the user-entered value for this research risk matches the notification value." />
  </bean>
  <bean id="ResearchRiskType-researchRiskTypeNotificationValue" parent="ResearchRiskType-researchRiskTypeNotificationValue-parentBean" />

  <bean id="ResearchRiskType-researchRiskTypeNotificationValue-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="researchRiskTypeNotificationValue" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Research Risk Type Notification Value" />
    <property name="shortLabel" value="Not. Val" />
    <property name="maxLength" value="1" />
    <property name="required" value="false" />
    <property name="control" >
      <bean parent="SelectControlDefinition"
            p:valuesFinderClass="org.kuali.kfs.module.cg.businessobject.options.ResearchRiskTypeNotificationValuesFinder"
            p:includeKeyInLabel="false" />
    </property>
    <property name="summary" value="Research Risk Type Notification Value" />
    <property name="description" value="Value on which to route to the notification group.  Valid options are Y (yes), N (no), A (all), X (never)." />
  </bean>
  <bean id="ResearchRiskType-versionNumber" parent="ResearchRiskType-versionNumber-parentBean" />

  <bean id="ResearchRiskType-versionNumber-parentBean" abstract="true" parent="GenericAttributes-versionNumber">
  </bean>

<!-- Business Object Inquiry Definition -->


  <bean id="ResearchRiskType-inquiryDefinition" parent="ResearchRiskType-inquiryDefinition-parentBean" />

  <bean id="ResearchRiskType-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
    <property name="title" value="Research Risk Type Inquiry" />
    <property name="inquirySections" >
      <list>
        <bean parent="InquirySectionDefinition">
          <property name="title" value="" />
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields" >
            <list>
              <bean parent="FieldDefinition"
                    p:attributeName="researchRiskTypeCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="controlAttributeTypeCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="researchRiskTypeDescription" />
              <bean parent="FieldDefinition"
                    p:attributeName="researchRiskTypeSortNumber" />
              <bean parent="FieldDefinition"
                    p:attributeName="researchRiskTypeNotificationGroupText" />
              <bean parent="FieldDefinition"
                    p:attributeName="researchRiskTypeNotificationValue" />
              <bean parent="FieldDefinition"
                    p:attributeName="dataObjectMaintenanceCodeActiveIndicator" />
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Business Object Lookup Definition -->


  <bean id="ResearchRiskType-lookupDefinition" parent="ResearchRiskType-lookupDefinition-parentBean" />

  <bean id="ResearchRiskType-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
    <property name="title" value="Research Risk Type Lookup" />
    <property name="instructions" value="Look up a Research Risk Type" />
    <property name="defaultSort" >
      <bean parent="SortDefinition">
      </bean>
    </property>
    <property name="lookupFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="researchRiskTypeCode" />
        <bean parent="FieldDefinition"
              p:attributeName="controlAttributeTypeCode" />
        <bean parent="FieldDefinition"
              p:attributeName="researchRiskTypeDescription" />
        <bean parent="FieldDefinition"
              p:attributeName="researchRiskTypeSortNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="researchRiskTypeNotificationGroupText" />
        <bean parent="FieldDefinition"
              p:attributeName="researchRiskTypeNotificationValue" />
        <bean parent="FieldDefinition"
              p:defaultValue="Y"
              p:attributeName="dataObjectMaintenanceCodeActiveIndicator" />
      </list>
    </property>
    <property name="resultFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="researchRiskTypeCode" />
        <bean parent="FieldDefinition"
              p:attributeName="controlAttributeTypeCode" />
        <bean parent="FieldDefinition"
              p:attributeName="researchRiskTypeDescription" />
        <bean parent="FieldDefinition"
              p:attributeName="researchRiskTypeSortNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="researchRiskTypeNotificationGroupText" />
        <bean parent="FieldDefinition"
              p:attributeName="researchRiskTypeNotificationValue" />
        <bean parent="FieldDefinition"
              p:attributeName="dataObjectMaintenanceCodeActiveIndicator" />
      </list>
    </property>
  </bean>
</beans>
