<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "../dataDictionary-1_0.dtd">
<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.module.kra.routingform.bo.RoutingFormKeyword">
        <customAttributeEnabled>false</customAttributeEnabled>

        <inquiry>
            <title>Routing Form Keyword Inquiry</title>

            <inquirySections><inquirySection title="Header Text"><inquiryFields>
                <field attributeName="routingFormKeywordDescription" />
                <field attributeName="documentNumber" />
            </inquiryFields></inquirySection></inquirySections>
        </inquiry>

        <lookup>
            <title>Routing Form Keyword Lookup</title>

            <instructions>Look up a Routing Form Keyword</instructions>

            <defaultSort sortAscending="true">
                <sortAttributes>
                    <sortAttribute attributeName="routingFormKeywordDescription" />
                    <sortAttribute attributeName="documentNumber" />
                </sortAttributes>
            </defaultSort>

            <lookupFields>
                <lookupField attributeName="routingFormKeywordDescription" required="false" />
                <lookupField attributeName="documentNumber" required="false" />
            </lookupFields>

            <resultFields>
                <field attributeName="routingFormKeywordDescription" />
                <field attributeName="documentNumber" />
            </resultFields>
        </lookup>

        <attributes>

            <attribute name="routingFormKeywordDescription" forceUppercase="true">
                <label>Routing Form Keyword Description</label>
                <shortLabel>Description</shortLabel>

                <maxLength>70</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="72" />
                </control>

                <summary>routingFormKeywordDescription</summary>
                <description>routingFormKeywordDescription description...</description>
			</attribute>

            <attribute name="documentNumber" forceUppercase="true">
                <label>Document Number</label>
                <shortLabel>Number</shortLabel>

                <maxLength>14</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="16" />
                </control>

                <summary>documentNumber</summary>
                <description>documentNumber description...</description>
			</attribute>

            <attributeReference name="versionNumber" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="versionNumber" />

        </attributes>
    </businessObject>
</dictionaryEntry>
