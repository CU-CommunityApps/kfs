<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="RoutingFormOtherCostShare" parent="RoutingFormOtherCostShare-parentBean" />

  <bean id="RoutingFormOtherCostShare-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.module.kra.routingform.bo.RoutingFormOtherCostShare" />
    <property name="inquiryDefinition" >
      <ref bean="RoutingFormOtherCostShare-inquiryDefinition" />
    </property>
    <property name="lookupDefinition" >
      <ref bean="RoutingFormOtherCostShare-lookupDefinition" />
    </property>
    <property name="attributes" >
      <list>
        <ref bean="RoutingFormOtherCostShare-routingFormCostShareAmount" />
        <ref bean="RoutingFormOtherCostShare-routingFormCostShareSequenceNumber" />
        <ref bean="RoutingFormOtherCostShare-routingFormCostShareSourceName" />
        <ref bean="RoutingFormOtherCostShare-documentNumber" />
        <ref bean="RoutingFormOtherCostShare-versionNumber" />
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->


  <bean id="RoutingFormOtherCostShare-routingFormCostShareAmount" parent="RoutingFormOtherCostShare-routingFormCostShareAmount-parentBean" />

  <bean id="RoutingFormOtherCostShare-routingFormCostShareAmount-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="routingFormCostShareAmount" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Routing Form Cost Share Amount" />
    <property name="shortLabel" value="Amount" />
    <property name="maxLength" value="19" />
    <property name="validationPattern" >
      <bean parent="FixedPointValidationPattern"
            p:precision="19"
            p:scale="2"
            p:allowNegative="true" />
    </property>
    <property name="control" >
      <bean parent="CurrencyControlDefinition"
            p:formattedMaxLength="26"
            p:size="10" />
    </property>
    <property name="summary" value="routingFormCostShareAmount" />
    <property name="description" value="routingFormCostShareAmount description..." />
  </bean>
  <bean id="RoutingFormOtherCostShare-routingFormCostShareSequenceNumber" parent="RoutingFormOtherCostShare-routingFormCostShareSequenceNumber-parentBean" />

  <bean id="RoutingFormOtherCostShare-routingFormCostShareSequenceNumber-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="routingFormCostShareSequenceNumber" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Routing Form Cost Share Sequence Number" />
    <property name="shortLabel" value="Number" />
    <property name="maxLength" value="4" />
    <property name="validationPattern" >
      <bean parent="NumericValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="HiddenControlDefinition" />
    </property>
    <property name="summary" value="routingFormCostShareSequenceNumber" />
    <property name="description" value="routingFormCostShareSequenceNumber description..." />
  </bean>
  <bean id="RoutingFormOtherCostShare-routingFormCostShareSourceName" parent="RoutingFormOtherCostShare-routingFormCostShareSourceName-parentBean" />

  <bean id="RoutingFormOtherCostShare-routingFormCostShareSourceName-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="routingFormCostShareSourceName" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Routing Form Cost Share Source Name" />
    <property name="shortLabel" value="Source" />
    <property name="maxLength" value="50" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern"
            p:allowWhitespace="true" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="52" />
    </property>
    <property name="summary" value="routingFormCostShareSourceName" />
    <property name="description" value="routingFormCostShareSourceName description..." />
  </bean>
  <bean id="RoutingFormOtherCostShare-documentNumber" parent="RoutingFormOtherCostShare-documentNumber-parentBean" />

  <bean id="RoutingFormOtherCostShare-documentNumber-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="documentNumber" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Document Number" />
    <property name="shortLabel" value="Number" />
    <property name="maxLength" value="14" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="HiddenControlDefinition" />
    </property>
    <property name="summary" value="documentNumber" />
    <property name="description" value="documentNumber description..." />
  </bean>
  <bean id="RoutingFormOtherCostShare-versionNumber" parent="RoutingFormOtherCostShare-versionNumber-parentBean" />

  <bean id="RoutingFormOtherCostShare-versionNumber-parentBean" abstract="true" parent="GenericAttributes-versionNumber">
  </bean>

<!-- Business Object Inquiry Definition -->


  <bean id="RoutingFormOtherCostShare-inquiryDefinition" parent="RoutingFormOtherCostShare-inquiryDefinition-parentBean" />

  <bean id="RoutingFormOtherCostShare-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
    <property name="title" value="Routing Form Other Cost Share Inquiry" />
    <property name="inquirySections" >
      <list>
        <bean parent="InquirySectionDefinition">
          <property name="title" value="Header Text" />
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields" >
            <list>
              <bean parent="FieldDefinition"
                    p:attributeName="routingFormCostShareAmount" />
              <bean parent="FieldDefinition"
                    p:attributeName="routingFormCostShareSequenceNumber" />
              <bean parent="FieldDefinition"
                    p:attributeName="routingFormCostShareSourceName" />
              <bean parent="FieldDefinition"
                    p:attributeName="documentNumber" />
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Business Object Lookup Definition -->


  <bean id="RoutingFormOtherCostShare-lookupDefinition" parent="RoutingFormOtherCostShare-lookupDefinition-parentBean" />

  <bean id="RoutingFormOtherCostShare-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
    <property name="title" value="Routing Form Other Cost Share Lookup" />
    <property name="instructions" value="Look up a Routing Form Other Cost Share" />
    <property name="defaultSort" >
      <bean parent="SortDefinition">
      </bean>
    </property>
    <property name="lookupFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="routingFormCostShareAmount" />
        <bean parent="FieldDefinition"
              p:attributeName="routingFormCostShareSequenceNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="routingFormCostShareSourceName" />
        <bean parent="FieldDefinition"
              p:attributeName="documentNumber" />
      </list>
    </property>
    <property name="resultFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="routingFormCostShareAmount" />
        <bean parent="FieldDefinition"
              p:attributeName="routingFormCostShareSequenceNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="routingFormCostShareSourceName" />
        <bean parent="FieldDefinition"
              p:attributeName="documentNumber" />
      </list>
    </property>
  </bean>
</beans>
