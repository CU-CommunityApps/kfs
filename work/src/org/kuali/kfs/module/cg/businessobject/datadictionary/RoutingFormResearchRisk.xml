<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="RoutingFormResearchRisk" parent="RoutingFormResearchRisk-parentBean" />

  <bean id="RoutingFormResearchRisk-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.kfs.module.cg.businessobject.RoutingFormResearchRisk" />
    <property name="inquiryDefinition" >
      <ref bean="RoutingFormResearchRisk-inquiryDefinition" />
    </property>
    <property name="lookupDefinition" >
      <ref bean="RoutingFormResearchRisk-lookupDefinition" />
    </property>
    <property name="objectLabel" value="Routing Form Research Risk" />
    <property name="attributes" >
      <list>
        <ref bean="RoutingFormResearchRisk-researchRiskDescription" />
        <ref bean="RoutingFormResearchRisk-documentNumber" />
        <ref bean="RoutingFormResearchRisk-researchRiskTypeCode" />
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->


  <bean id="RoutingFormResearchRisk-researchRiskDescription" parent="RoutingFormResearchRisk-researchRiskDescription-parentBean" />

  <bean id="RoutingFormResearchRisk-researchRiskDescription-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="researchRiskDescription" />
    <property name="label" value="Research Risk Description" />
    <property name="shortLabel" value="Describe" />
    <property name="maxLength" value="500" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern"
            p:allowWhitespace="true" />
    </property>
    <property name="control" >
      <bean parent="TextareaControlDefinition"
            p:rows="5"
            p:cols="80" />
    </property>
    
    
  </bean>
  <bean id="RoutingFormResearchRisk-documentNumber" parent="RoutingFormResearchRisk-documentNumber-parentBean" />

  <bean id="RoutingFormResearchRisk-documentNumber-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="documentNumber" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Document Number" />
    <property name="shortLabel" value="Number" />
    <property name="maxLength" value="14" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="16" />
    </property>
    
    
  </bean>
  <bean id="RoutingFormResearchRisk-researchRiskTypeCode" parent="RoutingFormResearchRisk-researchRiskTypeCode-parentBean" />

  <bean id="RoutingFormResearchRisk-researchRiskTypeCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="researchRiskTypeCode" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Research Risk Type Code" />
    <property name="shortLabel" value="Code" />
    <property name="maxLength" value="2" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="4" />
    </property>
    
    
  </bean>

<!-- Business Object Inquiry Definition -->


  <bean id="RoutingFormResearchRisk-inquiryDefinition" parent="RoutingFormResearchRisk-inquiryDefinition-parentBean" />

  <bean id="RoutingFormResearchRisk-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
    <property name="title" value="Routing Form Research Risk Inquiry" />
    <property name="inquirySections" >
      <list>
        <bean parent="InquirySectionDefinition">
          <property name="title" value="" />
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields" >
            <list>
              <bean parent="FieldDefinition"
                    p:attributeName="researchRiskDescription" />
              <bean parent="FieldDefinition"
                    p:attributeName="documentNumber" />
              <bean parent="FieldDefinition"
                    p:attributeName="researchRiskTypeCode" />
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Business Object Lookup Definition -->


  <bean id="RoutingFormResearchRisk-lookupDefinition" parent="RoutingFormResearchRisk-lookupDefinition-parentBean" />

  <bean id="RoutingFormResearchRisk-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
    <property name="title" value="Routing Form Research Risk Lookup" />
    <property name="instructions" value="Look up a Routing Form Research Risk" />
    <property name="defaultSort" >
      <bean parent="SortDefinition">
      </bean>
    </property>
    <property name="lookupFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="researchRiskDescription" />
        <bean parent="FieldDefinition"
              p:attributeName="documentNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="researchRiskTypeCode" />
      </list>
    </property>
    <property name="resultFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="researchRiskDescription" />
        <bean parent="FieldDefinition"
              p:attributeName="documentNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="researchRiskTypeCode" />
      </list>
    </property>
  </bean>
</beans>
