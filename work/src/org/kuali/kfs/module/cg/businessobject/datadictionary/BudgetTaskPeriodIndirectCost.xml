<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="BudgetTaskPeriodIndirectCost" parent="BudgetTaskPeriodIndirectCost-parentBean" />

  <bean id="BudgetTaskPeriodIndirectCost-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.module.kra.budget.bo.BudgetTaskPeriodIndirectCost" />
    <property name="attributes" >
      <list>
        <ref bean="BudgetTaskPeriodIndirectCost-documentNumber" />
        <ref bean="BudgetTaskPeriodIndirectCost-objectId" />
        <ref bean="BudgetTaskPeriodIndirectCost-versionNumber" />
        <ref bean="BudgetTaskPeriodIndirectCost-budgetManualIndirectCostRate" />
        <ref bean="BudgetTaskPeriodIndirectCost-budgetTaskSequenceNumber" />
        <ref bean="BudgetTaskPeriodIndirectCost-budgetPeriodSequenceNumber" />
        <ref bean="BudgetTaskPeriodIndirectCost-budgetManualMtdcAmount" />
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->


  <bean id="BudgetTaskPeriodIndirectCost-documentNumber" parent="BudgetTaskPeriodIndirectCost-documentNumber-parentBean" />

  <bean id="BudgetTaskPeriodIndirectCost-documentNumber-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="documentNumber" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Document Header Id" />
    <property name="shortLabel" value="Document Header Id" />
    <property name="maxLength" value="8" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="HiddenControlDefinition" />
    </property>
    <property name="summary" value="Document Header Id" />
    <property name="description" value="Document Header Id" />
  </bean>
  <bean id="BudgetTaskPeriodIndirectCost-objectId" parent="BudgetTaskPeriodIndirectCost-objectId-parentBean" />

  <bean id="BudgetTaskPeriodIndirectCost-objectId-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="objectId" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Object Id" />
    <property name="shortLabel" value="GUID" />
    <property name="maxLength" value="36" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="HiddenControlDefinition" />
    </property>
    <property name="summary" value="objectId summary..." />
    <property name="description" value="objectId description..." />
  </bean>
  <bean id="BudgetTaskPeriodIndirectCost-versionNumber" parent="BudgetTaskPeriodIndirectCost-versionNumber-parentBean" />

  <bean id="BudgetTaskPeriodIndirectCost-versionNumber-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="versionNumber" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Version Number" />
    <property name="shortLabel" value="Number" />
    <property name="maxLength" value="10" />
    <property name="validationPattern" >
      <bean parent="NumericValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="HiddenControlDefinition" />
    </property>
    <property name="summary" value="versionNumber summary..." />
    <property name="description" value="versionNumber description..." />
  </bean>
  <bean id="BudgetTaskPeriodIndirectCost-budgetManualIndirectCostRate" parent="BudgetTaskPeriodIndirectCost-budgetManualIndirectCostRate-parentBean" />

  <bean id="BudgetTaskPeriodIndirectCost-budgetManualIndirectCostRate-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="budgetManualIndirectCostRate" />
    <property name="forceUppercase" value="false" />
    <property name="label" value="Manual Indirect Cost Rate" />
    <property name="shortLabel" value="Manual Indirect Cost Rate" />
    <property name="maxLength" value="6" />
    <property name="validationPattern" >
      <bean parent="FloatingPointValidationPattern" />
    </property>
    <property name="required" value="true" />
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="6" />
    </property>
    <property name="summary" value="Manual Indirect Cost Rate" />
    <property name="description" value="Manual Indirect Cost Rate" />
  </bean>
  <bean id="BudgetTaskPeriodIndirectCost-budgetTaskSequenceNumber" parent="BudgetTaskPeriodIndirectCost-budgetTaskSequenceNumber-parentBean" />

  <bean id="BudgetTaskPeriodIndirectCost-budgetTaskSequenceNumber-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="budgetTaskSequenceNumber" />
    <property name="forceUppercase" value="false" />
    <property name="label" value="Budget Task Sequence Number" />
    <property name="shortLabel" value="Budget Task Sequence Number" />
    <property name="maxLength" value="10" />
    <property name="validationPattern" >
      <bean parent="NumericValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="HiddenControlDefinition" />
    </property>
    <property name="summary" value="Budget Task Sequence Number" />
    <property name="description" value="Budget Task Sequence Number" />
  </bean>
  <bean id="BudgetTaskPeriodIndirectCost-budgetPeriodSequenceNumber" parent="BudgetTaskPeriodIndirectCost-budgetPeriodSequenceNumber-parentBean" />

  <bean id="BudgetTaskPeriodIndirectCost-budgetPeriodSequenceNumber-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="budgetPeriodSequenceNumber" />
    <property name="forceUppercase" value="false" />
    <property name="label" value="Budget Period Sequence Number" />
    <property name="shortLabel" value="Budget Period Sequence Number" />
    <property name="maxLength" value="10" />
    <property name="control" >
      <bean parent="HiddenControlDefinition" />
    </property>
    <property name="summary" value="Budget Period Sequence Number" />
    <property name="description" value="Budget Period Sequence Number" />
  </bean>
  <bean id="BudgetTaskPeriodIndirectCost-budgetManualMtdcAmount" parent="BudgetTaskPeriodIndirectCost-budgetManualMtdcAmount-parentBean" />

  <bean id="BudgetTaskPeriodIndirectCost-budgetManualMtdcAmount-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="budgetManualMtdcAmount" />
    <property name="forceUppercase" value="false" />
    <property name="label" value="Manual MTDC Amount" />
    <property name="shortLabel" value="Manual MTDC Amount" />
    <property name="maxLength" value="12" />
    <property name="validationPattern" >
      <bean parent="FloatingPointValidationPattern" />
    </property>
    <property name="required" value="true" />
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="10" />
    </property>
    <property name="summary" value="Manual MTDC Amount" />
    <property name="description" value="Manual MTDC Amount" />
  </bean>
</beans>
