<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="BudgetThirdPartyCostShare" parent="BudgetThirdPartyCostShare-parentBean" />

  <bean id="BudgetThirdPartyCostShare-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.kfs.module.cg.businessobject.BudgetThirdPartyCostShare" />
    <property name="inquiryDefinition" >
      <ref bean="BudgetThirdPartyCostShare-inquiryDefinition" />
    </property>
    <property name="objectLabel" value="Budget Third Party Cost Share" />
    <property name="objectDescription" value="Budget Third Party Cost Share is the third party cost share for your Budget." />
    <property name="attributes" >
      <list>
        <ref bean="BudgetThirdPartyCostShare-documentNumber" />
        <ref bean="BudgetThirdPartyCostShare-budgetCostShareSequenceNumber" />
        <ref bean="BudgetThirdPartyCostShare-budgetCostShareDescription" />
        <ref bean="BudgetThirdPartyCostShare-budgetCostShareSourceName" />
        <ref bean="BudgetThirdPartyCostShare-versionNumber" />
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->


  <bean id="BudgetThirdPartyCostShare-documentNumber" parent="BudgetThirdPartyCostShare-documentNumber-parentBean" />

  <bean id="BudgetThirdPartyCostShare-documentNumber-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="documentNumber" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Budget Number" />
    <property name="shortLabel" value="Budget Number" />
    <property name="maxLength" value="12" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="12" />
    </property>
    <property name="summary" value="Budget Number..." />
    <property name="description" value="Budget Number..." />
  </bean>
  <bean id="BudgetThirdPartyCostShare-budgetCostShareSequenceNumber" parent="BudgetThirdPartyCostShare-budgetCostShareSequenceNumber-parentBean" />

  <bean id="BudgetThirdPartyCostShare-budgetCostShareSequenceNumber-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="budgetCostShareSequenceNumber" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Sequence Number" />
    <property name="shortLabel" value="Sequence Number" />
    <property name="maxLength" value="5" />
    <property name="validationPattern" >
      <bean parent="NumericValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="2" />
    </property>
    <property name="summary" value="Sequence Number" />
    <property name="description" value="Sequence Number" />
  </bean>
  <bean id="BudgetThirdPartyCostShare-budgetCostShareDescription" parent="BudgetThirdPartyCostShare-budgetCostShareDescription-parentBean" />

  <bean id="BudgetThirdPartyCostShare-budgetCostShareDescription-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="budgetCostShareDescription" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Description" />
    <property name="shortLabel" value="Description" />
    <property name="maxLength" value="50" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern"
            p:allowWhitespace="true" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="40" />
    </property>
    <property name="summary" value="Description" />
    <property name="description" value="Description" />
  </bean>
  <bean id="BudgetThirdPartyCostShare-budgetCostShareSourceName" parent="BudgetThirdPartyCostShare-budgetCostShareSourceName-parentBean" />

  <bean id="BudgetThirdPartyCostShare-budgetCostShareSourceName-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="budgetCostShareSourceName" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Source" />
    <property name="shortLabel" value="Name" />
    <property name="maxLength" value="50" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern"
            p:allowWhitespace="true" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="8" />
    </property>
    <property name="summary" value="Description" />
    <property name="description" value="Description" />
  </bean>
  <bean id="BudgetThirdPartyCostShare-versionNumber" parent="BudgetThirdPartyCostShare-versionNumber-parentBean" />

  <bean id="BudgetThirdPartyCostShare-versionNumber-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="versionNumber" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Version Number" />
    <property name="shortLabel" value="Number" />
    <property name="maxLength" value="10" />
    <property name="validationPattern" >
      <bean parent="NumericValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="-1" />
    </property>
    <property name="summary" value="versionNumber summary..." />
    <property name="description" value="versionNumber description..." />
  </bean>

<!-- Business Object Inquiry Definition -->


  <bean id="BudgetThirdPartyCostShare-inquiryDefinition" parent="BudgetThirdPartyCostShare-inquiryDefinition-parentBean" />

  <bean id="BudgetThirdPartyCostShare-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
    <property name="title" value="Budget Third Party Cost Share" />
    <property name="inquirySections" >
      <list>
        <bean parent="InquirySectionDefinition">
          <property name="title" value="" />
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields" >
            <list>
              <bean parent="FieldDefinition"
                    p:attributeName="budgetCostShareSequenceNumber" />
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>
</beans>
