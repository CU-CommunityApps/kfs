<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "../dataDictionary-1_0.dtd">
<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.module.kra.routingform.bo.RoutingFormConflictOfInterest">
        <customAttributeEnabled>false</customAttributeEnabled>

        <inquiry>
            <title>Routing Form Conflict Of Interest Inquiry</title>

            <inquirySections><inquirySection title="Header Text"><inquiryFields>
                <field attributeName="routingFormConflictOfInterestDescription" />
                <field attributeName="documentNumber" />
            </inquiryFields></inquirySection></inquirySections>
        </inquiry>

        <lookup>
            <title>Routing Form Conflict Of Interest Lookup</title>

            <instructions>Look up a Routing Form Conflict Of Interest</instructions>

            <defaultSort sortAscending="true">
                <sortAttributes>
                    <sortAttribute attributeName="documentNumber" />
                </sortAttributes>
            </defaultSort>

            <lookupFields>
                <lookupField attributeName="routingFormConflictOfInterestDescription" required="false" />
                <lookupField attributeName="documentNumber" required="false" />
            </lookupFields>

            <resultFields>
                <field attributeName="routingFormConflictOfInterestDescription" />
                <field attributeName="documentNumber" />
            </resultFields>
        </lookup>

        <attributes>

            <attribute name="routingFormConflictOfInterestDescription" forceUppercase="true">
                <label>Routing Form Conflict Of Interest Description</label>
                <shortLabel>Description</shortLabel>

                <maxLength>1000</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <textarea rows="5" cols="80"/>
                </control>

                <summary>routingFormConflictOfInterestDescription</summary>
                <description>routingFormConflictOfInterestDescription description...</description>
			</attribute>

            <attribute name="documentNumber" forceUppercase="true">
                <label>Document Number</label>
                <shortLabel>Number</shortLabel>

                <maxLength>14</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="16" />
                </control>

                <summary>documentNumber</summary>
                <description>documentNumber description...</description>
			</attribute>

            <attributeReference name="versionNumber" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="versionNumber" />

        </attributes>
    </businessObject>
</dictionaryEntry>
