<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="RoutingFormConflictOfInterest" parent="RoutingFormConflictOfInterest-parentBean" />

  <bean id="RoutingFormConflictOfInterest-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.module.kra.routingform.bo.RoutingFormConflictOfInterest" />
    <property name="inquiryDefinition" >
      <ref bean="RoutingFormConflictOfInterest-inquiryDefinition" />
    </property>
    <property name="lookupDefinition" >
      <ref bean="RoutingFormConflictOfInterest-lookupDefinition" />
    </property>
    <property name="attributes" >
      <list>
        <ref bean="RoutingFormConflictOfInterest-routingFormConflictOfInterestDescription" />
        <ref bean="RoutingFormConflictOfInterest-documentNumber" />
        <ref bean="RoutingFormConflictOfInterest-versionNumber" />
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->


  <bean id="RoutingFormConflictOfInterest-routingFormConflictOfInterestDescription" parent="RoutingFormConflictOfInterest-routingFormConflictOfInterestDescription-parentBean" />

  <bean id="RoutingFormConflictOfInterest-routingFormConflictOfInterestDescription-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="routingFormConflictOfInterestDescription" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Routing Form Conflict Of Interest Description" />
    <property name="shortLabel" value="Description" />
    <property name="maxLength" value="1000" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextareaControlDefinition"
            p:rows="5"
            p:cols="80" />
    </property>
    <property name="summary" value="routingFormConflictOfInterestDescription" />
    <property name="description" value="routingFormConflictOfInterestDescription description..." />
  </bean>
  <bean id="RoutingFormConflictOfInterest-documentNumber" parent="RoutingFormConflictOfInterest-documentNumber-parentBean" />

  <bean id="RoutingFormConflictOfInterest-documentNumber-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="documentNumber" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Document Number" />
    <property name="shortLabel" value="Number" />
    <property name="maxLength" value="14" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="16" />
    </property>
    <property name="summary" value="documentNumber" />
    <property name="description" value="documentNumber description..." />
  </bean>
  <bean id="RoutingFormConflictOfInterest-versionNumber" parent="RoutingFormConflictOfInterest-versionNumber-parentBean" />

  <bean id="RoutingFormConflictOfInterest-versionNumber-parentBean" abstract="true" parent="GenericAttributes-versionNumber">
  </bean>

<!-- Business Object Inquiry Definition -->


  <bean id="RoutingFormConflictOfInterest-inquiryDefinition" parent="RoutingFormConflictOfInterest-inquiryDefinition-parentBean" />

  <bean id="RoutingFormConflictOfInterest-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
    <property name="title" value="Routing Form Conflict Of Interest Inquiry" />
    <property name="inquirySections" >
      <list>
        <bean parent="InquirySectionDefinition">
          <property name="title" value="Header Text" />
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields" >
            <list>
              <bean parent="FieldDefinition"
                    p:attributeName="routingFormConflictOfInterestDescription" />
              <bean parent="FieldDefinition"
                    p:attributeName="documentNumber" />
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Business Object Lookup Definition -->


  <bean id="RoutingFormConflictOfInterest-lookupDefinition" parent="RoutingFormConflictOfInterest-lookupDefinition-parentBean" />

  <bean id="RoutingFormConflictOfInterest-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
    <property name="title" value="Routing Form Conflict Of Interest Lookup" />
    <property name="instructions" value="Look up a Routing Form Conflict Of Interest" />
    <property name="defaultSort" >
      <bean parent="SortDefinition">
      </bean>
    </property>
    <property name="lookupFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="routingFormConflictOfInterestDescription" />
        <bean parent="FieldDefinition"
              p:attributeName="documentNumber" />
      </list>
    </property>
    <property name="resultFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="routingFormConflictOfInterestDescription" />
        <bean parent="FieldDefinition"
              p:attributeName="documentNumber" />
      </list>
    </property>
  </bean>
</beans>
