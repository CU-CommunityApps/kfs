<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="BudgetInstitutionCostShare" parent="BudgetInstitutionCostShare-parentBean" />

  <bean id="BudgetInstitutionCostShare-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.kfs.module.cg.businessobject.BudgetInstitutionCostShare" />
    <property name="inquiryDefinition" >
      <ref bean="BudgetInstitutionCostShare-inquiryDefinition" />
    </property>
    <property name="objectLabel" value="Budget Institution Party Cost Share" />
    <property name="objectDescription" value="Budget Institution Party Cost Share is the institution party cost share for your Budget." />
    <property name="attributes" >
      <list>
        <ref bean="BudgetInstitutionCostShare-documentNumber" />
        <ref bean="BudgetInstitutionCostShare-budgetCostShareSequenceNumber" />
        <ref bean="BudgetInstitutionCostShare-chartOfAccountsCode" />
        <ref bean="BudgetInstitutionCostShare-organizationCode" />
        <ref bean="BudgetInstitutionCostShare-budgetCostShareDescription" />
        <ref bean="BudgetInstitutionCostShare-permissionIndicator" />
        <ref bean="BudgetInstitutionCostShare-versionNumber" />
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->


  <bean id="BudgetInstitutionCostShare-documentNumber" parent="BudgetInstitutionCostShare-documentNumber-parentBean" />

  <bean id="BudgetInstitutionCostShare-documentNumber-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="documentNumber" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Budget Number" />
    <property name="shortLabel" value="Budget Number" />
    <property name="maxLength" value="12" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="12" />
    </property>
    <property name="summary" value="Budget Number..." />
    <property name="description" value="Budget Number..." />
  </bean>
  <bean id="BudgetInstitutionCostShare-budgetCostShareSequenceNumber" parent="BudgetInstitutionCostShare-budgetCostShareSequenceNumber-parentBean" />

  <bean id="BudgetInstitutionCostShare-budgetCostShareSequenceNumber-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="budgetCostShareSequenceNumber" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Sequence Number" />
    <property name="shortLabel" value="Sequence Number" />
    <property name="maxLength" value="5" />
    <property name="validationPattern" >
      <bean parent="NumericValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="2" />
    </property>
    <property name="summary" value="Sequence Number" />
    <property name="description" value="Sequence Number" />
  </bean>
  <bean id="BudgetInstitutionCostShare-chartOfAccountsCode" parent="BudgetInstitutionCostShare-chartOfAccountsCode-parentBean" />

  <bean id="BudgetInstitutionCostShare-chartOfAccountsCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="chartOfAccountsCode" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Chart Code" />
    <property name="shortLabel" value="Chart" />
    <property name="maxLength" value="2" />
    <property name="validationPattern" >
      <bean parent="AlphaValidationPattern"
            p:exactLength="2" />
    </property>
    <property name="control" >
      <bean parent="SelectControlDefinition"
            p:valuesFinderClass="org.kuali.kfs.coa.businessobject.options.ChartValuesFinder"
            p:includeKeyInLabel="false" />
    </property>
    <property name="summary" value="Chart of Accounts Code" />
    <property name="description" value="Financial Chart of Accounts Code" />
  </bean>
  <bean id="BudgetInstitutionCostShare-organizationCode" parent="BudgetInstitutionCostShare-organizationCode-parentBean" />

  <bean id="BudgetInstitutionCostShare-organizationCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="organizationCode" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Organization Code" />
    <property name="shortLabel" value="Organization Code" />
    <property name="maxLength" value="4" />
    <property name="validationPattern" >
      <bean parent="AlphaValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="SelectControlDefinition"
            p:valuesFinderClass="org.kuali.kfs.coa.businessobject.options.ChartValuesFinder"
            p:includeKeyInLabel="false" />
    </property>
    <property name="summary" value="Organization Code" />
    <property name="description" value="Organization Code" />
  </bean>
  <bean id="BudgetInstitutionCostShare-budgetCostShareDescription" parent="BudgetInstitutionCostShare-budgetCostShareDescription-parentBean" />

  <bean id="BudgetInstitutionCostShare-budgetCostShareDescription-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="budgetCostShareDescription" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Description" />
    <property name="shortLabel" value="Description" />
    <property name="maxLength" value="50" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern"
            p:allowWhitespace="true" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="40" />
    </property>
    <property name="summary" value="Description" />
    <property name="description" value="Description" />
  </bean>
  <bean id="BudgetInstitutionCostShare-permissionIndicator" parent="BudgetInstitutionCostShare-permissionIndicator-parentBean" />

  <bean id="BudgetInstitutionCostShare-permissionIndicator-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="permissionIndicator" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Permission Indicator" />
    <property name="shortLabel" value="Permission" />
    <property name="maxLength" value="1" />
    <property name="control" >
      <bean parent="CheckboxControlDefinition" />
    </property>
    <property name="summary" value="Permission Indicator" />
    <property name="description" value="Indicates whether the cost share org reviewers have permission to the study." />
  </bean>
  <bean id="BudgetInstitutionCostShare-versionNumber" parent="BudgetInstitutionCostShare-versionNumber-parentBean" />

  <bean id="BudgetInstitutionCostShare-versionNumber-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="versionNumber" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Version Number" />
    <property name="shortLabel" value="Number" />
    <property name="maxLength" value="10" />
    <property name="validationPattern" >
      <bean parent="NumericValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="-1" />
    </property>
    <property name="summary" value="versionNumber summary..." />
    <property name="description" value="versionNumber description..." />
  </bean>

<!-- Business Object Inquiry Definition -->


  <bean id="BudgetInstitutionCostShare-inquiryDefinition" parent="BudgetInstitutionCostShare-inquiryDefinition-parentBean" />

  <bean id="BudgetInstitutionCostShare-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
    <property name="title" value="Budget Institution Cost Share" />
    <property name="inquirySections" >
      <list>
        <bean parent="InquirySectionDefinition">
          <property name="title" value="" />
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields" >
            <list>
              <bean parent="FieldDefinition"
                    p:attributeName="budgetCostShareSequenceNumber" />
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>
</beans>
