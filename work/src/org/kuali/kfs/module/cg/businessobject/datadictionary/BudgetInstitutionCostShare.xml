<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "../../../../core/datadictionary/dataDictionary-1_0.dtd">
<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.module.kra.budget.bo.BudgetInstitutionCostShare">


        <inquiry>
            <title>Budget Institution Cost Share</title>

            <inquirySections><inquirySection title="Header Text"><inquiryFields>
                <field attributeName="budgetCostShareSequenceNumber" />
            </inquiryFields></inquirySection></inquirySections>
        </inquiry>

        <attributes>

            <attribute name="documentNumber" forceUppercase="true">
                <label>Budget Number</label>
                <shortLabel>Budget Number</shortLabel>

                <maxLength>12</maxLength>
                <validationPattern>
                    <anyCharacter />
                </validationPattern>

                <control>
                    <text size="12" />
                </control>

                <summary>Budget Number...</summary>
                <description>Budget Number...</description>
            </attribute>

            <attribute name="budgetCostShareSequenceNumber" forceUppercase="true">
                <label>Sequence Number</label>
                <shortLabel>Sequence Number</shortLabel>

                <maxLength>5</maxLength>
                <validationPattern>
                    <numeric />
                </validationPattern>

                <control>
                    <text size="2" />
                </control>

                <summary>Sequence Number</summary>
                <description>Sequence Number</description>
            </attribute>

            <attribute name="chartOfAccountsCode" forceUppercase="true">
                <label>Chart Code</label>
                <shortLabel>Chart</shortLabel>

                <maxLength>2</maxLength>
                <validationPattern>
                    <alpha exactLength="2" />
                </validationPattern>

                <control>
                    <!-- text size="2" /-->
                    <!-- If we want to go back to the drop-down list, then use this:-->
                    <select valuesFinderClass="org.kuali.module.chart.lookup.keyvalues.ChartValuesFinder" />
                </control>

                <summary>Chart of Accounts Code</summary>
                <description>Financial Chart of Accounts Code</description>
            </attribute>

            <attribute name="organizationCode" forceUppercase="true">
                <label>Organization Code</label>
                <shortLabel>Organization Code</shortLabel>

                <maxLength>4</maxLength>
                <validationPattern>
                    <alpha/>
                </validationPattern>

                <control>
                    <!-- text size="2" /-->
                    <!-- If we want to go back to the drop-down list, then use this:-->
                    <select valuesFinderClass="org.kuali.module.chart.lookup.keyvalues.ChartValuesFinder" />
                </control>

                <summary>Organization Code</summary>
                <description>Organization Code</description>
            </attribute>


            <attribute name="budgetCostShareDescription" forceUppercase="true">
                <label>Description</label>
                <shortLabel>Description</shortLabel>

                <maxLength>50</maxLength>
                <validationPattern>
                    <anyCharacter allowWhitespace="true" />
                </validationPattern>

                <control>
                    <text size="40" />
                </control>

                <summary>Description</summary>
                <description>Description</description>
            </attribute>
            
            <attribute name="permissionIndicator" forceUppercase="true">
                <label>Permission Indicator</label>
                <shortLabel>Permission</shortLabel>

                <maxLength>1</maxLength>

                <control>
                    <checkbox />
                </control>

                <summary>Permission Indicator</summary>
                <description>Indicates whether the cost share org reviewers have permission to the study.</description>
            </attribute>

            <attribute name="versionNumber" forceUppercase="true">
                <label>Version Number</label>
                <shortLabel>Number</shortLabel>

                <maxLength>10</maxLength>

                <validationPattern>
                    <numeric />
                </validationPattern>

                <control>
                    <text size="-1" />
                </control>

                <summary>versionNumber summary...</summary>
                <description>versionNumber description...</description>
            </attribute>

        </attributes>

        <objectLabel>Budget Institution Party Cost Share</objectLabel>
        <objectDescription>Budget Institution Party Cost Share is the institution party cost share for your Budget.</objectDescription>
        
        <objectHelp parameterNamespace="KFS-RA" parameterDetailType="Budget" parameterName="INSTITUTION_COST_SHARE" />

    </businessObject>
</dictionaryEntry>
