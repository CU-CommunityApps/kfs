<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="RoutingFormProjectType" parent="RoutingFormProjectType-parentBean" />

  <bean id="RoutingFormProjectType-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.kfs.module.cg.businessobject.RoutingFormProjectType" />
    <property name="inquiryDefinition" >
      <ref bean="RoutingFormProjectType-inquiryDefinition" />
    </property>
    <property name="lookupDefinition" >
      <ref bean="RoutingFormProjectType-lookupDefinition" />
    </property>
    <property name="objectLabel" value="Routing Form Project Type" />
    <property name="attributes" >
      <list>
        <ref bean="RoutingFormProjectType-projectTypeCode" />
        <ref bean="RoutingFormProjectType-documentNumber" />
        <ref bean="RoutingFormProjectType-projectTypeSelectedIndicator" />
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->


  <bean id="RoutingFormProjectType-projectTypeCode" parent="RoutingFormProjectType-projectTypeCode-parentBean" />

  <bean id="RoutingFormProjectType-projectTypeCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="projectTypeCode" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Project Type" />
    <property name="shortLabel" value="Code" />
    <property name="maxLength" value="70" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="72" />
    </property>
    <property name="summary" value="projectTypeCode" />
    <property name="description" value="projectTypeCode description..." />
  </bean>
  <bean id="RoutingFormProjectType-documentNumber" parent="RoutingFormProjectType-documentNumber-parentBean" />

  <bean id="RoutingFormProjectType-documentNumber-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="documentNumber" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Document Number" />
    <property name="shortLabel" value="Number" />
    <property name="maxLength" value="14" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="16" />
    </property>
    <property name="summary" value="documentNumber" />
    <property name="description" value="documentNumber description..." />
  </bean>
  <bean id="RoutingFormProjectType-projectTypeSelectedIndicator" parent="RoutingFormProjectType-projectTypeSelectedIndicator-parentBean" />

  <bean id="RoutingFormProjectType-projectTypeSelectedIndicator-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="projectTypeSelectedIndicator" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Project Type Selected Indicator" />
    <property name="shortLabel" value="Indicator" />
    <property name="maxLength" value="1" />
    <property name="control" >
      <bean parent="CheckboxControlDefinition" />
    </property>
    <property name="summary" value="projectTypeSelectedIndicator" />
    <property name="description" value="projectTypeSelectedIndicator description..." />
  </bean>

<!-- Business Object Inquiry Definition -->


  <bean id="RoutingFormProjectType-inquiryDefinition" parent="RoutingFormProjectType-inquiryDefinition-parentBean" />

  <bean id="RoutingFormProjectType-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
    <property name="title" value="Routing Form Project Type Inquiry" />
    <property name="inquirySections" >
      <list>
        <bean parent="InquirySectionDefinition">
          <property name="title" value="" />
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields" >
            <list>
              <bean parent="FieldDefinition"
                    p:attributeName="projectTypeCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="documentNumber" />
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Business Object Lookup Definition -->


  <bean id="RoutingFormProjectType-lookupDefinition" parent="RoutingFormProjectType-lookupDefinition-parentBean" />

  <bean id="RoutingFormProjectType-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
    <property name="title" value="Routing Form Project Type Lookup" />
    <property name="instructions" value="Look up a Routing Form Project Type" />
    <property name="defaultSort" >
      <bean parent="SortDefinition">
      </bean>
    </property>
    <property name="lookupFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="projectTypeCode" />
        <bean parent="FieldDefinition"
              p:attributeName="documentNumber" />
      </list>
    </property>
    <property name="resultFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="projectTypeCode" />
        <bean parent="FieldDefinition"
              p:attributeName="documentNumber" />
      </list>
    </property>
  </bean>
</beans>
