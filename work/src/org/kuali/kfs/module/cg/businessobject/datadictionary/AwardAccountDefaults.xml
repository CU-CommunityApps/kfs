	<bean id="AwardAccountDefaults" parent="AwardAccountDefaults-parentBean" />

	<bean id="AwardAccountDefaults-parentBean" abstract="true"
		parent="BusinessObjectEntry">
		<property name="businessObjectClass"
			value="org.kuali.kfs.module.cg.businessobject.AwardAccountDefaults" />
		<property name="inquiryDefinition">
			<ref bean="AwardAccountDefaults-inquiryDefinition" />
		</property>
		<property name="lookupDefinition">
			<ref bean="AwardAccountDefaults-lookupDefinition" />
		</property>
		<property name="objectLabel" value="Award Account Defaults" />
		<property name="attributes">
			<list>
				<ref bean="AwardAccountDefaults-principalId" />
				<ref bean="AwardAccountDefaults-projectDirector.principalName" />
				<ref bean="AwardAccountDefaults-projectDirector.name" />
				<ref bean="AwardAccountDefaults-projectDirectorName" />
				<ref bean="AwardAccountDefaults-chartOfAccountsCode" />
				<ref bean="AwardAccountDefaults-accountNumber" />
				<ref bean="AwardAccountDefaults-account.accountName" />
				<ref bean="AwardAccount-active" />
			</list>
		</property>
		<property name="relationships">
			<list>
				<bean parent="RelationshipDefinition">
					<property name="objectAttributeName" value="projectDirector" />
					<property name="primitiveAttributes">
						<list>
							<bean parent="PrimitiveAttributeDefinition" p:sourceName="principalId"
								p:targetName="principalId" />
						</list>
					</property>
					<property name="supportAttributes">
						<list>
							<bean parent="SupportAttributeDefinition" p:identifier="true"
								p:sourceName="projectDirector.principalName" p:targetName="principalName" />
							<bean parent="SupportAttributeDefinition" p:sourceName="projectDirector.name"
								p:targetName="name" />
						</list>
					</property>
				</bean>
			</list>
		</property>
	</bean>

	<!-- Attribute Definitions -->
	<bean id="AwardAccountDefaults-principalId" parent="AwardAccount-principalId-parentBean" />

	<bean id="AwardAccountDefaults-projectDirector.principalName"
		parent="AwardAccount-projectDirector.principalName-parentBean" />

	<bean id="AwardAccountDefaults-projectDirector.name" parent="AwardAccount-projectDirector.name-parentBean" />

	<bean id="AwardAccountDefaults-projectDirectorName" parent="AwardAccount-projectDirectorName-parentBean" />

	<bean id="AwardAccountDefaults-chartOfAccountsCode" parent="AwardAccount-chartOfAccountsCode-parentBean" />

	<bean id="AwardAccountDefaults-accountNumber" parent="AwardAccount-accountNumber-parentBean" />

	<bean id="AwardAccountDefaults-account.accountName" parent="AwardAccount-account.accountName-parentBean" />

	<bean id="AwardAccountDefaults-active-parentBean" abstract="true"
		parent="GenericAttributes-activeIndicator">
		<property name="name" value="active" />
	</bean>



	<!-- Business Object Inquiry Definition -->

	<bean id="AwardAccountDefaults-inquiryDefinition" parent="AwardAccountDefaults-inquiryDefinition-parentBean" />

	<bean id="AwardAccountDefaults-inquiryDefinition-parentBean"
		abstract="true" parent="InquiryDefinition">
		<property name="title" value="Award Account Defaults Inquiry" />
		<property name="inquirySections">
			<list>
				<bean parent="InquirySectionDefinition">
					<property name="title" value="Award Account Default Details" />
					<property name="numberOfColumns" value="1" />
					<property name="inquiryFields">
						<list>
							<bean parent="FieldDefinition" p:attributeName="accountNumber" />
							<bean parent="FieldDefinition" p:attributeName="chartOfAccountsCode" />
							<bean parent="FieldDefinition" p:attributeName="principalId" />
							<bean parent="FieldDefinition" p:attributeName="active" />
						</list>
					</property>
				</bean>
			</list>
		</property>
	</bean>

	<!-- Business Object Lookup Definition -->


	<bean id="AwardAccountDefaults-lookupDefinition" parent="AwardAccountDefaults-lookupDefinition-parentBean" />

	<bean id="AwardAccountDefaults-lookupDefinition-parentBean"
		abstract="true" parent="LookupDefinition">
		<property name="title" value="Award Account Lookup" />

		<property name="defaultSort">
			<bean parent="SortDefinition">
			</bean>
		</property>
		<property name="lookupFields">
			<list>
				<bean parent="FieldDefinition" p:attributeName="accountNumber" />
				<bean parent="FieldDefinition" p:attributeName="chartOfAccountsCode" />
				<bean parent="FieldDefinition" p:attributeName="principalId" />
				<bean parent="FieldDefinition" p:attributeName="active" />
			</list>
		</property>
		<property name="resultFields">
			<list>
				<bean parent="FieldDefinition" p:attributeName="accountNumber" />
				<bean parent="FieldDefinition" p:attributeName="chartOfAccountsCode" />
				<bean parent="FieldDefinition" p:attributeName="principalId" />
				<bean parent="FieldDefinition" p:attributeName="active" />
			</list>
		</property>
	</bean>
</beans>
