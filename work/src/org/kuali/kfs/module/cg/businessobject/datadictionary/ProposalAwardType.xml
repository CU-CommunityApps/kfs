<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="ProposalAwardType" parent="ProposalAwardType-parentBean" />

  <bean id="ProposalAwardType-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.kfs.module.cg.businessobject.ProposalAwardType" />
    <property name="inquiryDefinition" >
      <ref bean="ProposalAwardType-inquiryDefinition" />
    </property>
    <property name="lookupDefinition" >
      <ref bean="ProposalAwardType-lookupDefinition" />
    </property>
    <property name="objectLabel" value="Proposal Type" />
    <property name="attributes" >
      <list>
        <ref bean="ProposalAwardType-proposalAwardTypeCode" />
        <ref bean="ProposalAwardType-proposalAwardTypeDescription" />
        <ref bean="ProposalAwardType-active" />
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->


  <bean id="ProposalAwardType-proposalAwardTypeCode" parent="ProposalAwardType-proposalAwardTypeCode-parentBean" />

  <bean id="ProposalAwardType-proposalAwardTypeCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="proposalAwardTypeCode" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Proposal Award Type Code" />
    <property name="shortLabel" value="Code" />
    <property name="maxLength" value="1" />
    <property name="validationPattern" >
      <bean parent="AlphaValidationPattern"
            p:exactLength="1" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="3" />
    </property>
  </bean>
  <bean id="ProposalAwardType-proposalAwardTypeDescription" parent="ProposalAwardType-proposalAwardTypeDescription-parentBean" />

  <bean id="ProposalAwardType-proposalAwardTypeDescription-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="proposalAwardTypeDescription" />
    <property name="label" value="Proposal Award Type Description" />
    <property name="shortLabel" value="Description" />
    <property name="maxLength" value="20" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern"
            p:allowWhitespace="true" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="22" />
    </property>
  </bean>
  <bean id="ProposalAwardType-active" parent="ProposalAwardType-active-parentBean" />

  <bean id="ProposalAwardType-active-parentBean" abstract="true" parent="GenericAttributes-activeIndicator">
    <property name="name" value="active" />
  </bean>

<!-- Business Object Inquiry Definition -->


  <bean id="ProposalAwardType-inquiryDefinition" parent="ProposalAwardType-inquiryDefinition-parentBean" />

  <bean id="ProposalAwardType-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
    <property name="title" value="Proposal Type Inquiry" />
    <property name="inquirySections" >
      <list>
        <bean parent="InquirySectionDefinition">
          <property name="title" value="Proposal Award Type Details" />
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields" >
            <list>
              <bean parent="FieldDefinition"
                    p:attributeName="proposalAwardTypeCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="proposalAwardTypeDescription" />
              <bean parent="FieldDefinition"
                    p:attributeName="active" />
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Business Object Lookup Definition -->


  <bean id="ProposalAwardType-lookupDefinition" parent="ProposalAwardType-lookupDefinition-parentBean" />

  <bean id="ProposalAwardType-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
    <property name="title" value="Proposal Type Lookup" />
    <property name="instructions" value="Look up a Proposal Award Type" />
    <property name="defaultSort" >
      <bean parent="SortDefinition">
      </bean>
    </property>
    <property name="lookupFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="proposalAwardTypeCode" />
        <bean parent="FieldDefinition"
              p:attributeName="proposalAwardTypeDescription" />
        <bean parent="FieldDefinition"
              p:attributeName="active" />
      </list>
    </property>
    <property name="resultFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="proposalAwardTypeCode" />
        <bean parent="FieldDefinition"
              p:attributeName="proposalAwardTypeDescription" />
        <bean parent="FieldDefinition"
              p:attributeName="active" />
      </list>
    </property>
  </bean>
</beans>
