<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "../dataDictionary-1_0.dtd">
<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.module.kra.routingform.bo.ProjectType">


        <inquiry>
            <title>Project Type Inquiry</title>

            <inquirySections><inquirySection title="Header Text"><inquiryFields>
                <field attributeName="projectTypeCode" />
                <field attributeName="projectTypeDescription" />
                <field attributeName="sortNumber" />
                <field attributeName="dataObjectMaintenanceCodeActiveIndicator" />
            </inquiryFields></inquirySection></inquirySections>
        </inquiry>

        <lookup>
            <title>Project Type Lookup</title>

            <instructions>Look up a Project Type</instructions>

            <defaultSort sortAscending="true">
                <sortAttributes>
                    <sortAttribute attributeName="projectTypeCode" />
                </sortAttributes>
            </defaultSort>

            <lookupFields>
                <lookupField attributeName="projectTypeCode" required="false" />
                <lookupField attributeName="projectTypeDescription" required="false" />
                <lookupField attributeName="sortNumber" required="false" />
                <lookupField attributeName="dataObjectMaintenanceCodeActiveIndicator" required="false" />
            </lookupFields>

            <resultFields>
                <field attributeName="projectTypeCode" />
                <field attributeName="projectTypeDescription" />
                <field attributeName="sortNumber" />
                <field attributeName="dataObjectMaintenanceCodeActiveIndicator" />
            </resultFields>
        </lookup>

        <attributes>

            <attribute name="projectTypeCode" forceUppercase="true">
                <label>Project Type Code</label>
                <shortLabel>Code</shortLabel>

                <maxLength>1</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="3" />
                </control>

                <summary>projectTypeCode</summary>
                <description>projectTypeCode description...</description>
			</attribute>

            <attribute name="projectTypeDescription" forceUppercase="false">
                <label>Project Type Description</label>
                <shortLabel>Description</shortLabel>

                <maxLength>40</maxLength>
                <validationPattern><anyCharacter allowWhitespace="true" /></validationPattern>

                <control>
                    <text size="42" />
                </control>

                <summary>projectTypeDescription</summary>
                <description>projectTypeDescription description...</description>
			</attribute>

            <attribute name="sortNumber" forceUppercase="true">
                <label>Sort Number</label>
                <shortLabel>Number</shortLabel>

                <maxLength>2</maxLength>
                <validationPattern><numeric /></validationPattern>

                <control>
                    <text size="4" />
                </control>

                <summary>sortNumber</summary>
                <description>sortNumber description...</description>
			</attribute>

            <attributeReference name="dataObjectMaintenanceCodeActiveIndicator" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="activeIndicator" />

            <attributeReference name="versionNumber" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="versionNumber" />

        </attributes>
        <objectLabel>Project Type</objectLabel>
    </businessObject>
</dictionaryEntry>
