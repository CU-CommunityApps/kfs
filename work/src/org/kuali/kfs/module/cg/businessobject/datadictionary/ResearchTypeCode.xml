<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="ResearchTypeCode" parent="ResearchTypeCode-parentBean" />

  <bean id="ResearchTypeCode-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.kfs.module.cg.businessobject.ResearchTypeCode" />
    <property name="inquiryDefinition" >
      <ref bean="ResearchTypeCode-inquiryDefinition" />
    </property>
    <property name="lookupDefinition" >
      <ref bean="ResearchTypeCode-lookupDefinition" />
    </property>
    <property name="objectLabel" value="Research Type" />
    <property name="attributes" >
      <list>
        <ref bean="ResearchTypeCode-researchTypeCode" />
        <ref bean="ResearchTypeCode-researchTypeDescription" />
        <ref bean="ResearchTypeCode-active" />
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->


  <bean id="ResearchTypeCode-researchTypeCode" parent="ResearchTypeCode-researchTypeCode-parentBean" />

  <bean id="ResearchTypeCode-researchTypeCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="researchTypeCode" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Research Type Code" />
    <property name="shortLabel" value="Code" />
    <property name="maxLength" value="1" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="3" />
    </property>
    
    
  </bean>
  <bean id="ResearchTypeCode-researchTypeDescription" parent="ResearchTypeCode-researchTypeDescription-parentBean" />

  <bean id="ResearchTypeCode-researchTypeDescription-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="researchTypeDescription" />
    <property name="label" value="Research Type Description" />
    <property name="shortLabel" value="Description" />
    <property name="maxLength" value="40" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern"
            p:allowWhitespace="true" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="42" />
    </property>
    
    
  </bean>
  <bean id="ResearchTypeCode-active" parent="ResearchTypeCode-active-parentBean" />

  <bean id="ResearchTypeCode-active-parentBean" abstract="true" parent="GenericAttributes-activeIndicator">
    <property name="name" value="active" />
  </bean>

<!-- Business Object Inquiry Definition -->


  <bean id="ResearchTypeCode-inquiryDefinition" parent="ResearchTypeCode-inquiryDefinition-parentBean" />

  <bean id="ResearchTypeCode-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
    <property name="title" value="Research Type Inquiry" />
    <property name="inquirySections" >
      <list>
        <bean parent="InquirySectionDefinition">
          <property name="title" value="" />
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields" >
            <list>
              <bean parent="FieldDefinition"
                    p:attributeName="researchTypeCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="researchTypeDescription" />
              <bean parent="FieldDefinition"
                    p:attributeName="active" />
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Business Object Lookup Definition -->


  <bean id="ResearchTypeCode-lookupDefinition" parent="ResearchTypeCode-lookupDefinition-parentBean" />

  <bean id="ResearchTypeCode-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
    <property name="title" value="Research Type Lookup" />
    <property name="instructions" value="Look up a Research Type" />
    <property name="defaultSort" >
      <bean parent="SortDefinition">
      </bean>
    </property>
    <property name="lookupFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="researchTypeCode" />
        <bean parent="FieldDefinition"
              p:attributeName="researchTypeDescription" />
        <bean parent="FieldDefinition"
              p:defaultValue="Y"
              p:attributeName="active" />
      </list>
    </property>
    <property name="resultFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="researchTypeCode" />
        <bean parent="FieldDefinition"
              p:attributeName="researchTypeDescription" />
        <bean parent="FieldDefinition"
              p:attributeName="active" />
      </list>
    </property>
  </bean>
</beans>
