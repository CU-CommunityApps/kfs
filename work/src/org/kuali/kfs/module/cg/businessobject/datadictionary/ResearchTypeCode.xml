<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "../dataDictionary-1_0.dtd">
<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.module.kra.routingform.bo.ResearchTypeCode">
        <customAttributeEnabled>false</customAttributeEnabled>

        <inquiry>
            <title>Research Type Inquiry</title>

            <inquirySections><inquirySection title="Header Text"><inquiryFields>
                <field attributeName="dataObjectMaintenanceCodeActiveIndicator" />
                <field attributeName="researchTypeCode" />
                <field attributeName="researchTypeDescription" />
            </inquiryFields></inquirySection></inquirySections>
        </inquiry>

        <lookup>
            <title>Research Type Lookup</title>

            <instructions>Look up a Research Type</instructions>

            <defaultSort sortAscending="true">
                <sortAttributes>
                    <sortAttribute attributeName="researchTypeCode" />
                </sortAttributes>
            </defaultSort>

            <lookupFields>
                <lookupField attributeName="dataObjectMaintenanceCodeActiveIndicator" required="false" />
                <lookupField attributeName="researchTypeCode" required="false" />
                <lookupField attributeName="researchTypeDescription" required="false" />
            </lookupFields>

            <resultFields>
                <field attributeName="dataObjectMaintenanceCodeActiveIndicator" />
                <field attributeName="researchTypeCode" />
                <field attributeName="researchTypeDescription" />
            </resultFields>
        </lookup>

        <attributes>

            <attribute name="dataObjectMaintenanceCodeActiveIndicator" forceUppercase="true">
                <label>Data Object Maintenance Code Active Indicator</label>
                <shortLabel>Indicator</shortLabel>

                <maxLength>1</maxLength>

                <control>
                    <checkbox />
                </control>

                <summary>dataObjectMaintenanceCodeActiveIndicator</summary>
                <description>dataObjectMaintenanceCodeActiveIndicator description...</description>
			</attribute>

            <attribute name="researchTypeCode" forceUppercase="true">
                <label>Research Type Code</label>
                <shortLabel>Code</shortLabel>

                <maxLength>1</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="3" />
                </control>

                <summary>researchTypeCode</summary>
                <description>researchTypeCode description...</description>
			</attribute>

            <attribute name="researchTypeDescription" forceUppercase="true">
                <label>Research Type Description</label>
                <shortLabel>Description</shortLabel>

                <maxLength>40</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="42" />
                </control>

                <summary>researchTypeDescription</summary>
                <description>researchTypeDescription description...</description>
			</attribute>

            <attributeReference name="versionNumber" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="versionNumber" />

        </attributes>
    </businessObject>
</dictionaryEntry>
