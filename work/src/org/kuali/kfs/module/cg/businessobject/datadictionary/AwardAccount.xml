<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="AwardAccount" parent="AwardAccount-parentBean" />

  <bean id="AwardAccount-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.kfs.module.cg.businessobject.AwardAccount" />
    <property name="inquiryDefinition" >
      <ref bean="AwardAccount-inquiryDefinition" />
    </property>
    <property name="lookupDefinition" >
      <ref bean="AwardAccount-lookupDefinition" />
    </property>
    <property name="objectLabel" value="Award Account" />
    <property name="attributes" >
      <list>
        <ref bean="AwardAccount-proposalNumber" />
        <ref bean="AwardAccount-personUniversalIdentifier" />
        <ref bean="AwardAccount-projectDirector.personUserIdentifier" />
        <ref bean="AwardAccount-projectDirector.personName" />
        <ref bean="AwardAccount-projectDirector.universalUser.personName" />
        <ref bean="AwardAccount-chartOfAccountsCode" />
        <ref bean="AwardAccount-accountNumber" />
        <ref bean="AwardAccount-account.accountName" />
        <ref bean="AwardAccount-award.proposal.federalPassThroughAgencyNumber" />
        <ref bean="AwardAccount-award.proposal.proposalFederalPassThroughIndicator" />
        <ref bean="AwardAccount-award.workgroupName" />
        <ref bean="AwardAccount-award.kualiGroupNames" />
        <ref bean="AwardAccount-newCollectionRecord" />
        <ref bean="AwardAccount-active" />
      </list>
    </property>
    <property name="relationships" >
      <list>
        <bean parent="RelationshipDefinition">
          <property name="objectAttributeName" value="projectDirector.universalUser" />
          <property name="primitiveAttributes" >
            <list>
              <bean parent="PrimitiveAttributeDefinition"
                    p:sourceName="projectDirector.personUniversalIdentifier"
                    p:targetName="personUniversalIdentifier" />
            </list>
          </property>
          <property name="supportAttributes" >
            <list>
              <bean parent="SupportAttributeDefinition"
                    p:identifier="true"
                    p:sourceName="projectDirector.personUserIdentifier"
                    p:targetName="personUserIdentifier" />
              <bean parent="SupportAttributeDefinition"
                    p:sourceName="projectDirector.personName"
                    p:targetName="personName" />
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->


  <bean id="AwardAccount-proposalNumber" parent="AwardAccount-proposalNumber-parentBean" />

  <bean id="AwardAccount-proposalNumber-parentBean" abstract="true" parent="Proposal-proposalNumber">
  </bean>
  <bean id="AwardAccount-personUniversalIdentifier" parent="AwardAccount-personUniversalIdentifier-parentBean" />

  <bean id="AwardAccount-personUniversalIdentifier-parentBean" abstract="true" parent="ProjectDirector-personUniversalIdentifier">
    <property name="label" value="Project Director ID" />
    <property name="shortLabel" value="Proj Dir ID" />
    <property name="control" >
      <bean parent="HiddenControlDefinition" />
    </property>
  </bean>
  <bean id="AwardAccount-projectDirector.personUserIdentifier" parent="AwardAccount-projectDirector.personUserIdentifier-parentBean" />

  <bean id="AwardAccount-projectDirector.personUserIdentifier-parentBean" abstract="true" parent="UniversalUser-personUserIdentifier">
    <property name="name" value="projectDirector.personUserIdentifier" />
    <property name="label" value="Project Director User ID" />
    <property name="required" value="false" />
  </bean>
  <bean id="AwardAccount-projectDirector.personName" parent="AwardAccount-projectDirector.personName-parentBean" />

  <bean id="AwardAccount-projectDirector.personName-parentBean" abstract="true" parent="UniversalUser-personName">
    <property name="name" value="projectDirector.personName" />
    <property name="label" value="Director Name" />
    <property name="required" value="false" />
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="40" />
    </property>
  </bean>
  <bean id="AwardAccount-projectDirector.universalUser.personName" parent="AwardAccount-projectDirector.universalUser.personName-parentBean" />

  <bean id="AwardAccount-projectDirector.universalUser.personName-parentBean" abstract="true" parent="AwardAccount-projectDirector.personName">
    <property name="name" value="projectDirector.universalUser.personName" />
  </bean>
  <bean id="AwardAccount-chartOfAccountsCode" parent="AwardAccount-chartOfAccountsCode-parentBean" />

  <bean id="AwardAccount-chartOfAccountsCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="chartOfAccountsCode" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Chart Of Accounts Code" />
    <property name="shortLabel" value="Code" />
    <property name="maxLength" value="2" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="SelectControlDefinition"
            p:valuesFinderClass="org.kuali.kfs.coa.businessobject.options.ChartValuesFinder"
            p:includeKeyInLabel="false" />
    </property>
  </bean>
  <bean id="AwardAccount-accountNumber" parent="AwardAccount-accountNumber-parentBean" />

  <bean id="AwardAccount-accountNumber-parentBean" abstract="true" parent="Account-accountNumber">
  </bean>
  <bean id="AwardAccount-account.accountName" parent="AwardAccount-account.accountName-parentBean" />

  <bean id="AwardAccount-account.accountName-parentBean" abstract="true" parent="Account-accountName">
    <property name="name" value="account.accountName" />
  </bean>
  <bean id="AwardAccount-award.proposal.federalPassThroughAgencyNumber" parent="AwardAccount-award.proposal.federalPassThroughAgencyNumber-parentBean" />

  <bean id="AwardAccount-award.proposal.federalPassThroughAgencyNumber-parentBean" abstract="true" parent="Proposal-federalPassThroughAgencyNumber">
    <property name="name" value="award.proposal.federalPassThroughAgencyNumber" />
    <property name="label" value="Proposal Federal Pass Through Agency Number" />
    <property name="shortLabel" value="Proposal Fed Pass Agency Num" />
  </bean>
  <bean id="AwardAccount-award.proposal.proposalFederalPassThroughIndicator" parent="AwardAccount-award.proposal.proposalFederalPassThroughIndicator-parentBean" />

  <bean id="AwardAccount-award.proposal.proposalFederalPassThroughIndicator-parentBean" abstract="true" parent="Proposal-proposalFederalPassThroughIndicator">
    <property name="name" value="award.proposal.proposalFederalPassThroughIndicator" />
  </bean>
  <bean id="AwardAccount-award.workgroupName" parent="AwardAccount-award.workgroupName-parentBean" />

  <bean id="AwardAccount-award.workgroupName-parentBean" abstract="true" parent="Award-workgroupName">
    <property name="name" value="award.workgroupName" />
  </bean>
  <bean id="AwardAccount-award.kualiGroupNames" parent="AwardAccount-award.kualiGroupNames-parentBean" />

  <bean id="AwardAccount-award.kualiGroupNames-parentBean" abstract="true" parent="Award-kualiGroupNames">
    <property name="name" value="award.kualiGroupNames" />
  </bean>
  <bean id="AwardAccount-newCollectionRecord" parent="AwardAccount-newCollectionRecord-parentBean" />

  <bean id="AwardAccount-newCollectionRecord-parentBean" abstract="true" parent="GenericAttributes-newCollectionRecord">
  </bean>
  <bean id="AwardAccount-active" parent="AwardAccount-active-parentBean" />

  <bean id="AwardAccount-active-parentBean" abstract="true" parent="GenericAttributes-activeIndicator">
    <property name="name" value="active" />
  </bean>


<!-- Business Object Inquiry Definition -->


  <bean id="AwardAccount-inquiryDefinition" parent="AwardAccount-inquiryDefinition-parentBean" />

  <bean id="AwardAccount-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
    <property name="title" value="Award Account Inquiry" />
    <property name="inquirySections" >
      <list>
        <bean parent="InquirySectionDefinition">
          <property name="title" value="Award Account Details" />
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields" >
            <list>
              <bean parent="FieldDefinition"
                    p:attributeName="accountNumber" />
              <bean parent="FieldDefinition"
                    p:attributeName="chartOfAccountsCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="personUniversalIdentifier" />
              <bean parent="FieldDefinition"
                    p:attributeName="proposalNumber" />
              <bean parent="FieldDefinition"
                    p:attributeName="active" /> 
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Business Object Lookup Definition -->


  <bean id="AwardAccount-lookupDefinition" parent="AwardAccount-lookupDefinition-parentBean" />

  <bean id="AwardAccount-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
    <property name="title" value="Award Account Lookup" />
    <property name="instructions" value="Look up an Award Account" />
    <property name="defaultSort" >
      <bean parent="SortDefinition">
      </bean>
    </property>
    <property name="lookupFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="accountNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="chartOfAccountsCode" />
        <bean parent="FieldDefinition"
              p:attributeName="personUniversalIdentifier" />
        <bean parent="FieldDefinition"
              p:attributeName="proposalNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="active" /> 
      </list>
    </property>
    <property name="resultFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="accountNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="chartOfAccountsCode" />
        <bean parent="FieldDefinition"
              p:attributeName="personUniversalIdentifier" />
        <bean parent="FieldDefinition"
              p:attributeName="proposalNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="active" /> 
      </list>
    </property>
  </bean>
</beans>
