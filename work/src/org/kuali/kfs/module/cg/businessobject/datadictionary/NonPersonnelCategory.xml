<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="NonPersonnelCategory" parent="NonPersonnelCategory-parentBean" />

  <bean id="NonPersonnelCategory-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.kfs.module.cg.businessobject.NonPersonnelCategory" />
    <property name="inquiryDefinition" >
      <ref bean="NonPersonnelCategory-inquiryDefinition" />
    </property>
    <property name="lookupDefinition" >
      <ref bean="NonPersonnelCategory-lookupDefinition" />
    </property>
    <property name="titleAttribute" value="code" />
    <property name="objectLabel" value="Non-Personnel Category" />
    <property name="attributes" >
      <list>
        <ref bean="NonPersonnelCategory-code" />
        <ref bean="NonPersonnelCategory-active" />
        <ref bean="NonPersonnelCategory-name" />
        <ref bean="NonPersonnelCategory-sortNumber" />
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->


  <bean id="NonPersonnelCategory-code" parent="NonPersonnelCategory-code-parentBean" />

  <bean id="NonPersonnelCategory-code-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="code" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Nonpersonnel Category Code" />
    <property name="shortLabel" value="Code" />
    <property name="maxLength" value="2" />
    <property name="validationPattern" >
      <bean parent="AlphaValidationPattern"
            p:exactLength="2" />
    </property>
    <property name="required" value="true" />
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="4" />
    </property>
    
    
  </bean>
  <bean id="NonPersonnelCategory-active" parent="NonPersonnelCategory-active-parentBean" />

  <bean id="NonPersonnelCategory-active-parentBean" abstract="true" parent="GenericAttributes-activeIndicator">
    <property name="name" value="active" />
  </bean>
  <bean id="NonPersonnelCategory-name" parent="NonPersonnelCategory-name-parentBean" />

  <bean id="NonPersonnelCategory-name-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="name" />
    <property name="label" value="Nonpersonnel Category Name" />
    <property name="shortLabel" value="Name" />
    <property name="maxLength" value="40" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern"
            p:allowWhitespace="true" />
    </property>
    <property name="required" value="true" />
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="42" />
    </property>
    
    
  </bean>
  <bean id="NonPersonnelCategory-sortNumber" parent="NonPersonnelCategory-sortNumber-parentBean" />

  <bean id="NonPersonnelCategory-sortNumber-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="sortNumber" />
    <property name="label" value="Nonpersonnel Category Sort Number" />
    <property name="shortLabel" value="Sort Number" />
    <property name="maxLength" value="2" />
    <property name="validationPattern" >
      <bean parent="NumericValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="2" />
    </property>
    
    
  </bean>

<!-- Business Object Inquiry Definition -->


  <bean id="NonPersonnelCategory-inquiryDefinition" parent="NonPersonnelCategory-inquiryDefinition-parentBean" />

  <bean id="NonPersonnelCategory-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
    <property name="title" value="Non-Personnel Category Inquiry" />
    <property name="inquirySections" >
      <list>
        <bean parent="InquirySectionDefinition">
          <property name="title" value="" />
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields" >
            <list>
              <bean parent="FieldDefinition"
                    p:attributeName="code" />
              <bean parent="FieldDefinition"
                    p:attributeName="name" />
              <bean parent="FieldDefinition"
                    p:attributeName="sortNumber" />
              <bean parent="FieldDefinition"
                    p:attributeName="active" />
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Business Object Lookup Definition -->


  <bean id="NonPersonnelCategory-lookupDefinition" parent="NonPersonnelCategory-lookupDefinition-parentBean" />

  <bean id="NonPersonnelCategory-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
    <property name="title" value="Non-Personnel Category Lookup" />
    <property name="instructions" value="Lookup a Nonpersonnel Category" />
    <property name="defaultSort" >
      <bean parent="SortDefinition">
        <property name="attributeNames" >
          <list>
            <value>code</value>
          </list>
        </property>
        <property name="sortAscending" value="false" />
      </bean>
    </property>
    <property name="lookupFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="code" />
        <bean parent="FieldDefinition"
              p:attributeName="name" />
        <bean parent="FieldDefinition"
              p:defaultValue="Y"
              p:attributeName="active" />
      </list>
    </property>
    <property name="resultFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="code"
              p:noInquiry="true" />
        <bean parent="FieldDefinition"
              p:attributeName="name" />
        <bean parent="FieldDefinition"
              p:attributeName="active" />
      </list>
    </property>
  </bean>
</beans>
