<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="ProposalResearchRisk" parent="ProposalResearchRisk-parentBean" />

  <bean id="ProposalResearchRisk-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.module.cg.bo.ProposalResearchRisk" />
    <property name="objectLabel" value="Proposal Research Risk" />
    <property name="attributes" >
      <list>
        <ref bean="ProposalResearchRisk-researchRiskTypeCode" />
        <ref bean="ProposalResearchRisk-proposalNumber" />
        <ref bean="ProposalResearchRisk-researchRiskTypeIndicator" />
        <ref bean="ProposalResearchRisk-newCollectionRecord" />
        <ref bean="ProposalResearchRisk-versionNumber" />
        <ref bean="ProposalResearchRisk-researchRiskType.researchRiskTypeDescription" />
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->


  <bean id="ProposalResearchRisk-researchRiskTypeCode" parent="ProposalResearchRisk-researchRiskTypeCode-parentBean" />

  <bean id="ProposalResearchRisk-researchRiskTypeCode-parentBean" abstract="true" parent="ResearchRiskType-researchRiskTypeCode">
    <property name="control" >
      <bean parent="HiddenControlDefinition" />
    </property>
  </bean>
  <bean id="ProposalResearchRisk-proposalNumber" parent="ProposalResearchRisk-proposalNumber-parentBean" />

  <bean id="ProposalResearchRisk-proposalNumber-parentBean" abstract="true" parent="Proposal-proposalNumber">
  </bean>
  <bean id="ProposalResearchRisk-researchRiskTypeIndicator" parent="ProposalResearchRisk-researchRiskTypeIndicator-parentBean" />

  <bean id="ProposalResearchRisk-researchRiskTypeIndicator-parentBean" abstract="true" parent="GenericAttributes-genericBoolean">
    <property name="name" value="researchRiskTypeIndicator" />
    <property name="label" value="Required" />
    <property name="shortLabel" value="Required" />
  </bean>
  <bean id="ProposalResearchRisk-newCollectionRecord" parent="ProposalResearchRisk-newCollectionRecord-parentBean" />

  <bean id="ProposalResearchRisk-newCollectionRecord-parentBean" abstract="true" parent="GenericAttributes-newCollectionRecord">
  </bean>
  <bean id="ProposalResearchRisk-versionNumber" parent="ProposalResearchRisk-versionNumber-parentBean" />

  <bean id="ProposalResearchRisk-versionNumber-parentBean" abstract="true" parent="GenericAttributes-versionNumber">
  </bean>
  <bean id="ProposalResearchRisk-researchRiskType.researchRiskTypeDescription" parent="ProposalResearchRisk-researchRiskType.researchRiskTypeDescription-parentBean" />

  <bean id="ProposalResearchRisk-researchRiskType.researchRiskTypeDescription-parentBean" abstract="true" parent="ResearchRiskType-researchRiskTypeDescription">
    <property name="name" value="researchRiskType.researchRiskTypeDescription" />
    <property name="label" value="Type" />
  </bean>
</beans>
