<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "../dataDictionary-1_0.dtd">
<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.module.kra.routingform.bo.RoutingFormInstitutionCostShare">
        <customAttributeEnabled>false</customAttributeEnabled>

        <inquiry>
            <title>Routing Form Institution Cost Share Inquiry</title>

            <inquiryFields>
                <field attributeName="accountNumber" />
                <field attributeName="chartOfAccountsCode" />
                <field attributeName="organizationCode" />
                <field attributeName="proposalCostShareAmount" />
                <field attributeName="proposalCostShareDescription" />
                <field attributeName="proposalCostShareSequenceNumber" />
                <field attributeName="researchDocumentNumber" />
            </inquiryFields>
        </inquiry>

        <lookup>
            <title>Routing Form Institution Cost Share Lookup</title>

			<menubar><![CDATA[<a href="maintenance.do?methodToCall=start&businessObjectClassName=org.kuali.module.kra.routingform.bo.RoutingFormInstitutionCostShare">Create New</a>]]></menubar>

            <instructions>Look up a Routing Form Institution Cost Share</instructions>

            <defaultSort sortAscending="true">
                <sortAttributes>
                    <sortAttribute attributeName="proposalCostShareSequenceNumber" />
                    <sortAttribute attributeName="researchDocumentNumber" />
                </sortAttributes>
            </defaultSort>

            <lookupFields>
                <lookupField attributeName="accountNumber" required="false" />
                <lookupField attributeName="chartOfAccountsCode" required="false" />
                <lookupField attributeName="organizationCode" required="false" />
                <lookupField attributeName="proposalCostShareAmount" required="false" />
                <lookupField attributeName="proposalCostShareDescription" required="false" />
                <lookupField attributeName="proposalCostShareSequenceNumber" required="false" />
                <lookupField attributeName="researchDocumentNumber" required="false" />
            </lookupFields>

            <resultFields>
                <field attributeName="accountNumber" />
                <field attributeName="chartOfAccountsCode" />
                <field attributeName="organizationCode" />
                <field attributeName="proposalCostShareAmount" />
                <field attributeName="proposalCostShareDescription" />
                <field attributeName="proposalCostShareSequenceNumber" />
                <field attributeName="researchDocumentNumber" />
            </resultFields>
        </lookup>

        <attributes>

            <attribute name="accountNumber" forceUppercase="true">
                <label>Account Number</label>
                <shortLabel>Number</shortLabel>

                <maxLength>7</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="9" />
                </control>

                <summary>accountNumber</summary>
                <description>accountNumber description...</description>
			</attribute>

            <attribute name="chartOfAccountsCode" forceUppercase="true">
                <label>Chart Of Accounts Code</label>
                <shortLabel>Code</shortLabel>

                <maxLength>2</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="4" />
                </control>

                <summary>chartOfAccountsCode</summary>
                <description>chartOfAccountsCode description...</description>
			</attribute>

            <attribute name="organizationCode" forceUppercase="true">
                <label>Organization Code</label>
                <shortLabel>Code</shortLabel>

                <maxLength>4</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="6" />
                </control>

                <summary>organizationCode</summary>
                <description>organizationCode description...</description>
			</attribute>

            <attribute name="proposalCostShareAmount" forceUppercase="true">
                <label>Proposal Cost Share Amount</label>
                <shortLabel>Amount</shortLabel>

                <maxLength>19</maxLength>
                <validationPattern><floatingPoint /></validationPattern>

                <control>
                    <text size="21" />
                </control>

                <summary>proposalCostShareAmount</summary>
                <description>proposalCostShareAmount description...</description>
			</attribute>

            <attribute name="proposalCostShareDescription" forceUppercase="true">
                <label>Proposal Cost Share Description</label>
                <shortLabel>Description</shortLabel>

                <maxLength>250</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <textarea rows="5" cols="80"/>
                </control>

                <summary>proposalCostShareDescription</summary>
                <description>proposalCostShareDescription description...</description>
			</attribute>

            <attribute name="proposalCostShareSequenceNumber" forceUppercase="true">
                <label>Proposal Cost Share Sequence Number</label>
                <shortLabel>Number</shortLabel>

                <maxLength>4</maxLength>
                <validationPattern><numeric /></validationPattern>

                <control>
                    <text size="6" />
                </control>

                <summary>proposalCostShareSequenceNumber</summary>
                <description>proposalCostShareSequenceNumber description...</description>
			</attribute>

            <attribute name="researchDocumentNumber" forceUppercase="true">
                <label>Research Document Number</label>
                <shortLabel>Number</shortLabel>

                <maxLength>14</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="16" />
                </control>

                <summary>researchDocumentNumber</summary>
                <description>researchDocumentNumber description...</description>
			</attribute>

            <attributeReference name="versionNumber" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="versionNumber" />

        </attributes>
    </businessObject>
</dictionaryEntry>
