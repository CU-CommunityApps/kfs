<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="RoutingFormInstitutionCostShare" parent="RoutingFormInstitutionCostShare-parentBean" />

  <bean id="RoutingFormInstitutionCostShare-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.kfs.module.cg.businessobject.RoutingFormInstitutionCostShare" />
    <property name="inquiryDefinition" >
      <ref bean="RoutingFormInstitutionCostShare-inquiryDefinition" />
    </property>
    <property name="lookupDefinition" >
      <ref bean="RoutingFormInstitutionCostShare-lookupDefinition" />
    </property>
    <property name="objectLabel" value="Routing Form Institution Cost Share" />
    <property name="attributes" >
      <list>
        <ref bean="RoutingFormInstitutionCostShare-accountNumber" />
        <ref bean="RoutingFormInstitutionCostShare-chartOfAccountsCode" />
        <ref bean="RoutingFormInstitutionCostShare-organizationCode" />
        <ref bean="RoutingFormInstitutionCostShare-routingFormCostShareAmount" />
        <ref bean="RoutingFormInstitutionCostShare-routingFormCostShareDescription" />
        <ref bean="RoutingFormInstitutionCostShare-routingFormCostShareSequenceNumber" />
        <ref bean="RoutingFormInstitutionCostShare-documentNumber" />
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->


  <bean id="RoutingFormInstitutionCostShare-accountNumber" parent="RoutingFormInstitutionCostShare-accountNumber-parentBean" />

  <bean id="RoutingFormInstitutionCostShare-accountNumber-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="accountNumber" />
    <property name="label" value="Account Number" />
    <property name="shortLabel" value="Account" />
    <property name="maxLength" value="7" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="9" />
    </property>
    <property name="summary" value="accountNumber" />
    <property name="description" value="accountNumber description..." />
  </bean>
  <bean id="RoutingFormInstitutionCostShare-chartOfAccountsCode" parent="RoutingFormInstitutionCostShare-chartOfAccountsCode-parentBean" />

  <bean id="RoutingFormInstitutionCostShare-chartOfAccountsCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="chartOfAccountsCode" />
    <property name="label" value="Chart Of Accounts Code" />
    <property name="shortLabel" value="Chart" />
    <property name="maxLength" value="2" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="SelectControlDefinition"
            p:valuesFinderClass="org.kuali.kfs.coa.businessobject.options.SimpleChartValuesFinder"
            p:includeKeyInLabel="false" />
    </property>
    <property name="summary" value="chartOfAccountsCode" />
    <property name="description" value="chartOfAccountsCode description..." />
  </bean>
  <bean id="RoutingFormInstitutionCostShare-organizationCode" parent="RoutingFormInstitutionCostShare-organizationCode-parentBean" />

  <bean id="RoutingFormInstitutionCostShare-organizationCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="organizationCode" />
    <property name="label" value="Organization Code" />
    <property name="shortLabel" value="Org" />
    <property name="maxLength" value="4" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="6" />
    </property>
    <property name="summary" value="organizationCode" />
    <property name="description" value="organizationCode description..." />
  </bean>
  <bean id="RoutingFormInstitutionCostShare-routingFormCostShareAmount" parent="RoutingFormInstitutionCostShare-routingFormCostShareAmount-parentBean" />

  <bean id="RoutingFormInstitutionCostShare-routingFormCostShareAmount-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="routingFormCostShareAmount" />
    <property name="label" value="Routing Form Cost Share Amount" />
    <property name="shortLabel" value="Amount" />
    <property name="maxLength" value="19" />
    <property name="validationPattern" >
      <bean parent="FixedPointValidationPattern"
            p:precision="19"
            p:scale="2"
            p:allowNegative="true" />
    </property>
    <property name="control" >
      <bean parent="CurrencyControlDefinition"
            p:formattedMaxLength="26"
            p:size="10" />
    </property>
    <property name="summary" value="routingFormCostShareAmount" />
    <property name="description" value="routingFormCostShareAmount description..." />
  </bean>
  <bean id="RoutingFormInstitutionCostShare-routingFormCostShareDescription" parent="RoutingFormInstitutionCostShare-routingFormCostShareDescription-parentBean" />

  <bean id="RoutingFormInstitutionCostShare-routingFormCostShareDescription-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="routingFormCostShareDescription" />
    <property name="label" value="Routing Form Cost Share Description" />
    <property name="shortLabel" value="Description" />
    <property name="maxLength" value="250" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern"
            p:allowWhitespace="true" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="50" />
    </property>
    <property name="summary" value="routingFormCostShareDescription" />
    <property name="description" value="routingFormCostShareDescription description..." />
  </bean>
  <bean id="RoutingFormInstitutionCostShare-routingFormCostShareSequenceNumber" parent="RoutingFormInstitutionCostShare-routingFormCostShareSequenceNumber-parentBean" />

  <bean id="RoutingFormInstitutionCostShare-routingFormCostShareSequenceNumber-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="routingFormCostShareSequenceNumber" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Routing Form Cost Share Sequence Number" />
    <property name="shortLabel" value="Number" />
    <property name="maxLength" value="4" />
    <property name="validationPattern" >
      <bean parent="NumericValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="HiddenControlDefinition" />
    </property>
    <property name="summary" value="routingFormCostShareSequenceNumber" />
    <property name="description" value="routingFormCostShareSequenceNumber description..." />
  </bean>
  <bean id="RoutingFormInstitutionCostShare-documentNumber" parent="RoutingFormInstitutionCostShare-documentNumber-parentBean" />

  <bean id="RoutingFormInstitutionCostShare-documentNumber-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="documentNumber" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Document Number" />
    <property name="shortLabel" value="Number" />
    <property name="maxLength" value="14" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="HiddenControlDefinition" />
    </property>
    <property name="summary" value="documentNumber" />
    <property name="description" value="documentNumber description..." />
  </bean>

<!-- Business Object Inquiry Definition -->


  <bean id="RoutingFormInstitutionCostShare-inquiryDefinition" parent="RoutingFormInstitutionCostShare-inquiryDefinition-parentBean" />

  <bean id="RoutingFormInstitutionCostShare-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
    <property name="title" value="Routing Form Institution Cost Share Inquiry" />
    <property name="inquirySections" >
      <list>
        <bean parent="InquirySectionDefinition">
          <property name="title" value="" />
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields" >
            <list>
              <bean parent="FieldDefinition"
                    p:attributeName="accountNumber" />
              <bean parent="FieldDefinition"
                    p:attributeName="chartOfAccountsCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="organizationCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="routingFormCostShareAmount" />
              <bean parent="FieldDefinition"
                    p:attributeName="routingFormCostShareDescription" />
              <bean parent="FieldDefinition"
                    p:attributeName="routingFormCostShareSequenceNumber" />
              <bean parent="FieldDefinition"
                    p:attributeName="documentNumber" />
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Business Object Lookup Definition -->


  <bean id="RoutingFormInstitutionCostShare-lookupDefinition" parent="RoutingFormInstitutionCostShare-lookupDefinition-parentBean" />

  <bean id="RoutingFormInstitutionCostShare-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
    <property name="title" value="Routing Form Institution Cost Share Lookup" />
    <property name="instructions" value="Look up a Routing Form Institution Cost Share" />
    <property name="defaultSort" >
      <bean parent="SortDefinition">
      </bean>
    </property>
    <property name="lookupFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="accountNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="chartOfAccountsCode" />
        <bean parent="FieldDefinition"
              p:attributeName="organizationCode" />
        <bean parent="FieldDefinition"
              p:attributeName="routingFormCostShareAmount" />
        <bean parent="FieldDefinition"
              p:attributeName="routingFormCostShareDescription" />
        <bean parent="FieldDefinition"
              p:attributeName="routingFormCostShareSequenceNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="documentNumber" />
      </list>
    </property>
    <property name="resultFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="accountNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="chartOfAccountsCode" />
        <bean parent="FieldDefinition"
              p:attributeName="organizationCode" />
        <bean parent="FieldDefinition"
              p:attributeName="routingFormCostShareAmount" />
        <bean parent="FieldDefinition"
              p:attributeName="routingFormCostShareDescription" />
        <bean parent="FieldDefinition"
              p:attributeName="routingFormCostShareSequenceNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="documentNumber" />
      </list>
    </property>
  </bean>
</beans>
