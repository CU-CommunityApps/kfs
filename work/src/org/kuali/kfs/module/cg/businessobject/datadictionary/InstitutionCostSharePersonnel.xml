<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "../../../../core/datadictionary/dataDictionary-1_0.dtd">
<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.module.kra.budget.bo.InstitutionCostSharePersonnel">
        <customAttributeEnabled>true</customAttributeEnabled>

        <inquiry>
            <title>Institution Cost Share Personnel</title>

            <inquiryFields>
                <field attributeName="researchDocumentNumber" />
            </inquiryFields>
        </inquiry>

        <attributes>

            <attribute name="researchDocumentNumber" forceUppercase="true">
                <label>Budget Number</label>
                <shortLabel>Budget Number</shortLabel>

                <maxLength>12</maxLength>
                <validationPattern>
                    <anyCharacter />
                </validationPattern>

                <control>
                    <text size="12" />
                </control>

                <summary>Budget Number...</summary>
                <description>Budget Number...</description>
            </attribute>

            <attribute name="chartOfAccountsCode" forceUppercase="true">
                <label>Source</label>
                <shortLabel>Code</shortLabel>

                <maxLength>2</maxLength>
                <validationPattern>
                    <alpha />
                </validationPattern>

                <control>
                    <!-- text size="2" /-->
                    <!-- If we want to go back to the drop-down list, then use this:-->
                    <select valuesFinderClass="org.kuali.core.lookup.keyvalues.ChartValuesFinder" />
                </control>

                <summary>Chart of Accounts Code</summary>
                <description>Financial Chart of Accounts Code</description>
            </attribute>

            <attribute name="organizationCode" forceUppercase="true">
                <label>Source</label>
                <shortLabel>Code</shortLabel>

                <maxLength>4</maxLength>
                <validationPattern>
                    <alpha />
                </validationPattern>

                <control>
                    <!-- text size="2" /-->
                    <!-- If we want to go back to the drop-down list, then use this:-->
                    <select valuesFinderClass="org.kuali.core.lookup.keyvalues.ChartValuesFinder" />
                </control>

                <summary>Organization Code</summary>
                <description>Organization Code</description>
            </attribute>


            <attribute name="budgetInstitutionCostSharePersonnelDescription" forceUppercase="true">
                <label>Description</label>
                <shortLabel>Description</shortLabel>

                <maxLength>50</maxLength>
                <validationPattern>
                    <anyCharacter allowWhitespace="true" />
                </validationPattern>

                <control>
                    <text size="40" />
                </control>

                <summary>Description</summary>
                <description>Description</description>
            </attribute>


            <attribute name="versionNumber" forceUppercase="true">
                <label>Version Number</label>
                <shortLabel>Number</shortLabel>

                <maxLength>10</maxLength>

                <validationPattern>
                    <numeric />
                </validationPattern>

                <control>
                    <text size="-1" />
                </control>

                <summary>versionNumber summary...</summary>
                <description>versionNumber description...</description>
            </attribute>

        </attributes>

    </businessObject>
</dictionaryEntry>

