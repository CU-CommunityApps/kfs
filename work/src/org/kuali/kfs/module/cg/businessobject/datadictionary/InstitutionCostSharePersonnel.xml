<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="InstitutionCostSharePersonnel" parent="InstitutionCostSharePersonnel-parentBean" />

  <bean id="InstitutionCostSharePersonnel-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.kfs.module.cg.businessobject.InstitutionCostSharePersonnel" />
    <property name="inquiryDefinition" >
      <ref bean="InstitutionCostSharePersonnel-inquiryDefinition" />
    </property>
    <property name="objectLabel" value="Institution Cost Share Personnel" />
    <property name="attributes" >
      <list>
        <ref bean="InstitutionCostSharePersonnel-documentNumber" />
        <ref bean="InstitutionCostSharePersonnel-chartOfAccountsCode" />
        <ref bean="InstitutionCostSharePersonnel-organizationCode" />
        <ref bean="InstitutionCostSharePersonnel-budgetInstitutionCostSharePersonnelDescription" />
        <ref bean="InstitutionCostSharePersonnel-versionNumber" />
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->


  <bean id="InstitutionCostSharePersonnel-documentNumber" parent="InstitutionCostSharePersonnel-documentNumber-parentBean" />

  <bean id="InstitutionCostSharePersonnel-documentNumber-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="documentNumber" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Budget Number" />
    <property name="shortLabel" value="Budget Number" />
    <property name="maxLength" value="12" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="12" />
    </property>
    <property name="summary" value="Budget Number..." />
    <property name="description" value="Budget Number..." />
  </bean>
  <bean id="InstitutionCostSharePersonnel-chartOfAccountsCode" parent="InstitutionCostSharePersonnel-chartOfAccountsCode-parentBean" />

  <bean id="InstitutionCostSharePersonnel-chartOfAccountsCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="chartOfAccountsCode" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Source" />
    <property name="shortLabel" value="Code" />
    <property name="maxLength" value="2" />
    <property name="validationPattern" >
      <bean parent="AlphaValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="SelectControlDefinition"
            p:valuesFinderClass="org.kuali.kfs.coa.businessobject.options.ChartValuesFinder"
            p:includeKeyInLabel="false" />
    </property>
    <property name="summary" value="Chart of Accounts Code" />
    <property name="description" value="Financial Chart of Accounts Code" />
  </bean>
  <bean id="InstitutionCostSharePersonnel-organizationCode" parent="InstitutionCostSharePersonnel-organizationCode-parentBean" />

  <bean id="InstitutionCostSharePersonnel-organizationCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="organizationCode" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Source" />
    <property name="shortLabel" value="Code" />
    <property name="maxLength" value="4" />
    <property name="validationPattern" >
      <bean parent="AlphaValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="SelectControlDefinition"
            p:valuesFinderClass="org.kuali.kfs.coa.businessobject.options.ChartValuesFinder"
            p:includeKeyInLabel="false" />
    </property>
    <property name="summary" value="Organization Code" />
    <property name="description" value="Organization Code" />
  </bean>
  <bean id="InstitutionCostSharePersonnel-budgetInstitutionCostSharePersonnelDescription" parent="InstitutionCostSharePersonnel-budgetInstitutionCostSharePersonnelDescription-parentBean" />

  <bean id="InstitutionCostSharePersonnel-budgetInstitutionCostSharePersonnelDescription-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="budgetInstitutionCostSharePersonnelDescription" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Description" />
    <property name="shortLabel" value="Description" />
    <property name="maxLength" value="50" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern"
            p:allowWhitespace="true" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="40" />
    </property>
    <property name="summary" value="Description" />
    <property name="description" value="Description" />
  </bean>
  <bean id="InstitutionCostSharePersonnel-versionNumber" parent="InstitutionCostSharePersonnel-versionNumber-parentBean" />

  <bean id="InstitutionCostSharePersonnel-versionNumber-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="versionNumber" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Version Number" />
    <property name="shortLabel" value="Number" />
    <property name="maxLength" value="10" />
    <property name="validationPattern" >
      <bean parent="NumericValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="-1" />
    </property>
    <property name="summary" value="versionNumber summary..." />
    <property name="description" value="versionNumber description..." />
  </bean>

<!-- Business Object Inquiry Definition -->


  <bean id="InstitutionCostSharePersonnel-inquiryDefinition" parent="InstitutionCostSharePersonnel-inquiryDefinition-parentBean" />

  <bean id="InstitutionCostSharePersonnel-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
    <property name="title" value="Institution Cost Share Personnel" />
    <property name="inquirySections" >
      <list>
        <bean parent="InquirySectionDefinition">
          <property name="title" value="Header Text" />
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields" >
            <list>
              <bean parent="FieldDefinition"
                    p:attributeName="documentNumber" />
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>
</beans>
