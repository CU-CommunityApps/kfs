		<property name="name" value="active" />
	</bean>

	<bean id="Agency-agencyAddresses.agencyAddressName" parent="Agency-agencyAddresses.agencyAddressName-parentBean" />

	<bean id="Agency-agencyAddresses.agencyAddressName-parentBean"
