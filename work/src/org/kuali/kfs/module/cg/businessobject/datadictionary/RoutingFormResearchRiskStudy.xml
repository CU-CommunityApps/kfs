    <property name="label" value="Exemption Number" />
    <property name="shortLabel" value="Number" />
    <property name="maxLength" value="10" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="12" />
    </property>
    <property name="summary" value="researchRiskExemptionNumber" />
    <property name="description" value="researchRiskExemptionNumber description..." />
  </bean>
  <bean id="RoutingFormResearchRiskStudy-researchRiskStudyApprovalDate" parent="RoutingFormResearchRiskStudy-researchRiskStudyApprovalDate-parentBean" />

  <bean id="RoutingFormResearchRiskStudy-researchRiskStudyApprovalDate-parentBean" abstract="true" parent="AttributeDefinition">
