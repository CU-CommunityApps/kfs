  <bean id="RoutingFormResearchRiskStudy-routingFormResearchRiskStudySequenceNumber-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="routingFormResearchRiskStudySequenceNumber" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Routing Form Research Risk Study Sequence Number" />
    <property name="shortLabel" value="Number" />
    <property name="maxLength" value="4" />
    <property name="validationPattern" >
      <bean parent="NumericValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="6" />
    </property>
    <property name="summary" value="routingFormResearchRiskStudySequenceNumber" />
    <property name="description" value="routingFormResearchRiskStudySequenceNumber description..." />
  </bean>
  <bean id="RoutingFormResearchRiskStudy-researchRiskStudyApprovalStatusCode" parent="RoutingFormResearchRiskStudy-researchRiskStudyApprovalStatusCode-parentBean" />

  <bean id="RoutingFormResearchRiskStudy-researchRiskStudyApprovalStatusCode-parentBean" abstract="true" parent="AttributeDefinition">
