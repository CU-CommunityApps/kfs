            p:size="10" />
    </property>
    <property name="summary" value="researchRiskStudyExpirationDate" />
    <property name="description" value="researchRiskStudyExpirationDate description..." />
  </bean>
  <bean id="RoutingFormResearchRiskStudy-researchRiskStudyName" parent="RoutingFormResearchRiskStudy-researchRiskStudyName-parentBean" />

  <bean id="RoutingFormResearchRiskStudy-researchRiskStudyName-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="researchRiskStudyName" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Research Risk Study Name" />
    <property name="shortLabel" value="Name" />
    <property name="maxLength" value="255" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextareaControlDefinition"
            p:rows="5"
            p:cols="80" />
    </property>
    <property name="summary" value="researchRiskStudyName" />
    <property name="description" value="researchRiskStudyName description..." />
  </bean>
  <bean id="RoutingFormResearchRiskStudy-researchRiskStudyNumber" parent="RoutingFormResearchRiskStudy-researchRiskStudyNumber-parentBean" />

  <bean id="RoutingFormResearchRiskStudy-researchRiskStudyNumber-parentBean" abstract="true" parent="AttributeDefinition">
