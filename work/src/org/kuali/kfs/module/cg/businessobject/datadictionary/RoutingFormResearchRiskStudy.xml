<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="RoutingFormResearchRiskStudy" parent="RoutingFormResearchRiskStudy-parentBean" />

  <bean id="RoutingFormResearchRiskStudy-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.module.kra.routingform.bo.RoutingFormResearchRiskStudy" />
    <property name="inquiryDefinition" >
      <ref bean="RoutingFormResearchRiskStudy-inquiryDefinition" />
    </property>
    <property name="lookupDefinition" >
      <ref bean="RoutingFormResearchRiskStudy-lookupDefinition" />
    </property>
    <property name="attributes" >
      <list>
        <ref bean="RoutingFormResearchRiskStudy-routingFormResearchRiskStudySequenceNumber" />
        <ref bean="RoutingFormResearchRiskStudy-researchRiskStudyApprovalStatusCode" />
        <ref bean="RoutingFormResearchRiskStudy-researchRiskExemptionNumber" />
        <ref bean="RoutingFormResearchRiskStudy-researchRiskStudyApprovalDate" />
        <ref bean="RoutingFormResearchRiskStudy-researchRiskStudyExpirationDate" />
        <ref bean="RoutingFormResearchRiskStudy-researchRiskStudyName" />
        <ref bean="RoutingFormResearchRiskStudy-researchRiskStudyNumber" />
        <ref bean="RoutingFormResearchRiskStudy-researchRiskStudyReviewCode" />
        <ref bean="RoutingFormResearchRiskStudy-researchRiskTypeCode" />
        <ref bean="RoutingFormResearchRiskStudy-documentNumber" />
        <ref bean="RoutingFormResearchRiskStudy-versionNumber" />
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->


