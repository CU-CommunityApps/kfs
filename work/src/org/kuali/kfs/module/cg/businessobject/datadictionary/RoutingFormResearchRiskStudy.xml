      <bean parent="TextControlDefinition"
            p:size="17" />
    </property>
    <property name="summary" value="researchRiskStudyNumber" />
    <property name="description" value="researchRiskStudyNumber description..." />
  </bean>
  <bean id="RoutingFormResearchRiskStudy-researchRiskStudyReviewCode" parent="RoutingFormResearchRiskStudy-researchRiskStudyReviewCode-parentBean" />

  <bean id="RoutingFormResearchRiskStudy-researchRiskStudyReviewCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="researchRiskStudyReviewCode" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Study Review Code" />
