  </bean>
  <bean id="RoutingFormResearchRiskStudy-versionNumber" parent="RoutingFormResearchRiskStudy-versionNumber-parentBean" />

  <bean id="RoutingFormResearchRiskStudy-versionNumber-parentBean" abstract="true" parent="GenericAttributes-versionNumber">
  </bean>

<!-- Business Object Inquiry Definition -->


  <bean id="RoutingFormResearchRiskStudy-inquiryDefinition" parent="RoutingFormResearchRiskStudy-inquiryDefinition-parentBean" />

  <bean id="RoutingFormResearchRiskStudy-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
    <property name="title" value="Routing Form Research Risk Study Inquiry" />
    <property name="inquirySections" >
      <list>
        <bean parent="InquirySectionDefinition">
          <property name="title" value="Header Text" />
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields" >
            <list>
              <bean parent="FieldDefinition"
                    p:attributeName="routingFormResearchRiskStudySequenceNumber" />
              <bean parent="FieldDefinition"
                    p:attributeName="researchRiskStudyApprovalStatusCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="researchRiskExemptionNumber" />
              <bean parent="FieldDefinition"
                    p:attributeName="researchRiskStudyApprovalDate" />
              <bean parent="FieldDefinition"
                    p:attributeName="researchRiskStudyName" />
              <bean parent="FieldDefinition"
                    p:attributeName="researchRiskStudyNumber" />
              <bean parent="FieldDefinition"
                    p:attributeName="researchRiskStudyReviewCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="researchRiskTypeCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="documentNumber" />
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Business Object Lookup Definition -->


  <bean id="RoutingFormResearchRiskStudy-lookupDefinition" parent="RoutingFormResearchRiskStudy-lookupDefinition-parentBean" />

  <bean id="RoutingFormResearchRiskStudy-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
    <property name="title" value="Routing Form Research Risk Study Lookup" />
    <property name="instructions" value="Look up a Routing Form Research Risk Study" />
    <property name="defaultSort" >
      <bean parent="SortDefinition">
      </bean>
    </property>
    <property name="lookupFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="routingFormResearchRiskStudySequenceNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="researchRiskStudyApprovalStatusCode" />
        <bean parent="FieldDefinition"
              p:attributeName="researchRiskExemptionNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="researchRiskStudyApprovalDate" />
        <bean parent="FieldDefinition"
              p:attributeName="researchRiskStudyName" />
        <bean parent="FieldDefinition"
              p:attributeName="researchRiskStudyNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="researchRiskStudyReviewCode" />
        <bean parent="FieldDefinition"
              p:attributeName="researchRiskTypeCode" />
        <bean parent="FieldDefinition"
              p:attributeName="documentNumber" />
      </list>
    </property>
    <property name="resultFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="routingFormResearchRiskStudySequenceNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="researchRiskStudyApprovalStatusCode" />
        <bean parent="FieldDefinition"
              p:attributeName="researchRiskExemptionNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="researchRiskStudyApprovalDate" />
        <bean parent="FieldDefinition"
              p:attributeName="researchRiskStudyName" />
        <bean parent="FieldDefinition"
              p:attributeName="researchRiskStudyNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="researchRiskStudyReviewCode" />
        <bean parent="FieldDefinition"
              p:attributeName="researchRiskTypeCode" />
        <bean parent="FieldDefinition"
              p:attributeName="documentNumber" />
      </list>
    </property>
  </bean>
</beans>
