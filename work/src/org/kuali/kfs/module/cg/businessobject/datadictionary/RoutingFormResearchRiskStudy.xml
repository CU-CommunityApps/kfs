  </bean>
  <bean id="RoutingFormResearchRiskStudy-researchRiskStudyExpirationDate" parent="RoutingFormResearchRiskStudy-researchRiskStudyExpirationDate-parentBean" />

  <bean id="RoutingFormResearchRiskStudy-researchRiskStudyExpirationDate-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="researchRiskStudyExpirationDate" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Expiration Date" />
    <property name="shortLabel" value="Date" />
