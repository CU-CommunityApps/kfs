      <bean parent="SelectControlDefinition"
            p:valuesFinderClass="org.kuali.module.kra.routingform.lookup.keyvalues.RoutingFormApprovalStatusValuesFinder"
            p:includeKeyInLabel="false" />
    </property>
    <property name="summary" value="researchRiskStudyApprovalStatusCode" />
    <property name="description" value="researchRiskStudyApprovalStatusCode description..." />
  </bean>
  <bean id="RoutingFormResearchRiskStudy-researchRiskExemptionNumber" parent="RoutingFormResearchRiskStudy-researchRiskExemptionNumber-parentBean" />

  <bean id="RoutingFormResearchRiskStudy-researchRiskExemptionNumber-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="researchRiskExemptionNumber" />
