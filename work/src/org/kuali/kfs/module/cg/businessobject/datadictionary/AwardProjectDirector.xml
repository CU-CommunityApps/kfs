<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="AwardProjectDirector" parent="AwardProjectDirector-parentBean" />

  <bean id="AwardProjectDirector-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.kfs.module.cg.businessobject.AwardProjectDirector" />
    <property name="inquiryDefinition" >
      <ref bean="AwardProjectDirector-inquiryDefinition" />
    </property>
    <property name="lookupDefinition" >
      <ref bean="AwardProjectDirector-lookupDefinition" />
    </property>
    <property name="objectLabel" value="Award Project Director" />
    <property name="attributes" >
      <list>
        <ref bean="AwardProjectDirector-personUniversalIdentifier" />
        <ref bean="AwardProjectDirector-projectDirector.personUserIdentifier" />
        <ref bean="AwardProjectDirector-projectDirector.personName" />
        <ref bean="AwardProjectDirector-proposalNumber" />
        <ref bean="AwardProjectDirector-awardPrimaryProjectDirectorIndicator" />
        <ref bean="AwardProjectDirector-awardProjectDirectorProjectTitle" />
        <ref bean="AwardProjectDirector-newCollectionRecord" />
        <ref bean="AwardProjectDirector-active" />
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->


  <bean id="AwardProjectDirector-personUniversalIdentifier" parent="AwardProjectDirector-personUniversalIdentifier-parentBean" />

  <bean id="AwardProjectDirector-personUniversalIdentifier-parentBean" abstract="true" parent="ProjectDirector-personUniversalIdentifier">
    <property name="label" value="Project Director ID" />
    <property name="shortLabel" value="Proj Dir ID" />
    <property name="control" >
      <bean parent="HiddenControlDefinition" />
    </property>
  </bean>
  <bean id="AwardProjectDirector-projectDirector.personUserIdentifier" parent="AwardProjectDirector-projectDirector.personUserIdentifier-parentBean" />

  <bean id="AwardProjectDirector-projectDirector.personUserIdentifier-parentBean" abstract="true" parent="UniversalUser-personUserIdentifier">
    <property name="name" value="projectDirector.personUserIdentifier" />
    <property name="label" value="Project Director User ID" />
    <property name="required" value="false" />
  </bean>
  <bean id="AwardProjectDirector-projectDirector.personName" parent="AwardProjectDirector-projectDirector.personName-parentBean" />

  <bean id="AwardProjectDirector-projectDirector.personName-parentBean" abstract="true" parent="UniversalUser-personName">
    <property name="name" value="projectDirector.personName" />
    <property name="label" value="Name" />
    <property name="required" value="false" />
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="40" />
    </property>
  </bean>
  <bean id="AwardProjectDirector-proposalNumber" parent="AwardProjectDirector-proposalNumber-parentBean" />

  <bean id="AwardProjectDirector-proposalNumber-parentBean" abstract="true" parent="Award-proposalNumber">
  </bean>
  <bean id="AwardProjectDirector-awardPrimaryProjectDirectorIndicator" parent="AwardProjectDirector-awardPrimaryProjectDirectorIndicator-parentBean" />

  <bean id="AwardProjectDirector-awardPrimaryProjectDirectorIndicator-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="awardPrimaryProjectDirectorIndicator" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Primary" />
    <property name="shortLabel" value="Primary" />
    <property name="maxLength" value="1" />
    <property name="control" >
      <bean parent="CheckboxControlDefinition" />
    </property>
  </bean>
  <bean id="AwardProjectDirector-awardProjectDirectorProjectTitle" parent="AwardProjectDirector-awardProjectDirectorProjectTitle-parentBean" />

  <bean id="AwardProjectDirector-awardProjectDirectorProjectTitle-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="awardProjectDirectorProjectTitle" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Title" />
    <property name="shortLabel" value="Title" />
    <property name="maxLength" value="100" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern"
            p:allowWhitespace="true" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="40" />
    </property>
  </bean>
  <bean id="AwardProjectDirector-newCollectionRecord" parent="AwardProjectDirector-newCollectionRecord-parentBean" />

  <bean id="AwardProjectDirector-newCollectionRecord-parentBean" abstract="true" parent="GenericAttributes-newCollectionRecord">
  </bean>
  <bean id="AwardProjectDirector-active" parent="AwardProjectDirector-active-parentBean" />

  <bean id="AwardProjectDirector-active-parentBean" abstract="true" parent="GenericAttributes-activeIndicator">
    <property name="name" value="active" />
  </bean>


<!-- Business Object Inquiry Definition -->


  <bean id="AwardProjectDirector-inquiryDefinition" parent="AwardProjectDirector-inquiryDefinition-parentBean" />

  <bean id="AwardProjectDirector-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
    <property name="title" value="Award Project Director Inquiry" />
    <property name="inquirySections" >
      <list>
        <bean parent="InquirySectionDefinition">
          <property name="title" value="Award Project Director Details" />
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields" >
            <list>
              <bean parent="FieldDefinition"
                    p:attributeName="personUniversalIdentifier" />
              <bean parent="FieldDefinition"
                    p:attributeName="proposalNumber" />
              <bean parent="FieldDefinition"
                    p:attributeName="awardPrimaryProjectDirectorIndicator" />
              <bean parent="FieldDefinition"
                    p:attributeName="awardProjectDirectorProjectTitle" />
              <bean parent="FieldDefinition"
                    p:attributeName="active" />
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Business Object Lookup Definition -->


  <bean id="AwardProjectDirector-lookupDefinition" parent="AwardProjectDirector-lookupDefinition-parentBean" />

  <bean id="AwardProjectDirector-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
    <property name="title" value="Award Project Director Lookup" />
    <property name="instructions" value="Look up an Award Project Director" />
    <property name="defaultSort" >
      <bean parent="SortDefinition">
      </bean>
    </property>
    <property name="lookupFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="personUniversalIdentifier" />
        <bean parent="FieldDefinition"
              p:attributeName="proposalNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="awardPrimaryProjectDirectorIndicator" />
        <bean parent="FieldDefinition"
              p:attributeName="awardProjectDirectorProjectTitle" />
        <bean parent="FieldDefinition"
              p:attributeName="active" />
      </list>
    </property>
    <property name="resultFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="personUniversalIdentifier" />
        <bean parent="FieldDefinition"
              p:attributeName="proposalNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="awardPrimaryProjectDirectorIndicator" />
        <bean parent="FieldDefinition"
              p:attributeName="awardProjectDirectorProjectTitle" />
        <bean parent="FieldDefinition"
              p:attributeName="active" />
      </list>
    </property>
  </bean>
</beans>
