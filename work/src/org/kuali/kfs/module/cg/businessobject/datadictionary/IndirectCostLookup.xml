<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="IndirectCostLookup" parent="IndirectCostLookup-parentBean" />

  <bean id="IndirectCostLookup-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.module.kra.budget.bo.IndirectCostLookup" />
    <property name="inquiryDefinition" >
      <ref bean="IndirectCostLookup-inquiryDefinition" />
    </property>
    <property name="lookupDefinition" >
      <ref bean="IndirectCostLookup-lookupDefinition" />
    </property>
    <property name="objectLabel" value="Indirect Cost Lookup" />
    <property name="attributes" >
      <list>
        <ref bean="IndirectCostLookup-budgetOnCampusIndicator" />
        <ref bean="IndirectCostLookup-budgetPurposeCode" />
        <ref bean="IndirectCostLookup-budgetIndirectCostRate" />
        <ref bean="IndirectCostLookup-active" />
        <ref bean="IndirectCostLookup-versionNumber" />
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->


  <bean id="IndirectCostLookup-budgetOnCampusIndicator" parent="IndirectCostLookup-budgetOnCampusIndicator-parentBean" />

  <bean id="IndirectCostLookup-budgetOnCampusIndicator-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="budgetOnCampusIndicator" />
    <property name="forceUppercase" value="false" />
    <property name="label" value="Budget On Campus Indicator" />
    <property name="shortLabel" value="on camput indicator" />
    <property name="maxLength" value="1" />
    <property name="required" value="true" />
    <property name="control" >
      <bean parent="CheckboxControlDefinition" />
    </property>
    <property name="summary" value="Budget On Campus Indicator." />
    <property name="description" value="Budget On Campus Indicator." />
  </bean>
  <bean id="IndirectCostLookup-budgetPurposeCode" parent="IndirectCostLookup-budgetPurposeCode-parentBean" />

  <bean id="IndirectCostLookup-budgetPurposeCode-parentBean" abstract="true" parent="Purpose-purposeCode">
    <property name="name" value="budgetPurposeCode" />
    <property name="label" value="Budget Purpose Code" />
    <property name="shortLabel" value="BdgtPrpsCd" />
    <property name="summary" value="Budget Purpose Code" />
    <property name="description" value="Budget Purpose Code entered has to correspond to an existing Purpose. See Purpose lookup for details." />
  </bean>
  <bean id="IndirectCostLookup-budgetIndirectCostRate" parent="IndirectCostLookup-budgetIndirectCostRate-parentBean" />

  <bean id="IndirectCostLookup-budgetIndirectCostRate-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="budgetIndirectCostRate" />
    <property name="forceUppercase" value="false" />
    <property name="label" value="Budget Indirect Cost Rate" />
    <property name="shortLabel" value="BdgtIdcCstRt" />
    <property name="maxLength" value="6" />
    <property name="validationPattern" >
      <bean parent="FloatingPointValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="7" />
    </property>
    <property name="summary" value="Budget Indirect Cost Rate." />
    <property name="description" value="Budget Indirect Cost Rate." />
  </bean>
  <bean id="IndirectCostLookup-active" parent="IndirectCostLookup-active-parentBean" />

  <bean id="IndirectCostLookup-active-parentBean" abstract="true" parent="GenericAttributes-activeIndicator">
    <property name="name" value="active" />
  </bean>
  <bean id="IndirectCostLookup-versionNumber" parent="IndirectCostLookup-versionNumber-parentBean" />

  <bean id="IndirectCostLookup-versionNumber-parentBean" abstract="true" parent="GenericAttributes-versionNumber">
  </bean>

<!-- Business Object Inquiry Definition -->


  <bean id="IndirectCostLookup-inquiryDefinition" parent="IndirectCostLookup-inquiryDefinition-parentBean" />

  <bean id="IndirectCostLookup-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
    <property name="title" value="Indirect Cost Lookup Inquiry" />
    <property name="inquirySections" >
      <list>
        <bean parent="InquirySectionDefinition">
          <property name="title" value="Header Text" />
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields" >
            <list>
              <bean parent="FieldDefinition"
                    p:attributeName="budgetOnCampusIndicator" />
              <bean parent="FieldDefinition"
                    p:attributeName="budgetPurposeCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="budgetIndirectCostRate" />
              <bean parent="FieldDefinition"
                    p:attributeName="active" />
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Business Object Lookup Definition -->


  <bean id="IndirectCostLookup-lookupDefinition" parent="IndirectCostLookup-lookupDefinition-parentBean" />

  <bean id="IndirectCostLookup-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
    <property name="title" value="Indirect Cost Lookup" />
    <property name="instructions" value="Lookup an Indirect Cost Lookup" />
    <property name="defaultSort" >
      <bean parent="SortDefinition">
        <property name="attributeNames" >
          <list>
            <value>budgetPurposeCode</value>
          </list>
        </property>
      </bean>
    </property>
    <property name="lookupFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="budgetOnCampusIndicator" />
        <bean parent="FieldDefinition"
              p:attributeName="budgetPurposeCode" />
        <bean parent="FieldDefinition"
              p:attributeName="budgetIndirectCostRate" />
        <bean parent="FieldDefinition"
              p:defaultValue="Y"
              p:attributeName="active" />
      </list>
    </property>
    <property name="resultFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="budgetOnCampusIndicator" />
        <bean parent="FieldDefinition"
              p:attributeName="budgetPurposeCode" />
        <bean parent="FieldDefinition"
              p:attributeName="budgetIndirectCostRate" />
        <bean parent="FieldDefinition"
              p:attributeName="active" />
      </list>
    </property>
  </bean>
</beans>
