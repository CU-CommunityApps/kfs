<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="BudgetPeriod" parent="BudgetPeriod-parentBean" />

  <bean id="BudgetPeriod-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.kfs.module.cg.businessobject.BudgetPeriod" />
    <property name="objectLabel" value="Budget Periods" />
    <property name="objectDescription" value="Budget Periods are the periods for your Budget." />
    <property name="attributes" >
      <list>
        <ref bean="BudgetPeriod-documentNumber" />
        <ref bean="BudgetPeriod-budgetPeriodSequenceNumber" />
        <ref bean="BudgetPeriod-budgetPeriodBeginDate" />
        <ref bean="BudgetPeriod-budgetPeriodEndDate" />
        <ref bean="BudgetPeriod-budgetPeriodParticipantsNumber" />
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->


  <bean id="BudgetPeriod-documentNumber" parent="BudgetPeriod-documentNumber-parentBean" />

  <bean id="BudgetPeriod-documentNumber-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="documentNumber" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Document Header Id" />
    <property name="shortLabel" value="Document Id" />
    <property name="maxLength" value="14" />
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="16" />
    </property>
    
    
  </bean>
  <bean id="BudgetPeriod-budgetPeriodSequenceNumber" parent="BudgetPeriod-budgetPeriodSequenceNumber-parentBean" />

  <bean id="BudgetPeriod-budgetPeriodSequenceNumber-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="budgetPeriodSequenceNumber" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Budget Period Sequence Number" />
    <property name="shortLabel" value="Budget Pd. Seq. No." />
    <property name="maxLength" value="2" />
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="4" />
    </property>
    
    
  </bean>
  <bean id="BudgetPeriod-budgetPeriodBeginDate" parent="BudgetPeriod-budgetPeriodBeginDate-parentBean" />

  <bean id="BudgetPeriod-budgetPeriodBeginDate-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="budgetPeriodBeginDate" />
    <property name="label" value="from" />
    <property name="shortLabel" value="Date" />
    <property name="maxLength" value="10" />
    <property name="required" value="true" />
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="11" />
    </property>
    
    
  </bean>
  <bean id="BudgetPeriod-budgetPeriodEndDate" parent="BudgetPeriod-budgetPeriodEndDate-parentBean" />

  <bean id="BudgetPeriod-budgetPeriodEndDate-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="budgetPeriodEndDate" />
    <property name="label" value="to" />
    <property name="shortLabel" value="Date" />
    <property name="maxLength" value="10" />
    <property name="required" value="true" />
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="11" />
    </property>
    
    
  </bean>
  <bean id="BudgetPeriod-budgetPeriodParticipantsNumber" parent="BudgetPeriod-budgetPeriodParticipantsNumber-parentBean" />

  <bean id="BudgetPeriod-budgetPeriodParticipantsNumber-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="budgetPeriodParticipantsNumber" />
    <property name="label" value="Number of Participants/Trainees" />
    <property name="shortLabel" value="Number" />
    <property name="maxLength" value="3" />
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="4" />
    </property>
    
    
  </bean>
</beans>
