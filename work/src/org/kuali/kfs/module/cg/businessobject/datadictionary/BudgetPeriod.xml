<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "../../../../core/datadictionary/dataDictionary-1_0.dtd">
<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.module.kra.budget.bo.BudgetPeriod">



        <attributes>

            <attribute name="documentNumber" forceUppercase="false">
                <label>Document Header Id</label>
                <shortLabel>Document Id</shortLabel>

                <maxLength>14</maxLength>

                <control>
                    <text size="16" />
                </control>

                <summary>The document's displayable identifier.</summary>
                <description>The document id is generated by the workflow environment and is unique to each installation of Kuali...</description>
            </attribute>

            <attribute name="budgetPeriodSequenceNumber" forceUppercase="false">
                <label>Budget Period Sequence Number</label>
                <shortLabel>Budget Pd. Seq. No.</shortLabel>

                <maxLength>2</maxLength>

                <control>
                    <text size="4" />
                </control>

                <summary>The budget period sequence number.</summary>
                <description>Sequence number of the budget period.</description>
            </attribute>

            <attribute name="budgetPeriodBeginDate" forceUppercase="false">
                <label>from</label>
                <shortLabel>Date</shortLabel>

                <maxLength>10</maxLength>

                <!-- validationPattern><date /></validationPattern commented out pending DateValidationPattern only accepting mm/dd/yyyy while Date's are yyyy-mm-dd -->
                <required>true</required>

                <control>
                    <text size="11" />
                </control>

                <summary>Begin period summary...</summary>
                <description>Begin period description...</description>
            </attribute>

            <attribute name="budgetPeriodEndDate" forceUppercase="false">
                <label>to</label>
                <shortLabel>Date</shortLabel>

                <maxLength>10</maxLength>

                <!-- validationPattern><date /></validationPattern commented out pending DateValidationPattern only accepting mm/dd/yyyy while Date's are yyyy-mm-dd -->
                <required>true</required>

                <control>
                    <text size="11" />
                </control>

                <summary>End period summary...</summary>
                <description>End period description...</description>
            </attribute>

            <attribute name="budgetPeriodParticipantsNumber" forceUppercase="false">
                <label>Number of Participants/Trainees</label>
                <shortLabel>Number</shortLabel>

                <maxLength>3</maxLength>

                <control>
                    <text size="4" />
                </control>

                <summary>The budget participants / trainees sequence number.</summary>
                <description>The budget participants / trainees sequence number.</description>
            </attribute>

            <!--<attributeReference name="budget" 
                sourceClassName="org.kuali.module.kra.budget.bo.Budget" 
                sourceAttributeName="budget">
                <label></label>
                <shortLabel></shortLabel>
                <summary></summary>
                <description></description>
                </attributeReference>-->

            <attribute name="objectId" forceUppercase="true">
                <label>Object Id</label>
                <shortLabel>GUID</shortLabel>

                <maxLength>36</maxLength>

                <validationPattern>
                    <anyCharacter />
                </validationPattern>

                <control>
                    <text size="36" />
                </control>

                <summary>objectId summary...</summary>
                <description>Budget period object identifier</description>
            </attribute>

            <attribute name="versionNumber" forceUppercase="true">
                <label>Version Number</label>
                <shortLabel>Number</shortLabel>

                <maxLength>10</maxLength>

                <validationPattern>
                    <numeric />
                </validationPattern>

                <control>
                    <text size="-1" />
                </control>

                <summary>versionNumber summary...</summary>
                <description>versionNumber description...</description>
            </attribute>
        </attributes>

        <objectLabel>Budget Periods</objectLabel>
        <objectDescription>Budget Periods are the periods for your Budget.</objectDescription>
        
        <objectHelp parameterNamespace="KFS-RA" parameterDetailType="Budget" parameterName="PERIOD" />

    </businessObject>
</dictionaryEntry>
