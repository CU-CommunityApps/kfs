<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="BudgetPeriod" parent="BudgetPeriod-parentBean" />

  <bean id="BudgetPeriod-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.module.kra.budget.bo.BudgetPeriod" />
    <property name="objectLabel" value="Budget Periods" />
    <property name="objectDescription" value="Budget Periods are the periods for your Budget." />
    <property name="attributes" >
      <list>
        <ref bean="BudgetPeriod-documentNumber" />
        <ref bean="BudgetPeriod-budgetPeriodSequenceNumber" />
        <ref bean="BudgetPeriod-budgetPeriodBeginDate" />
        <ref bean="BudgetPeriod-budgetPeriodEndDate" />
        <ref bean="BudgetPeriod-budgetPeriodParticipantsNumber" />
        <ref bean="BudgetPeriod-objectId" />
        <ref bean="BudgetPeriod-versionNumber" />
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->


  <bean id="BudgetPeriod-documentNumber" parent="BudgetPeriod-documentNumber-parentBean" />

  <bean id="BudgetPeriod-documentNumber-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="documentNumber" />
    <property name="forceUppercase" value="false" />
    <property name="label" value="Document Header Id" />
    <property name="shortLabel" value="Document Id" />
    <property name="maxLength" value="14" />
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="16" />
    </property>
    <property name="summary" value="The document&apos;s displayable identifier." />
    <property name="description" value="The document id is generated by the workflow environment and is unique to each installation of Kuali..." />
  </bean>
  <bean id="BudgetPeriod-budgetPeriodSequenceNumber" parent="BudgetPeriod-budgetPeriodSequenceNumber-parentBean" />

  <bean id="BudgetPeriod-budgetPeriodSequenceNumber-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="budgetPeriodSequenceNumber" />
    <property name="forceUppercase" value="false" />
    <property name="label" value="Budget Period Sequence Number" />
    <property name="shortLabel" value="Budget Pd. Seq. No." />
    <property name="maxLength" value="2" />
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="4" />
    </property>
    <property name="summary" value="The budget period sequence number." />
    <property name="description" value="Sequence number of the budget period." />
  </bean>
  <bean id="BudgetPeriod-budgetPeriodBeginDate" parent="BudgetPeriod-budgetPeriodBeginDate-parentBean" />

  <bean id="BudgetPeriod-budgetPeriodBeginDate-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="budgetPeriodBeginDate" />
    <property name="forceUppercase" value="false" />
    <property name="label" value="from" />
    <property name="shortLabel" value="Date" />
    <property name="maxLength" value="10" />
    <property name="required" value="true" />
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="11" />
    </property>
    <property name="summary" value="Begin period summary..." />
    <property name="description" value="Begin period description..." />
  </bean>
  <bean id="BudgetPeriod-budgetPeriodEndDate" parent="BudgetPeriod-budgetPeriodEndDate-parentBean" />

  <bean id="BudgetPeriod-budgetPeriodEndDate-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="budgetPeriodEndDate" />
    <property name="forceUppercase" value="false" />
    <property name="label" value="to" />
    <property name="shortLabel" value="Date" />
    <property name="maxLength" value="10" />
    <property name="required" value="true" />
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="11" />
    </property>
    <property name="summary" value="End period summary..." />
    <property name="description" value="End period description..." />
  </bean>
  <bean id="BudgetPeriod-budgetPeriodParticipantsNumber" parent="BudgetPeriod-budgetPeriodParticipantsNumber-parentBean" />

  <bean id="BudgetPeriod-budgetPeriodParticipantsNumber-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="budgetPeriodParticipantsNumber" />
    <property name="forceUppercase" value="false" />
    <property name="label" value="Number of Participants/Trainees" />
    <property name="shortLabel" value="Number" />
    <property name="maxLength" value="3" />
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="4" />
    </property>
    <property name="summary" value="The budget participants / trainees sequence number." />
    <property name="description" value="The budget participants / trainees sequence number." />
  </bean>
  <bean id="BudgetPeriod-objectId" parent="BudgetPeriod-objectId-parentBean" />

  <bean id="BudgetPeriod-objectId-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="objectId" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Object Id" />
    <property name="shortLabel" value="GUID" />
    <property name="maxLength" value="36" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="36" />
    </property>
    <property name="summary" value="objectId summary..." />
    <property name="description" value="Budget period object identifier" />
  </bean>
  <bean id="BudgetPeriod-versionNumber" parent="BudgetPeriod-versionNumber-parentBean" />

  <bean id="BudgetPeriod-versionNumber-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="versionNumber" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Version Number" />
    <property name="shortLabel" value="Number" />
    <property name="maxLength" value="10" />
    <property name="validationPattern" >
      <bean parent="NumericValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="-1" />
    </property>
    <property name="summary" value="versionNumber summary..." />
    <property name="description" value="versionNumber description..." />
  </bean>
</beans>
