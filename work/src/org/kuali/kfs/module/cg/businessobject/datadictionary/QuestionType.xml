<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "../dataDictionary-1_0.dtd">
<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.module.kra.routingform.bo.QuestionType">


        <inquiry>
            <title>Question Type Inquiry</title>

            <inquirySections><inquirySection title="Header Text"><inquiryFields>
                <field attributeName="questionTypeCode" />
                <field attributeName="questionTypeDescription" />
                <field attributeName="questionTypeSortNumber" />
                <field attributeName="questionTypeWorkgroupName" />
                <field attributeName="questionTypeNotificationValue" />
                <field attributeName="dataObjectMaintenanceCodeActiveIndicator" />
            </inquiryFields></inquirySection></inquirySections>
        </inquiry>

        <lookup>
            <title>Question Type Lookup</title>

            <instructions>Look up a Question Type</instructions>

            <defaultSort sortAscending="true">
                <sortAttributes>
                    <sortAttribute attributeName="questionTypeCode" />
                </sortAttributes>
            </defaultSort>

            <lookupFields>
                <lookupField attributeName="questionTypeCode" required="false" />
                <lookupField attributeName="questionTypeDescription" required="false" />
                <lookupField attributeName="questionTypeSortNumber" required="false" />
                <lookupField attributeName="questionTypeWorkgroupName" required="false" />
                <lookupField attributeName="questionTypeNotificationValue" required="false" />
                <lookupField attributeName="dataObjectMaintenanceCodeActiveIndicator" required="false" />
            </lookupFields>

            <resultFields>
                <field attributeName="questionTypeCode" />
                <field attributeName="questionTypeDescription" />
                <field attributeName="questionTypeSortNumber" />
                <field attributeName="questionTypeWorkgroupName" />
                <field attributeName="questionTypeNotificationValue" />
                <field attributeName="dataObjectMaintenanceCodeActiveIndicator" />
            </resultFields>
        </lookup>

        <attributes>

            <attribute name="dataObjectMaintenanceCodeActiveIndicator" forceUppercase="true">
                <label>Data Object Maintenance Code Active Indicator</label>
                <shortLabel>Indicator</shortLabel>

                <maxLength>1</maxLength>

                <control>
                    <checkbox />
                </control>

                <summary>dataObjectMaintenanceCodeActiveIndicator</summary>
                <description>dataObjectMaintenanceCodeActiveIndicator description...</description>
			</attribute>

            <attribute name="questionTypeCode" forceUppercase="true">
                <label>Question Type Code</label>
                <shortLabel>Code</shortLabel>

                <maxLength>1</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="3" />
                </control>

                <summary>questionTypeCode</summary>
                <description>questionTypeCode description...</description>
			</attribute>

            <attribute name="questionTypeDescription" forceUppercase="false">
                <label>Question Text</label>
                <shortLabel>Text</shortLabel>

                <maxLength>200</maxLength>

                <control>
                    <text size="32" />
                </control>

                <summary>questionTypeDescription</summary>
                <description>questionTypeDescription description...</description>
			</attribute>

            <attribute name="questionTypeSortNumber" forceUppercase="true">
                <label>Sort Number</label>
                <shortLabel>Number</shortLabel>

                <maxLength>2</maxLength>
                <validationPattern><numeric /></validationPattern>

                <control>
                    <text size="4" />
                </control>

                <summary>sortNumber</summary>
                <description>sortNumber description...</description>
			</attribute>
			
			<attribute name="questionTypeWorkgroupName" forceUppercase="true">
                <label>Question Type Workgroup Name</label>
                <shortLabel>Workgroup</shortLabel>

                <maxLength>70</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <workflowWorkgroup size="32" />
                </control>

                <summary>questionTypeWorkgroupName</summary>
                <description>questionTypeWorkgroupName description...</description>
			</attribute>
			
			<attribute name="questionTypeNotificationValue" forceUppercase="true">
				<label>Question Type Notification Value</label>
				<shortLabel>Not. Value</shortLabel>
				
				<maxLength>1</maxLength>
				
				<control>
					<text size="3" />
				</control>
				
				<summary>questionTypeNotificationValue</summary>
				<description>Value on which to route to the notification group.  Valid options are Y (yes), N (no), A (all), X (never).</description>
			</attribute>

            <attributeReference name="versionNumber" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="versionNumber" />

        </attributes>
        <objectLabel>Question Type</objectLabel>
    </businessObject>
</dictionaryEntry>