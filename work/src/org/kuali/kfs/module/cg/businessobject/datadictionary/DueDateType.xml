<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="DueDateType" parent="DueDateType-parentBean" />

  <bean id="DueDateType-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.kfs.module.cg.businessobject.DueDateType" />
    <property name="inquiryDefinition" >
      <ref bean="DueDateType-inquiryDefinition" />
    </property>
    <property name="lookupDefinition" >
      <ref bean="DueDateType-lookupDefinition" />
    </property>
    <property name="objectLabel" value="Due Date Type" />
    <property name="attributes" >
      <list>
        <ref bean="DueDateType-active" />
        <ref bean="DueDateType-approvalLeadTime" />
        <ref bean="DueDateType-dueDateDescription" />
        <ref bean="DueDateType-dueDateTypeCode" />
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->


  <bean id="DueDateType-active" parent="DueDateType-active-parentBean" />

  <bean id="DueDateType-active-parentBean" abstract="true" parent="GenericAttributes-activeIndicator">
    <property name="name" value="active" />
  </bean>
  <bean id="DueDateType-approvalLeadTime" parent="DueDateType-approvalLeadTime-parentBean" />

  <bean id="DueDateType-approvalLeadTime-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="approvalLeadTime" />
    <property name="label" value="Approval Lead Time" />
    <property name="shortLabel" value="Time" />
    <property name="maxLength" value="3" />
    <property name="validationPattern" >
      <bean parent="NumericValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="5" />
    </property>
    
    
  </bean>
  <bean id="DueDateType-dueDateDescription" parent="DueDateType-dueDateDescription-parentBean" />

  <bean id="DueDateType-dueDateDescription-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="dueDateDescription" />
    <property name="label" value="Due Date Description" />
    <property name="shortLabel" value="Description" />
    <property name="maxLength" value="20" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern"
            p:allowWhitespace="true" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="22" />
    </property>
    
    
  </bean>
  <bean id="DueDateType-dueDateTypeCode" parent="DueDateType-dueDateTypeCode-parentBean" />

  <bean id="DueDateType-dueDateTypeCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="dueDateTypeCode" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Due Date Type Code" />
    <property name="shortLabel" value="Code" />
    <property name="maxLength" value="2" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="4" />
    </property>
    
    
  </bean>

<!-- Business Object Inquiry Definition -->


  <bean id="DueDateType-inquiryDefinition" parent="DueDateType-inquiryDefinition-parentBean" />

  <bean id="DueDateType-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
    <property name="title" value="Due Date Type Inquiry" />
    <property name="inquirySections" >
      <list>
        <bean parent="InquirySectionDefinition">
          <property name="title" value="" />
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields" >
            <list>
              <bean parent="FieldDefinition"
                    p:attributeName="dueDateDescription" />
              <bean parent="FieldDefinition"
                    p:attributeName="dueDateTypeCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="active" />
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Business Object Lookup Definition -->


  <bean id="DueDateType-lookupDefinition" parent="DueDateType-lookupDefinition-parentBean" />

  <bean id="DueDateType-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
    <property name="title" value="Due Date Type Lookup" />
    <property name="instructions" value="Lookup a Due Date Type" />
    <property name="defaultSort" >
      <bean parent="SortDefinition">
      </bean>
    </property>
    <property name="lookupFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="dueDateDescription" />
        <bean parent="FieldDefinition"
              p:attributeName="dueDateTypeCode" />
        <bean parent="FieldDefinition"
              p:defaultValue="true"
              p:attributeName="active" />
      </list>
    </property>
    <property name="resultFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="dueDateDescription" />
        <bean parent="FieldDefinition"
              p:attributeName="dueDateTypeCode" />
        <bean parent="FieldDefinition"
              p:attributeName="active" />
      </list>
    </property>
  </bean>
</beans>
