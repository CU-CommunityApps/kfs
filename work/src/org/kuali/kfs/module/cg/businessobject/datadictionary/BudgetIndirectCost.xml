<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="BudgetIndirectCost" parent="BudgetIndirectCost-parentBean" />

  <bean id="BudgetIndirectCost-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.kfs.module.cg.businessobject.BudgetIndirectCost" />
    <property name="objectLabel" value="Budget Indirect Cost" />
    <property name="attributes" >
      <list>
        <ref bean="BudgetIndirectCost-documentNumber" />
        <ref bean="BudgetIndirectCost-budgetBaseCode" />
        <ref bean="BudgetIndirectCost-budgetIndirectCostCostShareIndicator" />
        <ref bean="BudgetIndirectCost-budgetUnrecoveredIndirectCostIndicator" />
        <ref bean="BudgetIndirectCost-budgetIndirectCostJustificationText" />
        <ref bean="BudgetIndirectCost-budgetManualMtdcIndicator" />
        <ref bean="BudgetIndirectCost-budgetManualRateIndicator" />
        <ref bean="BudgetIndirectCost-budgetPurposeCode" />
      </list>
    </property>
    <property name="collections" >
      <list>
        <bean parent="CollectionDefinition"
              p:name="budgetTaskPeriodIndirectCostItems"
              p:label="Task Period Indirect Cost"
              p:shortLabel="Task Period Indirect Cost"
              p:summary="Task Period Indirect Cost"
              p:description="Indirect cost for a certain task over a certain period." />
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->


  <bean id="BudgetIndirectCost-documentNumber" parent="BudgetIndirectCost-documentNumber-parentBean" />

  <bean id="BudgetIndirectCost-documentNumber-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="documentNumber" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Document Header Id" />
    <property name="shortLabel" value="Document Header Id" />
    <property name="maxLength" value="8" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="HiddenControlDefinition" />
    </property>
    <property name="summary" value="Document Header Id" />
    <property name="description" value="Document Header Id" />
  </bean>
  <bean id="BudgetIndirectCost-budgetBaseCode" parent="BudgetIndirectCost-budgetBaseCode-parentBean" />

  <bean id="BudgetIndirectCost-budgetBaseCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="budgetBaseCode" />
    <property name="forceUppercase" value="false" />
    <property name="label" value="Current Base Indicator" />
    <property name="shortLabel" value="Code" />
    <property name="maxLength" value="2" />
    <property name="control" >
      <bean parent="SelectControlDefinition"
            p:valuesFinderClass="org.kuali.kfs.module.cg.businessobject.options.BudgetBaseCodeValuesFinder"
            p:includeKeyInLabel="false" />
    </property>
    <property name="summary" value="Budget Base Code" />
    <property name="description" value="Budget Base Code" />
  </bean>
  <bean id="BudgetIndirectCost-budgetIndirectCostCostShareIndicator" parent="BudgetIndirectCost-budgetIndirectCostCostShareIndicator-parentBean" />

  <bean id="BudgetIndirectCost-budgetIndirectCostCostShareIndicator-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="budgetIndirectCostCostShareIndicator" />
    <property name="forceUppercase" value="false" />
    <property name="label" value="Indirect Cost" />
    <property name="shortLabel" value="Indicator" />
    <property name="maxLength" value="2" />
    <property name="control" >
      <bean parent="CheckboxControlDefinition" />
    </property>
    <property name="summary" value="Indirect Cost Share Indicator" />
    <property name="description" value="Indirect Cost Share Indicator" />
  </bean>
  <bean id="BudgetIndirectCost-budgetUnrecoveredIndirectCostIndicator" parent="BudgetIndirectCost-budgetUnrecoveredIndirectCostIndicator-parentBean" />

  <bean id="BudgetIndirectCost-budgetUnrecoveredIndirectCostIndicator-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="budgetUnrecoveredIndirectCostIndicator" />
    <property name="forceUppercase" value="false" />
    <property name="label" value="Unrecovered Indirect Cost" />
    <property name="shortLabel" value="Indicator" />
    <property name="maxLength" value="3" />
    <property name="control" >
      <bean parent="CheckboxControlDefinition" />
    </property>
    <property name="summary" value="Unrecovered Indirect Cost Indicator" />
    <property name="description" value="Unrecovered Indirect Cost Indicator" />
  </bean>
  <bean id="BudgetIndirectCost-budgetIndirectCostJustificationText" parent="BudgetIndirectCost-budgetIndirectCostJustificationText-parentBean" />

  <bean id="BudgetIndirectCost-budgetIndirectCostJustificationText-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="budgetIndirectCostJustificationText" />
    <property name="forceUppercase" value="false" />
    <property name="label" value="Justification for manual values" />
    <property name="shortLabel" value="Text" />
    <property name="maxLength" value="255" />
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="30" />
    </property>
    <property name="summary" value="Indirect Cost Justification" />
    <property name="description" value="Indirect Cost Justification" />
  </bean>
  <bean id="BudgetIndirectCost-budgetManualMtdcIndicator" parent="BudgetIndirectCost-budgetManualMtdcIndicator-parentBean" />

  <bean id="BudgetIndirectCost-budgetManualMtdcIndicator-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="budgetManualMtdcIndicator" />
    <property name="forceUppercase" value="false" />
    <property name="label" value="Manual MTDC Indicator" />
    <property name="shortLabel" value="Manual MTDC Indicator" />
    <property name="maxLength" value="3" />
    <property name="control" >
      <bean parent="CheckboxControlDefinition" />
    </property>
    <property name="summary" value="Manual MTDC Indicator" />
    <property name="description" value="Manual MTDC Indicator" />
  </bean>
  <bean id="BudgetIndirectCost-budgetManualRateIndicator" parent="BudgetIndirectCost-budgetManualRateIndicator-parentBean" />

  <bean id="BudgetIndirectCost-budgetManualRateIndicator-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="budgetManualRateIndicator" />
    <property name="forceUppercase" value="false" />
    <property name="label" value="Indirect Cost Rate Source" />
    <property name="shortLabel" value="Indicator" />
    <property name="maxLength" value="5" />
    <property name="control" >
      <bean parent="SelectControlDefinition"
            p:valuesFinderClass="org.kuali.kfs.module.cg.businessobject.options.BudgetManualRateIndicatorValuesFinder"
            p:includeKeyInLabel="false" />
    </property>
    <property name="summary" value="Manual Rate Indicator" />
    <property name="description" value="Manual Rate Indicator" />
  </bean>
  <bean id="BudgetIndirectCost-budgetPurposeCode" parent="BudgetIndirectCost-budgetPurposeCode-parentBean" />

  <bean id="BudgetIndirectCost-budgetPurposeCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="budgetPurposeCode" />
    <property name="forceUppercase" value="false" />
    <property name="label" value="Primary Purpose" />
    <property name="shortLabel" value="Code" />
    <property name="maxLength" value="2" />
    <property name="control" >
      <bean parent="SelectControlDefinition"
            p:valuesFinderClass="org.kuali.kfs.module.cg.businessobject.options.PurposeValuesFinder"
            p:includeKeyInLabel="false" />
    </property>
    <property name="summary" value="Purpose Code" />
    <property name="description" value="Purpose Code" />
  </bean>
</beans>
