<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="RoutingFormSubcontractor" parent="RoutingFormSubcontractor-parentBean" />

  <bean id="RoutingFormSubcontractor-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.kfs.module.cg.businessobject.RoutingFormSubcontractor" />
    <property name="inquiryDefinition" >
      <ref bean="RoutingFormSubcontractor-inquiryDefinition" />
    </property>
    <property name="lookupDefinition" >
      <ref bean="RoutingFormSubcontractor-lookupDefinition" />
    </property>
    <property name="objectLabel" value="Routing Form Subcontractor" />
    <property name="attributes" >
      <list>
        <ref bean="RoutingFormSubcontractor-routingFormSubcontractorAmount" />
        <ref bean="RoutingFormSubcontractor-routingFormSubcontractorNumber" />
        <ref bean="RoutingFormSubcontractor-routingFormSubcontractorSequenceNumber" />
        <ref bean="RoutingFormSubcontractor-documentNumber" />
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->


  <bean id="RoutingFormSubcontractor-routingFormSubcontractorAmount" parent="RoutingFormSubcontractor-routingFormSubcontractorAmount-parentBean" />

  <bean id="RoutingFormSubcontractor-routingFormSubcontractorAmount-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="routingFormSubcontractorAmount" />
    <property name="label" value="Routing Form Subcontractor Amount" />
    <property name="shortLabel" value="Amount" />
    <property name="maxLength" value="15" />
    <property name="validationPattern" >
      <bean parent="NumericValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="21" />
    </property>
    <property name="summary" value="routingFormSubcontractorAmount" />
    <property name="description" value="routingFormSubcontractorAmount description..." />
  </bean>
  <bean id="RoutingFormSubcontractor-routingFormSubcontractorNumber" parent="RoutingFormSubcontractor-routingFormSubcontractorNumber-parentBean" />

  <bean id="RoutingFormSubcontractor-routingFormSubcontractorNumber-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="routingFormSubcontractorNumber" />
    <property name="label" value="Routing Form Subcontractor Number" />
    <property name="shortLabel" value="Number" />
    <property name="maxLength" value="5" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="6" />
    </property>
    <property name="summary" value="routingFormSubcontractorNumber" />
    <property name="description" value="routingFormSubcontractorNumber description..." />
  </bean>
  <bean id="RoutingFormSubcontractor-routingFormSubcontractorSequenceNumber" parent="RoutingFormSubcontractor-routingFormSubcontractorSequenceNumber-parentBean" />

  <bean id="RoutingFormSubcontractor-routingFormSubcontractorSequenceNumber-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="routingFormSubcontractorSequenceNumber" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Routing Form Subcontractor Sequence Number" />
    <property name="shortLabel" value="Number" />
    <property name="maxLength" value="3" />
    <property name="validationPattern" >
      <bean parent="NumericValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="HiddenControlDefinition" />
    </property>
    <property name="summary" value="routingFormSubcontractorSequenceNumber" />
    <property name="description" value="routingFormSubcontractorSequenceNumber description..." />
  </bean>
  <bean id="RoutingFormSubcontractor-documentNumber" parent="RoutingFormSubcontractor-documentNumber-parentBean" />

  <bean id="RoutingFormSubcontractor-documentNumber-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="documentNumber" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Document Number" />
    <property name="shortLabel" value="Number" />
    <property name="maxLength" value="14" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="HiddenControlDefinition" />
    </property>
    <property name="summary" value="documentNumber" />
    <property name="description" value="documentNumber description..." />
  </bean>

<!-- Business Object Inquiry Definition -->


  <bean id="RoutingFormSubcontractor-inquiryDefinition" parent="RoutingFormSubcontractor-inquiryDefinition-parentBean" />

  <bean id="RoutingFormSubcontractor-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
    <property name="title" value="Routing Form Subcontractor Inquiry" />
    <property name="inquirySections" >
      <list>
        <bean parent="InquirySectionDefinition">
          <property name="title" value="" />
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields" >
            <list>
              <bean parent="FieldDefinition"
                    p:attributeName="routingFormSubcontractorAmount" />
              <bean parent="FieldDefinition"
                    p:attributeName="routingFormSubcontractorNumber" />
              <bean parent="FieldDefinition"
                    p:attributeName="routingFormSubcontractorSequenceNumber" />
              <bean parent="FieldDefinition"
                    p:attributeName="documentNumber" />
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Business Object Lookup Definition -->


  <bean id="RoutingFormSubcontractor-lookupDefinition" parent="RoutingFormSubcontractor-lookupDefinition-parentBean" />

  <bean id="RoutingFormSubcontractor-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
    <property name="title" value="Routing Form Subcontractor Lookup" />
    <property name="instructions" value="Look up a Routing Form Subcontractor" />
    <property name="defaultSort" >
      <bean parent="SortDefinition">
      </bean>
    </property>
    <property name="lookupFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="routingFormSubcontractorAmount" />
        <bean parent="FieldDefinition"
              p:attributeName="routingFormSubcontractorNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="routingFormSubcontractorSequenceNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="documentNumber" />
      </list>
    </property>
    <property name="resultFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="routingFormSubcontractorAmount" />
        <bean parent="FieldDefinition"
              p:attributeName="routingFormSubcontractorNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="routingFormSubcontractorSequenceNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="documentNumber" />
      </list>
    </property>
  </bean>
</beans>
