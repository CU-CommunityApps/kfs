<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "../dataDictionary-1_0.dtd">
<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.module.kra.routingform.bo.RoutingFormSubcontractor">
        <customAttributeEnabled>false</customAttributeEnabled>

        <inquiry>
            <title>Routing Form Subcontractor Inquiry</title>

            <inquiryFields>
                <field attributeName="proposalSubcontractorAmount" />
                <field attributeName="proposalSubcontractorName" />
                <field attributeName="proposalSubcontractorNumber" />
                <field attributeName="proposalSubcontractorSequenceNumber" />
                <field attributeName="researchDocumentNumber" />
            </inquiryFields>
        </inquiry>

        <lookup>
            <title>Routing Form Subcontractor Lookup</title>

			<menubar><![CDATA[<a href="maintenance.do?methodToCall=start&businessObjectClassName=org.kuali.module.kra.routingform.bo.RoutingFormSubcontractor">Create New</a>]]></menubar>

            <instructions>Look up a Routing Form Subcontractor</instructions>

            <defaultSort sortAscending="true">
                <sortAttributes>
                    <sortAttribute attributeName="researchDocumentNumber" />
                    <sortAttribute attributeName="proposalSubcontractorSequenceNumber" />
                </sortAttributes>
            </defaultSort>

            <lookupFields>
                <lookupField attributeName="proposalSubcontractorAmount" required="false" />
                <lookupField attributeName="proposalSubcontractorName" required="false" />
                <lookupField attributeName="proposalSubcontractorNumber" required="false" />
                <lookupField attributeName="proposalSubcontractorSequenceNumber" required="false" />
                <lookupField attributeName="researchDocumentNumber" required="false" />
            </lookupFields>

            <resultFields>
                <field attributeName="proposalSubcontractorAmount" />
                <field attributeName="proposalSubcontractorName" />
                <field attributeName="proposalSubcontractorNumber" />
                <field attributeName="proposalSubcontractorSequenceNumber" />
                <field attributeName="researchDocumentNumber" />
            </resultFields>
        </lookup>

        <attributes>

            <attribute name="proposalSubcontractorAmount" forceUppercase="true">
                <label>Proposal Subcontractor Amount</label>
                <shortLabel>Amount</shortLabel>

                <maxLength>19</maxLength>
                <validationPattern><floatingPoint /></validationPattern>

                <control>
                    <text size="21" />
                </control>

                <summary>proposalSubcontractorAmount</summary>
                <description>proposalSubcontractorAmount description...</description>
			</attribute>

            <attribute name="proposalSubcontractorName" forceUppercase="true">
                <label>Proposal Subcontractor Name</label>
                <shortLabel>Name</shortLabel>

                <maxLength>50</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="52" />
                </control>

                <summary>proposalSubcontractorName</summary>
                <description>proposalSubcontractorName description...</description>
			</attribute>

            <attribute name="proposalSubcontractorNumber" forceUppercase="true">
                <label>Proposal Subcontractor Number</label>
                <shortLabel>Number</shortLabel>

                <maxLength>5</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="7" />
                </control>

                <summary>proposalSubcontractorNumber</summary>
                <description>proposalSubcontractorNumber description...</description>
			</attribute>

            <attribute name="proposalSubcontractorSequenceNumber" forceUppercase="true">
                <label>Proposal Subcontractor Sequence Number</label>
                <shortLabel>Number</shortLabel>

                <maxLength>3</maxLength>
                <validationPattern><numeric /></validationPattern>

                <control>
                    <text size="5" />
                </control>

                <summary>proposalSubcontractorSequenceNumber</summary>
                <description>proposalSubcontractorSequenceNumber description...</description>
			</attribute>

            <attribute name="researchDocumentNumber" forceUppercase="true">
                <label>Research Document Number</label>
                <shortLabel>Number</shortLabel>

                <maxLength>14</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="16" />
                </control>

                <summary>researchDocumentNumber</summary>
                <description>researchDocumentNumber description...</description>
			</attribute>

            <attributeReference name="versionNumber" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="versionNumber" />

        </attributes>
    </businessObject>
</dictionaryEntry>
