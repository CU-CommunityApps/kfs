<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="RoutingFormResearchTypeCode" parent="RoutingFormResearchTypeCode-parentBean" />

  <bean id="RoutingFormResearchTypeCode-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.module.kra.routingform.bo.RoutingFormResearchTypeCode" />
    <property name="inquiryDefinition" >
      <ref bean="RoutingFormResearchTypeCode-inquiryDefinition" />
    </property>
    <property name="lookupDefinition" >
      <ref bean="RoutingFormResearchTypeCode-lookupDefinition" />
    </property>
    <property name="attributes" >
      <list>
        <ref bean="RoutingFormResearchTypeCode-documentNumber" />
        <ref bean="RoutingFormResearchTypeCode-researchTypeCode" />
        <ref bean="RoutingFormResearchTypeCode-versionNumber" />
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->


  <bean id="RoutingFormResearchTypeCode-documentNumber" parent="RoutingFormResearchTypeCode-documentNumber-parentBean" />

  <bean id="RoutingFormResearchTypeCode-documentNumber-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="documentNumber" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Document Number" />
    <property name="shortLabel" value="Number" />
    <property name="maxLength" value="14" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="16" />
    </property>
    <property name="summary" value="documentNumber" />
    <property name="description" value="documentNumber description..." />
  </bean>
  <bean id="RoutingFormResearchTypeCode-researchTypeCode" parent="RoutingFormResearchTypeCode-researchTypeCode-parentBean" />

  <bean id="RoutingFormResearchTypeCode-researchTypeCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="researchTypeCode" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Research Type Code" />
    <property name="shortLabel" value="Code" />
    <property name="maxLength" value="1" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="3" />
    </property>
    <property name="summary" value="researchTypeCode" />
    <property name="description" value="researchTypeCode description..." />
  </bean>
  <bean id="RoutingFormResearchTypeCode-versionNumber" parent="RoutingFormResearchTypeCode-versionNumber-parentBean" />

  <bean id="RoutingFormResearchTypeCode-versionNumber-parentBean" abstract="true" parent="GenericAttributes-versionNumber">
  </bean>

<!-- Business Object Inquiry Definition -->


  <bean id="RoutingFormResearchTypeCode-inquiryDefinition" parent="RoutingFormResearchTypeCode-inquiryDefinition-parentBean" />

  <bean id="RoutingFormResearchTypeCode-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
    <property name="title" value="Routing Form Research Type Code Inquiry" />
    <property name="inquirySections" >
      <list>
        <bean parent="InquirySectionDefinition">
          <property name="title" value="Header Text" />
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields" >
            <list>
              <bean parent="FieldDefinition"
                    p:attributeName="documentNumber" />
              <bean parent="FieldDefinition"
                    p:attributeName="researchTypeCode" />
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Business Object Lookup Definition -->


  <bean id="RoutingFormResearchTypeCode-lookupDefinition" parent="RoutingFormResearchTypeCode-lookupDefinition-parentBean" />

  <bean id="RoutingFormResearchTypeCode-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
    <property name="title" value="Routing Form Research Type Code Lookup" />
    <property name="instructions" value="Look up a Routing Form Research Type Code" />
    <property name="defaultSort" >
      <bean parent="SortDefinition">
      </bean>
    </property>
    <property name="lookupFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="documentNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="researchTypeCode" />
      </list>
    </property>
    <property name="resultFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="documentNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="researchTypeCode" />
      </list>
    </property>
  </bean>
</beans>
