<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="BudgetPeriodThirdPartyCostShare" parent="BudgetPeriodThirdPartyCostShare-parentBean" />

  <bean id="BudgetPeriodThirdPartyCostShare-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.module.kra.budget.bo.BudgetPeriodThirdPartyCostShare" />
    <property name="inquiryDefinition" >
      <ref bean="BudgetPeriodThirdPartyCostShare-inquiryDefinition" />
    </property>
    <property name="objectLabel" value="Budget Third Party Cost Share" />
    <property name="attributes" >
      <list>
        <ref bean="BudgetPeriodThirdPartyCostShare-documentNumber" />
        <ref bean="BudgetPeriodThirdPartyCostShare-budgetPeriodSequenceNumber" />
        <ref bean="BudgetPeriodThirdPartyCostShare-budgetCostShareSequenceNumber" />
        <ref bean="BudgetPeriodThirdPartyCostShare-budgetCostShareAmount" />
        <ref bean="BudgetPeriodThirdPartyCostShare-versionNumber" />
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->


  <bean id="BudgetPeriodThirdPartyCostShare-documentNumber" parent="BudgetPeriodThirdPartyCostShare-documentNumber-parentBean" />

  <bean id="BudgetPeriodThirdPartyCostShare-documentNumber-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="documentNumber" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Budget Number" />
    <property name="shortLabel" value="Budget Number" />
    <property name="maxLength" value="12" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="12" />
    </property>
    <property name="summary" value="Budget Number..." />
    <property name="description" value="Budget Number..." />
  </bean>
  <bean id="BudgetPeriodThirdPartyCostShare-budgetPeriodSequenceNumber" parent="BudgetPeriodThirdPartyCostShare-budgetPeriodSequenceNumber-parentBean" />

  <bean id="BudgetPeriodThirdPartyCostShare-budgetPeriodSequenceNumber-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="budgetPeriodSequenceNumber" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Sequence Number" />
    <property name="shortLabel" value="Sequence Number" />
    <property name="maxLength" value="5" />
    <property name="validationPattern" >
      <bean parent="NumericValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="2" />
    </property>
    <property name="summary" value="Sequence Number" />
    <property name="description" value="Sequence Number" />
  </bean>
  <bean id="BudgetPeriodThirdPartyCostShare-budgetCostShareSequenceNumber" parent="BudgetPeriodThirdPartyCostShare-budgetCostShareSequenceNumber-parentBean" />

  <bean id="BudgetPeriodThirdPartyCostShare-budgetCostShareSequenceNumber-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="budgetCostShareSequenceNumber" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Sequence Number" />
    <property name="shortLabel" value="Sequence Number" />
    <property name="maxLength" value="5" />
    <property name="validationPattern" >
      <bean parent="NumericValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="2" />
    </property>
    <property name="summary" value="Sequence Number" />
    <property name="description" value="Sequence Number" />
  </bean>
  <bean id="BudgetPeriodThirdPartyCostShare-budgetCostShareAmount" parent="BudgetPeriodThirdPartyCostShare-budgetCostShareAmount-parentBean" />

  <bean id="BudgetPeriodThirdPartyCostShare-budgetCostShareAmount-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="budgetCostShareAmount" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Amount" />
    <property name="shortLabel" value="Amount" />
    <property name="maxLength" value="50" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern"
            p:allowWhitespace="true" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="5" />
    </property>
    <property name="summary" value="Amount" />
    <property name="description" value="Amount" />
  </bean>
  <bean id="BudgetPeriodThirdPartyCostShare-versionNumber" parent="BudgetPeriodThirdPartyCostShare-versionNumber-parentBean" />

  <bean id="BudgetPeriodThirdPartyCostShare-versionNumber-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="versionNumber" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Version Number" />
    <property name="shortLabel" value="Number" />
    <property name="maxLength" value="10" />
    <property name="validationPattern" >
      <bean parent="NumericValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="-1" />
    </property>
    <property name="summary" value="versionNumber summary..." />
    <property name="description" value="versionNumber description..." />
  </bean>

<!-- Business Object Inquiry Definition -->


  <bean id="BudgetPeriodThirdPartyCostShare-inquiryDefinition" parent="BudgetPeriodThirdPartyCostShare-inquiryDefinition-parentBean" />

  <bean id="BudgetPeriodThirdPartyCostShare-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
    <property name="title" value="Budget Third Party Cost Share" />
    <property name="inquirySections" >
      <list>
        <bean parent="InquirySectionDefinition">
          <property name="title" value="Header Text" />
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields" >
            <list>
              <bean parent="FieldDefinition"
                    p:attributeName="budgetPeriodSequenceNumber" />
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>
</beans>
