<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "../../../core/datadictionary/dataDictionary-1_0.dtd">
<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.module.cg.bo.Award">

        <inquiry>
            <title>Award Inquiry</title>

            <inquirySections>
                <inquirySection title="Award Details">
                    <inquiryFields>
                        <field attributeName="proposalNumber" />
                        <field attributeName="proposal.proposalNumber" />
                        <field attributeName="agencyNumber" />
                        <field attributeName="agency.reportingName" />
                        <field attributeName="awardProjectTitle" />
                        <field attributeName="awardStatusCode" />

                        <field attributeName="awardBeginningDate" />
                        <field attributeName="awardEndingDate" />
                        <field attributeName="awardEntryDate" />
                        <field attributeName="awardClosingDate" />
                        <field attributeName="awardLastUpdateDate" />
                        <field attributeName="awardDirectCostAmount" />
                        <field attributeName="awardIndirectCostAmount" />
                        <field attributeName="awardTotalAmount" />
                        <field attributeName="agencyFuture1Amount" />
                        <field attributeName="agencyFuture2Amount" />
                        <field attributeName="agencyFuture3Amount" />

                        <field attributeName="grantDescriptionCode" />
                        <field attributeName="awardAddendumNumber" />
                        <field attributeName="proposalAwardTypeCode" />
                        <field attributeName="awardPurposeCode" />
                        <field attributeName="oldProposalNumber" />
                        <field attributeName="federalPassThroughIndicator" />
                        <field attributeName="federalPassThroughAgencyNumber" />
                        <field attributeName="federalPassThroughAgency.fullName" />
                        <field attributeName="federalPassThroughFundedAmount" />

                        <field attributeName="workgroupName" />
                        <field attributeName="kualiGroupNames" />
                        <field attributeName="agencyAnalystName" />
                        <field attributeName="analystTelephoneNumber" />

                        <field attributeName="letterOfCreditFundGroupCode" />
                        <field attributeName="awardDocumentNumber" />
                        <field attributeName="awardCommentText" />
                    </inquiryFields>
                </inquirySection>
                <inquirySection title="Organizations">
                    <inquiryFields>
                        <inquiryCollection name="awardOrganizations" businessObjectClass="org.kuali.module.cg.bo.AwardOrganization">
                            <field attributeName="chartOfAccountsCode" />
                            <field attributeName="organizationCode" />
                            <field attributeName="awardPrimaryOrganizationIndicator" />
                            <inquirySummaryFields>
                              <inquirySummaryField attributeName="chartOfAccountsCode" />
                              <inquirySummaryField attributeName="organizationCode" />
                              <inquirySummaryField attributeName="awardPrimaryOrganizationIndicator" />
                            </inquirySummaryFields>
                        </inquiryCollection>
                    </inquiryFields>
                </inquirySection>
                <inquirySection title="Project Directors">
                    <inquiryFields>
                        <inquiryCollection name="awardProjectDirectors" businessObjectClass="org.kuali.module.cg.bo.AwardProjectDirector">
                            <field attributeName="projectDirector.personUserIdentifier"/>
                            <field attributeName="projectDirector.personName"/>
                            <field attributeName="awardPrimaryProjectDirectorIndicator"/>
                            <field attributeName="awardProjectDirectorProjectTitle"/>
                            <inquirySummaryFields>
                                <inquirySummaryField attributeName="projectDirector.personName" />
                                <inquirySummaryField attributeName="awardPrimaryProjectDirectorIndicator" />
                                <inquirySummaryField attributeName="awardProjectDirectorProjectTitle" />
                            </inquirySummaryFields>
                        </inquiryCollection>
                    </inquiryFields>
                </inquirySection>
                <inquirySection title="Subcontractors">
                    <inquiryFields>
                        <inquiryCollection name="awardSubcontractors" businessObjectClass="org.kuali.module.cg.bo.AwardSubcontractor">
                            <field attributeName="subcontractorNumber" />
                            <field attributeName="subcontractor.subcontractorName" />
                            <field attributeName="awardSubcontractorDescription" />
                            <field attributeName="subcontractorAmount"/>
                            <inquirySummaryFields>
                                <inquirySummaryField attributeName="subcontractor.subcontractorName" />
                                <inquirySummaryField attributeName="awardSubcontractorAmount" />
                            </inquirySummaryFields>
                        </inquiryCollection>
                    </inquiryFields>
                </inquirySection>
                <inquirySection title="Accounts">
                    <inquiryFields>
                        <inquiryCollection name="awardAccounts" businessObjectClass="org.kuali.module.cg.bo.AwardAccount">
                            <field attributeName="accountNumber" />
                            <field attributeName="chartOfAccountsCode" />
                            <field attributeName="personUniversalIdentifier" />
                            <inquirySummaryFields>
                                <inquirySummaryField attributeName="accountNumber" />
                                <inquirySummaryField attributeName="chartOfAccountsCode" />
                            </inquirySummaryFields>
                        </inquiryCollection>
                    </inquiryFields>
                </inquirySection>
            </inquirySections>
        </inquiry>

        <lookup>
            <lookupableID>awardLookupable</lookupableID>
            <title>Award Lookup</title>

            <instructions>Look up an Award</instructions>

            <defaultSort sortAscending="true">
                <sortAttributes>
                    <sortAttribute attributeName="proposalNumber" />
                </sortAttributes>
            </defaultSort>

            <lookupFields>
                <lookupField attributeName="proposalNumber" required="false" />
                <lookupField attributeName="agencyNumber" required="false" />
                <lookupField attributeName="awardTotalAmount" required="false" />
                <lookupField attributeName="awardBeginningDate" required="false" />
                <lookupField attributeName="awardEndingDate" required="false" />
				<lookupField attributeName="awardProjectTitle" required="false" />
				<lookupField attributeName="proposal.grantNumber" required="false" />
				<lookupField attributeName="grantDescriptionCode" required="false" />
                <lookupField attributeName="awardStatusCode" required="false" />
                <lookupField attributeName="federalPassThroughAgencyNumber" required="false" />
                <lookupField attributeName="workgroupName" required="false" />
                <lookupField attributeName="awardPurposeCode" required="false" />
				<lookupField attributeName="awardSubcontractors.subcontractor.subcontractorNumber" required="false" />
				<lookupField attributeName="awardProjectDirectors.projectDirector.personUniversalIdentifier" required="false" />
				<lookupField attributeName="awardOrganizations.organization.organizationCode" required="false" />
                <lookupField attributeName="awardAccounts.account.accountNumber" required="false" />
            </lookupFields>

            <resultFields>
                <field attributeName="proposalNumber" />
                <field attributeName="agency.fullName" />
                <field attributeName="awardTotalAmount" />
                <field attributeName="awardBeginningDate" />
                <field attributeName="awardEndingDate" />
                <field attributeName="awardProjectTitle" />
				<field attributeName="proposal.grantNumber" />
				<field attributeName="grantDescriptionCode" />
                <field attributeName="awardStatusCode" />
                <field attributeName="primaryAwardOrganization.organizationCode" />
            </resultFields>
        </lookup>

        <titleAttribute>proposalNumber</titleAttribute>

        <attributes>
        
        	<!-- Following 2 attributes only used for labels for Workflow Doc Search -->
            <attributeReference name="routingChart"  sourceClassName="org.kuali.module.chart.bo.Org" sourceAttributeName="chartOfAccountsCode">
            	<label>Chart</label>
            	<shortLabel>Chart</shortLabel>
                <required>false</required>
                <control>
                    <hidden />
                </control>
            </attributeReference>

            <attributeReference name="routingOrg"  sourceClassName="org.kuali.module.chart.bo.Org" sourceAttributeName="organizationCode">
            	<label>Organization</label>
            	<shortLabel>Org</shortLabel>
                <required>false</required>
                <control>
                    <hidden />
                </control>
            </attributeReference>

			<attributeReference name="kualiGroupNames" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="genericBigText">
				<label>Kuali Workgroup Users</label>
				<shortLabel>Kuali Workgroup Users</shortLabel>
			</attributeReference>
			
			<attributeReference name="primaryAwardOrganization.organizationCode" sourceClassName="org.kuali.module.cg.bo.AwardOrganization" sourceAttributeName="organizationCode" >
				<label>Primary Organization Code</label>
				<shortLabel>Primary Org Cd</shortLabel>
			</attributeReference>

            <attributeReference name="proposal.proposalNumber"  sourceClassName="org.kuali.module.cg.bo.Proposal" sourceAttributeName="proposalNumber">
                <label>Link to Proposal</label>
                <shortLabel>PrpslLink</shortLabel>
            </attributeReference>
			
            <attribute name="proposalNumber" forceUppercase="true">
            	<label>Proposal Number</label>
            	<shortLabel>Prop Num</shortLabel>

                <maxLength>12</maxLength>
                <validationPattern><numeric /></validationPattern>

                <control>
                    <text size="14" />
                </control>
            </attribute>

            <attributeReference name="awardAccounts.account.accountNumber" sourceClassName="org.kuali.module.chart.bo.Account" sourceAttributeName="accountNumber">
				<label>Award Account Number</label>
				<shortLabel>Acct Num</shortLabel>
            </attributeReference>

            <attributeReference name="awardOrganizations.organization.organizationCode" sourceClassName="org.kuali.module.chart.bo.Org" sourceAttributeName="organizationCode">
				<label>Organization Code</label>
				<shortLabel>Org Cd</shortLabel>
            </attributeReference>
            
            <attributeReference name="awardSubcontractors.subcontractor.subcontractorNumber" sourceClassName="org.kuali.module.cg.bo.Subcontractor" sourceAttributeName="subcontractorNumber">
				<label>Subcontractor Number</label>
				<shortLabel>Subcontr Num</shortLabel>
            </attributeReference>
			
            <attributeReference name="awardProjectDirectors.projectDirector.personUniversalIdentifier" sourceClassName="org.kuali.module.cg.bo.ProjectDirector" sourceAttributeName="personUniversalIdentifier">
				<label>Project Director ID</label>
				<shortLabel>Proj Dir ID</shortLabel>
            </attributeReference>
			
            <attributeReference name="proposal.grantNumber" sourceClassName="org.kuali.module.cg.bo.Proposal" sourceAttributeName="grantNumber">
            </attributeReference>

            <attributeReference name="agency.fullName" sourceClassName="org.kuali.module.cg.bo.Agency" sourceAttributeName="fullName">
                <label>Agency Name</label>
            </attributeReference>

            <attribute name="agencyAnalystName" forceUppercase="true">
                <label>Agency Analyst Name</label>
                <shortLabel>Name</shortLabel>

                <maxLength>40</maxLength>
                <validationPattern><anyCharacter allowWhitespace="true"/></validationPattern>

                <control>
                    <text size="42" />
                </control>
			</attribute>

            <attributeReference name="agencyFuture1Amount" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="genericAmount">
                <label>Agency Future1 Amount</label>
                <shortLabel>Amount</shortLabel>

                <exclusiveMin>-0.001</exclusiveMin>

                <formatterClass>org.kuali.core.web.format.CurrencyFormatter</formatterClass>
			</attributeReference>

            <attributeReference name="agencyFuture2Amount" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="genericAmount">
                <label>Agency Future2 Amount</label>
                <shortLabel>Amount</shortLabel>

                <exclusiveMin>-0.001</exclusiveMin>
                <formatterClass>org.kuali.core.web.format.CurrencyFormatter</formatterClass>
			</attributeReference>

            <attributeReference name="agencyFuture3Amount" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="genericAmount">
                <label>Agency Future3 Amount</label>
                <shortLabel>Amount</shortLabel>

                <exclusiveMin>-0.001</exclusiveMin>

                <formatterClass>org.kuali.core.web.format.CurrencyFormatter</formatterClass>
			</attributeReference>

            <attributeReference name="agencyNumber" sourceClassName="org.kuali.module.cg.bo.Agency" sourceAttributeName="agencyNumber">
            </attributeReference>
            
            <attributeReference name="agency.reportingName" sourceClassName="org.kuali.module.cg.bo.Agency" sourceAttributeName="reportingName">
            </attributeReference>

            <attribute name="analystTelephoneNumber" forceUppercase="true">
                <label>Analyst Telephone Number</label>
                <shortLabel>Number</shortLabel>

                <maxLength>10</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="12" />
                </control>
			</attribute>

            <attribute name="awardAddendumNumber" forceUppercase="true">
                <label>Addendum Number</label>
                <shortLabel>Addendum</shortLabel>

                <maxLength>7</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="9" />
                </control>
			</attribute>

            <attributeReference name="awardAllocatedUniversityComputingServicesAmount" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="genericAmount">
                <label>Allocated University Computing Services Amount</label>
                <shortLabel>Computing Services</shortLabel>

                <formatterClass>org.kuali.core.web.format.CurrencyFormatter</formatterClass>
			</attributeReference>

            <attributeReference name="awardBeginningDate" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="genericDate">
                <label>Start Date</label>
                <shortLabel>Start</shortLabel>
				
			</attributeReference>

            <attributeReference name="awardClosingDate" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="genericDate">
                <label>Close Date</label>
                <shortLabel>Close</shortLabel>
				
			</attributeReference>

            <attribute name="awardCommentText" forceUppercase="true">
                <label>Comments</label>
                <shortLabel>Comments</shortLabel>

                <maxLength>250</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <textarea rows="5" cols="80"/>
                </control>

			</attribute>

            <attributeReference name="awardCreateTimestamp" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="genericTimestamp">
                <label>Created Timestamp</label>
                <shortLabel>Created</shortLabel>

			</attributeReference>

            <attributeReference name="awardDirectCostAmount" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="genericAmount">
                <label>Direct Cost Amount</label>
                <shortLabel>Direct</shortLabel>

                <exclusiveMin>-0.001</exclusiveMin>

                <formatterClass>org.kuali.core.web.format.CurrencyFormatter</formatterClass>
			</attributeReference>

            <attribute name="awardDocumentNumber" forceUppercase="true">
                <label>Award Document Number</label>
                <shortLabel>Number</shortLabel>

                <maxLength>12</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="14" />
                </control>

			</attribute>

            <attributeReference name="awardEndingDate" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="genericDate">
                <label>Stop Date</label>
                <shortLabel>Stop</shortLabel>
			</attributeReference>

            <attributeReference name="awardEntryDate" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="genericDate">
                <label>Entry Date</label>
                <shortLabel>Entry</shortLabel>
			</attributeReference>

            <attributeReference name="awardIndirectCostAmount" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="genericAmount">
                <label>Indirect Cost Amount</label>
                <shortLabel>Indirect</shortLabel>
				
                <exclusiveMin>-0.001</exclusiveMin>

                <formatterClass>org.kuali.core.web.format.CurrencyFormatter</formatterClass>
			</attributeReference>

            <attributeReference name="awardLastUpdateDate" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="genericDate">
                <label>Last Update</label>
                <shortLabel>Update</shortLabel>
			</attributeReference>

            <attribute name="awardProjectTitle">
                <label>Project Title</label>
                <shortLabel>Title</shortLabel>

                <maxLength>250</maxLength>
                <validationPattern><anyCharacter allowWhitespace="true" /></validationPattern>

                <control>
                    <textarea rows="5" cols="80"/>
                </control>
			</attribute>

            <attributeReference name="awardPurposeCode" sourceClassName="org.kuali.module.cg.bo.ProposalPurpose" sourceAttributeName="proposalPurposeCode">
                <label>Purpose</label>
                <shortLabel>Purpose</shortLabel>
                <control>
                    <select valuesFinderClass="org.kuali.module.cg.lookup.keyvalues.ProposalPurposeValuesFinder" />
                </control>
            </attributeReference>

            <attributeReference name="awardStatusCode" sourceClassName="org.kuali.module.cg.bo.AwardStatus" sourceAttributeName="awardStatusCode">
                <label>Status</label>
                <control>
                    <select valuesFinderClass="org.kuali.module.cg.lookup.keyvalues.AwardStatusValuesFinder" />
                </control>
            </attributeReference>

            <attributeReference name="awardTotalAmount" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="genericAmount">
                <label>Total Amount</label>
                <shortLabel>Total</shortLabel>
                <formatterClass>org.kuali.core.web.format.CurrencyFormatter</formatterClass>
			</attributeReference>

            <attributeReference name="federalFundedAmount" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="genericAmount">
                <label>Federal Funded Amount</label>
                <shortLabel>Federal</shortLabel>
                <formatterClass>org.kuali.core.web.format.CurrencyFormatter</formatterClass>
			</attributeReference>

            <attributeReference name="federalPassThroughAgencyNumber" sourceClassName="org.kuali.module.cg.bo.Agency" sourceAttributeName="agencyNumber">
                <label>Federal Pass Through Agency Number</label>
			</attributeReference>
			
			<attributeReference name="federalPassThroughAgency.fullName" sourceClassName="org.kuali.module.cg.bo.Agency" sourceAttributeName="fullName">
                <label>Federal Pass Through Agency Name</label>
                <shortLabel>FPT Agency Name</shortLabel>
            </attributeReference>
			
            <attributeReference name="federalPassThroughFundedAmount" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="genericAmount">
                <label>Federal Pass Through Funded Amount</label>
                <shortLabel>FPT Amount</shortLabel>
                <formatterClass>org.kuali.core.web.format.CurrencyFormatter</formatterClass>
			</attributeReference>

            <attribute name="federalPassThroughIndicator" forceUppercase="true">
                <label>Federal Pass Through</label>
                <shortLabel>FPT</shortLabel>

                <maxLength>1</maxLength>

                <control>
                    <checkbox />
                </control>
			</attribute>

            <attribute name="workgroupName" forceUppercase="true">
                <label>Workgroup Name</label>
                <shortLabel>Name</shortLabel>

                <maxLength>70</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="72" />
                </control>
			</attribute>

            <attributeReference name="grantDescriptionCode" sourceClassName="org.kuali.module.cg.bo.GrantDescription" sourceAttributeName="grantDescriptionCode">
                <label>Grant Description</label>
                <shortLabel>Description</shortLabel>
				<control>
                    <select valuesFinderClass="org.kuali.module.cg.lookup.keyvalues.GrantDescriptionValuesFinder" />
                </control>
			</attributeReference>

            <attributeReference name="letterOfCreditFundGroupCode" sourceClassName="org.kuali.module.cg.bo.LetterOfCreditFundGroup" sourceAttributeName="letterOfCreditFundGroupCode">
                <label>Letter Of Credit Fund Group</label>
				<control>
                    <select valuesFinderClass="org.kuali.module.cg.lookup.keyvalues.LetterOfCreditFundGroupValuesFinder" />
                </control>
			</attributeReference>

            <attribute name="oldProposalNumber" forceUppercase="true">
                <label>Old Proposal Number</label>
                <shortLabel>Old</shortLabel>

                <maxLength>8</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="10" />
                </control>
			</attribute>

            <attributeReference name="proposalAwardTypeCode" sourceClassName="org.kuali.module.cg.bo.ProposalAwardType" sourceAttributeName="proposalAwardTypeCode">
                <label>Award Type</label>
                <shortLabel>Type</shortLabel>
				<control>
                    <select valuesFinderClass="org.kuali.module.cg.lookup.keyvalues.ProposalAwardTypeValuesFinder" />
                </control>
			</attributeReference>
			
            <attributeReference name="versionNumber" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="versionNumber" />

        </attributes>
        
        <collections>
            <collection name="awardProjectDirectors">
                <label>Project Directors</label>
                <shortLabel>Project Director</shortLabel>
                <elementLabel>Project Director</elementLabel>
            </collection>
            <collection name="awardAccounts">
                <label>Award Accounts</label>
                <shortLabel>Accounts</shortLabel>
                <elementLabel>Account</elementLabel>
            </collection>
            <collection name="awardOrganizations">
                <label>Award Organizations</label>
                <shortLabel>Organizations</shortLabel>
                <elementLabel>Organization</elementLabel>
            </collection>
            <collection name="awardSubcontractors">
                <label>Award Subcontractors</label>
                <shortLabel>Subcontractors</shortLabel>
                <elementLabel>Subcontractor</elementLabel>
            </collection>
        </collections>
        
        <relationships>
            <relationship objectAttributeName="awardOrganizations.organization" >
                <primitiveAttribute sourceName="awardOrganizations.organization.organizationCode" targetName="organizationCode" />
            </relationship>
            <relationship objectAttributeName="awardSubcontractors.subcontractor" >
                <primitiveAttribute sourceName="awardSubcontractors.subcontractor.subcontractorNumber" targetName="subcontractorNumber" />
            </relationship>
            <relationship objectAttributeName="awardProjectDirectors.projectDirector" >
                <primitiveAttribute sourceName="awardProjectDirectors.projectDirector.personUniversalIdentifier" targetName="personUniversalIdentifier" />
            </relationship>
            <relationship objectAttributeName="awardAccounts.account" >
                <primitiveAttribute sourceName="awardAccounts.account.accountNumber" targetName="accountNumber" />
            </relationship>
            <relationship objectAttributeName="primaryAwardOrganization" >
            	<primitiveAttribute sourceName="primaryAwardOrganization.organizationCode" targetName="organizationCode" />
            </relationship>
        </relationships>
        <objectLabel>Award</objectLabel>
    </businessObject>
</dictionaryEntry>
