<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="NonpersonnelSubCategory" parent="NonpersonnelSubCategory-parentBean" />

  <bean id="NonpersonnelSubCategory-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.kfs.module.cg.businessobject.NonpersonnelSubCategory" />
    <property name="inquiryDefinition" >
      <ref bean="NonpersonnelSubCategory-inquiryDefinition" />
    </property>
    <property name="lookupDefinition" >
      <ref bean="NonpersonnelSubCategory-lookupDefinition" />
    </property>
    <property name="titleAttribute" value="code" />
    <property name="objectLabel" value="Non-Personnel Sub-Category" />
    <property name="attributes" >
      <list>
        <ref bean="NonpersonnelSubCategory-code" />
        <ref bean="NonpersonnelSubCategory-active" />
        <ref bean="NonpersonnelSubCategory-nonpersonnelMtdcExcludedIndicator" />
        <ref bean="NonpersonnelSubCategory-nonpersonnelModularExcludedIndicator" />
        <ref bean="NonpersonnelSubCategory-name" />
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->


  <bean id="NonpersonnelSubCategory-code" parent="NonpersonnelSubCategory-code-parentBean" />

  <bean id="NonpersonnelSubCategory-code-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="code" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Nonpersonnel Sub Category Code" />
    <property name="shortLabel" value="Code" />
    <property name="maxLength" value="3" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="required" value="true" />
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="4" />
    </property>
    <property name="summary" value="Nonpersonnel Sub Category Code" />
    <property name="description" value="Nonpersonnel Sub Category Code" />
  </bean>
  <bean id="NonpersonnelSubCategory-active" parent="NonpersonnelSubCategory-active-parentBean" />

  <bean id="NonpersonnelSubCategory-active-parentBean" abstract="true" parent="GenericAttributes-activeIndicator">
    <property name="name" value="active" />
  </bean>
  <bean id="NonpersonnelSubCategory-nonpersonnelMtdcExcludedIndicator" parent="NonpersonnelSubCategory-nonpersonnelMtdcExcludedIndicator-parentBean" />

  <bean id="NonpersonnelSubCategory-nonpersonnelMtdcExcludedIndicator-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="nonpersonnelMtdcExcludedIndicator" />
    <property name="label" value="MTDC Excluded Indicator" />
    <property name="shortLabel" value="mtdc excluded" />
    <property name="maxLength" value="1" />
    <property name="control" >
      <bean parent="CheckboxControlDefinition" />
    </property>
    <property name="summary" value="Excluded Indicator indicates whether an Nonpersonnel Sub Category is excluded from Indirect Cost Calculations." />
    <property name="description" value="This attribute is used to describe whether the associated Nonpersonnel Sub Category is excluded from Indirect Cost Calculations." />
  </bean>
  <bean id="NonpersonnelSubCategory-nonpersonnelModularExcludedIndicator" parent="NonpersonnelSubCategory-nonpersonnelModularExcludedIndicator-parentBean" />

  <bean id="NonpersonnelSubCategory-nonpersonnelModularExcludedIndicator-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="nonpersonnelModularExcludedIndicator" />
    <property name="label" value="Modular Excluded Indicator" />
    <property name="shortLabel" value="Modular excluded" />
    <property name="maxLength" value="1" />
    <property name="control" >
      <bean parent="CheckboxControlDefinition" />
    </property>
    <property name="summary" value="Excluded Indicator indicates whether an Nonpersonnel Sub Category is excluded from Direct Cost on Modular Budgets." />
    <property name="description" value="This attribute is used to describe whether the associated Nonpersonnel Sub Category is excluded from Direct Cost on Modular Budgets." />
  </bean>
  <bean id="NonpersonnelSubCategory-name" parent="NonpersonnelSubCategory-name-parentBean" />

  <bean id="NonpersonnelSubCategory-name-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="name" />
    <property name="label" value="Nonpersonnel Sub Category Name" />
    <property name="shortLabel" value="Name" />
    <property name="maxLength" value="40" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern"
            p:allowWhitespace="true" />
    </property>
    <property name="required" value="true" />
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="42" />
    </property>
    <property name="summary" value="Nonpersonnel Sub Category Name" />
    <property name="description" value="The name of the Nonpersonnel Sub Category." />
  </bean>

<!-- Business Object Inquiry Definition -->


  <bean id="NonpersonnelSubCategory-inquiryDefinition" parent="NonpersonnelSubCategory-inquiryDefinition-parentBean" />

  <bean id="NonpersonnelSubCategory-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
    <property name="title" value="Non-Personnel Sub-Category Inquiry" />
    <property name="inquirySections" >
      <list>
        <bean parent="InquirySectionDefinition">
          <property name="title" value="" />
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields" >
            <list>
              <bean parent="FieldDefinition"
                    p:attributeName="code" />
              <bean parent="FieldDefinition"
                    p:attributeName="name" />
              <bean parent="FieldDefinition"
                    p:attributeName="nonpersonnelMtdcExcludedIndicator" />
              <bean parent="FieldDefinition"
                    p:attributeName="nonpersonnelModularExcludedIndicator" />
              <bean parent="FieldDefinition"
                    p:attributeName="active" />
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Business Object Lookup Definition -->


  <bean id="NonpersonnelSubCategory-lookupDefinition" parent="NonpersonnelSubCategory-lookupDefinition-parentBean" />

  <bean id="NonpersonnelSubCategory-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
    <property name="title" value="Non-Personnel Sub-Category Lookup" />
    <property name="instructions" value="Lookup a Nonpersonnel Sub Category" />
    <property name="defaultSort" >
      <bean parent="SortDefinition">
        <property name="attributeNames" >
          <list>
            <value>code</value>
          </list>
        </property>
        <property name="sortAscending" value="false" />
      </bean>
    </property>
    <property name="lookupFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="code" />
        <bean parent="FieldDefinition"
              p:attributeName="name" />
        <bean parent="FieldDefinition"
              p:attributeName="nonpersonnelMtdcExcludedIndicator" />
        <bean parent="FieldDefinition"
              p:attributeName="nonpersonnelModularExcludedIndicator" />
        <bean parent="FieldDefinition"
              p:defaultValue="Y"
              p:attributeName="active" />
      </list>
    </property>
    <property name="resultFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="code" />
        <bean parent="FieldDefinition"
              p:attributeName="name" />
        <bean parent="FieldDefinition"
              p:attributeName="nonpersonnelMtdcExcludedIndicator" />
        <bean parent="FieldDefinition"
              p:attributeName="nonpersonnelModularExcludedIndicator" />
        <bean parent="FieldDefinition"
              p:attributeName="active" />
      </list>
    </property>
  </bean>
</beans>
