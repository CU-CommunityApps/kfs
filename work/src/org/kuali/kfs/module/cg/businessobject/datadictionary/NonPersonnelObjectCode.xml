<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="NonPersonnelObjectCode" parent="NonPersonnelObjectCode-parentBean" />

  <bean id="NonPersonnelObjectCode-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.kfs.module.cg.businessobject.NonPersonnelObjectCode" />
    <property name="inquiryDefinition" >
      <ref bean="NonPersonnelObjectCode-inquiryDefinition" />
    </property>
    <property name="lookupDefinition" >
      <ref bean="NonPersonnelObjectCode-lookupDefinition" />
    </property>
    <property name="objectLabel" value="Non-Personnel Object Code" />
    <property name="attributes" >
      <list>
        <ref bean="NonPersonnelObjectCode-budgetNonpersonnelCategoryCode" />
        <ref bean="NonPersonnelObjectCode-nonpersonnelCategory.name" />
        <ref bean="NonPersonnelObjectCode-budgetNonpersonnelSubCategoryCode" />
        <ref bean="NonPersonnelObjectCode-nonpersonnelSubCategory.name" />
        <ref bean="NonPersonnelObjectCode-budgetNonpersonnelObjectCode" />
        <ref bean="NonPersonnelObjectCode-active" />
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->


  <bean id="NonPersonnelObjectCode-budgetNonpersonnelCategoryCode" parent="NonPersonnelObjectCode-budgetNonpersonnelCategoryCode-parentBean" />

  <bean id="NonPersonnelObjectCode-budgetNonpersonnelCategoryCode-parentBean" abstract="true" parent="NonPersonnelCategory-code">
    <property name="name" value="budgetNonpersonnelCategoryCode" />
    <property name="forceUppercase" value="true" />
  </bean>
  <bean id="NonPersonnelObjectCode-nonpersonnelCategory.name" parent="NonPersonnelObjectCode-nonpersonnelCategory.name-parentBean" />

  <bean id="NonPersonnelObjectCode-nonpersonnelCategory.name-parentBean" abstract="true" parent="NonPersonnelCategory-name">
    <property name="name" value="nonpersonnelCategory.name" />
  </bean>
  <bean id="NonPersonnelObjectCode-budgetNonpersonnelSubCategoryCode" parent="NonPersonnelObjectCode-budgetNonpersonnelSubCategoryCode-parentBean" >
  	<property name="forceUppercase" value="true" />
  </bean>

  <bean id="NonPersonnelObjectCode-budgetNonpersonnelSubCategoryCode-parentBean" abstract="true" parent="NonPersonnelSubCategory-code">
    <property name="name" value="budgetNonpersonnelSubCategoryCode" />
  </bean>
  <bean id="NonPersonnelObjectCode-nonpersonnelSubCategory.name" parent="NonPersonnelObjectCode-nonpersonnelSubCategory.name-parentBean" />

  <bean id="NonPersonnelObjectCode-nonpersonnelSubCategory.name-parentBean" abstract="true" parent="NonPersonnelSubCategory-name">
    <property name="name" value="nonpersonnelSubCategory.name" />
  </bean>
  <bean id="NonPersonnelObjectCode-budgetNonpersonnelObjectCode" parent="NonPersonnelObjectCode-budgetNonpersonnelObjectCode-parentBean" />

  <bean id="NonPersonnelObjectCode-budgetNonpersonnelObjectCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="budgetNonpersonnelObjectCode" />
    <property name="label" value="Object Code" />
    <property name="shortLabel" value="Obj Cd" />
    <property name="maxLength" value="4" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern"
            p:allowWhitespace="true" />
    </property>
    <property name="required" value="true" />
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="4" />
    </property>
    <property name="summary" value="Object Code" />
    <property name="description" value="The assigned object code." />
  </bean>
  <bean id="NonPersonnelObjectCode-active" parent="NonPersonnelObjectCode-active-parentBean" />

  <bean id="NonPersonnelObjectCode-active-parentBean" abstract="true" parent="GenericAttributes-activeIndicator">
    <property name="name" value="active" />
  </bean>

<!-- Business Object Inquiry Definition -->


  <bean id="NonPersonnelObjectCode-inquiryDefinition" parent="NonPersonnelObjectCode-inquiryDefinition-parentBean" />

  <bean id="NonPersonnelObjectCode-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
    <property name="title" value="Non-Personnel Object Code Inquiry" />
    <property name="inquirySections" >
      <list>
        <bean parent="InquirySectionDefinition">
          <property name="title" value="" />
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields" >
            <list>
              <bean parent="FieldDefinition"
                    p:attributeName="budgetNonpersonnelObjectCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="nonpersonnelCategory" />
              <bean parent="FieldDefinition"
                    p:attributeName="nonpersonnelSubCategory" />
              <bean parent="FieldDefinition"
                    p:attributeName="active" />
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Business Object Lookup Definition -->


  <bean id="NonPersonnelObjectCode-lookupDefinition" parent="NonPersonnelObjectCode-lookupDefinition-parentBean" />

  <bean id="NonPersonnelObjectCode-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
    <property name="title" value="Non-Personnel Object Code Lookup" />
    <property name="instructions" value="Lookup a Nonpersonnel Object Code" />
    <property name="defaultSort" >
      <bean parent="SortDefinition">
        <property name="attributeNames" >
          <list>
            <value>nonpersonnelCategory.name</value>
          </list>
        </property>
      </bean>
    </property>
    <property name="lookupFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="nonpersonnelCategory.name" />
        <bean parent="FieldDefinition"
              p:attributeName="nonpersonnelSubCategory.name" />
        <bean parent="FieldDefinition"
              p:attributeName="budgetNonpersonnelObjectCode" />
        <bean parent="FieldDefinition"
              p:defaultValue="Y"
              p:attributeName="active" />
      </list>
    </property>
    <property name="resultFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="nonpersonnelCategory.name" />
        <bean parent="FieldDefinition"
              p:attributeName="nonpersonnelSubCategory.name" />
        <bean parent="FieldDefinition"
              p:attributeName="budgetNonpersonnelObjectCode" />
        <bean parent="FieldDefinition"
              p:attributeName="active" />
      </list>
    </property>
  </bean>
</beans>
