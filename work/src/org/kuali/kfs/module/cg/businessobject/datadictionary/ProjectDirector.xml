<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="ProjectDirector" parent="ProjectDirector-parentBean" />

  <bean id="ProjectDirector-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.kfs.module.cg.businessobject.ProjectDirector" />
    <property name="inquiryDefinition" >
      <ref bean="ProjectDirector-inquiryDefinition" />
    </property>
    <property name="lookupDefinition" >
      <ref bean="ProjectDirector-lookupDefinition" />
    </property>
    <property name="objectLabel" value="Project Director" />
    <property name="attributes" >
      <list>
        <ref bean="ProjectDirector-person.personPayrollIdentifier" />
        <ref bean="ProjectDirector-principalId" />
        <ref bean="ProjectDirector-person.principalName" />
        <ref bean="ProjectDirector-person.name" />
        <ref bean="ProjectDirector-person.lastName" />
        <ref bean="ProjectDirector-person.firstName" />
        <ref bean="ProjectDirector-person.campusCode" />
        <ref bean="ProjectDirector-person.primaryDepartmentCode" />
        <ref bean="ProjectDirector-person.employeeStatusCode" />
        <ref bean="ProjectDirector-person.employeeTypeCode" />
        <ref bean="ProjectDirector-active" />
      </list>
    </property>
    <property name="relationships" >
      <list>
        <bean parent="RelationshipDefinition">
          <property name="objectAttributeName" value="person" />
          <property name="primitiveAttributes" >
            <list>
              <bean parent="PrimitiveAttributeDefinition"
                    p:sourceName="principalId"
                    p:targetName="principalId" />
            </list>
          </property>
          <property name="supportAttributes" >
            <list>
              <bean parent="SupportAttributeDefinition"
                    p:identifier="true"
                    p:sourceName="person.principalName"
                    p:targetName="principalName" />
              <bean parent="SupportAttributeDefinition"
                    p:sourceName="person.name"
                    p:targetName="name" />
              <bean parent="SupportAttributeDefinition"
                    p:sourceName="person.primaryDepartmentCode"
                    p:targetName="primaryDepartmentCode" />
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->


  <bean id="ProjectDirector-person.personPayrollIdentifier" parent="ProjectDirector-person.personPayrollIdentifier-parentBean" />

  <bean id="ProjectDirector-person.personPayrollIdentifier-parentBean" abstract="true" parent="PersonImpl-principalId">
    <property name="name" value="person.personPayrollIdentifier" />
  </bean>
  <bean id="ProjectDirector-principalId" parent="ProjectDirector-principalId-parentBean" />

  <bean id="ProjectDirector-principalId-parentBean" abstract="true" parent="GenericAttributes-genericSystemId">
    <property name="name" value="principalId" />
    <property name="label" value="Universal ID" />
    <property name="shortLabel" value="ID" />
    <property name="required" value="true" />
  </bean>
  <bean id="ProjectDirector-person.principalName" parent="ProjectDirector-person.principalName-parentBean" />

  <bean id="ProjectDirector-person.principalName-parentBean" abstract="true" parent="PersonImpl-principalName">
    <property name="name" value="person.principalName" />
  </bean>
  <bean id="ProjectDirector-person.name" parent="ProjectDirector-person.name-parentBean" />

  <bean id="ProjectDirector-person.name-parentBean" abstract="true" parent="PersonImpl-name">
    <property name="name" value="person.name" />
    <property name="label" value="Project Director Name" />
    <property name="shortLabel" value="Proj Dir Name" />
  </bean>
  <bean id="ProjectDirector-person.lastName" parent="ProjectDirector-person.lastName-parentBean" />

  <bean id="ProjectDirector-person.lastName-parentBean" abstract="true" parent="PersonImpl-lastName">
    <property name="name" value="person.lastName" />
    <property name="label" value="Project Director Last Name" />
    <property name="shortLabel" value="Proj Dir Last Name" />
  </bean>
  <bean id="ProjectDirector-person.firstName" parent="ProjectDirector-person.firstName-parentBean" />

  <bean id="ProjectDirector-person.firstName-parentBean" abstract="true" parent="PersonImpl-firstName">
    <property name="name" value="person.firstName" />
    <property name="label" value="Project Director First Name" />
    <property name="shortLabel" value="Proj Dir First Name" />
  </bean>
  <bean id="ProjectDirector-person.campusCode" parent="ProjectDirector-person.campusCode-parentBean" />

  <bean id="ProjectDirector-person.campusCode-parentBean" abstract="true" parent="Campus-campusCode">
    <property name="name" value="person.campusCode" />
  </bean>
  <bean id="ProjectDirector-person.primaryDepartmentCode" parent="ProjectDirector-person.primaryDepartmentCode-parentBean" />

  <bean id="ProjectDirector-person.primaryDepartmentCode-parentBean" abstract="true" parent="PersonImpl-primaryDepartmentCode">
    <property name="name" value="person.primaryDepartmentCode" />
    <property name="label" value="Dept ID" />
    <property name="shortLabel" value="Dept ID" />
  </bean>
  <bean id="ProjectDirector-person.employeeStatusCode" parent="ProjectDirector-person.employeeStatusCode-parentBean" />

  <bean id="ProjectDirector-person.employeeStatusCode-parentBean" abstract="true" parent="PersonImpl-employeeStatusCode">
    <property name="name" value="person.employeeStatusCode" />
    <property name="label" value="Employee Status" />
    <property name="shortLabel" value="Empl Status" />
  </bean>
  <bean id="ProjectDirector-person.employeeTypeCode" parent="ProjectDirector-person.employeeTypeCode-parentBean" />

  <bean id="ProjectDirector-person.employeeTypeCode-parentBean" abstract="true" parent="PersonImpl-employeeTypeCode">
    <property name="name" value="person.employeeTypeCode" />
    <property name="label" value="Employee Type" />
    <property name="shortLabel" value="Empl Type" />
  </bean>
  <bean id="ProjectDirector-active" parent="ProjectDirector-active-parentBean" />

  <bean id="ProjectDirector-active-parentBean" abstract="true" parent="GenericAttributes-activeIndicator">
      <property name="name" value="active" />
  </bean>

<!-- Business Object Inquiry Definition -->


  <bean id="ProjectDirector-inquiryDefinition" parent="ProjectDirector-inquiryDefinition-parentBean" />

  <bean id="ProjectDirector-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
    <property name="title" value="Project Director Inquiry" />
    <property name="inquirySections" >
      <list>
        <bean parent="InquirySectionDefinition">
          <property name="title" value="Project Director Details" />
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields" >
            <list>
              <bean parent="FieldDefinition"
                    p:attributeName="principalId" />
              <bean parent="FieldDefinition"
                    p:attributeName="person.principalName" />
              <bean parent="FieldDefinition"
                    p:attributeName="person.name" />
              <bean parent="FieldDefinition"
                    p:attributeName="person.campusCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="person.primaryDepartmentCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="person.employeeStatusCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="person.employeeTypeCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="active" />
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Business Object Lookup Definition -->


  <bean id="ProjectDirector-lookupDefinition" parent="ProjectDirector-lookupDefinition-parentBean" />

  <bean id="ProjectDirector-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
    <property name="title" value="Project Director Lookup" />
    <property name="instructions" value="Lookup a Project Director" />
    <property name="defaultSort" >
      <bean parent="SortDefinition">
        <property name="attributeNames" >
          <list>
            <value>person.name</value>
          </list>
        </property>
      </bean>
    </property>
    <property name="lookupFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="person.principalName" />
        <bean parent="FieldDefinition"
              p:attributeName="principalId" />
        <bean parent="FieldDefinition"
              p:attributeName="person.personPayrollIdentifier" />
        <bean parent="FieldDefinition"
              p:attributeName="person.lastName" />
        <bean parent="FieldDefinition"
              p:attributeName="person.firstName" />
        <bean parent="FieldDefinition"
              p:attributeName="person.primaryDepartmentCode" />
        <bean parent="FieldDefinition"
              p:attributeName="person.employeeStatusCode" />
        <bean parent="FieldDefinition"
              p:attributeName="person.employeeTypeCode" />
        <bean parent="FieldDefinition"
              p:defaultValue="true"
              p:attributeName="active" />
      </list>
    </property>
    <property name="resultFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="person.principalName" />
        <bean parent="FieldDefinition"
              p:attributeName="principalId" />
        <bean parent="FieldDefinition"
              p:attributeName="person.personPayrollIdentifier" />
        <bean parent="FieldDefinition"
              p:attributeName="person.name" />
        <bean parent="FieldDefinition"
              p:attributeName="person.primaryDepartmentCode" />
        <bean parent="FieldDefinition"
              p:attributeName="person.employeeStatusCode" />
        <bean parent="FieldDefinition"
              p:attributeName="person.employeeTypeCode" />
         <bean parent="FieldDefinition"
              p:attributeName="active" />
      </list>
    </property>
  </bean>
</beans>

