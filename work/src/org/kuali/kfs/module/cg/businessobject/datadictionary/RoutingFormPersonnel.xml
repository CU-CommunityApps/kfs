        <ref bean="RoutingFormPersonnel-personUniversalIdentifier" />
        <ref bean="RoutingFormPersonnel-personZipCode" />
      </list>
    </property>
    <property name="relationships" >
      <list>
        <dd:relationship objectAttribute="personZip">
        	<dd:primitiveAttribute source="personCountryCode" target="postalCountryCode" />
        	<dd:primitiveAttribute source="personStateCode" target="postalStateCode" /> 
            <dd:primitiveAttribute source="personZipCode" target="postalZipCode" />
        </dd:relationship>
        <dd:relationship objectAttribute="personState">
        	<dd:primitiveAttribute source="personCountryCode" target="postalCountryCode" />
            <dd:primitiveAttribute source="personStateCode" target="postalStateCode" />            
        </dd:relationship>
        <dd:relationship objectAttribute="personCountry">
            <dd:primitiveAttribute source="personCountryCode" target="postalCountryCode" />
        </dd:relationship>
      </list>
    </property>    
  </bean>

