<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "../dataDictionary-1_0.dtd">
<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.module.kra.routingform.bo.RoutingFormDueDateType">
        <customAttributeEnabled>false</customAttributeEnabled>

        <inquiry>
            <title>Routing Form Due Date Type Inquiry</title>

            <inquiryFields>
                <field attributeName="dataObjectMaintenanceCodeActiveIndicator" />
                <field attributeName="routingFormApprovalLeadTime" />
                <field attributeName="routingFormDueDateDescription" />
                <field attributeName="routingFormDueDateTypeCode" />
            </inquiryFields>
        </inquiry>

        <lookup>
            <title>Routing Form Due Date Type Lookup</title>

            <instructions>Look up a Routing Form Due Date Type</instructions>

            <defaultSort sortAscending="true">
                <sortAttributes>
                    <sortAttribute attributeName="routingFormDueDateTypeCode" />
                </sortAttributes>
            </defaultSort>

            <lookupFields>
                <lookupField attributeName="dataObjectMaintenanceCodeActiveIndicator" required="false" />
                <lookupField attributeName="routingFormApprovalLeadTime" required="false" />
                <lookupField attributeName="routingFormDueDateDescription" required="false" />
                <lookupField attributeName="routingFormDueDateTypeCode" required="false" />
            </lookupFields>

            <resultFields>
                <field attributeName="dataObjectMaintenanceCodeActiveIndicator" />
                <field attributeName="routingFormApprovalLeadTime" />
                <field attributeName="routingFormDueDateDescription" />
                <field attributeName="routingFormDueDateTypeCode" />
            </resultFields>
        </lookup>

        <attributes>

            <attribute name="dataObjectMaintenanceCodeActiveIndicator" forceUppercase="true">
                <label>Data Object Maintenance Code Active Indicator</label>
                <shortLabel>Indicator</shortLabel>

                <maxLength>1</maxLength>

                <control>
                    <checkbox />
                </control>

                <summary>dataObjectMaintenanceCodeActiveIndicator</summary>
                <description>dataObjectMaintenanceCodeActiveIndicator description...</description>
			</attribute>

            <attribute name="routingFormApprovalLeadTime" forceUppercase="true">
                <label>Routing Form Approval Lead Time</label>
                <shortLabel>Time</shortLabel>

                <maxLength>3</maxLength>
                <validationPattern><numeric /></validationPattern>

                <control>
                    <text size="5" />
                </control>

                <summary>routingFormApprovalLeadTime</summary>
                <description>routingFormApprovalLeadTime description...</description>
			</attribute>

            <attribute name="routingFormDueDateDescription" forceUppercase="true">
                <label>Routing Form Due Date Description</label>
                <shortLabel>Description</shortLabel>

                <maxLength>20</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="22" />
                </control>

                <summary>routingFormDueDateDescription</summary>
                <description>routingFormDueDateDescription description...</description>
			</attribute>

            <attribute name="routingFormDueDateTypeCode" forceUppercase="true">
                <label>Routing Form Due Date Type Code</label>
                <shortLabel>Code</shortLabel>

                <maxLength>2</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="4" />
                </control>

                <summary>routingFormDueDateTypeCode</summary>
                <description>routingFormDueDateTypeCode description...</description>
			</attribute>

            <attributeReference name="versionNumber" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="versionNumber" />

        </attributes>
    </businessObject>
</dictionaryEntry>
