<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="RoutingFormDueDateType" parent="RoutingFormDueDateType-parentBean" />

  <bean id="RoutingFormDueDateType-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.kfs.module.cg.businessobject.RoutingFormDueDateType" />
    <property name="inquiryDefinition" >
      <ref bean="RoutingFormDueDateType-inquiryDefinition" />
    </property>
    <property name="lookupDefinition" >
      <ref bean="RoutingFormDueDateType-lookupDefinition" />
    </property>
    <property name="objectLabel" value="Routing Form Due Date Type" />
    <property name="attributes" >
      <list>
        <ref bean="RoutingFormDueDateType-documentNumber" />
        <ref bean="RoutingFormDueDateType-dueDateTypeCode" />
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->


  <bean id="RoutingFormDueDateType-documentNumber" parent="RoutingFormDueDateType-documentNumber-parentBean" />

  <bean id="RoutingFormDueDateType-documentNumber-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="documentNumber" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Document Number" />
    <property name="shortLabel" value="Number" />
    <property name="maxLength" value="14" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="16" />
    </property>
    
    
  </bean>
  <bean id="RoutingFormDueDateType-dueDateTypeCode" parent="RoutingFormDueDateType-dueDateTypeCode-parentBean" />

  <bean id="RoutingFormDueDateType-dueDateTypeCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="dueDateTypeCode" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Due Date Type Code" />
    <property name="shortLabel" value="Code" />
    <property name="maxLength" value="2" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="4" />
    </property>
    
    
  </bean>

<!-- Business Object Inquiry Definition -->


  <bean id="RoutingFormDueDateType-inquiryDefinition" parent="RoutingFormDueDateType-inquiryDefinition-parentBean" />

  <bean id="RoutingFormDueDateType-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
    <property name="title" value="Routing Form Due Date Type Inquiry" />
    <property name="inquirySections" >
      <list>
        <bean parent="InquirySectionDefinition">
          <property name="title" value="" />
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields" >
            <list>
              <bean parent="FieldDefinition"
                    p:attributeName="documentNumber" />
              <bean parent="FieldDefinition"
                    p:attributeName="dueDateTypeCode" />
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Business Object Lookup Definition -->


  <bean id="RoutingFormDueDateType-lookupDefinition" parent="RoutingFormDueDateType-lookupDefinition-parentBean" />

  <bean id="RoutingFormDueDateType-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
    <property name="title" value="Routing Form Due Date Type Lookup" />
    <property name="instructions" value="Lookup a Routing Form Due Date Type" />
    <property name="defaultSort" >
      <bean parent="SortDefinition">
      </bean>
    </property>
    <property name="lookupFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="documentNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="dueDateTypeCode" />
      </list>
    </property>
    <property name="resultFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="documentNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="dueDateTypeCode" />
      </list>
    </property>
  </bean>
</beans>
