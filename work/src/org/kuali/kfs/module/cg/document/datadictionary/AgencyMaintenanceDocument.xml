<dictionaryEntry>
	<maintenanceDocument>
        <businessObjectClass>org.kuali.module.cg.bo.Agency</businessObjectClass>
        <maintainableClass>org.kuali.core.maintenance.KualiMaintainableImpl</maintainableClass>

        <documentAuthorizerClass>org.kuali.core.document.MaintenanceDocumentAuthorizerBase</documentAuthorizerClass>
        <authorizations>
            <authorization action="initiate">
                <workgroups>
                    <workgroup>kualiUniversalGroup</workgroup>
                </workgroups>
            </authorization>
        </authorizations>

		<documentTypeName>KualiContractsAndGrantsAgencyMaintenanceDocument</documentTypeName>
		<documentTypeCode>AGCY</documentTypeCode>

		<label>Contracts And Grants Agency Maintenance Document</label>
		<shortLabel>CGAgencyMaintDoc</shortLabel>
		
		<summary>Contracts And Grants Agency Maintenance Document</summary>
		<description>Document used to create or update Contracts and Grants Agency objects</description>
        
        <lockingKeys>
			<field attributeName="agencyNumber" />
		</lockingKeys>        

	    <maintainableSections>
	        <maintainableSection title="Edit Contracts and Grants Agency">  
	        	<maintainableItems>         					
					<maintainableField name="agencyNumber" required="true" />
					<maintainableField name="reportingName" required="true"/>
					<maintainableField name="fullName" required="true"/>
					<maintainableField name="agencyTypeCode" required="true" />
					<maintainableField name="historicalIndicator" />
					<maintainableField name="inStateIndicator" />
					<maintainableField name="indirectAmount" />
					<maintainableField name="reportsToAgency.agencyNumber" />
					<maintainableField name="versionNumber" />
				</maintainableItems>
	        </maintainableSection>
	    </maintainableSections>
	</maintenanceDocument>
</dictionaryEntry>
