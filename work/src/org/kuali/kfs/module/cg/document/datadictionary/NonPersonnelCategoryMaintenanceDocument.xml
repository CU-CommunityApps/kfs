<?xml version="1.0" encoding="UTF-8"?><beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p" xsi:schemaLocation="http://www.springframework.org/schema/beans         http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="NonPersonnelCategoryMaintenanceDocument" parent="NonPersonnelCategoryMaintenanceDocument-parentBean"/>

  <bean id="NonPersonnelCategoryMaintenanceDocument-parentBean" abstract="true" parent="MaintenanceDocumentEntry">
    <property name="businessObjectClass" value="org.kuali.kfs.module.cg.businessobject.NonPersonnelCategory"/>
    <property name="maintainableClass" value="org.kuali.rice.kns.maintenance.KualiMaintainableImpl"/>
    <property name="maintainableSections">
      <list>
        <ref bean="NonPersonnelCategoryMaintenanceDocument-EditNonpersonnelCode"/>
      </list>
    </property>
    <property name="lockingKeys">
      <list>
        <value>code</value>
      </list>
    </property>
    
    
    
    
    
    <property name="documentTypeName" value="NPC"/>
    <property name="documentAuthorizerClass" value="org.kuali.kfs.sys.document.authorization.FinancialSystemMaintenanceDocumentAuthorizerBase"/>
    <property name="workflowAttributes">
      <ref bean="NonPersonnelCategoryMaintenanceDocument-workflowAttributes"/>
    </property>
  </bean>

<!-- Maintenance Section Definitions -->


  <bean id="NonPersonnelCategoryMaintenanceDocument-EditNonpersonnelCode" parent="NonPersonnelCategoryMaintenanceDocument-EditNonpersonnelCode-parentBean"/>

  <bean id="NonPersonnelCategoryMaintenanceDocument-EditNonpersonnelCode-parentBean" abstract="true" parent="MaintainableSectionDefinition">
    <property name="id" value="Edit Nonpersonnel Code"/>
    <property name="title" value="Edit Nonpersonnel Code"/>
    <property name="maintainableItems">
      <list>
        <bean parent="MaintainableFieldDefinition" p:name="code" p:required="true"/>
        <bean parent="MaintainableFieldDefinition" p:name="name" p:required="true"/>
        <bean parent="MaintainableFieldDefinition" p:name="sortNumber"/>
        <bean parent="MaintainableFieldDefinition" p:name="active" p:defaultValue="true"/>
      </list>
    </property>
  </bean>

	<!-- workflow attributes -->
  <bean id="NonPersonnelCategoryMaintenanceDocument-workflowAttributes" parent="NonPersonnelCategoryMaintenanceDocument-workflowAttributes-parentBean"/>
  
  <bean id="NonPersonnelCategoryMaintenanceDocument-workflowAttributes-parentBean" class="org.kuali.rice.kns.datadictionary.WorkflowAttributes" abstract="true"/>

</beans>