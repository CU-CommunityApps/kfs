<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="GraduateAssistantRateMaintenanceDocument" parent="GraduateAssistantRateMaintenanceDocument-parentBean" />

  <bean id="GraduateAssistantRateMaintenanceDocument-parentBean" abstract="true" parent="MaintenanceDocumentEntry">
    <property name="businessObjectClass" value="org.kuali.module.kra.budget.bo.GraduateAssistantRate" />
    <property name="maintainableClass" value="org.kuali.core.maintenance.KualiMaintainableImpl" />
    <property name="maintainableSections" >
      <list>
        <ref bean="GraduateAssistantRateMaintenanceDocument-EditGraduateAssistantRate" />
      </list>
    </property>
    <property name="defaultExistenceChecks" >
      <list>
        <bean parent="ReferenceDefinition"
              p:attributeName="campus"
              p:attributeToHighlightOnFail="campusCode" />
      </list>
    </property>
    <property name="lockingKeys" >
      <list>
        <value>campusCode</value>
      </list>
    </property>
    <property name="label" value="Graduate Assistant Rate" />
    <property name="shortLabel" value="Graduate Assistant Rate" />
    <property name="summary" value="Graduate Assistant Rate maintenance document" />
    <property name="description" value="Document used to create or update Graduate Assistant Rate objects" />
    <property name="helpDefinition" >
      <bean parent="HelpDefinition"
            p:parameterNamespace="KFS-RA"
            p:parameterName="GRADUATE_ASSISTANT_RATE"
            p:parameterDetailType="GraduateAssistantRate" />
    </property>
    <property name="documentTypeName" value="GraduateAssistantRateMaintenanceDocument" />
    <property name="documentAuthorizerClass" value="org.kuali.core.document.authorization.MaintenanceDocumentAuthorizerBase" />
    <property name="documentTypeCode" value="GAR" />
    <property name="workflowProperties" >
      <ref bean="GraduateAssistantRateMaintenanceDocument-workflowProperties" />
    </property>
    <property name="authorizations" >
      <list>
        <bean parent="AuthorizationDefinition">
          <property name="action" value="initiate" />
          <property name="authorizedGroups" >
            <set>
              <value>RA_PRE_AWARD_MAINTENANCE_USERS</value>
            </set>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Maintenance Section Definitions -->


  <bean id="GraduateAssistantRateMaintenanceDocument-EditGraduateAssistantRate" parent="GraduateAssistantRateMaintenanceDocument-EditGraduateAssistantRate-parentBean" />

  <bean id="GraduateAssistantRateMaintenanceDocument-EditGraduateAssistantRate-parentBean" abstract="true" parent="MaintainableSectionDefinition">
    <property name="id" value="Edit Graduate Assistant Rate" />
    <property name="title" value="Edit Graduate Assistant Rate" />
    <property name="maintainableItems" >
      <list>
        <bean parent="MaintainableFieldDefinition"
              p:name="campusCode"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="campusMaximumPeriod1Rate"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="campusMaximumPeriod2Rate"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="campusMaximumPeriod3Rate" />
        <bean parent="MaintainableFieldDefinition"
              p:name="campusMaximumPeriod4Rate" />
        <bean parent="MaintainableFieldDefinition"
              p:name="campusMaximumPeriod5Rate" />
        <bean parent="MaintainableFieldDefinition"
              p:name="campusMaximumPeriod6Rate" />
        <bean parent="MaintainableFieldDefinition"
              p:name="active"
              p:defaultValue="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="versionNumber" />
      </list>
    </property>
  </bean>

<!-- Exported Workflow Properties -->


  <bean id="GraduateAssistantRateMaintenanceDocument-workflowProperties" parent="GraduateAssistantRateMaintenanceDocument-workflowProperties-parentBean" />

  <bean id="GraduateAssistantRateMaintenanceDocument-workflowProperties-parentBean" abstract="true" parent="WorkflowProperties">
  </bean>
</beans>
