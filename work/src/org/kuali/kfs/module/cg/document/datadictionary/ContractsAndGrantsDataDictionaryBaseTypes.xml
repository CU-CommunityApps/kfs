<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd"
>
	<bean id="WorkflowAttributeMetaData-researchRiskCode" abstract="true">
    	<property name="businessObjectClassName" value="org.kuali.kfs.module.cg.businessobject.ProposalResearchRisk" />
    	<property name="attributeName" value="researchRiskTypeCode" />
    </bean>
    <bean id="RoutingAttribute-researchRiskCode" parent="WorkflowAttributeMetaData-researchRiskCode" class="org.kuali.rice.kns.datadictionary.RoutingAttribute" />

	<bean id="DocumentValuePathGroup-ContractsAndGrantsProjectDirector-principalId" class="org.kuali.rice.kns.datadictionary.DocumentValuePathGroup">
   		<property name="documentValues">
	   		<list>
	   			<value>projectDirector.principalId</value>
	   		</list>
   		</property>
   	</bean>
</beans>