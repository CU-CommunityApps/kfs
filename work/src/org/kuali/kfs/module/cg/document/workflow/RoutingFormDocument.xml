<?xml version='1.0' encoding='UTF-8'?> 
<data xmlns="ns:workflow" xmlns:fo="http://www.w3.org/1999/XSL/Format" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="ns:workflow resource:WorkflowData">
  <documentTypes xmlns="ns:workflow/DocumentType" xsi:schemaLocation="ns:workflow/DocumentType resource:DocumentType">
    <documentType>
      <name>KRFD</name>
      <parent>RA</parent>
      <docHandler>${application.url}/researchRoutingFormMainPage.do?methodToCall=docHandler</docHandler>
      <routePaths>
        <routePath>
          <start name="AdHoc" nextNode="ResearchAdHoc"/>
          <requests name="ResearchAdHoc" nextNode="ProjectManagement"/>
          <requests name="ProjectManagement" nextNode="OrganizationHierarchy"/>
          <requests name="OrganizationHierarchy" nextNode="ResearchRisk"/>
          <requests name="ResearchRisk" nextNode="QuestionType"/>
          <requests name="QuestionType"/>
        </routePath>
      </routePaths>
      <routeNodes>
        <start name="AdHoc"/>
        <role name="ResearchAdHoc">
          <activationType>P</activationType>
          <qualifierResolverClass>org.kuali.rice.kns.workflow.attribute.DataDictionaryQualifierResolver</qualifierResolverClass>
        </role>
        <role name="ProjectManagement">
          <qualifierResolverClass>org.kuali.rice.kns.workflow.attribute.DataDictionaryQualifierResolver</qualifierResolverClass>
          <activationType>P</activationType>
        </role>
        <role name="OrganizationHierarchy">
          <activationType>P</activationType>
          <qualifierResolverClass>org.kuali.rice.kns.workflow.attribute.DataDictionaryQualifierResolver</qualifierResolverClass>
        </role>
        <role name="ResearchRisk">
          <qualifierResolverClass>org.kuali.rice.kns.workflow.attribute.DataDictionaryQualifierResolver</qualifierResolverClass>
          <activationType>P</activationType>
        </role>
        <role name="QuestionType">
          <qualifierResolverClass>org.kuali.rice.kns.workflow.attribute.DataDictionaryQualifierResolver</qualifierResolverClass>
          <activationType>P</activationType>
        </role>
      </routeNodes>
    </documentType>
  </documentTypes>
</data>