<?xml version="1.0" encoding="UTF-8"?><beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p" xsi:schemaLocation="http://www.springframework.org/schema/beans         http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="NonPersonnelObjectCodeMaintenanceDocument" parent="NonPersonnelObjectCodeMaintenanceDocument-parentBean"/>

  <bean id="NonPersonnelObjectCodeMaintenanceDocument-parentBean" abstract="true" parent="MaintenanceDocumentEntry">
    <property name="businessObjectClass" value="org.kuali.kfs.module.cg.businessobject.NonPersonnelObjectCode"/>
    <property name="maintainableClass" value="org.kuali.rice.kns.maintenance.KualiMaintainableImpl"/>
    <property name="maintainableSections">
      <list>
        <ref bean="NonPersonnelObjectCodeMaintenanceDocument-EditNonpersonnelObjectCode"/>
      </list>
    </property>
    <property name="defaultExistenceChecks">
      <list>
        <bean parent="ReferenceDefinition" p:attributeName="nonpersonnelCategory" p:attributeToHighlightOnFail="budgetNonpersonnelCategoryCode"/>
        <bean parent="ReferenceDefinition" p:attributeName="nonpersonnelSubCategory" p:attributeToHighlightOnFail="budgetNonpersonnelSubCategoryCode"/>
      </list>
    </property>
    <property name="lockingKeys">
      <list>
        <value>budgetNonpersonnelCategoryCode</value>
        <value>budgetNonpersonnelSubCategoryCode</value>
      </list>
    </property>
    <property name="label" value="Non-Personnel Object Code"/>
    <property name="shortLabel" value="Non-Personnel Object Code"/>
    <property name="summary" value="Object Code maintenance document"/>
    <property name="description" value="Document used to create or update Object Code objects"/>
    <property name="helpDefinition">
      <bean parent="HelpDefinition" p:parameterClass="org.kuali.kfs.module.cg.businessobject.NonPersonnelObjectCode" p:parameterName="NONPERSONNEL_OBJECT_CODE"/>
    </property>
    <property name="documentTypeName" value="NPOC"/>
    <property name="documentAuthorizerClass" value="org.kuali.kfs.sys.document.authorization.FinancialSystemMaintenanceDocumentAuthorizerBase"/>
    <property name="workflowAttributes">
      <ref bean="NonPersonnelObjectCodeMaintenanceDocument-workflowAttributes"/>
    </property>
  </bean>

<!-- Maintenance Section Definitions -->


  <bean id="NonPersonnelObjectCodeMaintenanceDocument-EditNonpersonnelObjectCode" parent="NonPersonnelObjectCodeMaintenanceDocument-EditNonpersonnelObjectCode-parentBean"/>

  <bean id="NonPersonnelObjectCodeMaintenanceDocument-EditNonpersonnelObjectCode-parentBean" abstract="true" parent="MaintainableSectionDefinition">
    <property name="id" value="Edit Nonpersonnel Object Code"/>
    <property name="title" value="Edit Nonpersonnel Object Code"/>
    <property name="maintainableItems">
      <list>
        <bean parent="MaintainableFieldDefinition" p:name="budgetNonpersonnelCategoryCode" p:required="true"/>
        <bean parent="MaintainableFieldDefinition" p:name="budgetNonpersonnelSubCategoryCode" p:required="true"/>
        <bean parent="MaintainableFieldDefinition" p:name="budgetNonpersonnelObjectCode" p:required="true"/>
        <bean parent="MaintainableFieldDefinition" p:name="active" p:defaultValue="true"/>
      </list>
    </property>
  </bean>

<!-- Exported Workflow Properties -->


  <bean id="NonPersonnelObjectCodeMaintenanceDocument-workflowAttributes" parent="NonPersonnelObjectCodeMaintenanceDocument-workflowAttributes-parentBean"/>

  <bean id="NonPersonnelObjectCodeMaintenanceDocument-workflowAttributes-parentBean" abstract="true" parent="WorkflowAttributes"/>
</beans>