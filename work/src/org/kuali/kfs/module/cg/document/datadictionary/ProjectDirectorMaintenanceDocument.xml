<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="ProjectDirectorMaintenanceDocument" parent="ProjectDirectorMaintenanceDocument-parentBean" />

  <bean id="ProjectDirectorMaintenanceDocument-parentBean" abstract="true" parent="MaintenanceDocumentEntry">
    <property name="businessObjectClass" value="org.kuali.kfs.module.cg.businessobject.ProjectDirector" />
    <property name="maintainableClass" value="org.kuali.rice.kns.maintenance.KualiMaintainableImpl" />
    <property name="maintainableSections" >
      <list>
        <ref bean="ProjectDirectorMaintenanceDocument-EditProjectDirector" />
      </list>
    </property>
    <property name="lockingKeys" >
      <list>
        <value>person.principalName</value>
      </list>
    </property>
    <property name="label" value="Project Director" />
    <property name="shortLabel" value="Project Director" />
    <property name="summary" value="Project Director Maintenance Document" />
    <property name="description" value="Document used to create or update Project Director objects" />
    <property name="helpDefinition" >
      <bean parent="HelpDefinition" p:parameterClass="org.kuali.kfs.module.cg.businessobject.ProjectDirector" p:parameterName="PROJECT_DIRECTOR" />
    </property>
    <property name="documentTypeName" value="ProjectDirectorMaintenanceDocument" />
    <property name="documentAuthorizerClass" value="org.kuali.kfs.sys.document.authorization.FinancialSystemMaintenanceDocumentAuthorizerBase" />
    <property name="documentTypeCode" value="ADPD" />
    <property name="workflowProperties" >
      <ref bean="ProjectDirectorMaintenanceDocument-workflowProperties" />
    </property>
    <property name="authorizations" >
      <list>
        <bean parent="AuthorizationDefinition">
          <property name="action" value="initiate" />
          <property name="authorizedGroups" >
            <set>
              <value>SY_MAINTENANCE_USERS</value>
            </set>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Maintenance Section Definitions -->


  <bean id="ProjectDirectorMaintenanceDocument-EditProjectDirector" parent="ProjectDirectorMaintenanceDocument-EditProjectDirector-parentBean" />

  <bean id="ProjectDirectorMaintenanceDocument-EditProjectDirector-parentBean" abstract="true" parent="MaintainableSectionDefinition">
    <property name="id" value="Edit Project Director" />
    <property name="title" value="Edit Project Director" />
    <property name="maintainableItems" >
      <list>
        <bean parent="MaintainableFieldDefinition"
              p:name="person.principalName"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="active" />
      </list>
    </property>
  </bean>

<!-- Exported Workflow Properties -->


  <bean id="ProjectDirectorMaintenanceDocument-workflowProperties" parent="ProjectDirectorMaintenanceDocument-workflowProperties-parentBean" />

  <bean id="ProjectDirectorMaintenanceDocument-workflowProperties-parentBean" abstract="true" parent="WorkflowProperties">
  </bean>
</beans>

