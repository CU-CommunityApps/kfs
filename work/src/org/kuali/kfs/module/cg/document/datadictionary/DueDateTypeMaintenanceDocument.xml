<?xml version="1.0" encoding="UTF-8"?><beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p" xmlns:dd="http://rice.kuali.org/dd" xsi:schemaLocation="http://www.springframework.org/schema/beans         http://www.springframework.org/schema/beans/spring-beans-2.0.xsd         http://rice.kuali.org/dd         http://rice.kuali.org/dd/dd.xsd">

  <bean id="DueDateTypeMaintenanceDocument" parent="DueDateTypeMaintenanceDocument-parentBean"/>

  <bean id="DueDateTypeMaintenanceDocument-parentBean" abstract="true" parent="MaintenanceDocumentEntry">
    <property name="businessObjectClass" value="org.kuali.kfs.module.cg.businessobject.DueDateType"/>
    <property name="maintainableClass" value="org.kuali.kfs.sys.document.FinancialSystemMaintainable"/>
    <property name="maintainableSections">
      <list>
        <ref bean="DueDateTypeMaintenanceDocument-EditDueDateType"/>
      </list>
    </property>
    <property name="lockingKeys">
      <list>
        <value>dueDateTypeCode</value>
      </list>
    </property>
    
    
    
    
    
    <property name="documentTypeName" value="DDT"/>
    <property name="documentAuthorizerClass" value="org.kuali.kfs.sys.document.authorization.FinancialSystemMaintenanceDocumentAuthorizerBase"/>
    <property name="workflowAttributes">
    	<ref bean="DueDateTypeMaintenanceDocument-workflowAttributes"/>
    </property>
  </bean>

<!-- Maintenance Section Definitions -->

  <bean id="DueDateTypeMaintenanceDocument-EditDueDateType" parent="DueDateTypeMaintenanceDocument-EditDueDateType-parentBean"/>

  <bean id="DueDateTypeMaintenanceDocument-EditDueDateType-parentBean" abstract="true" parent="MaintainableSectionDefinition">
    <property name="id" value="Edit Due Date Type"/>
    <property name="title" value="Edit Due Date Type"/>
    <property name="maintainableItems">
      <list>
        <bean parent="MaintainableFieldDefinition" p:name="dueDateTypeCode" p:required="true"/>
        <bean parent="MaintainableFieldDefinition" p:name="dueDateDescription" p:required="true"/>
        <bean parent="MaintainableFieldDefinition" p:name="active" p:defaultValue="true"/>
      </list>
    </property>
  </bean>
  
    <!-- workflow attributes -->
  <bean id="DueDateTypeMaintenanceDocument-workflowAttributes" parent="DueDateTypeMaintenanceDocument-workflowAttributes-parentBean"/>
  
  <bean id="DueDateTypeMaintenanceDocument-workflowAttributes-parentBean" class="org.kuali.rice.kns.datadictionary.WorkflowAttributes" abstract="true"/>
</beans>