<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="IndirectCostLookupMaintenanceDocument" parent="IndirectCostLookupMaintenanceDocument-parentBean" />

  <bean id="IndirectCostLookupMaintenanceDocument-parentBean" abstract="true" parent="MaintenanceDocumentEntry">
    <property name="businessObjectClass" value="org.kuali.module.kra.budget.bo.IndirectCostLookup" />
    <property name="maintainableClass" value="org.kuali.core.maintenance.KualiMaintainableImpl" />
    <property name="maintainableSections" >
      <list>
        <ref bean="IndirectCostLookupMaintenanceDocument-EditIndirectCostLookup" />
      </list>
    </property>
    <property name="defaultExistenceChecks" >
      <list>
        <bean parent="ReferenceDefinition"
              p:attributeName="budgetPurpose"
              p:attributeToHighlightOnFail="budgetPurposeCode" />
      </list>
    </property>
    <property name="lockingKeys" >
      <list>
        <value>budgetOnCampusIndicator</value>
        <value>budgetPurposeCode</value>
      </list>
    </property>
    <property name="label" value="Indirect Cost Lookup Maintenance Document" />
    <property name="shortLabel" value="IndirectCostLookupMainDoc" />
    <property name="summary" value="Indirect Cost Lookup maintenance document" />
    <property name="description" value="Document used to create or update Indirect Cost Lookup objects" />
    <property name="helpDefinition" >
      <bean parent="HelpDefinition"
            p:parameterNamespace="KFS-RA"
            p:parameterName="INDIRECT_COST_LOOKUP"
            p:parameterDetailType="IndirectCostLookup" />
    </property>
    <property name="documentTypeName" value="IndirectCostLookupMaintenanceDocument" />
    <property name="businessRulesClass" value="org.kuali.module.kra.budget.rules.IndirectCostLookupRule" />
    <property name="documentAuthorizerClass" value="org.kuali.kfs.authorization.FinancialSystemMaintenanceDocumentAuthorizerBase" />
    <property name="documentTypeCode" value="IDCL" />
    <property name="workflowProperties" >
      <ref bean="IndirectCostLookupMaintenanceDocument-workflowProperties" />
    </property>
    <property name="authorizations" >
      <list>
        <bean parent="AuthorizationDefinition">
          <property name="action" value="initiate" />
          <property name="authorizedGroups" >
            <set>
              <value>RA_PRE_AWARD_MAINTENANCE_USERS</value>
            </set>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Maintenance Section Definitions -->


  <bean id="IndirectCostLookupMaintenanceDocument-EditIndirectCostLookup" parent="IndirectCostLookupMaintenanceDocument-EditIndirectCostLookup-parentBean" />

  <bean id="IndirectCostLookupMaintenanceDocument-EditIndirectCostLookup-parentBean" abstract="true" parent="MaintainableSectionDefinition">
    <property name="id" value="Edit Indirect Cost Lookup" />
    <property name="title" value="Edit Indirect Cost Lookup" />
    <property name="maintainableItems" >
      <list>
        <bean parent="MaintainableFieldDefinition"
              p:name="budgetOnCampusIndicator" />
        <bean parent="MaintainableFieldDefinition"
              p:name="budgetPurposeCode"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="budgetIndirectCostRate"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="active"
              p:defaultValue="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="versionNumber" />
      </list>
    </property>
  </bean>

<!-- Exported Workflow Properties -->


  <bean id="IndirectCostLookupMaintenanceDocument-workflowProperties" parent="IndirectCostLookupMaintenanceDocument-workflowProperties-parentBean" />

  <bean id="IndirectCostLookupMaintenanceDocument-workflowProperties-parentBean" abstract="true" parent="WorkflowProperties">
  </bean>
</beans>
