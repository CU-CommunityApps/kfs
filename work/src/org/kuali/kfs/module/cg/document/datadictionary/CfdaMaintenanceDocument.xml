<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="CfdaMaintenanceDocument" parent="CfdaMaintenanceDocument-parentBean" />

  <bean id="CfdaMaintenanceDocument-parentBean" abstract="true" parent="MaintenanceDocumentEntry">
    <property name="businessObjectClass" value="org.kuali.module.cg.bo.Cfda" />
    <property name="maintainableClass" value="org.kuali.core.maintenance.KualiMaintainableImpl" />
    <property name="maintainableSections" >
      <list>
        <ref bean="CfdaMaintenanceDocument-EditCatalogofFederalDomesticAssistanceReference" />
      </list>
    </property>
    <property name="lockingKeys" >
      <list>
        <value>cfdaNumber</value>
      </list>
    </property>
    <property name="label" value="CFDA Reference" />
    <property name="shortLabel" value="CFDA Reference" />
    <property name="summary" value="Catalog of Federal Domestic Assistance Reference Maintenance Document" />
    <property name="description" value="Document used to create or update Catalog of Federal Domestic Assistance Reference objects" />
    <property name="helpDefinition" >
      <bean parent="HelpDefinition"
            p:parameterNamespace="KFS-CG"
            p:parameterName="CATALOG_OF_FEDERAL_DOMESTIC_ASSISTANCE_REFERENCE"
            p:parameterDetailType="Cfda" />
    </property>
    <property name="documentTypeName" value="CfdaMaintenanceDocument" />
    <property name="documentAuthorizerClass" value="org.kuali.kfs.authorization.FinancialSystemMaintenanceDocumentAuthorizerBase" />
    <property name="documentTypeCode" value="CFDA" />
    <property name="workflowProperties" >
      <ref bean="CfdaMaintenanceDocument-workflowProperties" />
    </property>
    <property name="authorizations" >
      <list>
        <bean parent="AuthorizationDefinition">
          <property name="action" value="initiate" />
          <property name="authorizedGroups" >
            <set>
              <value>kualiUniversalGroup</value>
            </set>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Maintenance Section Definitions -->


  <bean id="CfdaMaintenanceDocument-EditCatalogofFederalDomesticAssistanceReference" parent="CfdaMaintenanceDocument-EditCatalogofFederalDomesticAssistanceReference-parentBean" />

  <bean id="CfdaMaintenanceDocument-EditCatalogofFederalDomesticAssistanceReference-parentBean" abstract="true" parent="MaintainableSectionDefinition">
    <property name="id" value="Edit Catalog of Federal Domestic Assistance Reference" />
    <property name="title" value="Edit Catalog of Federal Domestic Assistance Reference" />
    <property name="maintainableItems" >
      <list>
        <bean parent="MaintainableFieldDefinition"
              p:name="cfdaNumber"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="cfdaProgramTitleName"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="cfdaMaintenanceTypeId"
              p:defaultValue="MANUAL"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="cfdaStatusCode"
              p:defaultValue="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="versionNumber" />
      </list>
    </property>
  </bean>

<!-- Exported Workflow Properties -->


  <bean id="CfdaMaintenanceDocument-workflowProperties" parent="CfdaMaintenanceDocument-workflowProperties-parentBean" />

  <bean id="CfdaMaintenanceDocument-workflowProperties-parentBean" abstract="true" parent="WorkflowProperties">
  </bean>
</beans>
