<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="AwardStatusMaintenanceDocument" parent="AwardStatusMaintenanceDocument-parentBean" />

  <bean id="AwardStatusMaintenanceDocument-parentBean" abstract="true" parent="MaintenanceDocumentEntry">
    <property name="businessObjectClass" value="org.kuali.kfs.module.cg.businessobject.AwardStatus" />
    <property name="maintainableClass" value="org.kuali.core.maintenance.KualiMaintainableImpl" />
    <property name="maintainableSections" >
      <list>
        <ref bean="AwardStatusMaintenanceDocument-EditAwardStatus" />
      </list>
    </property>
    <property name="lockingKeys" >
      <list>
        <value>awardStatusCode</value>
      </list>
    </property>
    <property name="label" value="Award Status" />
    <property name="shortLabel" value="Award Status" />
    <property name="summary" value="Award Maintenance Document" />
    <property name="description" value="Document used to create or update Award Status objects" />
    <property name="helpDefinition" >
      <bean parent="HelpDefinition" p:parameterClass="org.kuali.kfs.module.cg.businessobject.AwardStatus" p:parameterName="AWARD_STATUS" />
    </property>
    <property name="documentTypeName" value="AwardStatusMaintenanceDocument" />
    <property name="documentAuthorizerClass" value="org.kuali.kfs.sys.document.authorization.FinancialSystemMaintenanceDocumentAuthorizerBase" />
    <property name="documentTypeCode" value="AWDS" />
    <property name="workflowProperties" >
      <ref bean="AwardStatusMaintenanceDocument-workflowProperties" />
    </property>
    <property name="authorizations" >
      <list>
        <bean parent="AuthorizationDefinition">
          <property name="action" value="initiate" />
          <property name="authorizedGroups" >
            <set>
              <value>kualiUniversalGroup</value>
            </set>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Maintenance Section Definitions -->


  <bean id="AwardStatusMaintenanceDocument-EditAwardStatus" parent="AwardStatusMaintenanceDocument-EditAwardStatus-parentBean" />

  <bean id="AwardStatusMaintenanceDocument-EditAwardStatus-parentBean" abstract="true" parent="MaintainableSectionDefinition">
    <property name="id" value="Edit Award Status" />
    <property name="title" value="Edit Award Status" />
    <property name="maintainableItems" >
      <list>
        <bean parent="MaintainableFieldDefinition"
              p:name="awardStatusCode"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="awardStatusDescription"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="rowActiveIndicator"
              p:defaultValue="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="versionNumber" />
      </list>
    </property>
  </bean>

<!-- Exported Workflow Properties -->


  <bean id="AwardStatusMaintenanceDocument-workflowProperties" parent="AwardStatusMaintenanceDocument-workflowProperties-parentBean" />

  <bean id="AwardStatusMaintenanceDocument-workflowProperties-parentBean" abstract="true" parent="WorkflowProperties">
  </bean>
</beans>
