<?xml version='1.0' encoding='UTF-8'?>
<data xmlns="ns:workflow" xmlns:fo="http://www.w3.org/1999/XSL/Format" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="ns:workflow resource:WorkflowData">
  <documentTypes xmlns="ns:workflow/DocumentType" xsi:schemaLocation="ns:workflow/DocumentType resource:DocumentType">
    <documentType>
      <name>KBD</name>
      <parent>ResearchTransactionalDocument</parent>
      <docHandler>${application.url}/researchBudgetParameters.do?methodToCall=docHandler</docHandler>
      <routePaths>
        <routePath>
          <start name="AdHoc" nextNode="ProjectManagement"/>
          <requests name="ProjectManagement" nextNode="OrganizationHierarchy"/>
          <requests name="OrganizationHierarchy"/>
        </routePath>
      </routePaths>
      <routeNodes>
        <start name="AdHoc"/>
        <role name="ProjectManagement">
          <qualifierResolverClass>org.kuali.rice.kns.workflow.attribute.DataDictionaryQualifierResolver</qualifierResolverClass>
        </role>
        <role name="OrganizationHierarchy">
		  <activationType>P</activationType>
          <qualifierResolverClass>org.kuali.rice.kns.workflow.attribute.DataDictionaryQualifierResolver</qualifierResolverClass>
        </role>
      </routeNodes>
    </documentType>
  </documentTypes>
</data>