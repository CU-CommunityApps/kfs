	<bean id="ProposalAutoCreateDefaultsMaintenanceDocument"
		parent="ProposalAutoCreateDefaultsMaintenanceDocument-parentBean" />

	<bean id="ProposalAutoCreateDefaultsMaintenanceDocument-parentBean"
		abstract="true" parent="MaintenanceDocumentEntry"
		p:businessObjectClass="org.kuali.kfs.module.cg.businessobject.ProposalAutoCreateDefaults"
		p:maintainableClass="org.kuali.kfs.sys.document.FinancialSystemMaintainable">
		<property name="maintainableSections">
			<list>
				<ref
					bean="ProposalAutoCreateDefaultsMaintenanceDocument-ProposalMaintenance" />
			</list>
		</property>
		<property name="defaultExistenceChecks">
			<list>
				<bean parent="ReferenceDefinition" p:attributeName="organization"
					p:attributeToHighlightOnFail="organizationCode" />
			</list>
		</property>
		<property name="lockingKeys">
			<list>
				<value>kcUnit</value>
			</list>
		</property>
		<property name="documentTypeName" value="PRPSLACD" />
		<property name="businessRulesClass"
			value="org.kuali.kfs.module.cg.document.validation.impl.ProposalAutoCreateDefaultsRule" />
		<property name="documentAuthorizerClass"
			value="org.kuali.kfs.sys.document.authorization.FinancialSystemMaintenanceDocumentAuthorizerBase" />
		<property name="webScriptFiles">
			<list>
				<value>../scripts/module/cg/proposalDocument.js</value>
				<value>../dwr/interface/OrganizationService.js</value>
			</list>
		</property>
		<property name="workflowAttributes"
			ref="ProposalAutoCreateDefaultsMaintenanceDocument-workflowAttributes" />

	</bean>

	<!-- Maintenance Section Definitions -->
	<bean
		id="ProposalAutoCreateDefaultsMaintenanceDocument-ProposalMaintenance"
		parent="ProposalAutoCreateDefaultsMaintenanceDocument-ProposalMaintenance-parentBean" />

	<bean
		id="ProposalAutoCreateDefaultsMaintenanceDocument-ProposalMaintenance-parentBean"
		abstract="true" parent="MaintainableSectionDefinition">
		<property name="title" value="ProposalAutoCreateDefault Maintenance" />
		<property name="maintainableItems">
			<list>
				<bean parent="MaintainableFieldDefinition" p:name="kcUnit"
					p:required="true" />
				<bean parent="MaintainableFieldDefinition" p:name="kcUnitName"
					p:unconditionallyReadOnly="true" />
				<bean parent="MaintainableFieldDefinition" p:name="proposalIndirectCostAmount"
					p:required="true" />
				<bean parent="MaintainableFieldDefinition" p:name="proposalAwardTypeCode"
					p:required="true" />
				<bean parent="MaintainableFieldDefinition" p:name="proposalPurposeCode"
					p:required="true" />
				<bean parent="MaintainableFieldDefinition" p:name="chartOfAccountsCode"
					p:required="true" p:webUILeaveFieldFunction="organizationNameLookup" />
				<bean parent="MaintainableFieldDefinition" p:name="organizationCode"
					p:required="true" p:webUILeaveFieldFunction="organizationNameLookup" />
				<bean parent="MaintainableFieldDefinition" p:name="organization.organizationName"
					p:unconditionallyReadOnly="true" />
				<bean parent="MaintainableFieldDefinition" p:name="principalId"
					p:unconditionallyReadOnly="true" />
				<bean parent="MaintainableFieldDefinition" p:name="projectDirector.principalName"
					p:required="true" p:webUILeaveFieldFunction="proposalDirectorIDLookup" />
				<bean parent="MaintainableFieldDefinition" p:name="projectDirector.name"
					p:unconditionallyReadOnly="true" />
				<bean parent="MaintainableFieldDefinition" p:name="active"
					p:defaultValue="true" />
			</list>
		</property>
	</bean>

	<!-- workflow attributes -->
	<bean
		id="ProposalAutoCreateDefaultsMaintenanceDocument-workflowAttributes"
		parent="ProposalAutoCreateDefaultsMaintenanceDocument-workflowAttributes-parentBean" />
	<bean
		id="ProposalAutoCreateDefaultsMaintenanceDocument-workflowAttributes-parentBean"
		abstract="true" parent="WorkflowAttributes">
	</bean>
</beans>
