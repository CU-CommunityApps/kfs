	        	<maintainableItems>
					<maintainableField name="number" required="true"/>
					<maintainableField name="name" required="true"/>
					<maintainableField name="addressLine1" required="true"/>
					<maintainableField name="addressLine2" />
					<maintainableField name="city" required="true"/>
					<maintainableField name="stateCode" required="true"/>
					<maintainableField name="zipCode" required="true"/>
