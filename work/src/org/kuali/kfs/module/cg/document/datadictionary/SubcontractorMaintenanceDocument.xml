<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "../../../../core/datadictionary/dataDictionary-1_0.dtd">

<dictionaryEntry>
    <maintenanceDocument>
        <businessObjectClass>org.kuali.module.cg.bo.Subcontractor</businessObjectClass>
        <maintainableClass>org.kuali.core.maintenance.KualiMaintainableImpl</maintainableClass>

        <documentAuthorizerClass>org.kuali.core.document.MaintenanceDocumentAuthorizerBase</documentAuthorizerClass>
        <authorizations>
            <authorization action="initiate">
                <workgroups>
                    <workgroup>kualiUniversalGroup</workgroup>
                </workgroups>
            </authorization>
        </authorizations>

        <documentTypeName>KualiContractsAndGrantsSubcontractorMaintenanceDocument</documentTypeName>
        <documentTypeCode>SUBC</documentTypeCode>

        <label>Subcontractor Maintenance Document</label>
        <shortLabel>SubcontractorMainDoc</shortLabel>

        <summary>Subcontractor Maintenance Document</summary>
        <description>Document used to create or update Subcontractor objects</description>

        <lockingKeys>
            <field attributeName="subcontractorNumber" />
        </lockingKeys>

        <maintainableSections>
            <maintainableSection title="Edit Subcontractor">
                <maintainableItems>
                    <maintainableField name="subcontractorNumber" required="true" />
                    <maintainableField name="subcontractorName" required="true" />
                    <maintainableField name="subcontractorAddressLine1" required="true" />
                    <maintainableField name="subcontractorAddressLine2" />
                    <maintainableField name="subcontractorCity" required="true" />
                    <maintainableField name="subcontractorStateCode" required="true" />
                    <maintainableField name="subcontractorZipCode" required="true" />
                    <maintainableField name="subcontractorCountryCode" required="true" />
                    <maintainableField name="versionNumber" />
                </maintainableItems>
            </maintainableSection>
        </maintainableSections>
    </maintenanceDocument>
</dictionaryEntry>
