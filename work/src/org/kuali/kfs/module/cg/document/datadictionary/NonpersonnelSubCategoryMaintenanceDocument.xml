<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="NonpersonnelSubCategoryMaintenanceDocument" parent="NonpersonnelSubCategoryMaintenanceDocument-parentBean" />

  <bean id="NonpersonnelSubCategoryMaintenanceDocument-parentBean" abstract="true" parent="MaintenanceDocumentEntry">
    <property name="businessObjectClass" value="org.kuali.module.kra.budget.bo.NonpersonnelSubCategory" />
    <property name="maintainableClass" value="org.kuali.core.maintenance.KualiMaintainableImpl" />
    <property name="maintainableSections" >
      <list>
        <ref bean="NonpersonnelSubCategoryMaintenanceDocument-EditNonpersonnelSubCategory" />
      </list>
    </property>
    <property name="lockingKeys" >
      <list>
        <value>code</value>
      </list>
    </property>
    <property name="label" value="Non-Personnel Sub-Category" />
    <property name="shortLabel" value="Non-Personnel Sub-Category" />
    <property name="summary" value="Nonpersonnel Sub Category maintenance document" />
    <property name="description" value="Document used to create or update Nonpersonnel Sub Category objects" />
    <property name="helpDefinition" >
      <bean parent="HelpDefinition"
            p:parameterNamespace="KFS-RA"
            p:parameterName="NONPERSONNEL_SUB_CATEGORY"
            p:parameterDetailType="NonpersonnelSubCategory" />
    </property>
    <property name="documentTypeName" value="NonpersonnelSubCategoryMaintenanceDocument" />
    <property name="documentAuthorizerClass" value="org.kuali.core.document.authorization.MaintenanceDocumentAuthorizerBase" />
    <property name="documentTypeCode" value="NPSC" />
    <property name="workflowProperties" >
      <ref bean="NonpersonnelSubCategoryMaintenanceDocument-workflowProperties" />
    </property>
    <property name="authorizations" >
      <list>
        <bean parent="AuthorizationDefinition">
          <property name="action" value="initiate" />
          <property name="authorizedGroups" >
            <set>
              <value>RA_PRE_AWARD_MAINTENANCE_USERS</value>
            </set>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Maintenance Section Definitions -->


  <bean id="NonpersonnelSubCategoryMaintenanceDocument-EditNonpersonnelSubCategory" parent="NonpersonnelSubCategoryMaintenanceDocument-EditNonpersonnelSubCategory-parentBean" />

  <bean id="NonpersonnelSubCategoryMaintenanceDocument-EditNonpersonnelSubCategory-parentBean" abstract="true" parent="MaintainableSectionDefinition">
    <property name="id" value="Edit Nonpersonnel Sub Category" />
    <property name="title" value="Edit Nonpersonnel Sub Category" />
    <property name="maintainableItems" >
      <list>
        <bean parent="MaintainableFieldDefinition"
              p:name="code"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="name"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="nonpersonnelMtdcExcludedIndicator" />
        <bean parent="MaintainableFieldDefinition"
              p:name="nonpersonnelModularExcludedIndicator" />
        <bean parent="MaintainableFieldDefinition"
              p:name="active"
              p:defaultValue="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="versionNumber" />
      </list>
    </property>
  </bean>

<!-- Exported Workflow Properties -->


  <bean id="NonpersonnelSubCategoryMaintenanceDocument-workflowProperties" parent="NonpersonnelSubCategoryMaintenanceDocument-workflowProperties-parentBean" />

  <bean id="NonpersonnelSubCategoryMaintenanceDocument-workflowProperties-parentBean" abstract="true" parent="WorkflowProperties">
  </bean>
</beans>
