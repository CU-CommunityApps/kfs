<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="ProposalAwardTypeMaintenanceDocument" parent="ProposalAwardTypeMaintenanceDocument-parentBean" />

  <bean id="ProposalAwardTypeMaintenanceDocument-parentBean" abstract="true" parent="MaintenanceDocumentEntry">
    <property name="businessObjectClass" value="org.kuali.module.cg.bo.ProposalAwardType" />
    <property name="maintainableClass" value="org.kuali.core.maintenance.KualiMaintainableImpl" />
    <property name="maintainableSections" >
      <list>
        <ref bean="ProposalAwardTypeMaintenanceDocument-EditProposalAwardType" />
      </list>
    </property>
    <property name="lockingKeys" >
      <list>
        <value>proposalAwardTypeCode</value>
      </list>
    </property>
    <property name="label" value="Proposal Type" />
    <property name="shortLabel" value="Proposal Type" />
    <property name="summary" value="Proposal Award Type Maintenance Document" />
    <property name="description" value="Document used to create or update Proposal Award Type objects" />
    <property name="helpDefinition" >
      <bean parent="HelpDefinition"
            p:parameterNamespace="KFS-CG"
            p:parameterName="PROPOSAL_AWARD_TYPE"
            p:parameterDetailType="ProposalAwardType" />
    </property>
    <property name="documentTypeName" value="ProposalAwardTypeMaintenanceDocument" />
    <property name="documentAuthorizerClass" value="org.kuali.kfs.authorization.FinancialSystemMaintenanceDocumentAuthorizerBase" />
    <property name="documentTypeCode" value="PATY" />
    <property name="workflowProperties" >
      <ref bean="ProposalAwardTypeMaintenanceDocument-workflowProperties" />
    </property>
    <property name="authorizations" >
      <list>
        <bean parent="AuthorizationDefinition">
          <property name="action" value="initiate" />
          <property name="authorizedGroups" >
            <set>
              <value>kualiUniversalGroup</value>
            </set>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Maintenance Section Definitions -->


  <bean id="ProposalAwardTypeMaintenanceDocument-EditProposalAwardType" parent="ProposalAwardTypeMaintenanceDocument-EditProposalAwardType-parentBean" />

  <bean id="ProposalAwardTypeMaintenanceDocument-EditProposalAwardType-parentBean" abstract="true" parent="MaintainableSectionDefinition">
    <property name="id" value="Edit Proposal Award Type" />
    <property name="title" value="Edit Proposal Award Type" />
    <property name="maintainableItems" >
      <list>
        <bean parent="MaintainableFieldDefinition"
              p:name="proposalAwardTypeCode"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="proposalAwardTypeDescription"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="rowActiveIndicator"
              p:defaultValue="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="versionNumber" />
      </list>
    </property>
  </bean>

<!-- Exported Workflow Properties -->


  <bean id="ProposalAwardTypeMaintenanceDocument-workflowProperties" parent="ProposalAwardTypeMaintenanceDocument-workflowProperties-parentBean" />

  <bean id="ProposalAwardTypeMaintenanceDocument-workflowProperties-parentBean" abstract="true" parent="WorkflowProperties">
  </bean>
</beans>
