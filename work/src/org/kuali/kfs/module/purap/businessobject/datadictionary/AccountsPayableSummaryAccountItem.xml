<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="AccountsPayableSummaryAccountItem" parent="AccountsPayableSummaryAccountItem-parentBean" />

  <bean id="AccountsPayableSummaryAccountItem-parentBean" abstract="true" parent="BusinessObjectEntry">
  	<property name="objectLabel" value="Accounts Payable Summary Account Item" />
    <property name="businessObjectClass" value="org.kuali.kfs.module.purap.businessobject.AccountsPayableSummaryAccountItem" />   
    <property name="attributes" >
      <list>
        <ref bean="AccountsPayableSummaryAccountItem-accountIdentifier" />
        <ref bean="AccountsPayableSummaryAccountItem-itemLineNumber" />
        <ref bean="AccountsPayableSummaryAccountItem-itemAccountTotalAmount" />
      </list>
    </property>
  </bean>
  <bean id="AccountsPayableSummaryAccountItem-accountIdentifier" parent="AccountsPayableSummaryAccountItem-accountIdentifier-parentBean" />

  <bean id="AccountsPayableSummaryAccountItem-accountIdentifier-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="accountIdentifier" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Account Identifier" />
    <property name="shortLabel" value="Identifier" />
    <property name="maxLength" value="10" />
    <property name="validationPattern" >
      <bean parent="NumericValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="12" />
    </property>
  </bean>
  <bean id="AccountsPayableSummaryAccountItem-itemLineNumber" parent="AccountsPayableSummaryAccountItem-itemLineNumber-parentBean" />

  <bean id="AccountsPayableSummaryAccountItem-itemLineNumber-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="itemLineNumber" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Item Line Number" />
    <property name="shortLabel" value="Line" />
    <property name="maxLength" value="3" />
    <property name="validationPattern" >
      <bean parent="NumericValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="12" />
    </property>
  </bean>
  <bean id="AccountsPayableSummaryAccountItem-itemAccountTotalAmount" parent="AccountsPayableSummaryAccountItem-itemAccountTotalAmount-parentBean" />

  <bean id="AccountsPayableSummaryAccountItem-itemAccountTotalAmount-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="itemAccountTotalAmount" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Item Account Total Amount" />
    <property name="shortLabel" value="Item Amount" />
    <property name="maxLength" value="19" />
    <property name="validationPattern" >
      <bean parent="FloatingPointValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="20" />
    </property>
  </bean>
</beans>