<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "../dataDictionary-1_0.dtd">
<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.module.purap.bo.ReceivingLineItem">
 
        <inquiry>
            <title>Receiving Line Item Inquiry</title>
             
            <inquirySections>
                <inquirySection title="Header Text">
                    <inquiryFields>
                        <field attributeName="documentNumber" />
                        <field attributeName="itemCatalogNumber" />
                        <field attributeName="itemDamagedTotalQuantity" />
                        <field attributeName="itemDescription" />
                        <field attributeName="itemLineNumber" />
                        <field attributeName="itemOrderedQuantity" />
                        <field attributeName="itemReasonAddedCode" />
                        <field attributeName="itemReceivedTotalQuantity" />
                        <field attributeName="itemReturnedTotalQuantity" />
                        <field attributeName="itemTypeCode" />
                        <field attributeName="itemUnitOfMeasureCode" />
                        <field attributeName="purchaseOrderIdentifier" />
                        <field attributeName="receivingLineItemIdentifier" />
                    </inquiryFields>
                </inquirySection>
            </inquirySections>  
        </inquiry>

        <lookup>
            <title>Receiving Line Item Lookup</title>

            <instructions>Look up a Receiving Line Item</instructions>

            <defaultSort sortAscending="true">
                <sortAttributes>
                    <sortAttribute attributeName="receivingLineItemIdentifier" />
                </sortAttributes>
            </defaultSort>

            <lookupFields>
                <lookupField attributeName="documentNumber" required="false" />
                <lookupField attributeName="itemCatalogNumber" required="false" />
                <lookupField attributeName="itemDamagedTotalQuantity" required="false" />
                <lookupField attributeName="itemDescription" required="false" />
                <lookupField attributeName="itemLineNumber" required="false" />
                <lookupField attributeName="itemOrderedQuantity" required="false" />
                <lookupField attributeName="itemReasonAddedCode" required="false" />
                <lookupField attributeName="itemReceivedTotalQuantity" required="false" />
                <lookupField attributeName="itemReturnedTotalQuantity" required="false" />
                <lookupField attributeName="itemTypeCode" required="false" />
                <lookupField attributeName="itemUnitOfMeasureCode" required="false" />
                <lookupField attributeName="purchaseOrderIdentifier" required="false" />
                <lookupField attributeName="receivingLineItemIdentifier" required="false" />
            </lookupFields>

            <resultFields>
                <field attributeName="documentNumber" />
                <field attributeName="itemCatalogNumber" />
                <field attributeName="itemDamagedTotalQuantity" />
                <field attributeName="itemDescription" />
                <field attributeName="itemLineNumber" />
                <field attributeName="itemOrderedQuantity" />
                <field attributeName="itemReasonAddedCode" />
                <field attributeName="itemReceivedTotalQuantity" />
                <field attributeName="itemReturnedTotalQuantity" />
                <field attributeName="itemTypeCode" />
                <field attributeName="itemUnitOfMeasureCode" />
                <field attributeName="purchaseOrderIdentifier" />
                <field attributeName="receivingLineItemIdentifier" />
            </resultFields>
        </lookup>

        <attributes>

            <attribute name="documentNumber" forceUppercase="true">
                <label>Document Number</label>
                <shortLabel>Number</shortLabel>

                <maxLength>14</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="16" />
                </control>

                <summary>documentNumber</summary>
                <description>documentNumber description...</description>
			</attribute>

            <attribute name="itemCatalogNumber" forceUppercase="true">
                <label>Item Catalog Number</label>
                <shortLabel>Catalog #</shortLabel>

                <maxLength>30</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="15" />
                </control>

                <summary>itemCatalogNumber</summary>
                <description>itemCatalogNumber description...</description>
			</attribute>

            <attribute name="itemDamagedTotalQuantity" forceUppercase="true">
                <label>Item Damaged Total Quantity</label>
                <shortLabel>Qty Damaged</shortLabel>

                <maxLength>11</maxLength>
                <validationPattern><floatingPoint /></validationPattern>

                <control>
                    <text size="13" />
                </control>

                <summary>itemDamagedTotalQuantity</summary>
                <description>itemDamagedTotalQuantity description...</description>
			</attribute>

            <attribute name="itemDescription" forceUppercase="true">
                <label>Item Description</label>
                <shortLabel>Description</shortLabel>

                <maxLength>4000</maxLength>
                <validationPattern><anyCharacter allowWhitespace="true"/></validationPattern>

                <control>
                    <textarea rows="1" cols="45"/>
                </control>

                <summary>itemDescription</summary>
                <description>itemDescription description...</description>
			</attribute>

            <attribute name="itemLineNumber" forceUppercase="true">
                <label>Item Line Number</label>
                <shortLabel>Line #</shortLabel>

                <maxLength>3</maxLength>
                <validationPattern><numeric /></validationPattern>

                <control>
                    <text size="5" />
                </control>

                <summary>itemLineNumber</summary>
                <description>itemLineNumber description...</description>
			</attribute>

            <attributeReference name="itemOrderedQuantity" sourceClassName="org.kuali.module.purap.bo.PurApAttributeReferenceDummy" sourceAttributeName="genericItemQuantity">
                <label>Item Ordered Quantity</label>
                <shortLabel>Qty Ordered</shortLabel>
                <summary>itemOrderedQuantity</summary>
                <description>itemOrderedQuantity description...</description>
			</attributeReference>

            <attributeReference name="itemReceivedPriorQuantity" sourceClassName="org.kuali.module.purap.bo.PurApAttributeReferenceDummy" sourceAttributeName="genericItemQuantity">
                <label>Prior Qty Received</label>
                <shortLabel>Prior Qty Received</shortLabel>
                <summary>itemReceivedPriorQuantity</summary>
                <description>itemReceivedPriorQuantity description...</description>
			</attributeReference>

            <attributeReference name="itemReceivedToBeQuantity" sourceClassName="org.kuali.module.purap.bo.PurApAttributeReferenceDummy" sourceAttributeName="genericItemQuantity">
                <label>To be Received</label>
                <shortLabel>To be Received</shortLabel>
                <summary>itemReceivedToBeQuantity</summary>
                <description>itemReceivedToBeQuantity description...</description>
			</attributeReference>

            <attribute name="itemReasonAddedCode" forceUppercase="true">
                <label>Item Reason Added Code</label>
                <shortLabel>Addition Reason</shortLabel>

                <maxLength>4</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

				<control>
					<select valuesFinderClass="org.kuali.module.purap.lookup.keyvalues.ItemReasonAddedValuesFinder" />
				</control>

                <summary>itemReasonAddedCode</summary>
                <description>itemReasonAddedCode description...</description>
			</attribute>

            <attributeReference name="itemReceivedTotalQuantity" sourceClassName="org.kuali.module.purap.bo.PurApAttributeReferenceDummy" sourceAttributeName="genericItemQuantity">
                <label>Item Received Total Quantity</label>
                <shortLabel>Qty Received</shortLabel>
                <summary>itemReceivedTotalQuantity</summary>
                <description>itemReceivedTotalQuantity description...</description>
			</attributeReference>

            <attributeReference name="itemReturnedTotalQuantity" sourceClassName="org.kuali.module.purap.bo.PurApAttributeReferenceDummy" sourceAttributeName="genericItemQuantity">
                <label>Item Returned Total Quantity</label>
                <shortLabel>Qty Returned</shortLabel>
                <summary>itemReturnedTotalQuantity</summary>
                <description>itemReturnedTotalQuantity description...</description>
			</attributeReference>

            <attribute name="itemTypeCode" forceUppercase="true">
                <label>Item Type Code</label>
                <shortLabel>Code</shortLabel>

                <maxLength>4</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="6" />
                </control>

                <summary>itemTypeCode</summary>
                <description>itemTypeCode description...</description>
			</attribute>

            <attribute name="itemUnitOfMeasureCode"
            	forceUppercase="true">
            	<label>Item Unit Of Measure Code</label>
            	<shortLabel>UOM</shortLabel>

            	<maxLength>4</maxLength>
            	<validationPattern>
            		<anyCharacter />
            	</validationPattern>

            	<control>
            		<select
            			valuesFinderClass="org.kuali.kfs.lookup.keyvalues.UnitOfMeasureValuesFinder" />
            	</control>

            </attribute>


            <attribute name="purchaseOrderIdentifier" forceUppercase="true">
                <label>Purchase Order Identifier</label>
                <shortLabel>Identifier</shortLabel>

                <maxLength>9</maxLength>
                <validationPattern><numeric /></validationPattern>

                <control>
                    <text size="11" />
                </control>

                <summary>purchaseOrderIdentifier</summary>
                <description>purchaseOrderIdentifier description...</description>
			</attribute>

            <attribute name="receivingLineItemIdentifier" forceUppercase="true">
                <label>Receiving Line Item Identifier</label>
                <shortLabel>Identifier</shortLabel>

                <maxLength>10</maxLength>
                <validationPattern><numeric /></validationPattern>

                <control>
                    <text size="12" />
                </control>

                <summary>receivingLineItemIdentifier</summary>
                <description>receivingLineItemIdentifier description...</description>
			</attribute>

            <attributeReference name="versionNumber" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="versionNumber" />

        </attributes>
    </businessObject>
</dictionaryEntry>
