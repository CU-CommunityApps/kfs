<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "../dataDictionary-1_0.dtd">
<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.module.purap.bo.RequisitionStatus">
        <customAttributeEnabled>false</customAttributeEnabled>

        <inquiry>
            <title>Requisition Status Inquiry</title>

            <inquiryFields>
                <field attributeName="statusCode" />
                <field attributeName="statusDescription" />
                <field attributeName="dataObjectMaintenanceCodeActiveIndicator" />
            </inquiryFields>
        </inquiry>

        <lookup>
            <title>Requisition Status Lookup</title>

			<menubar><![CDATA[<a href="maintenance.do?methodToCall=start&businessObjectClassName=org.kuali.module.purap.bo.RequisitionStatus">Create New</a>]]></menubar>

            <instructions>Lookup an Requisition Status</instructions>

            <defaultSort sortAscending="true">
                <sortAttributes>
                    <sortAttribute attributeName="statusDescription" />
                </sortAttributes>
            </defaultSort>

            <lookupFields>
                <lookupField attributeName="statusCode" required="false" />
                <lookupField attributeName="statusDescription" required="false" />
                <lookupField attributeName="dataObjectMaintenanceCodeActiveIndicator" required="false" />
            </lookupFields>

            <resultFields>
                <field attributeName="statusCode" />
                <field attributeName="statusDescription" />
                <field attributeName="dataObjectMaintenanceCodeActiveIndicator" />
            </resultFields>
        </lookup>
        
		<attributes>
		
            <attribute name="statusCode" forceUppercase="true">
                <label>Requisition Status Code</label>
                <shortLabel>Code</shortLabel>

                <maxLength>4</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="6" />
                </control>

                <summary>requisitionStatusCode</summary>
                <description>requisitionStatusCode description...</description>
			</attribute>

            <attribute name="statusDescription">
                <label>Requisition Status Description</label>
                <shortLabel>Description</shortLabel>

                <maxLength>45</maxLength>
                <validationPattern><anyCharacter allowWhitespace="true" /></validationPattern>

                <control>
                    <text size="47" />
                </control>

                <summary>requisitionStatusDescription</summary>
                <description>requisitionStatusDescription description...</description>
			</attribute>
			
			<attributeReference name="dataObjectMaintenanceCodeActiveIndicator" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="activeIndicator" />

            <attributeReference name="versionNumber" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="versionNumber" />
			
            
        </attributes>
    </businessObject>
</dictionaryEntry>
