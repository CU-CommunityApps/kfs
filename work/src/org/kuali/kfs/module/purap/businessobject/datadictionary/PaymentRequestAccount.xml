      <bean parent="SelectControlDefinition"
            p:valuesFinderClass="org.kuali.kfs.coa.businessobject.options.SimpleChartValuesFinder"
            p:includeKeyInLabel="false" />
    </property>
    <property name="description" value="The code that identifies the Chart of Accounts for an Account. Kuali accommodates more than one Chart of Accounts." />
  </bean>
  <bean id="PaymentRequestAccount-postingYear" parent="PaymentRequestAccount-postingYear-parentBean" />

  <bean id="PaymentRequestAccount-postingYear-parentBean" abstract="true" parent="GenericAttributes-genericFiscalYear">
    <property name="name" value="postingYear" />
