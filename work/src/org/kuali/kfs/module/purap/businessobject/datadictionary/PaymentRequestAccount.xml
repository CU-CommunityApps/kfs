    </property>
    <property name="summary" value="&amp;nbsp;" />
    <property name="description" value="The system code for that identifies the system from which the original encumbering transaction came, selected from a pulldown list." />
  </bean>
  <bean id="PaymentRequestAccount-overrideCode" parent="PaymentRequestAccount-overrideCode-parentBean" />

  <bean id="PaymentRequestAccount-overrideCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="overrideCode" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Override Code" />
