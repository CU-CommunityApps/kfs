<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "../dataDictionary-1_0.dtd">
<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.module.purap.bo.PaymentRequestAccount">
        <customAttributeEnabled>false</customAttributeEnabled>

        <inquiry>
            <title>Payment Request Account Inquiry</title>

            <inquirySections><inquirySection title="Header Text"><inquiryFields>
                <field attributeName="accountLinePercent" />
                <field attributeName="accountNumber" />
                <field attributeName="chartOfAccountsCode" />
                <field attributeName="disencumberedAmount" />
                <field attributeName="financialObjectCode" />
                <field attributeName="financialSubObjectCode" />
                <field attributeName="itemAccountTotalAmount" />
                <field attributeName="organizationReferenceId" />
                <field attributeName="paymentRequestAccountIdentifier" />
                <field attributeName="paymentRequestItemIdentifier" />
                <field attributeName="projectCode" />
                <field attributeName="subAccountNumber" />
            </inquiryFields></inquirySection></inquirySections>
        </inquiry>

        <lookup>
            <title>Payment Request Account Lookup</title>

            <instructions>Lookup a Payment Request Account</instructions>

            <defaultSort sortAscending="true">
                <sortAttributes>
                    <sortAttribute attributeName="accountLinePercent" />
                </sortAttributes>
            </defaultSort>

            <lookupFields>
                <lookupField attributeName="accountLinePercent" required="false" />
                <lookupField attributeName="accountNumber" required="false" />
                <lookupField attributeName="chartOfAccountsCode" required="false" />
                <lookupField attributeName="disencumberedAmount" required="false" />
                <lookupField attributeName="financialObjectCode" required="false" />
                <lookupField attributeName="financialSubObjectCode" required="false" />
                <lookupField attributeName="itemAccountTotalAmount" required="false" />
                <lookupField attributeName="organizationReferenceId" required="false" />
                <lookupField attributeName="paymentRequestAccountIdentifier" required="false" />
                <lookupField attributeName="paymentRequestItemIdentifier" required="false" />
                <lookupField attributeName="projectCode" required="false" />
                <lookupField attributeName="subAccountNumber" required="false" />
            </lookupFields>

            <resultFields>
                <field attributeName="accountLinePercent" />
                <field attributeName="accountNumber" />
                <field attributeName="chartOfAccountsCode" />
                <field attributeName="disencumberedAmount" />
                <field attributeName="financialObjectCode" />
                <field attributeName="financialSubObjectCode" />
                <field attributeName="itemAccountTotalAmount" />
                <field attributeName="organizationReferenceId" />
                <field attributeName="paymentRequestAccountIdentifier" />
                <field attributeName="paymentRequestItemIdentifier" />
                <field attributeName="projectCode" />
                <field attributeName="subAccountNumber" />
            </resultFields>
        </lookup>

        <attributes>

            <attribute name="accountLinePercent" forceUppercase="true">
                <label>Account Line Percent</label>
                <shortLabel>Percent</shortLabel>

                <maxLength>7</maxLength>
                <validationPattern><floatingPoint /></validationPattern>

                <control>
                    <text size="9" />
                </control>

                <summary>accountLinePercent</summary>
                <description>accountLinePercent description...</description>
			</attribute>

            <attribute name="accountNumber" forceUppercase="true">
                <label>Account Number</label>
                <shortLabel>Number</shortLabel>

                <maxLength>7</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="9" />
                </control>

                <summary>accountNumber</summary>
                <description>accountNumber description...</description>
			</attribute>

            <attribute name="chartOfAccountsCode" forceUppercase="true">
                <label>Chart Of Accounts Code</label>
                <shortLabel>Code</shortLabel>

                <maxLength>2</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="4" />
                </control>

                <summary>chartOfAccountsCode</summary>
                <description>chartOfAccountsCode description...</description>
			</attribute>

            <attribute name="disencumberedAmount" forceUppercase="true">
                <label>Disencumbered Amount</label>
                <shortLabel>Amount</shortLabel>

                <maxLength>19</maxLength>
                <validationPattern><floatingPoint /></validationPattern>

                <control>
                    <text size="21" />
                </control>

                <summary>disencumberedAmount</summary>
                <description>disencumberedAmount description...</description>
			</attribute>

            <attribute name="financialObjectCode" forceUppercase="true">
                <label>Financial Object Code</label>
                <shortLabel>Code</shortLabel>

                <maxLength>4</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="6" />
                </control>

                <summary>financialObjectCode</summary>
                <description>financialObjectCode description...</description>
			</attribute>

            <attribute name="financialSubObjectCode" forceUppercase="true">
                <label>Financial Sub Object Code</label>
                <shortLabel>Code</shortLabel>

                <maxLength>3</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="5" />
                </control>

                <summary>financialSubObjectCode</summary>
                <description>financialSubObjectCode description...</description>
			</attribute>

            <attribute name="itemAccountTotalAmount" forceUppercase="true">
                <label>Item Account Total Amount</label>
                <shortLabel>Amount</shortLabel>

                <maxLength>19</maxLength>
                <validationPattern><floatingPoint /></validationPattern>

                <control>
                    <text size="21" />
                </control>

                <summary>itemAccountTotalAmount</summary>
                <description>itemAccountTotalAmount description...</description>
			</attribute>

            <attribute name="organizationReferenceId" forceUppercase="true">
                <label>Organization Reference Id</label>
                <shortLabel>Id</shortLabel>

                <maxLength>8</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="10" />
                </control>

                <summary>organizationReferenceId</summary>
                <description>organizationReferenceId description...</description>
			</attribute>

            <attribute name="paymentRequestAccountIdentifier" forceUppercase="true">
                <label>Payment Request Account Identifier</label>
                <shortLabel>Identifier</shortLabel>

                <maxLength>10</maxLength>
                <validationPattern><numeric /></validationPattern>

                <control>
                    <text size="12" />
                </control>

                <summary>paymentRequestAccountIdentifier</summary>
                <description>paymentRequestAccountIdentifier description...</description>
			</attribute>

            <attribute name="paymentRequestItemIdentifier" forceUppercase="true">
                <label>Payment Request Item Identifier</label>
                <shortLabel>Identifier</shortLabel>

                <maxLength>10</maxLength>
                <validationPattern><numeric /></validationPattern>

                <control>
                    <text size="12" />
                </control>

                <summary>paymentRequestItemIdentifier</summary>
                <description>paymentRequestItemIdentifier description...</description>
			</attribute>

            <attribute name="projectCode" forceUppercase="true">
                <label>Project Code</label>
                <shortLabel>Code</shortLabel>

                <maxLength>10</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="12" />
                </control>

                <summary>projectCode</summary>
                <description>projectCode description...</description>
			</attribute>

            <attribute name="subAccountNumber" forceUppercase="true">
                <label>Sub Account Number</label>
                <shortLabel>Number</shortLabel>

                <maxLength>5</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="7" />
                </control>

                <summary>subAccountNumber</summary>
                <description>subAccountNumber description...</description>
			</attribute>

            <attribute name="versionNumber" forceUppercase="true">
                <label>Version Number</label>
                <shortLabel>Number</shortLabel>

                <maxLength>8</maxLength>
                <validationPattern><numeric /></validationPattern>

                <control>
                    <text size="10" />
                </control>

                <summary>versionNumber</summary>
                <description>versionNumber description...</description>
			</attribute>

        </attributes>
    </businessObject>
</dictionaryEntry>
