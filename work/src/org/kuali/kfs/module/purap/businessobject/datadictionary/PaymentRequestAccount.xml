  <bean id="PaymentRequestAccount-referenceTypeCode" parent="PaymentRequestAccount-referenceTypeCode-parentBean" />

  <bean id="PaymentRequestAccount-referenceTypeCode-parentBean" abstract="true" parent="DocumentType-documentTypeCode">
    <property name="name" value="referenceTypeCode" />
    <property name="label" value="Reference Type Code" />
    <property name="shortLabel" value="Ref Type Code" />
    <property name="required" value="false" />
    <property name="summary" value="&amp;nbsp;" />
    <property name="description" value="The system code for the type of the system from which the original encumbering transaction came, selected from a pulldown list." />
  </bean>
  <bean id="PaymentRequestAccount-projectCode" parent="PaymentRequestAccount-projectCode-parentBean" />

  <bean id="PaymentRequestAccount-projectCode-parentBean" abstract="true" parent="ProjectCode-code">
    <property name="name" value="projectCode" />
    <property name="required" value="false" />
    <property name="summary" value="&amp;nbsp;" />
    <property name="description" value="The title of the project." />
  </bean>
  <bean id="PaymentRequestAccount-financialSubObjectCode" parent="PaymentRequestAccount-financialSubObjectCode-parentBean" />

  <bean id="PaymentRequestAccount-financialSubObjectCode-parentBean" abstract="true" parent="SubObjCd-financialSubObjectCode">
