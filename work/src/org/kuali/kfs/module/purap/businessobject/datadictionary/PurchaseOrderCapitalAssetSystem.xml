<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="PurchaseOrderCapitalAssetSystem" parent="PurchaseOrderCapitalAssetSystem-parentBean" />

  <bean id="PurchaseOrderCapitalAssetSystem-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.module.purap.bo.PurchaseOrderCapitalAssetSystem" />
    <property name="inquiryDefinition" >
      <ref bean="PurchaseOrderCapitalAssetSystem-inquiryDefinition" />
    </property>
    <property name="lookupDefinition" >
      <ref bean="PurchaseOrderCapitalAssetSystem-lookupDefinition" />
    </property>
    <property name="attributes" >
      <list>
        <ref bean="PurchaseOrderCapitalAssetSystem-capitalAssetManufacturerIsVendorIndicator" />
        <ref bean="PurchaseOrderCapitalAssetSystem-capitalAssetManufacturerName" />
        <ref bean="PurchaseOrderCapitalAssetSystem-capitalAssetModelDescription" />
        <ref bean="PurchaseOrderCapitalAssetSystem-capitalAssetNotReceivedCurrentFiscalYearIndicator" />
        <ref bean="PurchaseOrderCapitalAssetSystem-capitalAssetSystemDescription" />
        <ref bean="PurchaseOrderCapitalAssetSystem-capitalAssetSystemNumber" />
        <ref bean="PurchaseOrderCapitalAssetSystem-capitalAssetTypeCode" />
        <ref bean="PurchaseOrderCapitalAssetSystem-documentNumber" />
        <ref bean="PurchaseOrderCapitalAssetSystem-versionNumber" />
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->


  <bean id="PurchaseOrderCapitalAssetSystem-capitalAssetManufacturerIsVendorIndicator" parent="PurchaseOrderCapitalAssetSystem-capitalAssetManufacturerIsVendorIndicator-parentBean" />

  <bean id="PurchaseOrderCapitalAssetSystem-capitalAssetManufacturerIsVendorIndicator-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="capitalAssetManufacturerIsVendorIndicator" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Capital Asset Manufacturer Is Vendor Indicator" />
    <property name="shortLabel" value="Indicator" />
    <property name="maxLength" value="1" />
    <property name="control" >
      <bean parent="CheckboxControlDefinition" />
    </property>
    <property name="summary" value="capitalAssetManufacturerIsVendorIndicator" />
    <property name="description" value="capitalAssetManufacturerIsVendorIndicator description..." />
  </bean>
  <bean id="PurchaseOrderCapitalAssetSystem-capitalAssetManufacturerName" parent="PurchaseOrderCapitalAssetSystem-capitalAssetManufacturerName-parentBean" />

  <bean id="PurchaseOrderCapitalAssetSystem-capitalAssetManufacturerName-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="capitalAssetManufacturerName" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Capital Asset Manufacturer Name" />
    <property name="shortLabel" value="Name" />
    <property name="maxLength" value="45" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="47" />
    </property>
    <property name="summary" value="capitalAssetManufacturerName" />
    <property name="description" value="capitalAssetManufacturerName description..." />
  </bean>
  <bean id="PurchaseOrderCapitalAssetSystem-capitalAssetModelDescription" parent="PurchaseOrderCapitalAssetSystem-capitalAssetModelDescription-parentBean" />

  <bean id="PurchaseOrderCapitalAssetSystem-capitalAssetModelDescription-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="capitalAssetModelDescription" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Capital Asset Model Description" />
    <property name="shortLabel" value="Description" />
    <property name="maxLength" value="45" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="47" />
    </property>
    <property name="summary" value="capitalAssetModelDescription" />
    <property name="description" value="capitalAssetModelDescription description..." />
  </bean>
  <bean id="PurchaseOrderCapitalAssetSystem-capitalAssetNotReceivedCurrentFiscalYearIndicator" parent="PurchaseOrderCapitalAssetSystem-capitalAssetNotReceivedCurrentFiscalYearIndicator-parentBean" />

  <bean id="PurchaseOrderCapitalAssetSystem-capitalAssetNotReceivedCurrentFiscalYearIndicator-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="capitalAssetNotReceivedCurrentFiscalYearIndicator" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Capital Asset Not Received Current Fiscal Year Indicator" />
    <property name="shortLabel" value="Indicator" />
    <property name="maxLength" value="1" />
    <property name="control" >
      <bean parent="CheckboxControlDefinition" />
    </property>
    <property name="summary" value="capitalAssetNotReceivedCurrentFiscalYearIndicator" />
    <property name="description" value="capitalAssetNotReceivedCurrentFiscalYearIndicator description..." />
  </bean>
  <bean id="PurchaseOrderCapitalAssetSystem-capitalAssetSystemDescription" parent="PurchaseOrderCapitalAssetSystem-capitalAssetSystemDescription-parentBean" />

  <bean id="PurchaseOrderCapitalAssetSystem-capitalAssetSystemDescription-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="capitalAssetSystemDescription" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Capital Asset System Description" />
    <property name="shortLabel" value="Description" />
    <property name="maxLength" value="4000" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextareaControlDefinition"
            p:rows="5"
            p:cols="80" />
    </property>
    <property name="summary" value="capitalAssetSystemDescription" />
    <property name="description" value="capitalAssetSystemDescription description..." />
  </bean>
  <bean id="PurchaseOrderCapitalAssetSystem-capitalAssetSystemNumber" parent="PurchaseOrderCapitalAssetSystem-capitalAssetSystemNumber-parentBean" />

  <bean id="PurchaseOrderCapitalAssetSystem-capitalAssetSystemNumber-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="capitalAssetSystemNumber" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Capital Asset System Number" />
    <property name="shortLabel" value="Number" />
    <property name="maxLength" value="10" />
    <property name="validationPattern" >
      <bean parent="NumericValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="12" />
    </property>
    <property name="summary" value="capitalAssetSystemNumber" />
    <property name="description" value="capitalAssetSystemNumber description..." />
  </bean>
  <bean id="PurchaseOrderCapitalAssetSystem-capitalAssetTypeCode" parent="PurchaseOrderCapitalAssetSystem-capitalAssetTypeCode-parentBean" />

  <bean id="PurchaseOrderCapitalAssetSystem-capitalAssetTypeCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="capitalAssetTypeCode" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Capital Asset Type Code" />
    <property name="shortLabel" value="Code" />
    <property name="maxLength" value="7" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="9" />
    </property>
    <property name="summary" value="capitalAssetTypeCode" />
    <property name="description" value="capitalAssetTypeCode description..." />
  </bean>
  <bean id="PurchaseOrderCapitalAssetSystem-documentNumber" parent="PurchaseOrderCapitalAssetSystem-documentNumber-parentBean" />

  <bean id="PurchaseOrderCapitalAssetSystem-documentNumber-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="documentNumber" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Document Number" />
    <property name="shortLabel" value="Number" />
    <property name="maxLength" value="14" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="16" />
    </property>
    <property name="summary" value="documentNumber" />
    <property name="description" value="documentNumber description..." />
  </bean>
  <bean id="PurchaseOrderCapitalAssetSystem-versionNumber" parent="PurchaseOrderCapitalAssetSystem-versionNumber-parentBean" />

  <bean id="PurchaseOrderCapitalAssetSystem-versionNumber-parentBean" abstract="true" parent="AttributeReferenceDummy-versionNumber">
  </bean>

<!-- Business Object Inquiry Definition -->


  <bean id="PurchaseOrderCapitalAssetSystem-inquiryDefinition" parent="PurchaseOrderCapitalAssetSystem-inquiryDefinition-parentBean" />

  <bean id="PurchaseOrderCapitalAssetSystem-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
    <property name="title" value="Purchase Order Capital Asset System Inquiry" />
    <property name="inquirySections" >
      <list>
        <bean parent="InquirySectionDefinition">
          <property name="title" value="Header Text" />
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields" >
            <list>
              <bean parent="FieldDefinition"
                    p:attributeName="capitalAssetManufacturerIsVendorIndicator" />
              <bean parent="FieldDefinition"
                    p:attributeName="capitalAssetManufacturerName" />
              <bean parent="FieldDefinition"
                    p:attributeName="capitalAssetModelDescription" />
              <bean parent="FieldDefinition"
                    p:attributeName="capitalAssetNotReceivedCurrentFiscalYearIndicator" />
              <bean parent="FieldDefinition"
                    p:attributeName="capitalAssetSystemDescription" />
              <bean parent="FieldDefinition"
                    p:attributeName="capitalAssetSystemNumber" />
              <bean parent="FieldDefinition"
                    p:attributeName="capitalAssetTypeCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="documentNumber" />
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Business Object Lookup Definition -->


  <bean id="PurchaseOrderCapitalAssetSystem-lookupDefinition" parent="PurchaseOrderCapitalAssetSystem-lookupDefinition-parentBean" />

  <bean id="PurchaseOrderCapitalAssetSystem-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
    <property name="title" value="Purchase Order Capital Asset System Lookup" />
    <property name="instructions" value="Look up a Purchase Order Capital Asset System" />
    <property name="defaultSort" >
      <bean parent="SortDefinition">
      </bean>
    </property>
    <property name="lookupFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="capitalAssetManufacturerIsVendorIndicator" />
        <bean parent="FieldDefinition"
              p:attributeName="capitalAssetManufacturerName" />
        <bean parent="FieldDefinition"
              p:attributeName="capitalAssetModelDescription" />
        <bean parent="FieldDefinition"
              p:attributeName="capitalAssetNotReceivedCurrentFiscalYearIndicator" />
        <bean parent="FieldDefinition"
              p:attributeName="capitalAssetSystemDescription" />
        <bean parent="FieldDefinition"
              p:attributeName="capitalAssetSystemNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="capitalAssetTypeCode" />
        <bean parent="FieldDefinition"
              p:attributeName="documentNumber" />
      </list>
    </property>
    <property name="resultFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="capitalAssetManufacturerIsVendorIndicator" />
        <bean parent="FieldDefinition"
              p:attributeName="capitalAssetManufacturerName" />
        <bean parent="FieldDefinition"
              p:attributeName="capitalAssetModelDescription" />
        <bean parent="FieldDefinition"
              p:attributeName="capitalAssetNotReceivedCurrentFiscalYearIndicator" />
        <bean parent="FieldDefinition"
              p:attributeName="capitalAssetSystemDescription" />
        <bean parent="FieldDefinition"
              p:attributeName="capitalAssetSystemNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="capitalAssetTypeCode" />
        <bean parent="FieldDefinition"
              p:attributeName="documentNumber" />
      </list>
    </property>
  </bean>
</beans>
