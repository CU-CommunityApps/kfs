<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "../dataDictionary-1_0.dtd" [
<!ENTITY purApItemBaseAttributes SYSTEM "../dictionary/entity/PurApItemBaseAttributes.xml">
]>
<dictionaryEntry>
	<businessObject
		businessObjectClass="org.kuali.module.purap.bo.CreditMemoItem">

		<attributes>

			<attribute name="itemLineNumber" forceUppercase="true">
				<label>Item Line Number</label>
				<shortLabel>#</shortLabel>

				<maxLength>3</maxLength>
				<validationPattern>
					<numeric />
				</validationPattern>

				<control>
					<text size="5" />
				</control>

			</attribute>

			<attributeReference name="poInvoicedTotalQuantity"
				sourceClassName="org.kuali.module.purap.bo.PurApAttributeReferenceDummy"
				sourceAttributeName="genericItemQuantity">
				<label>PO Qty Invoiced</label>
				<shortLabel>PO Qty</shortLabel>
			</attributeReference>

			<attributeReference name="preqInvoicedTotalQuantity"
				sourceClassName="org.kuali.module.purap.bo.PurApAttributeReferenceDummy"
				sourceAttributeName="genericItemQuantity">
				<label>PREQ Qty Invoiced</label>
				<shortLabel>PREQ Qty Invoiced</shortLabel>
			</attributeReference>

			<attributeReference name="poUnitPrice"
				sourceClassName="org.kuali.module.purap.bo.PurApAttributeReferenceDummy"
				sourceAttributeName="genericItemUnitPrice">
				<label>PO Unit Price</label>
				<shortLabel>PO Unit Price</shortLabel>

			</attributeReference>

			<attributeReference name="preqUnitPrice"
				sourceClassName="org.kuali.module.purap.bo.PurApAttributeReferenceDummy"
				sourceAttributeName="genericItemUnitPrice">
				<label>PREQ Unit Price</label>
				<shortLabel>PREQ Unit Price</shortLabel>

			</attributeReference>

			<attributeReference name="poExtendedPrice"
				sourceClassName="org.kuali.core.bo.AttributeReferenceDummy"
				sourceAttributeName="genericAmount">
				<label>Total Invoice Cost</label>
				<shortLabel>Total Invoice Cost</shortLabel>
			</attributeReference>

			<attributeReference name="preqExtendedPrice"
				sourceClassName="org.kuali.core.bo.AttributeReferenceDummy"
				sourceAttributeName="genericAmount">
				<label>Total Invoice Cost</label>
				<shortLabel>Total Invoice Cost</shortLabel>
			</attributeReference>

			<attribute name="itemTypeCode" forceUppercase="true">
				<label>Item Type Code</label>
				<shortLabel>Item Type</shortLabel>
				<maxLength>4</maxLength>
				<validationPattern>
					<anyCharacter />
				</validationPattern>
				<required>true</required>
				<control>
					<select
						valuesFinderClass="org.kuali.module.purap.lookup.keyvalues.ItemTypeValuesFinder" />
				</control>
			</attribute>

			<attributeReference name="itemQuantity"
				sourceClassName="org.kuali.module.purap.bo.PurApAttributeReferenceDummy"
				sourceAttributeName="genericItemQuantity">
				<label>CM Cty</label>
				<shortLabel>CM Qty</shortLabel>
			</attributeReference>

			<attributeReference name="itemUnitPrice"
				sourceClassName="org.kuali.module.purap.bo.PurApAttributeReferenceDummy"
				sourceAttributeName="genericItemUnitPrice">
				<label>CM Unit Price</label>
				<shortLabel>CM Unit Price</shortLabel>
			</attributeReference>

			<attributeReference name="extendedPrice"
				sourceClassName="org.kuali.core.bo.AttributeReferenceDummy"
				sourceAttributeName="genericAmount">
				<label>Credit Processed</label>
				<shortLabel>Credit Processed</shortLabel>
			</attributeReference>

			<attribute name="itemCatalogNumber" forceUppercase="true">
				<label>Item Catalog Number</label>
				<shortLabel>Catalog #</shortLabel>

				<maxLength>30</maxLength>
				<validationPattern>
					<anyCharacter />
				</validationPattern>

				<control>
					<text size="15" />
				</control>
			</attribute>

			<attribute name="itemDescription" forceUppercase="true">
				<label>Item Description</label>
				<shortLabel>Description</shortLabel>
				<maxLength>4000</maxLength>
				<validationPattern>
					<anyCharacter allowWhitespace="true" />
				</validationPattern>
				<control>
					<textarea rows="4" cols="50" />
				</control>
			</attribute>

			<attributeReference name="versionNumber"
				sourceClassName="org.kuali.core.bo.AttributeReferenceDummy"
				sourceAttributeName="versionNumber" />

		</attributes>
	</businessObject>
</dictionaryEntry>
