<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "../dataDictionary-1_0.dtd">
<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.module.purap.bo.RequisitionItemCapitalAsset">
        <customAttributeEnabled>false</customAttributeEnabled>

        <inquiry>
            <title>Requisition Item Capital Asset Inquiry</title>

            <inquiryFields>
                <field attributeName="capitalAssetNumber" />
                <field attributeName="requisitionItemIdentifier" />
                <field attributeName="requisitionItemCapitalAssetIdentifier" />
            </inquiryFields>
        </inquiry>

        <lookup>
            <title>Requisition Item Capital Asset Lookup</title>

			<menubar><![CDATA[<a href="maintenance.do?methodToCall=start&businessObjectClassName=org.kuali.module.purap.bo.RequisitionItemCapitalAsset">Create New</a>]]></menubar>

            <instructions>Lookup an Requisition Item Capital Asset</instructions>

            <defaultSort sortAscending="true">
                <sortAttributes>
                    <sortAttribute attributeName="capitalAssetNumber" />
                </sortAttributes>
            </defaultSort>

            <lookupFields>
                <lookupField attributeName="capitalAssetNumber" required="false" />
                <lookupField attributeName="requisitionItemIdentifier" required="false" />
                <lookupField attributeName="requisitionItemCapitalAssetIdentifier" required="false" />
            </lookupFields>

            <resultFields>
                <field attributeName="capitalAssetNumber" />
                <field attributeName="requisitionItemIdentifier" />
                <field attributeName="requisitionItemCapitalAssetIdentifier" />
            </resultFields>
        </lookup>

        <attributes>

            <attribute name="capitalAssetNumber" forceUppercase="true">
                <label>Capital Asset Number</label>
                <shortLabel>Number</shortLabel>

                <maxLength>12</maxLength>
                <validationPattern><numeric /></validationPattern>

                <control>
                    <text size="14" />
                </control>

                <summary>capitalAssetNumber</summary>
                <description>capitalAssetNumber description...</description>
			</attribute>

            <attribute name="requisitionItemIdentifier" forceUppercase="true">
                <label>Requisition Item Identifier</label>
                <shortLabel>Identifier</shortLabel>

                <maxLength>10</maxLength>
                <validationPattern><numeric /></validationPattern>

                <control>
                    <text size="12" />
                </control>

                <summary>requisitionItemIdentifier</summary>
                <description>requisitionItemIdentifier description...</description>
			</attribute>

            <attribute name="requisitionItemCapitalAssetIdentifier" forceUppercase="true">
                <label>Requisition Item Capital Asset Identifier</label>
                <shortLabel>Identifier</shortLabel>

                <maxLength>10</maxLength>
                <validationPattern><numeric /></validationPattern>

                <control>
                    <text size="12" />
                </control>

                <summary>requisitionItemCapitalAssetIdentifier</summary>
                <description>requisitionItemCapitalAssetIdentifier description...</description>
			</attribute>

            <attribute name="versionNumber" forceUppercase="true">
                <label>Version Number</label>
                <shortLabel>Number</shortLabel>

                <maxLength>8</maxLength>
                <validationPattern><numeric /></validationPattern>

                <control>
                    <text size="10" />
                </control>

                <summary>versionNumber</summary>
                <description>versionNumber description...</description>
			</attribute>

        </attributes>
    </businessObject>
</dictionaryEntry>
