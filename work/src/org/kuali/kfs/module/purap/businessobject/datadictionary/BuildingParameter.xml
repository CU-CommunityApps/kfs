<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="BuildingParameter" parent="BuildingParameter-parentBean" />

  <bean id="BuildingParameter-parentBean" abstract="true" parent="BusinessObjectEntry">
  	<property name="objectLabel" value="Building Parameter" />
    <property name="businessObjectClass" value="org.kuali.kfs.module.purap.businessobject.BuildingParameter" />
    <property name="inquiryDefinition" >
      <ref bean="BuildingParameter-inquiryDefinition" />
    </property>
    <property name="lookupDefinition" >
      <ref bean="BuildingParameter-lookupDefinition" />
    </property>
    <property name="attributes" >
      <list>
        <ref bean="BuildingParameter-buildingCode" />
        <ref bean="BuildingParameter-buildingOverrideAddressCityName" />
        <ref bean="BuildingParameter-buildingOverrideAddressStateCode" />
        <ref bean="BuildingParameter-buildingOverrideAddressZipCode" />
        <ref bean="BuildingParameter-buildingOverrideCode" />
        <ref bean="BuildingParameter-buildingOverrideName" />
        <ref bean="BuildingParameter-buildingOverrideStreetAddress" />
        <ref bean="BuildingParameter-campusCode" />
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->


  <bean id="BuildingParameter-buildingCode" parent="BuildingParameter-buildingCode-parentBean" />

  <bean id="BuildingParameter-buildingCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="buildingCode" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Building Code" />
    <property name="shortLabel" value="Building Code" />
    <property name="maxLength" value="10" />
    <property name="validationPattern" >
      <bean parent="AlphaNumericValidationPattern" />
    </property>
    <property name="required" value="true" />
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="10" />
    </property>
  </bean>
  <bean id="BuildingParameter-buildingOverrideAddressCityName" parent="BuildingParameter-buildingOverrideAddressCityName-parentBean" />

  <bean id="BuildingParameter-buildingOverrideAddressCityName-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="buildingOverrideAddressCityName" />
    <property name="label" value="Building Override Address City Name" />
    <property name="shortLabel" value="Name" />
    <property name="maxLength" value="45" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="47" />
    </property>
  </bean>
  <bean id="BuildingParameter-buildingOverrideAddressStateCode" parent="BuildingParameter-buildingOverrideAddressStateCode-parentBean" />

  <bean id="BuildingParameter-buildingOverrideAddressStateCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="buildingOverrideAddressStateCode" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Building Override Address State Code" />
    <property name="shortLabel" value="Code" />
    <property name="maxLength" value="2" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="4" />
    </property>
  </bean>
  <bean id="BuildingParameter-buildingOverrideAddressZipCode" parent="BuildingParameter-buildingOverrideAddressZipCode-parentBean" />

  <bean id="BuildingParameter-buildingOverrideAddressZipCode-parentBean" abstract="true" parent="PostalZipCode-postalZipCode">
    <property name="name" value="buildingOverrideAddressZipCode" />
    <property name="label" value="Building Override Address Zip Code" />
    <property name="shortLabel" value="Code" />
  </bean>
  <bean id="BuildingParameter-buildingOverrideCode" parent="BuildingParameter-buildingOverrideCode-parentBean" />

  <bean id="BuildingParameter-buildingOverrideCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="buildingOverrideCode" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Building Override Code" />
    <property name="shortLabel" value="Code" />
    <property name="maxLength" value="4" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="6" />
    </property>
  </bean>
  <bean id="BuildingParameter-buildingOverrideName" parent="BuildingParameter-buildingOverrideName-parentBean" />

  <bean id="BuildingParameter-buildingOverrideName-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="buildingOverrideName" />
    <property name="label" value="Building Override Name" />
    <property name="shortLabel" value="Name" />
    <property name="maxLength" value="45" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="47" />
    </property>
  </bean>
  <bean id="BuildingParameter-buildingOverrideStreetAddress" parent="BuildingParameter-buildingOverrideStreetAddress-parentBean" />

  <bean id="BuildingParameter-buildingOverrideStreetAddress-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="buildingOverrideStreetAddress" />
    <property name="label" value="Building Override Street Address" />
    <property name="shortLabel" value="Address" />
    <property name="maxLength" value="45" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="47" />
    </property>
  </bean>
  <bean id="BuildingParameter-campusCode" parent="BuildingParameter-campusCode-parentBean" />

  <bean id="BuildingParameter-campusCode-parentBean" abstract="true" parent="VendorGenericAttributes-campusCode">
    <property name="required" value="true" />
  </bean>

<!-- Business Object Inquiry Definition -->


  <bean id="BuildingParameter-inquiryDefinition" parent="BuildingParameter-inquiryDefinition-parentBean" />

  <bean id="BuildingParameter-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
    <property name="title" value="Building Parameter Inquiry" />
    <property name="inquirySections" >
      <list>
        <bean parent="InquirySectionDefinition">
          <property name="title" value="" />
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields" >
            <list>
              <bean parent="FieldDefinition"
                    p:attributeName="buildingCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="buildingOverrideAddressCityName" />
              <bean parent="FieldDefinition"
                    p:attributeName="buildingOverrideAddressStateCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="buildingOverrideAddressZipCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="buildingOverrideCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="buildingOverrideName" />
              <bean parent="FieldDefinition"
                    p:attributeName="buildingOverrideStreetAddress" />
              <bean parent="FieldDefinition"
                    p:attributeName="campusCode" />
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Business Object Lookup Definition -->


  <bean id="BuildingParameter-lookupDefinition" parent="BuildingParameter-lookupDefinition-parentBean" />

  <bean id="BuildingParameter-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
    <property name="title" value="Building Parameter Lookup" />
    <property name="instructions" value="Look up a Building Parameter" />
    <property name="defaultSort" >
      <bean parent="SortDefinition">
      </bean>
    </property>
    <property name="lookupFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="buildingCode" />
        <bean parent="FieldDefinition"
              p:attributeName="buildingOverrideAddressCityName" />
        <bean parent="FieldDefinition"
              p:attributeName="buildingOverrideAddressStateCode" />
        <bean parent="FieldDefinition"
              p:attributeName="buildingOverrideAddressZipCode" />
        <bean parent="FieldDefinition"
              p:attributeName="buildingOverrideCode" />
        <bean parent="FieldDefinition"
              p:attributeName="buildingOverrideName" />
        <bean parent="FieldDefinition"
              p:attributeName="buildingOverrideStreetAddress" />
        <bean parent="FieldDefinition"
              p:attributeName="campusCode" />
      </list>
    </property>
    <property name="resultFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="buildingCode" />
        <bean parent="FieldDefinition"
              p:attributeName="buildingOverrideAddressCityName" />
        <bean parent="FieldDefinition"
              p:attributeName="buildingOverrideAddressStateCode" />
        <bean parent="FieldDefinition"
              p:attributeName="buildingOverrideAddressZipCode" />
        <bean parent="FieldDefinition"
              p:attributeName="buildingOverrideCode" />
        <bean parent="FieldDefinition"
              p:attributeName="buildingOverrideName" />
        <bean parent="FieldDefinition"
              p:attributeName="buildingOverrideStreetAddress" />
        <bean parent="FieldDefinition"
              p:attributeName="campusCode" />
      </list>
    </property>
  </bean>
</beans>
