<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "../dataDictionary-1_0.dtd">
<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.module.purap.bo.BuildingParameter">
 
        <inquiry>
            <title>Building Parameter Inquiry</title>
             
            <inquirySections>
                <inquirySection title="Header Text">
                    <inquiryFields>
                        <field attributeName="buildingCode" />
                        <field attributeName="buildingOverrideAddressCityName" />
                        <field attributeName="buildingOverrideAddressStateCode" />
                        <field attributeName="buildingOverrideAddressZipCode" />
                        <field attributeName="buildingOverrideCode" />
                        <field attributeName="buildingOverrideName" />
                        <field attributeName="buildingOverrideStreetAddress" />
                        <field attributeName="campusCode" />
                    </inquiryFields>
                </inquirySection>
            </inquirySections>  
        </inquiry>

        <lookup>
            <title>Building Parameter Lookup</title>

            <instructions>Look up a Building Parameter</instructions>

            <defaultSort sortAscending="true">
                <sortAttributes>
                    <sortAttribute attributeName="campusCode" />
                    <sortAttribute attributeName="buildingCode" />
                    <sortAttribute attributeName="buildingOverrideCode" />
                </sortAttributes>
            </defaultSort>

            <lookupFields>
                <lookupField attributeName="buildingCode" required="false" />
                <lookupField attributeName="buildingOverrideAddressCityName" required="false" />
                <lookupField attributeName="buildingOverrideAddressStateCode" required="false" />
                <lookupField attributeName="buildingOverrideAddressZipCode" required="false" />
                <lookupField attributeName="buildingOverrideCode" required="false" />
                <lookupField attributeName="buildingOverrideName" required="false" />
                <lookupField attributeName="buildingOverrideStreetAddress" required="false" />
                <lookupField attributeName="campusCode" required="false" />
            </lookupFields>

            <resultFields>
                <field attributeName="buildingCode" />
                <field attributeName="buildingOverrideAddressCityName" />
                <field attributeName="buildingOverrideAddressStateCode" />
                <field attributeName="buildingOverrideAddressZipCode" />
                <field attributeName="buildingOverrideCode" />
                <field attributeName="buildingOverrideName" />
                <field attributeName="buildingOverrideStreetAddress" />
                <field attributeName="campusCode" />
            </resultFields>
        </lookup>

        <attributes>

            <attribute name="buildingCode" forceUppercase="true">
                <label>Building Code</label>
                <shortLabel>Building Code</shortLabel>

                <maxLength>10</maxLength>
                <validationPattern>
                    <alphaNumeric />
                </validationPattern>
                <required>true</required>

                <control>
                    <text size="10" />
                </control>

            </attribute>

            <attribute name="buildingOverrideAddressCityName" forceUppercase="true">
                <label>Building Override Address City Name</label>
                <shortLabel>Name</shortLabel>

                <maxLength>45</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="47" />
                </control>

			</attribute>

            <attribute name="buildingOverrideAddressStateCode" forceUppercase="true">
                <label>Building Override Address State Code</label>
                <shortLabel>Code</shortLabel>

                <maxLength>2</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="4" />
                </control>

			</attribute>

            <attribute name="buildingOverrideAddressZipCode" forceUppercase="true">
                <label>Building Override Address Zip Code</label>
                <shortLabel>Code</shortLabel>

                <maxLength>11</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="13" />
                </control>

			</attribute>

            <attribute name="buildingOverrideCode" forceUppercase="true">
                <label>Building Override Code</label>
                <shortLabel>Code</shortLabel>

                <maxLength>4</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="6" />
                </control>

			</attribute>

            <attribute name="buildingOverrideName" forceUppercase="true">
                <label>Building Override Name</label>
                <shortLabel>Name</shortLabel>

                <maxLength>45</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="47" />
                </control>

			</attribute>

            <attribute name="buildingOverrideStreetAddress" forceUppercase="true">
                <label>Building Override Street Address</label>
                <shortLabel>Address</shortLabel>

                <maxLength>45</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="47" />
                </control>

			</attribute>

			<attributeReference name="campusCode"
				sourceClassName="org.kuali.module.vendor.bo.VendorAttributeReferenceDummy" 
				sourceAttributeName="campusCode">
                <required>true</required>
			</attributeReference>

            <attributeReference name="versionNumber" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="versionNumber" />

        </attributes>
    </businessObject>
</dictionaryEntry>
