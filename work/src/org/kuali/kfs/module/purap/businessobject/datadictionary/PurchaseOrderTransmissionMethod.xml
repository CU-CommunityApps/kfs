<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "../dataDictionary-1_0.dtd">
<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.module.purap.bo.PurchaseOrderTransmissionMethod">
        <customAttributeEnabled>false</customAttributeEnabled>

        <inquiry>
            <title>Purchase Order Transmission Method Inquiry</title>

            <inquiryFields>
                <field attributeName="purchaseOrderTransmissionMethodCode" />
                <field attributeName="purchaseOrderTransmissionMethodDescription" />
                <field attributeName="dataObjectMaintenanceCodeActiveIndicator" />
            </inquiryFields>
        </inquiry>

        <lookup>
            <title>Purchase Order Transmission Method Lookup</title>

            <instructions>Lookup a Purchase Order Transmission Method</instructions>

            <defaultSort sortAscending="true">
                <sortAttributes>
                    <sortAttribute attributeName="purchaseOrderTransmissionMethodDescription" />
                </sortAttributes>
            </defaultSort>

            <lookupFields>
                <lookupField attributeName="purchaseOrderTransmissionMethodCode" required="false" />
                <lookupField attributeName="purchaseOrderTransmissionMethodDescription" required="false" />
                <lookupField attributeName="dataObjectMaintenanceCodeActiveIndicator" required="false" />
            </lookupFields>

            <resultFields>
                <field attributeName="purchaseOrderTransmissionMethodCode" />
                <field attributeName="purchaseOrderTransmissionMethodDescription" />
                <field attributeName="dataObjectMaintenanceCodeActiveIndicator" />
            </resultFields>
        </lookup>

        <attributes>

            <attribute name="purchaseOrderTransmissionMethodCode" forceUppercase="true">
                <label>Purchase Order Transmission Method Code</label>
                <shortLabel>Code</shortLabel>

                <maxLength>4</maxLength>
                <validationPattern><anyCharacter /></validationPattern>
                <required>true</required>

                <control>
                    <text size="6" />
                </control>

                <summary>purchaseOrderTransmissionMethodCode</summary>
                <description>purchaseOrderTransmissionMethodCode description...</description>
			</attribute>

            <attribute name="purchaseOrderTransmissionMethodDescription" forceUppercase="true">
                <label>Purchase Order Transmission Method Description</label>
                <shortLabel>Description</shortLabel>

                <maxLength>45</maxLength>
                <validationPattern><anyCharacter allowWhitespace="true"/></validationPattern>
                <required>true</required>

                <control>
                    <text size="47" />
                </control>

                <summary>purchaseOrderTransmissionMethodDescription</summary>
                <description>purchaseOrderTransmissionMethodDescription description...</description>
			</attribute>
			
			<attributeReference name="dataObjectMaintenanceCodeActiveIndicator" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="activeIndicator" />

            <attributeReference name="versionNumber" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="versionNumber" />
		
        </attributes>
    </businessObject>
</dictionaryEntry>
