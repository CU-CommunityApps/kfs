<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="PaymentChangeAccount" parent="PaymentChangeAccount-parentBean" />

  <bean id="PaymentChangeAccount-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.module.purap.bo.PaymentChangeAccount" />
    <property name="inquiryDefinition" >
      <ref bean="PaymentChangeAccount-inquiryDefinition" />
    </property>
    <property name="lookupDefinition" >
      <ref bean="PaymentChangeAccount-lookupDefinition" />
    </property>
    <property name="attributes" >
      <list>
        <ref bean="PaymentChangeAccount-paymentChangeIdentifier" />
        <ref bean="PaymentChangeAccount-itemAccountTotalAmount" />
        <ref bean="PaymentChangeAccount-itemLineNumber" />
        <ref bean="PaymentChangeAccount-versionNumber" />
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->


  <bean id="PaymentChangeAccount-paymentChangeIdentifier" parent="PaymentChangeAccount-paymentChangeIdentifier-parentBean" />

  <bean id="PaymentChangeAccount-paymentChangeIdentifier-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="paymentChangeIdentifier" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Payment Change Identifier" />
    <property name="shortLabel" value="Identifier" />
    <property name="maxLength" value="10" />
    <property name="validationPattern" >
      <bean parent="NumericValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="12" />
    </property>
  </bean>

  <bean id="PaymentChangeAccount-itemAccountTotalAmount" parent="PaymentChangeAccount-itemAccountTotalAmount-parentBean" />

  <bean id="PaymentChangeAccount-itemAccountTotalAmount-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="itemAccountTotalAmount" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Item Account Total Amount" />
    <property name="shortLabel" value="Amount" />
    <property name="maxLength" value="19" />
    <property name="validationPattern" >
      <bean parent="FloatingPointValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="21" />
    </property>
  </bean>
  <bean id="PaymentChangeAccount-itemLineNumber" parent="PaymentChangeAccount-itemLineNumber-parentBean" />

  <bean id="PaymentChangeAccount-itemLineNumber-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="itemLineNumber" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Item Line Number" />
    <property name="shortLabel" value="Number" />
    <property name="maxLength" value="3" />
    <property name="validationPattern" >
      <bean parent="NumericValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="5" />
    </property>
  </bean>
  <bean id="PaymentChangeAccount-versionNumber" parent="PaymentChangeAccount-versionNumber-parentBean" />

  <bean id="PaymentChangeAccount-versionNumber-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="versionNumber" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Version Number" />
    <property name="shortLabel" value="Number" />
    <property name="maxLength" value="8" />
    <property name="validationPattern" >
      <bean parent="NumericValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="10" />
    </property>
  </bean>

<!-- Business Object Inquiry Definition -->


  <bean id="PaymentChangeAccount-inquiryDefinition" parent="PaymentChangeAccount-inquiryDefinition-parentBean" />

  <bean id="PaymentChangeAccount-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
    <property name="title" value="Payment Change Account Inquiry" />
    <property name="inquirySections" >
      <list>
        <bean parent="InquirySectionDefinition">
          <property name="title" value="Header Text" />
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields" >
            <list>
              <bean parent="FieldDefinition"
                    p:attributeName="paymentChangeIdentifier" />
              <bean parent="FieldDefinition"
                    p:attributeName="itemAccountTotalAmount" />
              <bean parent="FieldDefinition"
                    p:attributeName="itemLineNumber" />
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Business Object Lookup Definition -->


  <bean id="PaymentChangeAccount-lookupDefinition" parent="PaymentChangeAccount-lookupDefinition-parentBean" />

  <bean id="PaymentChangeAccount-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
    <property name="title" value="Payment Change Account Lookup" />
    <property name="instructions" value="Lookup a Payment Change Account" />
    <property name="defaultSort" >
      <bean parent="SortDefinition">
      </bean>
    </property>
    <property name="lookupFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="paymentChangeIdentifier" />
        <bean parent="FieldDefinition"
              p:attributeName="itemAccountTotalAmount" />
        <bean parent="FieldDefinition"
              p:attributeName="itemLineNumber" />
      </list>
    </property>
    <property name="resultFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="paymentChangeIdentifier" />
        <bean parent="FieldDefinition"
              p:attributeName="itemAccountTotalAmount" />
        <bean parent="FieldDefinition"
              p:attributeName="itemLineNumber" />
      </list>
    </property>
  </bean>
</beans>
