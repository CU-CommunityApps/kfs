  <bean id="PurchaseOrderAccount-referenceTypeCode" parent="PurchaseOrderAccount-referenceTypeCode-parentBean" />

  <bean id="PurchaseOrderAccount-referenceTypeCode-parentBean" abstract="true" parent="DocumentType-documentTypeCode">
    <property name="name" value="referenceTypeCode" />
    <property name="label" value="Reference Type Code" />
    <property name="shortLabel" value="Ref Type Code" />
