  <bean id="PurchaseOrderAccount-sequenceNumber" parent="PurchaseOrderAccount-sequenceNumber-parentBean" />

  <bean id="PurchaseOrderAccount-sequenceNumber-parentBean" abstract="true" parent="GenericAttributes-transactionEntrySequenceId">
    <property name="name" value="sequenceNumber" />
