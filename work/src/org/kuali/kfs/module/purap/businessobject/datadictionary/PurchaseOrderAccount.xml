  </bean>
  <bean id="PurchaseOrderAccount-overrideCode" parent="PurchaseOrderAccount-overrideCode-parentBean" />

  <bean id="PurchaseOrderAccount-overrideCode-parentBean" abstract="true" parent="AttributeDefinition">
