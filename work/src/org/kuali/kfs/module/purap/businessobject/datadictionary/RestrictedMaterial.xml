<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "../dataDictionary-1_0.dtd">
<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.module.purap.bo.RestrictedMaterial">

        <inquiry>
            <title>Restricted Material Inquiry</title>

            <inquirySections><inquirySection title="Header Text"><inquiryFields>
                <field attributeName="restrictedMaterialCode" />
                <field attributeName="restrictedMaterialDescription" />
                <field attributeName="restrictedMaterialDefaultDescription" />
                <field attributeName="restrictedMaterialWorkgroupName" />
                <field attributeName="active" />
            </inquiryFields></inquirySection></inquirySections>
        </inquiry>

        <lookup>
            <title>Restricted Material Lookup</title>

            <instructions>Lookup an Restricted Material</instructions>

            <defaultSort sortAscending="true">
                <sortAttributes>
                    <sortAttribute attributeName="restrictedMaterialDescription" />
                </sortAttributes>
            </defaultSort>

            <lookupFields>
                <lookupField attributeName="restrictedMaterialCode" required="false" />
                <lookupField attributeName="restrictedMaterialDescription" required="false" />
                <lookupField attributeName="restrictedMaterialDefaultDescription" required="false" />
                <lookupField attributeName="restrictedMaterialWorkgroupName" required="false" />
                <lookupField attributeName="active" required="false" />
            </lookupFields>

            <resultFields>
                <field attributeName="restrictedMaterialCode" />
                <field attributeName="restrictedMaterialDescription" />
                <field attributeName="restrictedMaterialDefaultDescription" />
                <field attributeName="restrictedMaterialWorkgroupName" />
                <field attributeName="active" />
            </resultFields>
        </lookup>

        <titleAttribute>restrictedMaterialCode</titleAttribute>

        <attributes>

            <attribute name="restrictedMaterialCode" forceUppercase="true">
                <label>Restricted Material Code</label>
                <shortLabel>Code</shortLabel>

                <maxLength>4</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="6" />
                </control>

			</attribute>

            <attribute name="restrictedMaterialDefaultDescription">
                <label>Restricted Material Default Description</label>
                <shortLabel>Description</shortLabel>

                <maxLength>45</maxLength>
                <validationPattern><anyCharacter allowWhitespace="true"/></validationPattern>

                <control>
                    <text size="47" />
                </control>

			</attribute>

            <attribute name="restrictedMaterialDescription">
                <label>Restricted Material Description</label>
                <shortLabel>Description</shortLabel>

                <maxLength>45</maxLength>
                <validationPattern><anyCharacter allowWhitespace="true" /></validationPattern>

                <control>
                    <text size="47" />
                </control>

			</attribute>

            <attribute name="restrictedMaterialWorkgroupName" forceUppercase="true">
                <label>Restricted Material Workgroup Name</label>
                <shortLabel>Name</shortLabel>

                <maxLength>45</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="47" />
                </control>

			</attribute>

            <attributeReference name="active" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="activeIndicator" />
            
            <attributeReference name="versionNumber" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="versionNumber" />

        </attributes>
    </businessObject>
</dictionaryEntry>
