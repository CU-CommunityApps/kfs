<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="PurchaseOrderItemCapitalAsset" parent="PurchaseOrderItemCapitalAsset-parentBean" />

  <bean id="PurchaseOrderItemCapitalAsset-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.module.purap.bo.PurchaseOrderItemCapitalAsset" />
    <property name="inquiryDefinition" >
      <ref bean="PurchaseOrderItemCapitalAsset-inquiryDefinition" />
    </property>
    <property name="lookupDefinition" >
      <ref bean="PurchaseOrderItemCapitalAsset-lookupDefinition" />
    </property>
    <property name="attributes" >
      <list>
        <ref bean="PurchaseOrderItemCapitalAsset-capitalAssetNumber" />
        <ref bean="PurchaseOrderItemCapitalAsset-purchaseOrderItemIdentifier" />
        <ref bean="PurchaseOrderItemCapitalAsset-purchaseOrderItemCapitalAssetIdentifier" />
        <ref bean="PurchaseOrderItemCapitalAsset-versionNumber" />
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->


  <bean id="PurchaseOrderItemCapitalAsset-capitalAssetNumber" parent="PurchaseOrderItemCapitalAsset-capitalAssetNumber-parentBean" />

  <bean id="PurchaseOrderItemCapitalAsset-capitalAssetNumber-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="capitalAssetNumber" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Capital Asset Number" />
    <property name="shortLabel" value="Number" />
    <property name="maxLength" value="12" />
    <property name="validationPattern" >
      <bean parent="NumericValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="14" />
    </property>
  </bean>
  <bean id="PurchaseOrderItemCapitalAsset-purchaseOrderItemIdentifier" parent="PurchaseOrderItemCapitalAsset-purchaseOrderItemIdentifier-parentBean" />

  <bean id="PurchaseOrderItemCapitalAsset-purchaseOrderItemIdentifier-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="purchaseOrderItemIdentifier" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Purchase Order Item Identifier" />
    <property name="shortLabel" value="Identifier" />
    <property name="maxLength" value="10" />
    <property name="validationPattern" >
      <bean parent="NumericValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="12" />
    </property>
  </bean>
  <bean id="PurchaseOrderItemCapitalAsset-purchaseOrderItemCapitalAssetIdentifier" parent="PurchaseOrderItemCapitalAsset-purchaseOrderItemCapitalAssetIdentifier-parentBean" />

  <bean id="PurchaseOrderItemCapitalAsset-purchaseOrderItemCapitalAssetIdentifier-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="purchaseOrderItemCapitalAssetIdentifier" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Purchase Order Item Capital Asset Identifier" />
    <property name="shortLabel" value="Identifier" />
    <property name="maxLength" value="10" />
    <property name="validationPattern" >
      <bean parent="NumericValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="12" />
    </property>
  </bean>
  <bean id="PurchaseOrderItemCapitalAsset-versionNumber" parent="PurchaseOrderItemCapitalAsset-versionNumber-parentBean" />

  <bean id="PurchaseOrderItemCapitalAsset-versionNumber-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="versionNumber" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Version Number" />
    <property name="shortLabel" value="Number" />
    <property name="maxLength" value="8" />
    <property name="validationPattern" >
      <bean parent="NumericValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="10" />
    </property>
  </bean>

<!-- Business Object Inquiry Definition -->


  <bean id="PurchaseOrderItemCapitalAsset-inquiryDefinition" parent="PurchaseOrderItemCapitalAsset-inquiryDefinition-parentBean" />

  <bean id="PurchaseOrderItemCapitalAsset-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
    <property name="title" value="Purchase Order Item Capital Asset Inquiry" />
    <property name="inquirySections" >
      <list>
        <bean parent="InquirySectionDefinition">
          <property name="title" value="Header Text" />
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields" >
            <list>
              <bean parent="FieldDefinition"
                    p:attributeName="capitalAssetNumber" />
              <bean parent="FieldDefinition"
                    p:attributeName="purchaseOrderItemIdentifier" />
              <bean parent="FieldDefinition"
                    p:attributeName="purchaseOrderItemCapitalAssetIdentifier" />
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Business Object Lookup Definition -->


  <bean id="PurchaseOrderItemCapitalAsset-lookupDefinition" parent="PurchaseOrderItemCapitalAsset-lookupDefinition-parentBean" />

  <bean id="PurchaseOrderItemCapitalAsset-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
    <property name="title" value="Purchase Order Item Capital Asset Lookup" />
    <property name="instructions" value="Lookup an Purchase Order Item Capital Asset" />
    <property name="defaultSort" >
      <bean parent="SortDefinition">
      </bean>
    </property>
    <property name="lookupFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="capitalAssetNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="purchaseOrderItemIdentifier" />
        <bean parent="FieldDefinition"
              p:attributeName="purchaseOrderItemCapitalAssetIdentifier" />
      </list>
    </property>
    <property name="resultFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="capitalAssetNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="purchaseOrderItemIdentifier" />
        <bean parent="FieldDefinition"
              p:attributeName="purchaseOrderItemCapitalAssetIdentifier" />
      </list>
    </property>
  </bean>
</beans>
