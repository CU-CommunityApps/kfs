<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="PaymentChange" parent="PaymentChange-parentBean" />

  <bean id="PaymentChange-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.module.purap.bo.PaymentChange" />
    <property name="inquiryDefinition" >
      <ref bean="PaymentChange-inquiryDefinition" />
    </property>
    <property name="lookupDefinition" >
      <ref bean="PaymentChange-lookupDefinition" />
    </property>
    <property name="attributes" >
      <list>
        <ref bean="PaymentChange-paymentChangeIdentifier" />
        <ref bean="PaymentChange-creditMemoIdentifier" />
        <ref bean="PaymentChange-paymentRequestIdentifier" />
        <ref bean="PaymentChange-versionNumber" />
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->


  <bean id="PaymentChange-paymentChangeIdentifier" parent="PaymentChange-paymentChangeIdentifier-parentBean" />

  <bean id="PaymentChange-paymentChangeIdentifier-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="paymentChangeIdentifier" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Payment Change Identifier" />
    <property name="shortLabel" value="Identifier" />
    <property name="maxLength" value="10" />
    <property name="validationPattern" >
      <bean parent="NumericValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="12" />
    </property>
  </bean>
  <bean id="PaymentChange-creditMemoIdentifier" parent="PaymentChange-creditMemoIdentifier-parentBean" />

  <bean id="PaymentChange-creditMemoIdentifier-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="creditMemoIdentifier" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Credit Memo Identifier" />
    <property name="shortLabel" value="Identifier" />
    <property name="maxLength" value="9" />
    <property name="validationPattern" >
      <bean parent="NumericValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="11" />
    </property>
  </bean>
  <bean id="PaymentChange-paymentRequestIdentifier" parent="PaymentChange-paymentRequestIdentifier-parentBean" />

  <bean id="PaymentChange-paymentRequestIdentifier-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="paymentRequestIdentifier" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Payment Request Identifier" />
    <property name="shortLabel" value="Identifier" />
    <property name="maxLength" value="9" />
    <property name="validationPattern" >
      <bean parent="NumericValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="11" />
    </property>
  </bean>
  <bean id="PaymentChange-versionNumber" parent="PaymentChange-versionNumber-parentBean" />

  <bean id="PaymentChange-versionNumber-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="versionNumber" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Version Number" />
    <property name="shortLabel" value="Number" />
    <property name="maxLength" value="8" />
    <property name="validationPattern" >
      <bean parent="NumericValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="10" />
    </property>
  </bean>

<!-- Business Object Inquiry Definition -->


  <bean id="PaymentChange-inquiryDefinition" parent="PaymentChange-inquiryDefinition-parentBean" />

  <bean id="PaymentChange-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
    <property name="title" value="Payment Change Inquiry" />
    <property name="inquirySections" >
      <list>
        <bean parent="InquirySectionDefinition">
          <property name="title" value="Header Text" />
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields" >
            <list>
              <bean parent="FieldDefinition"
                    p:attributeName="paymentChangeIdentifier" />
              <bean parent="FieldDefinition"
                    p:attributeName="creditMemoIdentifier" />
              <bean parent="FieldDefinition"
                    p:attributeName="paymentRequestIdentifier" />
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Business Object Lookup Definition -->


  <bean id="PaymentChange-lookupDefinition" parent="PaymentChange-lookupDefinition-parentBean" />

  <bean id="PaymentChange-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
    <property name="title" value="Payment Change Lookup" />
    <property name="instructions" value="Lookup a Payment Change" />
    <property name="defaultSort" >
      <bean parent="SortDefinition">
      </bean>
    </property>
    <property name="lookupFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="paymentChangeIdentifier" />
        <bean parent="FieldDefinition"
              p:attributeName="creditMemoIdentifier" />
        <bean parent="FieldDefinition"
              p:attributeName="paymentRequestIdentifier" />
      </list>
    </property>
    <property name="resultFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="paymentChangeIdentifier" />
        <bean parent="FieldDefinition"
              p:attributeName="creditMemoIdentifier" />
        <bean parent="FieldDefinition"
              p:attributeName="paymentRequestIdentifier" />
      </list>
    </property>
  </bean>
</beans>
