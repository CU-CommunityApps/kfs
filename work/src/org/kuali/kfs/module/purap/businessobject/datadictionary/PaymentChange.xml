<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "../dataDictionary-1_0.dtd">
<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.module.purap.bo.PaymentChange">

        <inquiry>
            <title>Payment Change Inquiry</title>

            <inquirySections><inquirySection title="Header Text"><inquiryFields>
                <field attributeName="paymentChangeIdentifier" />
                <field attributeName="creditMemoIdentifier" />
                <field attributeName="paymentRequestIdentifier" />
            </inquiryFields></inquirySection></inquirySections>
        </inquiry>

        <lookup>
            <title>Payment Change Lookup</title>

            <instructions>Lookup a Payment Change</instructions>

            <defaultSort sortAscending="true">
                <sortAttributes>
                    <sortAttribute attributeName="paymentChangeIdentifier" />
                </sortAttributes>
            </defaultSort>

            <lookupFields>
                <lookupField attributeName="paymentChangeIdentifier" required="false" />
                <lookupField attributeName="creditMemoIdentifier" required="false" />
                <lookupField attributeName="paymentRequestIdentifier" required="false" />
            </lookupFields>

            <resultFields>
                <field attributeName="paymentChangeIdentifier" />
                <field attributeName="creditMemoIdentifier" />
                <field attributeName="paymentRequestIdentifier" />
            </resultFields>
        </lookup>

        <attributes>

            <attribute name="paymentChangeIdentifier" forceUppercase="true">
                <label>Payment Change Identifier</label>
                <shortLabel>Identifier</shortLabel>

                <maxLength>10</maxLength>
                <validationPattern><numeric /></validationPattern>

                <control>
                    <text size="12" />
                </control>

			</attribute>

            <attribute name="creditMemoIdentifier" forceUppercase="true">
                <label>Credit Memo Identifier</label>
                <shortLabel>Identifier</shortLabel>

                <maxLength>9</maxLength>
                <validationPattern><numeric /></validationPattern>

                <control>
                    <text size="11" />
                </control>

			</attribute>

            <attribute name="paymentRequestIdentifier" forceUppercase="true">
                <label>Payment Request Identifier</label>
                <shortLabel>Identifier</shortLabel>

                <maxLength>9</maxLength>
                <validationPattern><numeric /></validationPattern>

                <control>
                    <text size="11" />
                </control>

			</attribute>

            <attribute name="versionNumber" forceUppercase="true">
                <label>Version Number</label>
                <shortLabel>Number</shortLabel>

                <maxLength>8</maxLength>
                <validationPattern><numeric /></validationPattern>

                <control>
                    <text size="10" />
                </control>

			</attribute>

        </attributes>
    </businessObject>
</dictionaryEntry>
