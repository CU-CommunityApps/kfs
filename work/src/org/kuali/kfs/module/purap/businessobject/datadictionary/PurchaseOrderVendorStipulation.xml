<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "../dataDictionary-1_0.dtd">
<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.module.purap.bo.PurchaseOrderVendorStipulation">

        <inquiry>
            <title>Purchase Order Vendor Stipulation Inquiry</title>

            <inquirySections><inquirySection title="Header Text"><inquiryFields>
                 <field attributeName="purchaseOrderVendorStipulationIdentifier" />
                <field attributeName="vendorStipulationAuthorEmployeeIdentifier" />
                <field attributeName="vendorStipulationCreateDate" />
                <field attributeName="vendorStipulationDescription" />
            </inquiryFields></inquirySection></inquirySections>
        </inquiry>

        <lookup>
            <title>Purchase Order Vendor Stipulation Lookup</title>

            <instructions>Lookup a Purchase Order Vendor Stipulation</instructions>

            <defaultSort sortAscending="true">
                <sortAttributes>
                    <sortAttribute attributeName="vendorStipulationDescription" />
                </sortAttributes>
            </defaultSort>

            <lookupFields>
                <lookupField attributeName="purchaseOrderVendorStipulationIdentifier" required="false" />
                <lookupField attributeName="vendorStipulationAuthorEmployeeIdentifier" required="false" />
                <lookupField attributeName="vendorStipulationCreateDate" required="false" />
                <lookupField attributeName="vendorStipulationDescription" required="false" />
            </lookupFields>

            <resultFields>
                <field attributeName="purchaseOrderVendorStipulationIdentifier" />
                <field attributeName="vendorStipulationAuthorEmployeeIdentifier" />
                <field attributeName="vendorStipulationCreateDate" />
                <field attributeName="vendorStipulationDescription" />
            </resultFields>
        </lookup>

        <attributes>

            <attribute name="purchaseOrderVendorStipulationIdentifier" forceUppercase="true">
                <label>Purchase Order Vendor Stipulation Identifier</label>
                <shortLabel>Stipulation Identifier</shortLabel>

                <maxLength>10</maxLength>
                <validationPattern><numeric /></validationPattern>

                <control>
                    <text size="12" />
                </control>

			</attribute>

            <attribute name="vendorStipulationAuthorEmployeeIdentifier" forceUppercase="true">
                <label>Vendor Stipulation Author Employee Identifier</label>
                <shortLabel>Stipulation Author Identifier</shortLabel>

                <maxLength>11</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="13" />
                </control>

			</attribute>

            <attribute name="vendorStipulationCreateDate" forceUppercase="true">
                <label>Vendor Stipulation Create Date</label>
                <shortLabel>Date</shortLabel>

                <maxLength>7</maxLength>
                <validationPattern><date /></validationPattern>

                <control>
                    <text size="9" />
                </control>

			</attribute>

            <attribute name="vendorStipulationDescription" forceUppercase="false">
                <label>Note Text</label>
                <shortLabel>Note Text</shortLabel>

                <maxLength>4000</maxLength>
                <validationPattern><anyCharacter allowWhitespace="true" /></validationPattern>
                <required>true</required>

                <control>
                    <textarea rows="5" cols="80"/>
                </control>

			</attribute>

            <attributeReference name="versionNumber" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="versionNumber" />

        </attributes>
    </businessObject>
</dictionaryEntry>
