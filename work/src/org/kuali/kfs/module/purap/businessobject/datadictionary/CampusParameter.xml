<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="CampusParameter" parent="CampusParameter-parentBean" />

  <bean id="CampusParameter-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.module.purap.bo.CampusParameter" />
    <property name="inquiryDefinition" >
      <ref bean="CampusParameter-inquiryDefinition" />
    </property>
    <property name="lookupDefinition" >
      <ref bean="CampusParameter-lookupDefinition" />
    </property>
    <property name="titleAttribute" value="campusCode" />
    <property name="objectLabel" value="Campus Parameter" />
    <property name="attributes" >
      <list>
        <ref bean="CampusParameter-campusAccountsPayableEmailAddress" />
        <ref bean="CampusParameter-campusCode" />
        <ref bean="CampusParameter-campusPurchasingDirectorName" />
        <ref bean="CampusParameter-campusPurchasingDirectorTitle" />
        <ref bean="CampusParameter-purchasingInstitutionName" />
        <ref bean="CampusParameter-purchasingDepartmentName" />
        <ref bean="CampusParameter-purchasingDepartmentLine1Address" />
        <ref bean="CampusParameter-purchasingDepartmentLine2Address" />
        <ref bean="CampusParameter-purchasingDepartmentCityName" />
        <ref bean="CampusParameter-purchasingDepartmentStateCode" />
        <ref bean="CampusParameter-purchasingDepartmentZipCode" />
        <ref bean="CampusParameter-purchasingDepartmentCountryCode" />
        <ref bean="CampusParameter-versionNumber" />
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->


  <bean id="CampusParameter-campusAccountsPayableEmailAddress" parent="CampusParameter-campusAccountsPayableEmailAddress-parentBean" />

  <bean id="CampusParameter-campusAccountsPayableEmailAddress-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="campusAccountsPayableEmailAddress" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Campus Accounts Payable Email Address" />
    <property name="shortLabel" value="Address" />
    <property name="maxLength" value="100" />
    <property name="validationPattern" >
      <bean parent="EmailAddressValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="45" />
    </property>
  </bean>
  <bean id="CampusParameter-campusCode" parent="CampusParameter-campusCode-parentBean" />

  <bean id="CampusParameter-campusCode-parentBean" abstract="true" parent="VendorAttributeReferenceDummy-campusCode">
  </bean>
  <bean id="CampusParameter-campusPurchasingDirectorName" parent="CampusParameter-campusPurchasingDirectorName-parentBean" />

  <bean id="CampusParameter-campusPurchasingDirectorName-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="campusPurchasingDirectorName" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Campus Purchasing Director Name" />
    <property name="shortLabel" value="Name" />
    <property name="maxLength" value="45" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern"
            p:allowWhitespace="true" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="45" />
    </property>
  </bean>
  <bean id="CampusParameter-campusPurchasingDirectorTitle" parent="CampusParameter-campusPurchasingDirectorTitle-parentBean" />

  <bean id="CampusParameter-campusPurchasingDirectorTitle-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="campusPurchasingDirectorTitle" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Campus Purchasing Director Title" />
    <property name="shortLabel" value="Title" />
    <property name="maxLength" value="45" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern"
            p:allowWhitespace="true" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="45" />
    </property>
  </bean>
  <bean id="CampusParameter-purchasingInstitutionName" parent="CampusParameter-purchasingInstitutionName-parentBean" />

  <bean id="CampusParameter-purchasingInstitutionName-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="purchasingInstitutionName" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Purchasing Institution Name" />
    <property name="shortLabel" value="Institution Name" />
    <property name="maxLength" value="45" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern"
            p:allowWhitespace="true" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="45" />
    </property>
  </bean>
  <bean id="CampusParameter-purchasingDepartmentName" parent="CampusParameter-purchasingDepartmentName-parentBean" />

  <bean id="CampusParameter-purchasingDepartmentName-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="purchasingDepartmentName" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Purchasing Department Name" />
    <property name="shortLabel" value="Dept Name" />
    <property name="maxLength" value="45" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern"
            p:allowWhitespace="true" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="45" />
    </property>
  </bean>
  <bean id="CampusParameter-purchasingDepartmentLine1Address" parent="CampusParameter-purchasingDepartmentLine1Address-parentBean" />

  <bean id="CampusParameter-purchasingDepartmentLine1Address-parentBean" abstract="true" parent="VendorAttributeReferenceDummy-line1Address">
    <property name="name" value="purchasingDepartmentLine1Address" />
    <property name="required" value="false" />
  </bean>
  <bean id="CampusParameter-purchasingDepartmentLine2Address" parent="CampusParameter-purchasingDepartmentLine2Address-parentBean" />

  <bean id="CampusParameter-purchasingDepartmentLine2Address-parentBean" abstract="true" parent="VendorAttributeReferenceDummy-line2Address">
    <property name="name" value="purchasingDepartmentLine2Address" />
    <property name="required" value="false" />
  </bean>
  <bean id="CampusParameter-purchasingDepartmentCityName" parent="CampusParameter-purchasingDepartmentCityName-parentBean" />

  <bean id="CampusParameter-purchasingDepartmentCityName-parentBean" abstract="true" parent="VendorAttributeReferenceDummy-cityName">
    <property name="name" value="purchasingDepartmentCityName" />
    <property name="required" value="false" />
  </bean>
  <bean id="CampusParameter-purchasingDepartmentStateCode" parent="CampusParameter-purchasingDepartmentStateCode-parentBean" />

  <bean id="CampusParameter-purchasingDepartmentStateCode-parentBean" abstract="true" parent="VendorAttributeReferenceDummy-stateCode">
    <property name="name" value="purchasingDepartmentStateCode" />
    <property name="required" value="false" />
  </bean>
  <bean id="CampusParameter-purchasingDepartmentZipCode" parent="CampusParameter-purchasingDepartmentZipCode-parentBean" />

  <bean id="CampusParameter-purchasingDepartmentZipCode-parentBean" abstract="true" parent="VendorAttributeReferenceDummy-zipCode">
    <property name="name" value="purchasingDepartmentZipCode" />
    <property name="required" value="false" />
  </bean>
  <bean id="CampusParameter-purchasingDepartmentCountryCode" parent="CampusParameter-purchasingDepartmentCountryCode-parentBean" />

  <bean id="CampusParameter-purchasingDepartmentCountryCode-parentBean" abstract="true" parent="VendorAttributeReferenceDummy-countryCode">
    <property name="name" value="purchasingDepartmentCountryCode" />
    <property name="required" value="false" />
  </bean>
  <bean id="CampusParameter-versionNumber" parent="CampusParameter-versionNumber-parentBean" />

  <bean id="CampusParameter-versionNumber-parentBean" abstract="true" parent="AttributeReferenceDummy-versionNumber">
  </bean>

<!-- Business Object Inquiry Definition -->


  <bean id="CampusParameter-inquiryDefinition" parent="CampusParameter-inquiryDefinition-parentBean" />

  <bean id="CampusParameter-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
    <property name="title" value="Campus Parameter Inquiry" />
    <property name="inquirySections" >
      <list>
        <bean parent="InquirySectionDefinition">
          <property name="title" value="Header Text" />
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields" >
            <list>
              <bean parent="FieldDefinition"
                    p:attributeName="campusCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="campusPurchasingDirectorName" />
              <bean parent="FieldDefinition"
                    p:attributeName="campusPurchasingDirectorTitle" />
              <bean parent="FieldDefinition"
                    p:attributeName="campusAccountsPayableEmailAddress" />
              <bean parent="FieldDefinition"
                    p:attributeName="purchasingInstitutionName" />
              <bean parent="FieldDefinition"
                    p:attributeName="purchasingDepartmentName" />
              <bean parent="FieldDefinition"
                    p:attributeName="purchasingDepartmentLine1Address" />
              <bean parent="FieldDefinition"
                    p:attributeName="purchasingDepartmentLine2Address" />
              <bean parent="FieldDefinition"
                    p:attributeName="purchasingDepartmentCityName" />
              <bean parent="FieldDefinition"
                    p:attributeName="purchasingDepartmentStateCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="purchasingDepartmentZipCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="purchasingDepartmentCountryCode" />
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Business Object Lookup Definition -->


  <bean id="CampusParameter-lookupDefinition" parent="CampusParameter-lookupDefinition-parentBean" />

  <bean id="CampusParameter-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
    <property name="title" value="Campus Parameter Lookup" />
    <property name="instructions" value="Look up a Campus Parameter" />
    <property name="defaultSort" >
      <bean parent="SortDefinition">
      </bean>
    </property>
    <property name="lookupFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="campusCode" />
        <bean parent="FieldDefinition"
              p:attributeName="campusPurchasingDirectorName" />
        <bean parent="FieldDefinition"
              p:attributeName="campusPurchasingDirectorTitle" />
        <bean parent="FieldDefinition"
              p:attributeName="campusAccountsPayableEmailAddress" />
      </list>
    </property>
    <property name="resultFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="campusCode" />
        <bean parent="FieldDefinition"
              p:attributeName="campusPurchasingDirectorName" />
        <bean parent="FieldDefinition"
              p:attributeName="campusPurchasingDirectorTitle" />
        <bean parent="FieldDefinition"
              p:attributeName="campusAccountsPayableEmailAddress" />
      </list>
    </property>
  </bean>
</beans>
