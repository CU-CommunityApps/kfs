<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "../dataDictionary-1_0.dtd">
<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.module.purap.bo.CampusParameter">
        <customAttributeEnabled>false</customAttributeEnabled>

        <inquiry>
            <title>Campus Parameter Inquiry</title>

            <inquiryFields>
                <field attributeName="campusCode" />
                <field attributeName="campusPurchasingDirectorName" />
                <field attributeName="campusPurchasingDirectorTitle" />
                <field attributeName="campusAccountsPayableEmailAddress" />
            </inquiryFields>
        </inquiry>

        <lookup>
            <title>Campus Parameter Lookup</title>

			<menubar><![CDATA[<a href="maintenance.do?methodToCall=start&businessObjectClassName=org.kuali.module.purap.bo.CampusParameter">Create New</a>]]></menubar>

            <instructions>Look up a Campus Parameter</instructions>

            <defaultSort sortAscending="true">
                <sortAttributes>
                    <sortAttribute attributeName="campusCode" />
                </sortAttributes>
            </defaultSort>

            <lookupFields>
                <lookupField attributeName="campusCode" required="false" />
                <lookupField attributeName="campusPurchasingDirectorName" required="false" />
                <lookupField attributeName="campusPurchasingDirectorTitle" required="false" />
                <lookupField attributeName="campusAccountsPayableEmailAddress" required="false" />
            </lookupFields>

            <resultFields>
                <field attributeName="campusCode" />
                <field attributeName="campusPurchasingDirectorName" />
                <field attributeName="campusPurchasingDirectorTitle" />
                <field attributeName="campusAccountsPayableEmailAddress" />
            </resultFields>
        </lookup>

        <attributes>

            <attribute name="campusAccountsPayableEmailAddress" forceUppercase="true">
                <label>Campus Accounts Payable Email Address</label>
                <shortLabel>Address</shortLabel>

                <maxLength>100</maxLength>
                <validationPattern><emailAddress/></validationPattern>

                <control>
                    <text size="102" />
                </control>

                <summary>campusAccountsPayableEmailAddress</summary>
                <description>campusAccountsPayableEmailAddress description...</description>
			</attribute>

            <attribute name="campusCode" forceUppercase="true">
                <label>Campus Code</label>
                <shortLabel>Code</shortLabel>

                <maxLength>2</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="4" />
                </control>

                <summary>campusCode</summary>
                <description>campusCode description...</description>
			</attribute>

            <attribute name="campusPurchasingDirectorName" forceUppercase="true">
                <label>Campus Purchasing Director Name</label>
                <shortLabel>Name</shortLabel>

                <maxLength>45</maxLength>
                <validationPattern><anyCharacter allowWhitespace="true"/></validationPattern>

                <control>
                    <text size="47" />
                </control>

                <summary>campusPurchasingDirectorName</summary>
                <description>campusPurchasingDirectorName description...</description>
			</attribute>

            <attribute name="campusPurchasingDirectorTitle" forceUppercase="true">
                <label>Campus Purchasing Director Title</label>
                <shortLabel>Title</shortLabel>

                <maxLength>45</maxLength>
                <validationPattern><anyCharacter  allowWhitespace="true"/></validationPattern>

                <control>
                    <text size="47" />
                </control>

                <summary>campusPurchasingDirectorTitle</summary>
                <description>campusPurchasingDirectorTitle description...</description>
			</attribute>

            <attributeReference name="versionNumber" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="versionNumber" />

        </attributes>
    </businessObject>
</dictionaryEntry>
