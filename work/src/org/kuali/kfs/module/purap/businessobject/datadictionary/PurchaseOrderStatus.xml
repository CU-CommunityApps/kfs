<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "../dataDictionary-1_0.dtd">
<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.module.purap.bo.PurchaseOrderStatus">
        <customAttributeEnabled>false</customAttributeEnabled>

        <inquiry>
            <title>Purchase Order Status Inquiry</title>

            <inquiryFields>
                <field attributeName="purchaseOrderStatusCode" />
                <field attributeName="purchaseOrderStatusDescription" />
                <field attributeName="dataObjectMaintenanceCodeActiveIndicator" />
            </inquiryFields>
        </inquiry>

        <lookup>
            <title>Purchase Order Status Lookup</title>

			<menubar><![CDATA[<a href="maintenance.do?methodToCall=start&businessObjectClassName=org.kuali.module.purap.bo.PurchaseOrderStatus">Create New</a>]]></menubar>

            <instructions>Lookup a Purchase Order Status</instructions>

            <defaultSort sortAscending="true">
                <sortAttributes>
                    <sortAttribute attributeName="purchaseOrderStatusDescription" />
                </sortAttributes>
            </defaultSort>

            <lookupFields>
                <lookupField attributeName="purchaseOrderStatusCode" required="false" />
                <lookupField attributeName="purchaseOrderStatusDescription" required="false" />
                <lookupField attributeName="dataObjectMaintenanceCodeActiveIndicator" required="false" />
            </lookupFields>

            <resultFields>
                <field attributeName="purchaseOrderStatusCode" />
                <field attributeName="purchaseOrderStatusDescription" />
                <field attributeName="dataObjectMaintenanceCodeActiveIndicator" />
            </resultFields>
        </lookup>

        <attributes>

            <attribute name="purchaseOrderStatusCode" forceUppercase="true">
                <label>Purchase Order Status Code</label>
                <shortLabel>Code</shortLabel>

                <maxLength>4</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="6" />
                </control>

                <summary>purchaseOrderStatusCode</summary>
                <description>purchaseOrderStatusCode description...</description>
			</attribute>

            <attribute name="purchaseOrderStatusDescription">
                <label>Purchase Order Status Description</label>
                <shortLabel>Description</shortLabel>

                <maxLength>45</maxLength>
                <validationPattern><anyCharacter allowWhitespace="true" /></validationPattern>

                <control>
                    <text size="47" />
                </control>

                <summary>purchaseOrderStatusDescription</summary>
                <description>purchaseOrderStatusDescription description...</description>
			</attribute>

            <attributeReference name="dataObjectMaintenanceCodeActiveIndicator" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="activeIndicator" />

            <attributeReference name="versionNumber" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="versionNumber" />

        </attributes>
    </businessObject>
</dictionaryEntry>
