<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "../../../core/datadictionary/dataDictionary-1_0.dtd">
<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.module.purap.bo.PurapGeneralLedgerPendingEntry">

        <inquiry>
            <title>Purchasing General Ledger Pending Entry</title>
            <inquirySections><inquirySection title="Header Text"><inquiryFields>
                <field attributeName="universityFiscalYear" />
                <field attributeName="chartOfAccountsCode" />
                <field attributeName="accountNumber" />
                <field attributeName="subAccountNumber" />
                <field attributeName="financialObjectCode" />
                <field attributeName="financialSubObjectCode" />
                <field attributeName="financialBalanceTypeCode" />
                <field attributeName="financialObjectTypeCode" />
                <field attributeName="universityFiscalPeriodCode" />
                
                <field attributeName="financialSystemOriginationCode" />
                <field attributeName="financialDocumentTypeCode" />
                <field attributeName="documentNumber" />
                <field attributeName="transactionLedgerEntrySequenceNumber" />
                <field attributeName="transactionLedgerEntryDescription" />
                <field attributeName="transactionLedgerEntryAmount" />
                <field attributeName="transactionDebitCreditCode" />
                <field attributeName="transactionDate" />
                <field attributeName="organizationDocumentNumber" />
                <field attributeName="projectCode" />
                <field attributeName="organizationReferenceId" />
                <field attributeName="referenceFinancialDocumentTypeCode" />
                <field attributeName="referenceFinancialSystemOriginationCode" />
                <field attributeName="referenceFinancialDocumentNumber" />
                
                <field attributeName="financialDocumentReversalDate" />
                <field attributeName="transactionEncumbranceUpdateCode" />
                <field attributeName="transactionEntryOffsetIndicator" />
                <field attributeName="financialDocumentApprovedCode" />
                <field attributeName="acctSufficientFundsFinObjCd" />
                <field attributeName="transactionEntryProcessedTs" />
                <field attributeName="documentHeader.financialDocumentStatusCode" />
            </inquiryFields></inquirySection></inquirySections>
        </inquiry>
        <lookup>
            <lookupableID>glPendingEntryLookupable</lookupableID>
            <title>Purchasing General Ledger Pending Entry Lookup</title>
            <menubar>
                <![CDATA[&nbsp;]]>
            </menubar>
            <instructions>Lookup Purchasing General Ledger Pending Entries</instructions>
            <defaultSort attributeName="transactionLedgerEntrySequenceNumber" sortAscending="true" />
            <lookupFields>
                <lookupField attributeName="universityFiscalYear">
                    <defaultValueFinderClass>org.kuali.kfs.lookup.valuefinder.FiscalYearFinder</defaultValueFinderClass>
                </lookupField>
                <lookupField attributeName="chartOfAccountsCode" />
                <lookupField attributeName="accountNumber" />
                <lookupField attributeName="subAccountNumber" />
                <lookupField attributeName="financialObjectCode" />
                <lookupField attributeName="financialSubObjectCode" />
                
                <lookupField attributeName="financialBalanceTypeCode">
                	 <defaultValueFinderClass>org.kuali.module.gl.web.optionfinder.GLActualBalanceTypeOptionFinder</defaultValueFinderClass>
                </lookupField>
                
                <lookupField attributeName="financialObjectTypeCode" />
                <lookupField attributeName="universityFiscalPeriodCode">
                    <defaultValueFinderClass>org.kuali.module.gl.web.optionfinder.GLPeriodCodeOptionFinder</defaultValueFinderClass>
                </lookupField>
                                
                <lookupField attributeName="financialDocumentTypeCode" />
                <lookupField attributeName="financialSystemOriginationCode" />
                <lookupField attributeName="documentNumber" />
                <lookupField attributeName="organizationDocumentNumber" />
                <lookupField attributeName="projectCode" />
                <lookupField attributeName="organizationReferenceId" />
                
                <lookupField attributeName="referenceFinancialDocumentTypeCode" />
                <lookupField attributeName="referenceFinancialSystemOriginationCode" />
                <lookupField attributeName="referenceFinancialDocumentNumber" />
                <lookupField attributeName="documentHeader.financialDocumentStatusCode" />
            </lookupFields>
            
            <resultFields>
                <field attributeName="universityFiscalYear" />
                <field attributeName="chartOfAccountsCode" />
                <field attributeName="accountNumber" />
                <field attributeName="subAccountNumber" />
                <field attributeName="financialObjectCode" />
                <field attributeName="financialSubObjectCode" />

                <field attributeName="financialBalanceTypeCode" />
                <field attributeName="financialObjectTypeCode" />

                <field attributeName="universityFiscalPeriodCode" />

                <field attributeName="financialDocumentTypeCode" />
                <field attributeName="financialSystemOriginationCode" />
                <field attributeName="documentNumber" />

                <field attributeName="transactionLedgerEntryDescription" />
                <field attributeName="transactionLedgerEntryAmount" />

                <field attributeName="transactionDebitCreditCode" />
                <field attributeName="transactionDate" />
                <field attributeName="organizationDocumentNumber" />

                <field attributeName="projectCode" />
                <field attributeName="organizationReferenceId" />

                <field attributeName="referenceFinancialDocumentTypeCode" />
                <field attributeName="referenceFinancialSystemOriginationCode" />
                <field attributeName="referenceFinancialDocumentNumber" />
            </resultFields>
        </lookup>
        <attributes>
            <attributeReference name="universityFiscalYear" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="genericFiscalYear">
                <label>Fiscal Year</label>
                <shortLabel>Fiscal Year</shortLabel>
                
            </attributeReference>

            <attributeReference name="chartOfAccountsCode" sourceClassName="org.kuali.module.chart.bo.Chart" sourceAttributeName="chartOfAccountsCode">
                <control>
                    <text size="2" />
                </control>
                
            </attributeReference>

            <attributeReference name="accountNumber" sourceClassName="org.kuali.module.chart.bo.Account" sourceAttributeName="accountNumber">
                
            </attributeReference>

            <attributeReference name="subAccountNumber" sourceClassName="org.kuali.module.chart.bo.SubAccount" sourceAttributeName="subAccountNumber">
                
            </attributeReference>

            <attributeReference name="financialObjectCode" sourceClassName="org.kuali.module.chart.bo.ObjectCode" sourceAttributeName="financialObjectCode">
                
            </attributeReference>

            <attributeReference name="financialSubObjectCode" sourceClassName="org.kuali.module.chart.bo.SubObjCd" sourceAttributeName="financialSubObjectCode">
                
            </attributeReference>

            <attributeReference name="documentNumber" sourceClassName="org.kuali.core.bo.DocumentHeader" sourceAttributeName="documentNumber">
                
            </attributeReference>

            <attributeReference name="transactionLedgerEntrySequenceNumber" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="transactionEntrySequenceId" />

            <attributeReference name="financialBalanceTypeCode" sourceClassName="org.kuali.module.chart.bo.codes.BalanceTyp" sourceAttributeName="code">
                <label>Balance Type Code</label>
                <shortLabel>Balance Type</shortLabel>
                <maxLength>2</maxLength>
                <control>
                    <text size="2" />
                </control>

            </attributeReference>

            <attributeReference name="transactionDate" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="genericTimestamp">
                <label>Transaction Date</label>
                <shortLabel>Date</shortLabel>

            </attributeReference>

            <attribute name="transactionLedgerEntryDescription" forceUppercase="false">
                <label>Transaction Ledger Entry Description</label>
                <shortLabel>Description</shortLabel>
                <maxLength>40</maxLength>
                <control>
                    <text size="40" />
                </control>

            </attribute>

            <attribute name="organizationReferenceId" forceUppercase="false">
                <label>Organization Reference Id</label>
                <shortLabel>Org. Ref. Id</shortLabel>
                <maxLength>8</maxLength>
                <control>
                    <text size="10" />
                </control>
             </attribute>

            <attributeReference name="financialObjectTypeCode" sourceClassName="org.kuali.module.chart.bo.ObjectType" sourceAttributeName="code">
                
            </attributeReference>

            <attributeReference name="transactionLedgerEntryAmount" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="genericAmount">
                <label>Transaction Ledger Entry Amount</label>
                <shortLabel>Amount</shortLabel>

                <formatterClass>org.kuali.core.web.format.CurrencyFormatter</formatterClass>
            </attributeReference>

            <attribute name="referenceFinancialSystemOriginationCode" forceUppercase="false">
                <label>Reference Origin Code</label>
                <shortLabel>Ref. Origin Code</shortLabel>
                <maxLength>2</maxLength>
                <control>
                    <text size="3" />
                </control>

            </attribute>

            <attribute name="referenceFinancialDocumentTypeCode" forceUppercase="true">
                <label>Reference Document Type Code</label>
                <shortLabel>Ref Doc Type Code</shortLabel>
                <maxLength>4</maxLength>
                <control>
                    <text size="5" />
                </control>

            </attribute>

            <attribute name="referenceFinancialDocumentNumber" forceUppercase="false">
                <label>Reference Document Number</label>
                <shortLabel>Number</shortLabel>
                <maxLength>9</maxLength>
                <control>
                    <text size="11" />
                </control>

            </attribute>

            <attributeReference name="transactionEntryOffsetIndicator" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="genericBoolean">
                <label>Transaction Entry Offset Code</label>
                <shortLabel>Entry Offset Ind.</shortLabel>

            </attributeReference>

            <attribute name="transactionEncumbranceUpdateCode" forceUppercase="true">
                <label>Encumbrance Update Code</label>
                <shortLabel>Encmbr. Updt. Code</shortLabel>
                <maxLength>1</maxLength>
                <control>
                    <text size="3" />
                </control>

            </attribute>

            <attributeReference name="financialDocumentReversalDate" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="genericTimestamp">
                <label>Document Reversal Date</label>
                <shortLabel>Doc. Reversal Date</shortLabel>

            </attributeReference>
			
            <attributeReference name="universityFiscalPeriodCode" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="universityFiscalAccountingPeriod">
                <label>Fiscal Period</label>
                <shortLabel>Fiscal Period</shortLabel>
                <maxLength>2</maxLength>
                <control>
                    <text size="2" />
                </control>

            </attributeReference>
            
            <attribute name="organizationDocumentNumber" forceUppercase="false">
                <label>Organization Document Number</label>
                <shortLabel>Org. Doc. Nbr.</shortLabel>
                <maxLength>10</maxLength>
                <control>
                    <text size="12" />
                </control>

            </attribute>

            <attribute name="financialSystemOriginationCode" forceUppercase="false">
                <label>Origin Code</label>
                <shortLabel>Origin Code</shortLabel>
                <maxLength>2</maxLength>
                <control>
                    <text size="3" />
                </control>

            </attribute>

            <attribute name="financialDocumentTypeCode" forceUppercase="true">
                <label>Document Type Code</label>
                <shortLabel>Doc. Type Code</shortLabel>
                <maxLength>4</maxLength>
                <control>
                    <text size="4" />
                </control>

            </attribute>

            <attribute name="transactionDebitCreditCode" forceUppercase="true">
                <label>Debit Credit Code</label>
                <shortLabel>D/C</shortLabel>
                <maxLength>1</maxLength>
                <control>
                    <text size="2" />
                </control>

            </attribute>

            <attributeReference name="acctSufficientFundsFinObjCd" sourceClassName="org.kuali.module.chart.bo.ObjectCode" sourceAttributeName="financialObjectCode" />
            
            <attributeReference name="projectCode" sourceClassName="org.kuali.module.chart.bo.ProjectCode" sourceAttributeName="code" />
            
            <attributeReference name="versionNumber" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="versionNumber" />
            
            <attributeReference name="transactionEntryProcessedTs" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="genericTimestamp">
                <label>Transaction Entry Processed</label>
                <shortLabel>Entry Processed</shortLabel>

            </attributeReference>
            
            <attribute name="documentHeader.financialDocumentStatusCode" forceUppercase="false">
				<label>Pending Entry Approved Indicator</label>
                <shortLabel>Pending Entry Approved Indicator</shortLabel>

                <maxLength>2</maxLength>
                <control>
                    <radio valuesFinderClass="org.kuali.core.lookup.keyvalues.ApprovedDocumentIndicatorValuesFinder" />
                </control>

                <formatterClass>org.kuali.core.web.format.BooleanFormatter</formatterClass>
            </attribute>
        </attributes>
    </businessObject>
</dictionaryEntry>
