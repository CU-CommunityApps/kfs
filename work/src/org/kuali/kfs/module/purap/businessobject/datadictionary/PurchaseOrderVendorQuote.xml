<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "../dataDictionary-1_0.dtd">
<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.module.purap.bo.PurchaseOrderVendorQuote">

        <inquiry>
            <title>Purchase Order Vendor Quote Inquiry</title>

            <inquirySections><inquirySection title="Header Text"><inquiryFields>
                <field attributeName="purchaseOrderQuoteAwardDate" />
                <field attributeName="purchaseOrderQuotePriceExpirationDate" />
                <field attributeName="purchaseOrderQuoteRankNumber" />
                <field attributeName="purchaseOrderQuoteStatusCode" />
                <field attributeName="purchaseOrderQuoteTransmitDate" />
                <field attributeName="purchaseOrderQuoteTransmitTypeCode" />
                <field attributeName="purchaseOrderVendorQuoteIdentifier" />
                <field attributeName="vendorAttentionName" />
                <field attributeName="vendorCityName" />
                <field attributeName="vendorDetailAssignedIdentifier" />
                <field attributeName="vendorEmailAddress" />
                <field attributeName="vendorFaxNumber" />
                <field attributeName="vendorHeaderGeneratedIdentifier" />
                <field attributeName="vendorLine1Address" />
                <field attributeName="vendorLine2Address" />
                <field attributeName="vendorName" />
                <field attributeName="vendorPhoneNumber" />
                <field attributeName="vendorPostalCode" />
                <field attributeName="vendorStateCode" />
            </inquiryFields></inquirySection></inquirySections>
        </inquiry>

        <lookup>
            <title>Purchase Order Vendor Quote Lookup</title>

            <instructions>Lookup an Purchase Order Vendor Quote</instructions>

            <defaultSort sortAscending="true">
                <sortAttributes>
                    <sortAttribute attributeName="purchaseOrderVendorQuoteIdentifier" />
                </sortAttributes>
            </defaultSort>

            <lookupFields>
                <lookupField attributeName="purchaseOrderQuoteAwardDate" required="false" />
                <lookupField attributeName="purchaseOrderQuotePriceExpirationDate" required="false" />
                <lookupField attributeName="purchaseOrderQuoteRankNumber" required="false" />
                <lookupField attributeName="purchaseOrderQuoteStatusCode" required="false" />
                <lookupField attributeName="purchaseOrderQuoteTransmitDate" required="false" />
                <lookupField attributeName="purchaseOrderQuoteTransmitTypeCode" required="false" />
                <lookupField attributeName="purchaseOrderVendorQuoteIdentifier" required="false" />
                <lookupField attributeName="vendorAttentionName" required="false" />
                <lookupField attributeName="vendorCityName" required="false" />
                <lookupField attributeName="vendorDetailAssignedIdentifier" required="false" />
                <lookupField attributeName="vendorEmailAddress" required="false" />
                <lookupField attributeName="vendorFaxNumber" required="false" />
                <lookupField attributeName="vendorHeaderGeneratedIdentifier" required="false" />
                <lookupField attributeName="vendorLine1Address" required="false" />
                <lookupField attributeName="vendorLine2Address" required="false" />
                <lookupField attributeName="vendorName" required="false" />
                <lookupField attributeName="vendorPhoneNumber" required="false" />
                <lookupField attributeName="vendorPostalCode" required="false" />
                <lookupField attributeName="vendorStateCode" required="false" />
            </lookupFields>

            <resultFields>
                <field attributeName="purchaseOrderQuoteAwardDate" />
                <field attributeName="purchaseOrderQuotePriceExpirationDate" />
                <field attributeName="purchaseOrderQuoteRankNumber" />
                <field attributeName="purchaseOrderQuoteStatusCode" />
                <field attributeName="purchaseOrderQuoteTransmitDate" />
                <field attributeName="purchaseOrderQuoteTransmitTypeCode" />
                <field attributeName="purchaseOrderVendorQuoteIdentifier" />
                <field attributeName="vendorAttentionName" />
                <field attributeName="vendorCityName" />
                <field attributeName="vendorDetailAssignedIdentifier" />
                <field attributeName="vendorEmailAddress" />
                <field attributeName="vendorFaxNumber" />
                <field attributeName="vendorHeaderGeneratedIdentifier" />
                <field attributeName="vendorLine1Address" />
                <field attributeName="vendorLine2Address" />
                <field attributeName="vendorName" />
                <field attributeName="vendorPhoneNumber" />
                <field attributeName="vendorPostalCode" />
                <field attributeName="vendorStateCode" />
            </resultFields>
        </lookup>

        <attributes>

             <attribute name="purchaseOrderQuoteAwardDate" forceUppercase="true">
                <label>Award Date</label>
                <shortLabel>Date</shortLabel>

                <maxLength>10</maxLength>
                <validationPattern><date /></validationPattern>

                <control>
                    <text size="11" />
                </control>

                <formatterClass>org.kuali.core.web.format.DateFormatter</formatterClass>
			</attribute>

            <attribute name="purchaseOrderQuotePriceExpirationDate" forceUppercase="true">
                <label>Pricing Expiration Date</label>
                <shortLabel>Date</shortLabel>

                <maxLength>10</maxLength>
                <validationPattern><date /></validationPattern>

                <control>
                    <text size="11" />
                </control>

                <formatterClass>org.kuali.core.web.format.DateFormatter</formatterClass>
			</attribute>

            <attribute name="purchaseOrderQuoteRankNumber" forceUppercase="true">
                <label>Rank</label>
                <shortLabel>Number</shortLabel>

                <maxLength>3</maxLength>
                <validationPattern><numeric /></validationPattern>

                <control>
                    <text size="5" />
                </control>

			</attribute>

            <attribute name="purchaseOrderQuoteStatusCode" forceUppercase="true">
                <label>Quote Status</label>
                <shortLabel>Code</shortLabel>

                <maxLength>4</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <select valuesFinderClass="org.kuali.module.purap.lookup.keyvalues.PurchaseOrderVendorQuoteStatusCodeValuesFinder" />
                </control>

			</attribute>

            <attribute name="purchaseOrderQuoteTransmitDate" forceUppercase="true">
                <label>Transmit Date</label>
                <shortLabel>Date</shortLabel>

                <maxLength>7</maxLength>
                <validationPattern><date /></validationPattern>

                <control>
                    <text size="9" />
                </control>

			</attribute>

            <attribute name="purchaseOrderQuoteTransmitTypeCode" forceUppercase="true">
                <label>Transmit</label>
                <shortLabel>Code</shortLabel>

                <maxLength>50</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <select valuesFinderClass="org.kuali.module.purap.lookup.keyvalues.PurchaseOrderVendorQuoteTransmitTypeValuesFinder" />
                </control>

			</attribute>

            <attribute name="purchaseOrderVendorQuoteIdentifier" forceUppercase="true">
                <label>Purchase Order Vendor Quote Identifier</label>
                <shortLabel>Identifier</shortLabel>

                <maxLength>9</maxLength>
                <validationPattern><numeric /></validationPattern>

                <control>
                    <text size="11" />
                </control>

			</attribute>

			<attributeReference name="vendorName" sourceClassName="org.kuali.module.vendor.bo.VendorDetail" sourceAttributeName="vendorName" >
				<required>true</required>
			</attributeReference>                 							

			<attributeReference name="vendorNumber" sourceClassName="org.kuali.module.vendor.bo.VendorDetail" sourceAttributeName="vendorNumber" />         							

			<attributeReference name="vendorHeaderGeneratedIdentifier" 
				sourceClassName="org.kuali.module.vendor.bo.VendorDetail" 
				sourceAttributeName="vendorHeaderGeneratedIdentifier" >
				<control>
        	<text size="12" />
        </control>
      </attributeReference>
      
      <attributeReference name="vendorDetailAssignedIdentifier" 
      	sourceClassName="org.kuali.module.vendor.bo.VendorDetail" 
      	sourceAttributeName="vendorDetailAssignedIdentifier" > 
				<control>
        	<text size="12" />
        </control>
      </attributeReference>

			<attributeReference name="vendorAttentionName" sourceClassName="org.kuali.module.vendor.bo.VendorAddress" sourceAttributeName="vendorAttentionName" />
			<attributeReference name="vendorLine1Address" sourceClassName="org.kuali.module.vendor.bo.VendorAddress" sourceAttributeName="vendorLine1Address" />
			<attributeReference name="vendorLine2Address" sourceClassName="org.kuali.module.vendor.bo.VendorAddress" sourceAttributeName="vendorLine2Address" />
			<attributeReference name="vendorCityName" sourceClassName="org.kuali.module.vendor.bo.VendorAddress" sourceAttributeName="vendorCityName" />					
			<attributeReference name="vendorStateCode" sourceClassName="org.kuali.module.vendor.bo.VendorAddress" sourceAttributeName="vendorStateCode" />							
			<attributeReference name="vendorPostalCode" sourceClassName="org.kuali.module.vendor.bo.VendorAddress" sourceAttributeName="vendorZipCode" >
				<required>true</required>
			</attributeReference>                 				
			<attributeReference name="vendorCountryCode" sourceClassName="org.kuali.module.vendor.bo.VendorAddress" sourceAttributeName="vendorCountryCode" >	
				<required>false</required>
			</attributeReference>                 							
			<attributeReference name="vendorEmailAddress" sourceClassName="org.kuali.module.vendor.bo.VendorAddress" sourceAttributeName="vendorAddressEmailAddress" />
			<attributeReference name="vendorFaxNumber" sourceClassName="org.kuali.module.vendor.bo.VendorAddress" sourceAttributeName="vendorFaxNumber" />
      <attributeReference name="vendorPhoneNumber" sourceClassName="org.kuali.module.vendor.bo.VendorAttributeReferenceDummy"	sourceAttributeName="phoneNumberNoValidation">
      	<label>Vendor Phone Number</label>
        <shortLabel>Vendor Phone</shortLabel>
			</attributeReference>      
			<attributeReference name="versionNumber" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="versionNumber" />

        </attributes>
    </businessObject>
</dictionaryEntry>
