<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "../dataDictionary-1_0.dtd" [
<!ENTITY purApItemBaseAttributes SYSTEM "../dictionary/entity/PurApItemBaseAttributes.xml">
]>
<dictionaryEntry>
	<businessObject businessObjectClass="org.kuali.module.purap.bo.PaymentRequestItem">

		<inquiry>
			<title>Payment Request Item Inquiry</title>

			<inquirySections>
				<inquirySection title="Header Text">
					<inquiryFields>
						<field attributeName="capitalAssetTransactionTypeCode" />
						<field attributeName="itemAssignedToTradeInIndicator" />
						<field attributeName="itemAuxiliaryPartIdentifier" />
						<field attributeName="itemCapitalAssetNoteText" />
						<field attributeName="itemCatalogNumber" />
						<field attributeName="itemDescription" />
						<field attributeName="itemLineNumber" />
						<field attributeName="itemQuantity" />
						<field attributeName="itemTypeCode" />
						<field attributeName="itemUnitOfMeasureCode" />
						<field attributeName="itemUnitPrice" />
						<field attributeName="purapDocumentIdentifier" />
						<field attributeName="itemIdentifier" />
						<field attributeName="purchaseOrderItemUnitPrice" />
						<field attributeName="originalAmountfromPO" />
						<field attributeName="itemOutstandingInvoiceQuantity" />
						<field attributeName="itemOutstandingInvoiceAmount" />
					</inquiryFields>
				</inquirySection>
			</inquirySections>
		</inquiry>

		<lookup>
			<title>Payment Request Item Lookup</title>

			<instructions>Lookup a Payment Request Item</instructions>

			<defaultSort sortAscending="true">
				<sortAttributes>
					<sortAttribute attributeName="itemDescription" />
				</sortAttributes>
			</defaultSort>

			<lookupFields>
				<lookupField attributeName="capitalAssetTransactionTypeCode" required="false" />
				<lookupField attributeName="itemAssignedToTradeInIndicator" required="false" />
				<lookupField attributeName="itemAuxiliaryPartIdentifier" required="false" />
				<lookupField attributeName="itemCapitalAssetNoteText" required="false" />
				<lookupField attributeName="itemCatalogNumber" required="false" />
				<lookupField attributeName="itemDescription" required="false" />
				<lookupField attributeName="itemLineNumber" required="false" />
				<lookupField attributeName="itemQuantity" required="false" />
				<lookupField attributeName="itemTypeCode" required="false" />
				<lookupField attributeName="itemUnitOfMeasureCode" required="false" />
				<lookupField attributeName="itemUnitPrice" required="false" />
				<lookupField attributeName="purapDocumentIdentifier" required="false" />
				<lookupField attributeName="itemIdentifier" required="false" />
				<lookupField attributeName="purchaseOrderItemUnitPrice" required="false" />
				<lookupField attributeName="originalAmountfromPO" required="false" />
				<lookupField attributeName="itemOutstandingInvoiceQuantity" required="false" />
				<lookupField attributeName="itemOutstandingInvoiceAmount" required="false" />
			</lookupFields>

			<resultFields>
				<field attributeName="capitalAssetTransactionTypeCode" />
				<field attributeName="itemAssignedToTradeInIndicator" />
				<field attributeName="itemAuxiliaryPartIdentifier" />
				<field attributeName="itemCapitalAssetNoteText" />
				<field attributeName="itemCatalogNumber" />
				<field attributeName="itemDescription" />
				<field attributeName="itemLineNumber" />
				<field attributeName="itemQuantity" />
				<field attributeName="itemTypeCode" />
				<field attributeName="itemUnitOfMeasureCode" />
				<field attributeName="itemUnitPrice" />
				<field attributeName="purapDocumentIdentifier" />
				<field attributeName="itemIdentifier" />
				<field attributeName="purchaseOrderItemUnitPrice" />
				<field attributeName="originalAmountfromPO" />
				<field attributeName="itemOutstandingInvoiceQuantity" />
				<field attributeName="itemOutstandingInvoiceAmount" />
			</resultFields>
		</lookup>

		<attributes>

			&purApItemBaseAttributes;

			<attributeReference name="purchaseOrderItemUnitPrice" sourceClassName="org.kuali.module.purap.bo.PurApAttributeReferenceDummy" sourceAttributeName="genericItemUnitPrice">
				<label>Purchase Order Item Unit Price</label>
				<shortLabel>PO Unit/Ext Price</shortLabel>

			</attributeReference>
			
			<attributeReference name="originalAmountfromPO" sourceClassName="org.kuali.module.purap.bo.PurApAttributeReferenceDummy" sourceAttributeName="genericItemUnitPrice">
				<label>Original Amount from Purchase Order</label>
				<shortLabel>Original Amount from PO</shortLabel>

			</attributeReference>

			<attributeReference name="itemOutstandingInvoiceQuantity" sourceClassName="org.kuali.module.purap.bo.PurApAttributeReferenceDummy" sourceAttributeName="genericItemQuantity">
				<label>Item Outstanding Invoice Quantity</label>
				<shortLabel>Open Qty</shortLabel>

			</attributeReference>

			<attributeReference name="itemOutstandingInvoiceAmount" sourceClassName="org.kuali.module.purap.bo.PurApAttributeReferenceDummy" sourceAttributeName="genericItemAmount">
				<label>Item Outstanding Invoiced Amount</label>
				<shortLabel>Outstanding Invoiced Amount</shortLabel>

			</attributeReference>

			<attributeReference name="poOutstandingQuantity" sourceClassName="org.kuali.module.purap.bo.PurApAttributeReferenceDummy" sourceAttributeName="genericItemQuantity">
				<label>Item Open Quantity</label>
				<shortLabel>Open Qty</shortLabel>

			</attributeReference>
			
			<attributeReference name="poOutstandingAmount" sourceClassName="org.kuali.module.purap.bo.PurApAttributeReferenceDummy" sourceAttributeName="genericItemAmount">
				<label>Item Outstanding Encumbered Amount</label>
				<shortLabel>Outstanding Encumbered Amount</shortLabel>

			</attributeReference>

			<attributeReference name="itemQuantity" sourceClassName="org.kuali.module.purap.bo.PurApAttributeReferenceDummy" sourceAttributeName="genericItemQuantity">
				<label>Quantity Invoiced</label>
				<shortLabel>Qty Invoiced</shortLabel>

			</attributeReference>

		</attributes>
	</businessObject>
</dictionaryEntry>
