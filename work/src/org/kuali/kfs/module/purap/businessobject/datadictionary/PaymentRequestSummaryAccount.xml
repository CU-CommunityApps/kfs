<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="PaymentRequestSummaryAccount" parent="PaymentRequestSummaryAccount-parentBean" />

  <bean id="PaymentRequestSummaryAccount-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.kfs.module.purap.businessobject.PaymentRequestSummaryAccount" />
    <property name="inquiryDefinition" >
      <ref bean="PaymentRequestSummaryAccount-inquiryDefinition" />
    </property>
    <property name="lookupDefinition" >
      <ref bean="PaymentRequestSummaryAccount-lookupDefinition" />
    </property>
    <property name="attributes" >
      <list>
        <ref bean="PaymentRequestSummaryAccount-accountIdentifier" />
        <ref bean="PaymentRequestSummaryAccount-accountLinePercent" />
        <ref bean="PaymentRequestSummaryAccount-accountNumber" />
        <ref bean="PaymentRequestSummaryAccount-amount" />
        <ref bean="PaymentRequestSummaryAccount-chartOfAccountsCode" />
        <ref bean="PaymentRequestSummaryAccount-disencumberedAmount" />
        <ref bean="PaymentRequestSummaryAccount-financialObjectCode" />
        <ref bean="PaymentRequestSummaryAccount-financialSubObjectCode" />
        <ref bean="PaymentRequestSummaryAccount-organizationReferenceId" />
        <ref bean="PaymentRequestSummaryAccount-projectCode" />
        <ref bean="PaymentRequestSummaryAccount-purapDocumentIdentifier" />
        <ref bean="PaymentRequestSummaryAccount-subAccountNumber" />
        <ref bean="PaymentRequestSummaryAccount-versionNumber" />
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->


  <bean id="PaymentRequestSummaryAccount-accountIdentifier" parent="PaymentRequestSummaryAccount-accountIdentifier-parentBean" />

  <bean id="PaymentRequestSummaryAccount-accountIdentifier-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="accountIdentifier" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Account Identifier" />
    <property name="shortLabel" value="Identifier" />
    <property name="maxLength" value="10" />
    <property name="validationPattern" >
      <bean parent="NumericValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="12" />
    </property>
  </bean>
  <bean id="PaymentRequestSummaryAccount-accountLinePercent" parent="PaymentRequestSummaryAccount-accountLinePercent-parentBean" />

  <bean id="PaymentRequestSummaryAccount-accountLinePercent-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="accountLinePercent" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Account Line Percent" />
    <property name="shortLabel" value="Percent" />
    <property name="maxLength" value="35" />
    <property name="validationPattern" >
      <bean parent="FloatingPointValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="37" />
    </property>
  </bean>
  <bean id="PaymentRequestSummaryAccount-accountNumber" parent="PaymentRequestSummaryAccount-accountNumber-parentBean" />

  <bean id="PaymentRequestSummaryAccount-accountNumber-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="accountNumber" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Account Number" />
    <property name="shortLabel" value="Number" />
    <property name="maxLength" value="7" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="9" />
    </property>
  </bean>
  <bean id="PaymentRequestSummaryAccount-amount" parent="PaymentRequestSummaryAccount-amount-parentBean" />

  <bean id="PaymentRequestSummaryAccount-amount-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="amount" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Amount" />
    <property name="shortLabel" value="Amount" />
    <property name="maxLength" value="19" />
    <property name="validationPattern" >
      <bean parent="FloatingPointValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="21" />
    </property>
  </bean>
  <bean id="PaymentRequestSummaryAccount-chartOfAccountsCode" parent="PaymentRequestSummaryAccount-chartOfAccountsCode-parentBean" />

  <bean id="PaymentRequestSummaryAccount-chartOfAccountsCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="chartOfAccountsCode" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Chart Of Accounts Code" />
    <property name="shortLabel" value="Code" />
    <property name="maxLength" value="2" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="4" />
    </property>
  </bean>
  <bean id="PaymentRequestSummaryAccount-disencumberedAmount" parent="PaymentRequestSummaryAccount-disencumberedAmount-parentBean" />

  <bean id="PaymentRequestSummaryAccount-disencumberedAmount-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="disencumberedAmount" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Disencumbered Amount" />
    <property name="shortLabel" value="Amount" />
    <property name="maxLength" value="19" />
    <property name="validationPattern" >
      <bean parent="FloatingPointValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="21" />
    </property>
  </bean>
  <bean id="PaymentRequestSummaryAccount-financialObjectCode" parent="PaymentRequestSummaryAccount-financialObjectCode-parentBean" />

  <bean id="PaymentRequestSummaryAccount-financialObjectCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="financialObjectCode" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Financial Object Code" />
    <property name="shortLabel" value="Code" />
    <property name="maxLength" value="4" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="6" />
    </property>
  </bean>
  <bean id="PaymentRequestSummaryAccount-financialSubObjectCode" parent="PaymentRequestSummaryAccount-financialSubObjectCode-parentBean" />

  <bean id="PaymentRequestSummaryAccount-financialSubObjectCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="financialSubObjectCode" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Financial Sub Object Code" />
    <property name="shortLabel" value="Code" />
    <property name="maxLength" value="3" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="5" />
    </property>
  </bean>
  <bean id="PaymentRequestSummaryAccount-organizationReferenceId" parent="PaymentRequestSummaryAccount-organizationReferenceId-parentBean" />

  <bean id="PaymentRequestSummaryAccount-organizationReferenceId-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="organizationReferenceId" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Organization Reference Id" />
    <property name="shortLabel" value="Id" />
    <property name="maxLength" value="8" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="10" />
    </property>
  </bean>
  <bean id="PaymentRequestSummaryAccount-projectCode" parent="PaymentRequestSummaryAccount-projectCode-parentBean" />

  <bean id="PaymentRequestSummaryAccount-projectCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="projectCode" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Project Code" />
    <property name="shortLabel" value="Code" />
    <property name="maxLength" value="10" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="12" />
    </property>
  </bean>
  <bean id="PaymentRequestSummaryAccount-purapDocumentIdentifier" parent="PaymentRequestSummaryAccount-purapDocumentIdentifier-parentBean" />

  <bean id="PaymentRequestSummaryAccount-purapDocumentIdentifier-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="purapDocumentIdentifier" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Purap Document Identifier" />
    <property name="shortLabel" value="Identifier" />
    <property name="maxLength" value="9" />
    <property name="validationPattern" >
      <bean parent="NumericValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="11" />
    </property>
  </bean>
  <bean id="PaymentRequestSummaryAccount-subAccountNumber" parent="PaymentRequestSummaryAccount-subAccountNumber-parentBean" />

  <bean id="PaymentRequestSummaryAccount-subAccountNumber-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="subAccountNumber" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Sub Account Number" />
    <property name="shortLabel" value="Number" />
    <property name="maxLength" value="5" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="7" />
    </property>
  </bean>
  <bean id="PaymentRequestSummaryAccount-versionNumber" parent="PaymentRequestSummaryAccount-versionNumber-parentBean" />

  <bean id="PaymentRequestSummaryAccount-versionNumber-parentBean" abstract="true" parent="GenericAttributes-versionNumber">
  </bean>

<!-- Business Object Inquiry Definition -->


  <bean id="PaymentRequestSummaryAccount-inquiryDefinition" parent="PaymentRequestSummaryAccount-inquiryDefinition-parentBean" />

  <bean id="PaymentRequestSummaryAccount-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
    <property name="title" value="Payment Request Summary Account Inquiry" />
    <property name="inquirySections" >
      <list>
        <bean parent="InquirySectionDefinition">
          <property name="title" value="Header Text" />
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields" >
            <list>
              <bean parent="FieldDefinition"
                    p:attributeName="accountIdentifier" />
              <bean parent="FieldDefinition"
                    p:attributeName="accountLinePercent" />
              <bean parent="FieldDefinition"
                    p:attributeName="accountNumber" />
              <bean parent="FieldDefinition"
                    p:attributeName="amount" />
              <bean parent="FieldDefinition"
                    p:attributeName="chartOfAccountsCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="disencumberedAmount" />
              <bean parent="FieldDefinition"
                    p:attributeName="financialObjectCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="financialSubObjectCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="organizationReferenceId" />
              <bean parent="FieldDefinition"
                    p:attributeName="projectCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="purapDocumentIdentifier" />
              <bean parent="FieldDefinition"
                    p:attributeName="subAccountNumber" />
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Business Object Lookup Definition -->


  <bean id="PaymentRequestSummaryAccount-lookupDefinition" parent="PaymentRequestSummaryAccount-lookupDefinition-parentBean" />

  <bean id="PaymentRequestSummaryAccount-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
    <property name="title" value="Payment Request Summary Account Lookup" />
    <property name="instructions" value="Lookup an Payment Request Summary Account" />
    <property name="defaultSort" >
      <bean parent="SortDefinition">
      </bean>
    </property>
    <property name="lookupFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="accountIdentifier" />
        <bean parent="FieldDefinition"
              p:attributeName="accountLinePercent" />
        <bean parent="FieldDefinition"
              p:attributeName="accountNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="amount" />
        <bean parent="FieldDefinition"
              p:attributeName="chartOfAccountsCode" />
        <bean parent="FieldDefinition"
              p:attributeName="disencumberedAmount" />
        <bean parent="FieldDefinition"
              p:attributeName="financialObjectCode" />
        <bean parent="FieldDefinition"
              p:attributeName="financialSubObjectCode" />
        <bean parent="FieldDefinition"
              p:attributeName="organizationReferenceId" />
        <bean parent="FieldDefinition"
              p:attributeName="projectCode" />
        <bean parent="FieldDefinition"
              p:attributeName="purapDocumentIdentifier" />
        <bean parent="FieldDefinition"
              p:attributeName="subAccountNumber" />
      </list>
    </property>
    <property name="resultFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="accountIdentifier" />
        <bean parent="FieldDefinition"
              p:attributeName="accountLinePercent" />
        <bean parent="FieldDefinition"
              p:attributeName="accountNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="amount" />
        <bean parent="FieldDefinition"
              p:attributeName="chartOfAccountsCode" />
        <bean parent="FieldDefinition"
              p:attributeName="disencumberedAmount" />
        <bean parent="FieldDefinition"
              p:attributeName="financialObjectCode" />
        <bean parent="FieldDefinition"
              p:attributeName="financialSubObjectCode" />
        <bean parent="FieldDefinition"
              p:attributeName="organizationReferenceId" />
        <bean parent="FieldDefinition"
              p:attributeName="projectCode" />
        <bean parent="FieldDefinition"
              p:attributeName="purapDocumentIdentifier" />
        <bean parent="FieldDefinition"
              p:attributeName="subAccountNumber" />
      </list>
    </property>
  </bean>
</beans>
