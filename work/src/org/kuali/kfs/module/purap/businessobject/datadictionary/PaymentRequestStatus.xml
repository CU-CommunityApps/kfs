<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "../dataDictionary-1_0.dtd">
<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.module.purap.bo.PaymentRequestStatus">
        <customAttributeEnabled>false</customAttributeEnabled>

        <inquiry>
            <title>Payment Request Status Inquiry</title>

            <inquiryFields>
                <field attributeName="paymentRequestStatusCode" />
                <field attributeName="paymentRequestStatusDescription" />
                <field attributeName="dataObjectMaintenanceCodeActiveIndicator" />
            </inquiryFields>
        </inquiry>

        <lookup>
            <title>Payment Request Status Lookup</title>

			<menubar><![CDATA[<a href="maintenance.do?methodToCall=start&businessObjectClassName=org.kuali.module.purap.bo.PaymentRequestStatus">Create New</a>]]></menubar>

            <instructions>Lookup a Payment Request Status</instructions>

            <defaultSort sortAscending="true">
                <sortAttributes>
                    <sortAttribute attributeName="paymentRequestStatusDescription" />
                </sortAttributes>
            </defaultSort>

            <lookupFields>
                <lookupField attributeName="paymentRequestStatusCode" required="false" />
                <lookupField attributeName="paymentRequestStatusDescription" required="false" />
                <lookupField attributeName="dataObjectMaintenanceCodeActiveIndicator" required="false" />
            </lookupFields>

            <resultFields>
                <field attributeName="paymentRequestStatusCode" />
                <field attributeName="paymentRequestStatusDescription" />
                <field attributeName="dataObjectMaintenanceCodeActiveIndicator" />
            </resultFields>
        </lookup>

        <attributes>
            <attribute name="paymentRequestStatusCode" forceUppercase="true">
                <label>Payment Request Status Code</label>
                <shortLabel>Code</shortLabel>

                <maxLength>4</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="6" />
                </control>

                <summary>paymentRequestStatusCode</summary>
                <description>paymentRequestStatusCode description...</description>
			</attribute>

            <attribute name="paymentRequestStatusDescription">
                <label>Payment Request Status Description</label>
                <shortLabel>Description</shortLabel>

                <maxLength>45</maxLength>
                <validationPattern><anyCharacter allowWhitespace="true"/></validationPattern>

                <control>
                    <text size="47" />
                </control>

                <summary>paymentRequestStatusDescription</summary>
                <description>paymentRequestStatusDescription description...</description>
			</attribute>

            <attributeReference name="dataObjectMaintenanceCodeActiveIndicator" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="activeIndicator" />

            <attributeReference name="versionNumber" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="versionNumber" />


        </attributes>
    </businessObject>
</dictionaryEntry>
