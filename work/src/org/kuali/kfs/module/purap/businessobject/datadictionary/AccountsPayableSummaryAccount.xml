<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="AccountsPayableSummaryAccount" parent="AccountsPayableSummaryAccount-parentBean" />

  <bean id="AccountsPayableSummaryAccount-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.kfs.module.purap.businessobject.AccountsPayableSummaryAccount" />
    <property name="inquiryDefinition" >
      <ref bean="AccountsPayableSummaryAccount-inquiryDefinition" />
    </property>
    <property name="lookupDefinition" >
      <ref bean="AccountsPayableSummaryAccount-lookupDefinition" />
    </property>
    <property name="attributes" >
      <list>
        <ref bean="AccountsPayableSummaryAccount-accountIdentifier" />
        <ref bean="AccountsPayableSummaryAccount-accountLinePercent" />
        <ref bean="AccountsPayableSummaryAccount-accountNumber" />
        <ref bean="AccountsPayableSummaryAccount-amount" />
        <ref bean="AccountsPayableSummaryAccount-chartOfAccountsCode" />
        <ref bean="AccountsPayableSummaryAccount-disencumberedAmount" />
        <ref bean="AccountsPayableSummaryAccount-financialObjectCode" />
        <ref bean="AccountsPayableSummaryAccount-financialSubObjectCode" />
        <ref bean="AccountsPayableSummaryAccount-organizationReferenceId" />
        <ref bean="AccountsPayableSummaryAccount-projectCode" />
        <ref bean="AccountsPayableSummaryAccount-purapDocumentIdentifier" />
        <ref bean="AccountsPayableSummaryAccount-subAccountNumber" />
        <ref bean="AccountsPayableSummaryAccount-versionNumber" />
        <ref bean="AccountsPayableSummaryAccount-creditMemoId" />
        <ref bean="AccountsPayableSummaryAccount-updateTimestamp" />  
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->


  <bean id="AccountsPayableSummaryAccount-accountIdentifier" parent="AccountsPayableSummaryAccount-accountIdentifier-parentBean" />

  <bean id="AccountsPayableSummaryAccount-accountIdentifier-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="accountIdentifier" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Account Identifier" />
    <property name="shortLabel" value="Identifier" />
    <property name="maxLength" value="10" />
    <property name="validationPattern" >
      <bean parent="NumericValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="12" />
    </property>
  </bean>
  <bean id="AccountsPayableSummaryAccount-accountLinePercent" parent="AccountsPayableSummaryAccount-accountLinePercent-parentBean" />

  <bean id="AccountsPayableSummaryAccount-accountLinePercent-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="accountLinePercent" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Account Line Percent" />
    <property name="shortLabel" value="Percent" />
    <property name="maxLength" value="35" />
    <property name="validationPattern" >
      <bean parent="FloatingPointValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="37" />
    </property>
  </bean>
  <bean id="AccountsPayableSummaryAccount-accountNumber" parent="AccountsPayableSummaryAccount-accountNumber-parentBean" />

  <bean id="AccountsPayableSummaryAccount-accountNumber-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="accountNumber" />
    <property name="label" value="Account Number" />
    <property name="shortLabel" value="Number" />
    <property name="maxLength" value="7" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="9" />
    </property>
  </bean>
  <bean id="AccountsPayableSummaryAccount-amount" parent="AccountsPayableSummaryAccount-amount-parentBean" />

  <bean id="AccountsPayableSummaryAccount-amount-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="amount" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Amount" />
    <property name="shortLabel" value="Amount" />
    <property name="maxLength" value="19" />
    <property name="validationPattern" >
      <bean parent="FloatingPointValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="21" />
    </property>
  </bean>
  <bean id="AccountsPayableSummaryAccount-chartOfAccountsCode" parent="AccountsPayableSummaryAccount-chartOfAccountsCode-parentBean" />

  <bean id="AccountsPayableSummaryAccount-chartOfAccountsCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="chartOfAccountsCode" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Chart Of Accounts Code" />
    <property name="shortLabel" value="Code" />
    <property name="maxLength" value="2" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="4" />
    </property>
  </bean>
  <bean id="AccountsPayableSummaryAccount-disencumberedAmount" parent="AccountsPayableSummaryAccount-disencumberedAmount-parentBean" />

  <bean id="AccountsPayableSummaryAccount-disencumberedAmount-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="disencumberedAmount" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Disencumbered Amount" />
    <property name="shortLabel" value="Amount" />
    <property name="maxLength" value="19" />
    <property name="validationPattern" >
      <bean parent="FloatingPointValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="21" />
    </property>
  </bean>
  <bean id="AccountsPayableSummaryAccount-financialObjectCode" parent="AccountsPayableSummaryAccount-financialObjectCode-parentBean" />

  <bean id="AccountsPayableSummaryAccount-financialObjectCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="financialObjectCode" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Financial Object Code" />
    <property name="shortLabel" value="Code" />
    <property name="maxLength" value="4" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="6" />
    </property>
  </bean>
  <bean id="AccountsPayableSummaryAccount-financialSubObjectCode" parent="AccountsPayableSummaryAccount-financialSubObjectCode-parentBean" />

  <bean id="AccountsPayableSummaryAccount-financialSubObjectCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="financialSubObjectCode" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Financial Sub Object Code" />
    <property name="shortLabel" value="Code" />
    <property name="maxLength" value="3" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="5" />
    </property>
  </bean>
  <bean id="AccountsPayableSummaryAccount-organizationReferenceId" parent="AccountsPayableSummaryAccount-organizationReferenceId-parentBean" />

  <bean id="AccountsPayableSummaryAccount-organizationReferenceId-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="organizationReferenceId" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Organization Reference Id" />
    <property name="shortLabel" value="Id" />
    <property name="maxLength" value="8" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="10" />
    </property>
  </bean>
  <bean id="AccountsPayableSummaryAccount-projectCode" parent="AccountsPayableSummaryAccount-projectCode-parentBean" />

  <bean id="AccountsPayableSummaryAccount-projectCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="projectCode" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Project Code" />
    <property name="shortLabel" value="Code" />
    <property name="maxLength" value="10" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="12" />
    </property>
  </bean>
  <bean id="AccountsPayableSummaryAccount-purapDocumentIdentifier" parent="AccountsPayableSummaryAccount-purapDocumentIdentifier-parentBean" />

  <bean id="AccountsPayableSummaryAccount-purapDocumentIdentifier-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="purapDocumentIdentifier" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Purap Document Identifier" />
    <property name="shortLabel" value="Identifier" />
    <property name="maxLength" value="9" />
    <property name="validationPattern" >
      <bean parent="NumericValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="11" />
    </property>
  </bean>
  <bean id="AccountsPayableSummaryAccount-subAccountNumber" parent="AccountsPayableSummaryAccount-subAccountNumber-parentBean" />

  <bean id="AccountsPayableSummaryAccount-subAccountNumber-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="subAccountNumber" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Sub Account Number" />
    <property name="shortLabel" value="Number" />
    <property name="maxLength" value="5" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="7" />
    </property>
  </bean>
  <bean id="AccountsPayableSummaryAccount-versionNumber" parent="AccountsPayableSummaryAccount-versionNumber-parentBean" />

  <bean id="AccountsPayableSummaryAccount-versionNumber-parentBean" abstract="true" parent="GenericAttributes-versionNumber">
  </bean>
 
  <bean id="AccountsPayableSummaryAccount-creditMemoId" parent="AccountsPayableSummaryAccount-creditMemoId-parentBean" />
 
  <bean id="AccountsPayableSummaryAccount-creditMemoId-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="creditMemoId" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Credit Memo Number" />
    <property name="shortLabel" value="Credit Memo" />
    <property name="maxLength" value="9" />
    <property name="validationPattern" >
      <bean parent="NumericValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="12" />
    </property>
  </bean>
  
  <bean id="AccountsPayableSummaryAccount-updateTimestamp" parent="AccountsPayableSummaryAccount-updateTimestamp-parentBean" />

  <bean id="AccountsPayableSummaryAccount-updateTimestamp-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="updateTimestamp" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Update Timestamp" />
    <property name="shortLabel" value="Timestamp" />
    <property name="validationPattern" >
      <bean parent="DateValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="12" />
    </property>
  </bean>

<!-- Business Object Inquiry Definition -->


  <bean id="AccountsPayableSummaryAccount-inquiryDefinition" parent="AccountsPayableSummaryAccount-inquiryDefinition-parentBean" />

  <bean id="AccountsPayableSummaryAccount-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
    <property name="title" value="Payment Request Summary Account Inquiry" />
    <property name="inquirySections" >
      <list>
        <bean parent="InquirySectionDefinition">
          <property name="title" value="" />
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields" >
            <list>
              <bean parent="FieldDefinition"
                    p:attributeName="accountIdentifier" />
              <bean parent="FieldDefinition"
                    p:attributeName="accountLinePercent" />
              <bean parent="FieldDefinition"
                    p:attributeName="accountNumber" />
              <bean parent="FieldDefinition"
                    p:attributeName="amount" />
              <bean parent="FieldDefinition"
                    p:attributeName="chartOfAccountsCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="disencumberedAmount" />
              <bean parent="FieldDefinition"
                    p:attributeName="financialObjectCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="financialSubObjectCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="organizationReferenceId" />
              <bean parent="FieldDefinition"
                    p:attributeName="projectCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="purapDocumentIdentifier" />
              <bean parent="FieldDefinition"
                    p:attributeName="subAccountNumber" />
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Business Object Lookup Definition -->


  <bean id="AccountsPayableSummaryAccount-lookupDefinition" parent="AccountsPayableSummaryAccount-lookupDefinition-parentBean" />

  <bean id="AccountsPayableSummaryAccount-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
    <property name="title" value="Payment Request Summary Account Lookup" />
    <property name="instructions" value="Lookup an Payment Request Summary Account" />
    <property name="defaultSort" >
      <bean parent="SortDefinition">
      </bean>
    </property>
    <property name="lookupFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="accountIdentifier" />
        <bean parent="FieldDefinition"
              p:attributeName="accountLinePercent" />
        <bean parent="FieldDefinition"
              p:attributeName="accountNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="amount" />
        <bean parent="FieldDefinition"
              p:attributeName="chartOfAccountsCode" />
        <bean parent="FieldDefinition"
              p:attributeName="disencumberedAmount" />
        <bean parent="FieldDefinition"
              p:attributeName="financialObjectCode" />
        <bean parent="FieldDefinition"
              p:attributeName="financialSubObjectCode" />
        <bean parent="FieldDefinition"
              p:attributeName="organizationReferenceId" />
        <bean parent="FieldDefinition"
              p:attributeName="projectCode" />
        <bean parent="FieldDefinition"
              p:attributeName="purapDocumentIdentifier" />
        <bean parent="FieldDefinition"
              p:attributeName="subAccountNumber" />
      </list>
    </property>
    <property name="resultFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="accountIdentifier" />
        <bean parent="FieldDefinition"
              p:attributeName="accountLinePercent" />
        <bean parent="FieldDefinition"
              p:attributeName="accountNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="amount" />
        <bean parent="FieldDefinition"
              p:attributeName="chartOfAccountsCode" />
        <bean parent="FieldDefinition"
              p:attributeName="disencumberedAmount" />
        <bean parent="FieldDefinition"
              p:attributeName="financialObjectCode" />
        <bean parent="FieldDefinition"
              p:attributeName="financialSubObjectCode" />
        <bean parent="FieldDefinition"
              p:attributeName="organizationReferenceId" />
        <bean parent="FieldDefinition"
              p:attributeName="projectCode" />
        <bean parent="FieldDefinition"
              p:attributeName="purapDocumentIdentifier" />
        <bean parent="FieldDefinition"
              p:attributeName="subAccountNumber" />
      </list>
    </property>
  </bean>
</beans>
