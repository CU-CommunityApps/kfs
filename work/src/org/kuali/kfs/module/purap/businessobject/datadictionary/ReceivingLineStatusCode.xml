 <beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">
	
<bean id="ReceivingLineStatusCode" parent="ReceivingLineStatusCode-parentBean" />

<bean id="ReceivingLineStatusCode-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.kfs.module.purap.businessobject.ReceivingLineStatusCode" />
   
    <property name="titleAttribute" value="ReceivingLineStatusCode" />
    <property name="objectLabel" value="Receiving Line Status Code" />
    <property name="attributes" >
      <list>
    	<ref bean="ReceivingLineStatusCode-receivingLineStatusCode" />
		<ref bean="ReceivingLineStatusCode-receivingLineStatusDescription" />
	  </list>
    </property>
</bean>
  
<bean id="ReceivingLineStatusCode-receivingLineStatusCode" parent="ReceivingLineStatusCode-receivingLineStatusCode-parentBean" />

<bean id="ReceivingLineStatusCode-receivingLineStatusCode-parentBean" abstract="true" parent="AttributeDefinition" >
	<property name="name" value="receivingLineStatusCode" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Receiving Line Status" />
    <property name="shortLabel" value="Status" />
    <property name="maxLength" value="4" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="4" />
    </property>
</bean>				
				
<bean id="ReceivingLineStatusCode-receivingLineStatusDescription" parent="ReceivingLineStatusCode-receivingLineStatusDescription-parentBean" />

<bean id="ReceivingLineStatusCode-receivingLineStatusDescription-parentBean" abstract="true" parent="PurApGenericAttributes-paymentRequestStatus" >
	<property name="name" value="receivingLineStatusDescription" />
</bean>
				
</beans>
