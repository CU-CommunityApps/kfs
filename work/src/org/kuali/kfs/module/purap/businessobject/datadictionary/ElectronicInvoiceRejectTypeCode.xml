<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="ElectronicInvoiceRejectTypeCode" parent="ElectronicInvoiceRejectTypeCode-parentBean" />

  <bean id="ElectronicInvoiceRejectTypeCode-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.kfs.module.purap.businessobject.ElectronicInvoiceRejectTypeCode" />
    <property name="inquiryDefinition" >
      <ref bean="ElectronicInvoiceRejectTypeCode-inquiryDefinition" />
    </property>
    <property name="lookupDefinition" >
      <ref bean="ElectronicInvoiceRejectTypeCode-lookupDefinition" />
    </property>
    <property name="attributes" >
      <list>
        <ref bean="ElectronicInvoiceRejectTypeCode-dataObjectMaintenanceCodeActiveIndicator" />
        <ref bean="ElectronicInvoiceRejectTypeCode-invoiceRejectReasonTypeCode" />
        <ref bean="ElectronicInvoiceRejectTypeCode-invoiceRejectReasonTypeDescription" />
        <ref bean="ElectronicInvoiceRejectTypeCode-versionNumber" />
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->


  <bean id="ElectronicInvoiceRejectTypeCode-dataObjectMaintenanceCodeActiveIndicator" parent="ElectronicInvoiceRejectTypeCode-dataObjectMaintenanceCodeActiveIndicator-parentBean" />

  <bean id="ElectronicInvoiceRejectTypeCode-dataObjectMaintenanceCodeActiveIndicator-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="dataObjectMaintenanceCodeActiveIndicator" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Data Object Maintenance Code Active Indicator" />
    <property name="shortLabel" value="Indicator" />
    <property name="maxLength" value="1" />
    <property name="control" >
      <bean parent="CheckboxControlDefinition" />
    </property>
  </bean>
  <bean id="ElectronicInvoiceRejectTypeCode-invoiceRejectReasonTypeCode" parent="ElectronicInvoiceRejectTypeCode-invoiceRejectReasonTypeCode-parentBean" />

  <bean id="ElectronicInvoiceRejectTypeCode-invoiceRejectReasonTypeCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="invoiceRejectReasonTypeCode" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Invoice Reject Reason Type Code" />
    <property name="shortLabel" value="Code" />
    <property name="maxLength" value="4" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="6" />
    </property>
  </bean>
  <bean id="ElectronicInvoiceRejectTypeCode-invoiceRejectReasonTypeDescription" parent="ElectronicInvoiceRejectTypeCode-invoiceRejectReasonTypeDescription-parentBean" />

  <bean id="ElectronicInvoiceRejectTypeCode-invoiceRejectReasonTypeDescription-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="invoiceRejectReasonTypeDescription" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Invoice Reject Reason Type Description" />
    <property name="shortLabel" value="Description" />
    <property name="maxLength" value="60" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="62" />
    </property>
  </bean>
  <bean id="ElectronicInvoiceRejectTypeCode-versionNumber" parent="ElectronicInvoiceRejectTypeCode-versionNumber-parentBean" />

  <bean id="ElectronicInvoiceRejectTypeCode-versionNumber-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="versionNumber" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Version Number" />
    <property name="shortLabel" value="Number" />
    <property name="maxLength" value="8" />
    <property name="validationPattern" >
      <bean parent="NumericValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="10" />
    </property>
  </bean>

<!-- Business Object Inquiry Definition -->


  <bean id="ElectronicInvoiceRejectTypeCode-inquiryDefinition" parent="ElectronicInvoiceRejectTypeCode-inquiryDefinition-parentBean" />

  <bean id="ElectronicInvoiceRejectTypeCode-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
    <property name="title" value="Electronic Invoice Reject Type Code Inquiry" />
    <property name="inquirySections" >
      <list>
        <bean parent="InquirySectionDefinition">
          <property name="title" value="" />
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields" >
            <list>
              <bean parent="FieldDefinition"
                    p:attributeName="dataObjectMaintenanceCodeActiveIndicator" />
              <bean parent="FieldDefinition"
                    p:attributeName="invoiceRejectReasonTypeCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="invoiceRejectReasonTypeDescription" />
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Business Object Lookup Definition -->


  <bean id="ElectronicInvoiceRejectTypeCode-lookupDefinition" parent="ElectronicInvoiceRejectTypeCode-lookupDefinition-parentBean" />

  <bean id="ElectronicInvoiceRejectTypeCode-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
    <property name="title" value="Electronic Invoice Reject Type Code Lookup" />
    <property name="instructions" value="Lookup an Electronic Invoice Reject Type Code" />
    <property name="defaultSort" >
      <bean parent="SortDefinition">
      </bean>
    </property>
    <property name="lookupFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="dataObjectMaintenanceCodeActiveIndicator" />
        <bean parent="FieldDefinition"
              p:attributeName="invoiceRejectReasonTypeCode" />
        <bean parent="FieldDefinition"
              p:attributeName="invoiceRejectReasonTypeDescription" />
      </list>
    </property>
    <property name="resultFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="dataObjectMaintenanceCodeActiveIndicator" />
        <bean parent="FieldDefinition"
              p:attributeName="invoiceRejectReasonTypeCode" />
        <bean parent="FieldDefinition"
              p:attributeName="invoiceRejectReasonTypeDescription" />
      </list>
    </property>
  </bean>
</beans>
