<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="RequisitionCapitalAssetSystem" parent="RequisitionCapitalAssetSystem-parentBean" />

  <bean id="RequisitionCapitalAssetSystem-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.kfs.module.purap.businessobject.RequisitionCapitalAssetSystem" />
    <property name="inquiryDefinition" >
      <ref bean="RequisitionCapitalAssetSystem-inquiryDefinition" />
    </property>
    <property name="lookupDefinition" >
      <ref bean="RequisitionCapitalAssetSystem-lookupDefinition" />
    </property>
    <property name="attributes" >
      <list>
        <ref bean="RequisitionCapitalAssetSystem-capitalAssetManufacturerIsVendorIndicator" />
        <ref bean="RequisitionCapitalAssetSystem-capitalAssetManufacturerName" />
        <ref bean="RequisitionCapitalAssetSystem-capitalAssetModelDescription" />
        <ref bean="RequisitionCapitalAssetSystem-capitalAssetNotReceivedCurrentFiscalYearIndicator" />
        <ref bean="RequisitionCapitalAssetSystem-capitalAssetSystemDescription" />
        <ref bean="RequisitionCapitalAssetSystem-capitalAssetSystemNumber" />
        <ref bean="RequisitionCapitalAssetSystem-capitalAssetTypeCode" />
        <ref bean="RequisitionCapitalAssetSystem-requisitionIdentifier" />
        <ref bean="RequisitionCapitalAssetSystem-versionNumber" />
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->


  <bean id="RequisitionCapitalAssetSystem-capitalAssetManufacturerIsVendorIndicator" parent="RequisitionCapitalAssetSystem-capitalAssetManufacturerIsVendorIndicator-parentBean" />

  <bean id="RequisitionCapitalAssetSystem-capitalAssetManufacturerIsVendorIndicator-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="capitalAssetManufacturerIsVendorIndicator" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Capital Asset Manufacturer Is Vendor Indicator" />
    <property name="shortLabel" value="Indicator" />
    <property name="maxLength" value="1" />
    <property name="control" >
      <bean parent="CheckboxControlDefinition" />
    </property>
    <property name="summary" value="capitalAssetManufacturerIsVendorIndicator" />
    <property name="description" value="capitalAssetManufacturerIsVendorIndicator description..." />
  </bean>
  <bean id="RequisitionCapitalAssetSystem-capitalAssetManufacturerName" parent="RequisitionCapitalAssetSystem-capitalAssetManufacturerName-parentBean" />

  <bean id="RequisitionCapitalAssetSystem-capitalAssetManufacturerName-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="capitalAssetManufacturerName" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Capital Asset Manufacturer Name" />
    <property name="shortLabel" value="Name" />
    <property name="maxLength" value="45" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="47" />
    </property>
    <property name="summary" value="capitalAssetManufacturerName" />
    <property name="description" value="capitalAssetManufacturerName description..." />
  </bean>
  <bean id="RequisitionCapitalAssetSystem-capitalAssetModelDescription" parent="RequisitionCapitalAssetSystem-capitalAssetModelDescription-parentBean" />

  <bean id="RequisitionCapitalAssetSystem-capitalAssetModelDescription-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="capitalAssetModelDescription" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Capital Asset Model Description" />
    <property name="shortLabel" value="Description" />
    <property name="maxLength" value="45" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="47" />
    </property>
    <property name="summary" value="capitalAssetModelDescription" />
    <property name="description" value="capitalAssetModelDescription description..." />
  </bean>
  <bean id="RequisitionCapitalAssetSystem-capitalAssetNotReceivedCurrentFiscalYearIndicator" parent="RequisitionCapitalAssetSystem-capitalAssetNotReceivedCurrentFiscalYearIndicator-parentBean" />

  <bean id="RequisitionCapitalAssetSystem-capitalAssetNotReceivedCurrentFiscalYearIndicator-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="capitalAssetNotReceivedCurrentFiscalYearIndicator" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Capital Asset Not Received Current Fiscal Year Indicator" />
    <property name="shortLabel" value="Indicator" />
    <property name="maxLength" value="1" />
    <property name="control" >
      <bean parent="CheckboxControlDefinition" />
    </property>
    <property name="summary" value="capitalAssetNotReceivedCurrentFiscalYearIndicator" />
    <property name="description" value="capitalAssetNotReceivedCurrentFiscalYearIndicator description..." />
  </bean>
  <bean id="RequisitionCapitalAssetSystem-capitalAssetSystemDescription" parent="RequisitionCapitalAssetSystem-capitalAssetSystemDescription-parentBean" />

  <bean id="RequisitionCapitalAssetSystem-capitalAssetSystemDescription-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="capitalAssetSystemDescription" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Capital Asset System Description" />
    <property name="shortLabel" value="Description" />
    <property name="maxLength" value="4000" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextareaControlDefinition"
            p:rows="5"
            p:cols="80" />
    </property>
    <property name="summary" value="capitalAssetSystemDescription" />
    <property name="description" value="capitalAssetSystemDescription description..." />
  </bean>
  <bean id="RequisitionCapitalAssetSystem-capitalAssetSystemNumber" parent="RequisitionCapitalAssetSystem-capitalAssetSystemNumber-parentBean" />

  <bean id="RequisitionCapitalAssetSystem-capitalAssetSystemNumber-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="capitalAssetSystemNumber" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Capital Asset System Number" />
    <property name="shortLabel" value="Number" />
    <property name="maxLength" value="10" />
    <property name="validationPattern" >
      <bean parent="NumericValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="12" />
    </property>
    <property name="summary" value="capitalAssetSystemNumber" />
    <property name="description" value="capitalAssetSystemNumber description..." />
  </bean>
  <bean id="RequisitionCapitalAssetSystem-capitalAssetTypeCode" parent="RequisitionCapitalAssetSystem-capitalAssetTypeCode-parentBean" />

  <bean id="RequisitionCapitalAssetSystem-capitalAssetTypeCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="capitalAssetTypeCode" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Capital Asset Type Code" />
    <property name="shortLabel" value="Code" />
    <property name="maxLength" value="7" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="9" />
    </property>
    <property name="summary" value="capitalAssetTypeCode" />
    <property name="description" value="capitalAssetTypeCode description..." />
  </bean>
  <bean id="RequisitionCapitalAssetSystem-requisitionIdentifier" parent="RequisitionCapitalAssetSystem-requisitionIdentifier-parentBean" />

  <bean id="RequisitionCapitalAssetSystem-requisitionIdentifier-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="requisitionIdentifier" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Requisition Identifier" />
    <property name="shortLabel" value="Identifier" />
    <property name="maxLength" value="8" />
    <property name="validationPattern" >
      <bean parent="NumericValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="10" />
    </property>
    <property name="summary" value="requisitionIdentifier" />
    <property name="description" value="requisitionIdentifier description..." />
  </bean>
  <bean id="RequisitionCapitalAssetSystem-versionNumber" parent="RequisitionCapitalAssetSystem-versionNumber-parentBean" />

  <bean id="RequisitionCapitalAssetSystem-versionNumber-parentBean" abstract="true" parent="GenericAttributes-versionNumber">
  </bean>

<!-- Business Object Inquiry Definition -->


  <bean id="RequisitionCapitalAssetSystem-inquiryDefinition" parent="RequisitionCapitalAssetSystem-inquiryDefinition-parentBean" />

  <bean id="RequisitionCapitalAssetSystem-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
    <property name="title" value="Requisition Capital Asset System Inquiry" />
    <property name="inquirySections" >
      <list>
        <bean parent="InquirySectionDefinition">
          <property name="title" value="" />
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields" >
            <list>
              <bean parent="FieldDefinition"
                    p:attributeName="capitalAssetManufacturerIsVendorIndicator" />
              <bean parent="FieldDefinition"
                    p:attributeName="capitalAssetManufacturerName" />
              <bean parent="FieldDefinition"
                    p:attributeName="capitalAssetModelDescription" />
              <bean parent="FieldDefinition"
                    p:attributeName="capitalAssetNotReceivedCurrentFiscalYearIndicator" />
              <bean parent="FieldDefinition"
                    p:attributeName="capitalAssetSystemDescription" />
              <bean parent="FieldDefinition"
                    p:attributeName="capitalAssetSystemNumber" />
              <bean parent="FieldDefinition"
                    p:attributeName="capitalAssetTypeCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="requisitionIdentifier" />
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Business Object Lookup Definition -->


  <bean id="RequisitionCapitalAssetSystem-lookupDefinition" parent="RequisitionCapitalAssetSystem-lookupDefinition-parentBean" />

  <bean id="RequisitionCapitalAssetSystem-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
    <property name="title" value="Requisition Capital Asset System Lookup" />
    <property name="instructions" value="Look up a Requisition Capital Asset System" />
    <property name="defaultSort" >
      <bean parent="SortDefinition">
      </bean>
    </property>
    <property name="lookupFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="capitalAssetManufacturerIsVendorIndicator" />
        <bean parent="FieldDefinition"
              p:attributeName="capitalAssetManufacturerName" />
        <bean parent="FieldDefinition"
              p:attributeName="capitalAssetModelDescription" />
        <bean parent="FieldDefinition"
              p:attributeName="capitalAssetNotReceivedCurrentFiscalYearIndicator" />
        <bean parent="FieldDefinition"
              p:attributeName="capitalAssetSystemDescription" />
        <bean parent="FieldDefinition"
              p:attributeName="capitalAssetSystemNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="capitalAssetTypeCode" />
        <bean parent="FieldDefinition"
              p:attributeName="requisitionIdentifier" />
      </list>
    </property>
    <property name="resultFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="capitalAssetManufacturerIsVendorIndicator" />
        <bean parent="FieldDefinition"
              p:attributeName="capitalAssetManufacturerName" />
        <bean parent="FieldDefinition"
              p:attributeName="capitalAssetModelDescription" />
        <bean parent="FieldDefinition"
              p:attributeName="capitalAssetNotReceivedCurrentFiscalYearIndicator" />
        <bean parent="FieldDefinition"
              p:attributeName="capitalAssetSystemDescription" />
        <bean parent="FieldDefinition"
              p:attributeName="capitalAssetSystemNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="capitalAssetTypeCode" />
        <bean parent="FieldDefinition"
              p:attributeName="requisitionIdentifier" />
      </list>
    </property>
  </bean>
</beans>
