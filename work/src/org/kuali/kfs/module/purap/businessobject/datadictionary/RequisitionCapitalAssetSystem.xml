<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.module.purap.bo.RequisitionCapitalAssetSystem">
 
        <inquiry>
            <title>Requisition Capital Asset System Inquiry</title>
             
            <inquirySections>
                <inquirySection title="Header Text">
                    <inquiryFields>
                        <field attributeName="capitalAssetManufacturerIsVendorIndicator" />
                        <field attributeName="capitalAssetManufacturerName" />
                        <field attributeName="capitalAssetModelDescription" />
                        <field attributeName="capitalAssetNotReceivedCurrentFiscalYearIndicator" />
                        <field attributeName="capitalAssetSystemDescription" />
                        <field attributeName="capitalAssetSystemNumber" />
                        <field attributeName="capitalAssetTypeCode" />
                        <field attributeName="requisitionIdentifier" />
                    </inquiryFields>
                </inquirySection>
            </inquirySections>  
        </inquiry>

        <lookup>
            <title>Requisition Capital Asset System Lookup</title>

            <instructions>Look up a Requisition Capital Asset System</instructions>

            <defaultSort sortAscending="true">
                <sortAttributes>
                    <sortAttribute attributeName="requisitionIdentifier" />
                    <sortAttribute attributeName="capitalAssetSystemNumber" />
                </sortAttributes>
            </defaultSort>

            <lookupFields>
                <lookupField attributeName="capitalAssetManufacturerIsVendorIndicator" required="false" />
                <lookupField attributeName="capitalAssetManufacturerName" required="false" />
                <lookupField attributeName="capitalAssetModelDescription" required="false" />
                <lookupField attributeName="capitalAssetNotReceivedCurrentFiscalYearIndicator" required="false" />
                <lookupField attributeName="capitalAssetSystemDescription" required="false" />
                <lookupField attributeName="capitalAssetSystemNumber" required="false" />
                <lookupField attributeName="capitalAssetTypeCode" required="false" />
                <lookupField attributeName="requisitionIdentifier" required="false" />
            </lookupFields>

            <resultFields>
                <field attributeName="capitalAssetManufacturerIsVendorIndicator" />
                <field attributeName="capitalAssetManufacturerName" />
                <field attributeName="capitalAssetModelDescription" />
                <field attributeName="capitalAssetNotReceivedCurrentFiscalYearIndicator" />
                <field attributeName="capitalAssetSystemDescription" />
                <field attributeName="capitalAssetSystemNumber" />
                <field attributeName="capitalAssetTypeCode" />
                <field attributeName="requisitionIdentifier" />
            </resultFields>
        </lookup>

        <attributes>

            <attribute name="capitalAssetManufacturerIsVendorIndicator" forceUppercase="true">
                <label>Capital Asset Manufacturer Is Vendor Indicator</label>
                <shortLabel>Indicator</shortLabel>

                <maxLength>1</maxLength>

                <control>
                    <checkbox />
                </control>

                <summary>capitalAssetManufacturerIsVendorIndicator</summary>
                <description>capitalAssetManufacturerIsVendorIndicator description...</description>
			</attribute>

            <attribute name="capitalAssetManufacturerName" forceUppercase="true">
                <label>Capital Asset Manufacturer Name</label>
                <shortLabel>Name</shortLabel>

                <maxLength>45</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="47" />
                </control>

                <summary>capitalAssetManufacturerName</summary>
                <description>capitalAssetManufacturerName description...</description>
			</attribute>

            <attribute name="capitalAssetModelDescription" forceUppercase="true">
                <label>Capital Asset Model Description</label>
                <shortLabel>Description</shortLabel>

                <maxLength>45</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="47" />
