                <summary>capitalAssetNotReceivedCurrentFiscalYearIndicator</summary>
                <description>capitalAssetNotReceivedCurrentFiscalYearIndicator description...</description>
			</attribute>

            <attribute name="capitalAssetSystemDescription" forceUppercase="true">
                <label>Capital Asset System Description</label>
                <shortLabel>Description</shortLabel>

                <maxLength>4000</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <textarea rows="5" cols="80"/>
                </control>

                <summary>capitalAssetSystemDescription</summary>
                <description>capitalAssetSystemDescription description...</description>
			</attribute>

            <attribute name="capitalAssetSystemNumber" forceUppercase="true">
                <label>Capital Asset System Number</label>
                <shortLabel>Number</shortLabel>

                <maxLength>10</maxLength>
                <validationPattern><numeric /></validationPattern>

                <control>
                    <text size="12" />
