<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "../dataDictionary-1_0.dtd">
<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.module.purap.bo.RecurringPaymentType">
        <customAttributeEnabled>false</customAttributeEnabled>

        <inquiry>
            <title>Recurring Payment Type Inquiry</title>

            <inquiryFields>
                <field attributeName="recurringPaymentTypeCode" />
                <field attributeName="recurringPaymentTypeDescription" />
                <field attributeName="dataObjectMaintenanceCodeActiveIndicator" />
            </inquiryFields>
        </inquiry>

        <lookup>
            <title>Recurring Payment Type Lookup</title>

			<menubar><![CDATA[<a href="maintenance.do?methodToCall=start&businessObjectClassName=org.kuali.module.purap.bo.RecurringPaymentType">Create New</a>]]></menubar>

            <instructions>Lookup a Recurring Payment Type</instructions>

            <defaultSort sortAscending="true">
                <sortAttributes>
                    <sortAttribute attributeName="recurringPaymentTypeDescription" />
                </sortAttributes>
            </defaultSort>

            <lookupFields>
                <lookupField attributeName="recurringPaymentTypeCode" required="false" />
                <lookupField attributeName="recurringPaymentTypeDescription" required="false" />
                <lookupField attributeName="dataObjectMaintenanceCodeActiveIndicator" required="false" />
            </lookupFields>

            <resultFields>
                <field attributeName="recurringPaymentTypeCode" />
                <field attributeName="recurringPaymentTypeDescription" />
                <field attributeName="dataObjectMaintenanceCodeActiveIndicator" />
            </resultFields>
        </lookup>

        <attributes>

            <attribute name="recurringPaymentTypeCode" forceUppercase="true">
                <label>Recurring Payment Type Code</label>
                <shortLabel>Code</shortLabel>

                <maxLength>4</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="6" />
                </control>

                <summary>recurringPaymentTypeCode</summary>
                <description>recurringPaymentTypeCode description...</description>
			</attribute>

            <attribute name="recurringPaymentTypeDescription">
                <label>Recurring Payment Type Description</label>
                <shortLabel>Description</shortLabel>

                <maxLength>45</maxLength>
                <validationPattern><anyCharacter allowWhitespace="true" /></validationPattern>

                <control>
                    <text size="47" />
                </control>

                <summary>recurringPaymentTypeDescription</summary>
                <description>recurringPaymentTypeDescription description...</description>
			</attribute>

            <attributeReference name="dataObjectMaintenanceCodeActiveIndicator" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="activeIndicator" />

            <attributeReference name="versionNumber" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="versionNumber" />

        </attributes>
    </businessObject>
</dictionaryEntry>
