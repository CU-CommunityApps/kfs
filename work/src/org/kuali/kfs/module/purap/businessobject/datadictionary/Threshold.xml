<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="Threshold" parent="Threshold-parentBean" />

  <bean id="Threshold-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.module.purap.bo.Threshold" />
    <property name="inquiryDefinition" >
      <ref bean="Threshold-inquiryDefinition" />
    </property>
    <property name="lookupDefinition" >
      <ref bean="Threshold-lookupDefinition" />
    </property>
    <property name="titleAttribute" value="thresholdIdentifier" />
    <property name="objectLabel" value="Threshold" />
    <property name="attributes" >
      <list>
        <ref bean="Threshold-thresholdIdentifier" />
        <ref bean="Threshold-accountTypeCode" />
        <ref bean="Threshold-chartOfAccountsCode" />
		<ref bean="Threshold-financialObjectCode" />
        <ref bean="Threshold-subFundGroupCode" />
        <ref bean="Threshold-organizationCode" />
        <ref bean="Threshold-purchasingCommodityCode" />
        <ref bean="Threshold-active" />                                
        <ref bean="Threshold-vendorHeaderGeneratedIdentifier" />        
	    <ref bean="Threshold-vendorDetailAssignedIdentifier" />        
	    <ref bean="Threshold-thresholdAmount" />    
  	    <ref bean="Threshold-versionNumber" />  
  	    <ref bean="Threshold-vendorNumber" />  
      </list>
    </property>
    
    <property name="relationships" >
      <list>
        <bean parent="RelationshipDefinition">
          <property name="objectAttributeName" value="vendorDetail" />
          <property name="primitiveAttributes" >
            <list>
              <bean parent="PrimitiveAttributeDefinition"
                    p:sourceName="vendorNumber"
                    p:targetName="vendorNumber" />
            </list>
          </property>
          <property name="supportAttributes" >
            <list>
              <bean parent="SupportAttributeDefinition"
                    p:sourceName="vendorHeaderGeneratedIdentifier"
                    p:targetName="vendorHeaderGeneratedIdentifier" />
              <bean parent="SupportAttributeDefinition"
                    p:sourceName="vendorDetailAssignedIdentifier"
                    p:targetName="vendorDetailAssignedIdentifier" />
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->


  <bean id="Threshold-thresholdIdentifier" parent="Threshold-thresholdIdentifier-parentBean" />

  <bean id="Threshold-thresholdIdentifier-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="thresholdIdentifier" />
    <property name="label" value="Threshold Identifier" />
    <property name="shortLabel" value="Identifier" />
    <property name="maxLength" value="10" />
    <property name="validationPattern" >
      <bean parent="NumericValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="HiddenControlDefinition"/>
    </property>
  </bean>
  
  <bean id="Threshold-thresholdAmount" parent="Threshold-thresholdAmount-parentBean" />

  <bean id="Threshold-thresholdAmount-parentBean" abstract="true" parent="PurApGenericAttributes-genericItemAmount">
    <property name="name" value="thresholdAmount" />
    <property name="label" value="Threshold Amount" />
    <property name="shortLabel" value="Amount" />
  </bean>
  
  <bean id="Threshold-accountTypeCode" parent="Threshold-accountTypeCode-parentBean" />

  <bean id="Threshold-accountTypeCode-parentBean" abstract="true" parent="AcctType-accountTypeCode">
	  <property name="control" >
	      <bean parent="SelectControlDefinition"
	            p:valuesFinderClass="org.kuali.module.chart.lookup.keyvalues.AcctTypeValuesFinder"
	            p:includeKeyInLabel="false" />
	  </property>
  </bean>

  <bean id="Threshold-chartOfAccountsCode" parent="Threshold-chartOfAccountsCode-parentBean" />

  <bean id="Threshold-chartOfAccountsCode-parentBean" abstract="true" parent="Chart-chartOfAccountsCode">
  </bean>

  <bean id="Threshold-financialObjectCode" parent="Threshold-financialObjectCode-parentBean" />

  <bean id="Threshold-financialObjectCode-parentBean" abstract="true" parent="ObjectCode-financialObjectCode">
  </bean>
  
  <bean id="Threshold-organizationCode" parent="Threshold-organizationCode-parentBean" />

  <bean id="Threshold-organizationCode-parentBean" abstract="true" parent="Org-organizationCode">
  </bean>
	
  <bean id="Threshold-purchasingCommodityCode" parent="Threshold-purchasingCommodityCode-parentBean" />

  <bean id="Threshold-purchasingCommodityCode-parentBean" abstract="true" parent="CommodityCode-purchasingCommodityCode">
  </bean>
	
  <bean id="Threshold-subFundGroupCode" parent="Threshold-subFundGroupCode-parentBean" />
  
  <bean id="Threshold-subFundGroupCode-parentBean" abstract="true" parent="SubFundGroup-subFundGroupCode">
  </bean>	
	
  <bean id="Threshold-vendorHeaderGeneratedIdentifier" parent="Threshold-vendorHeaderGeneratedIdentifier-parentBean" />
  
  <bean id="Threshold-vendorHeaderGeneratedIdentifier-parentBean" abstract="true" parent="VendorDetail-vendorHeaderGeneratedIdentifier">
    <property name="control" >
      <bean parent="HiddenControlDefinition"/>
    </property>
  </bean>	
	
  <bean id="Threshold-vendorDetailAssignedIdentifier" parent="Threshold-vendorDetailAssignedIdentifier-parentBean" />
  
  <bean id="Threshold-vendorDetailAssignedIdentifier-parentBean" abstract="true" parent="VendorDetail-vendorDetailAssignedIdentifier">
    <property name="control" >
      <bean parent="HiddenControlDefinition"/>
    </property>
  </bean>	
	
  <bean id="Threshold-active" parent="Threshold-active-parentBean" />
  
  <bean id="Threshold-active-parentBean" abstract="true" parent="GenericAttributes-activeIndicator">
	  <property name="name" value="active" />
  </bean>
	
  <bean id="Threshold-versionNumber" parent="Threshold-versionNumber-parentBean" />
  
  <bean id="Threshold-versionNumber-parentBean" abstract="true" parent="GenericAttributes-versionNumber">
  </bean>	
	
  <bean id="Threshold-vendorNumber" parent="Threshold-vendorNumber-parentBean" />
  
  <bean id="Threshold-vendorNumber-parentBean" abstract="true" parent="VendorDetail-vendorNumber">
    <property name="required" value="false" />
  </bean>	
	
<!-- Business Object Inquiry Definition -->

  <bean id="Threshold-inquiryDefinition" parent="Threshold-inquiryDefinition-parentBean" />

  <bean id="Threshold-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
    <property name="title" value="Purchase Order Status Inquiry" />
    <property name="inquirySections" >
      <list>
        <bean parent="InquirySectionDefinition">
          <property name="title" value="Header Text" />
          <property name="numberOfColumns" value="11" />
          <property name="inquiryFields" >
            <list>
              <bean parent="FieldDefinition"
                    p:attributeName="thresholdIdentifier" />
              <bean parent="FieldDefinition"
                    p:attributeName="accountTypeCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="chartOfAccountsCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="financialObjectCode" />
    		  <!-- <bean parent="FieldDefinition"
                    p:attributeName="universityFiscalYear" />-->
              <bean parent="FieldDefinition"
                    p:attributeName="subFundGroupCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="organizationCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="purchasingCommodityCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="active" />
              <bean parent="FieldDefinition"
                    p:attributeName="vendorHeaderGeneratedIdentifier" />
              <bean parent="FieldDefinition"
                    p:attributeName="vendorDetailAssignedIdentifier" />
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Business Object Lookup Definition -->


  <bean id="Threshold-lookupDefinition" parent="Threshold-lookupDefinition-parentBean" />

  <bean id="Threshold-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
    <property name="title" value="Threshold Lookup" />
    <property name="instructions" value="Lookup a Threshold" />
    <property name="lookupableID" value="thresholdLookupable" />
    <property name="defaultSort" >
      <bean parent="SortDefinition">
      </bean>
    </property>
    <property name="lookupFields" >
      <list>
    	 <bean parent="FieldDefinition"
                p:attributeName="thresholdIdentifier" />
         <bean parent="FieldDefinition"
                p:attributeName="chartOfAccountsCode" />
         <bean parent="FieldDefinition"
                p:attributeName="accountTypeCode" />
         <bean parent="FieldDefinition"
                p:attributeName="subFundGroupCode" />
         <bean parent="FieldDefinition"
                p:attributeName="purchasingCommodityCode" />
         <bean parent="FieldDefinition"
                p:attributeName="financialObjectCode" />
         <bean parent="FieldDefinition"
                p:attributeName="organizationCode" />
		 <!-- <bean parent="FieldDefinition"
                p:attributeName="universityFiscalYear" />-->
         <bean parent="FieldDefinition"
                p:attributeName="vendorNumber" />
         <bean parent="FieldDefinition"
                p:attributeName="active" />
      </list>
    </property>
    <property name="resultFields" >
      <list>
         <bean parent="FieldDefinition"
                p:attributeName="chartOfAccountsCode" />
         <bean parent="FieldDefinition"
                p:attributeName="accountTypeCode" />
         <bean parent="FieldDefinition"
                p:attributeName="subFundGroupCode" />
         <bean parent="FieldDefinition"
                p:attributeName="purchasingCommodityCode" />
         <bean parent="FieldDefinition"
                p:attributeName="financialObjectCode" />
         <bean parent="FieldDefinition"
                p:attributeName="organizationCode" />
         <bean parent="FieldDefinition"
                p:attributeName="vendorNumber" />
         <bean parent="FieldDefinition"
                p:attributeName="thresholdAmount" />
         <bean parent="FieldDefinition"
                p:attributeName="active" />
      </list>
    </property>
  </bean>
</beans>