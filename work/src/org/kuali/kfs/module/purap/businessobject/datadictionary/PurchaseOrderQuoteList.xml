<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "../dataDictionary-1_0.dtd">
<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.module.purap.bo.PurchaseOrderQuoteList">
        <customAttributeEnabled>false</customAttributeEnabled>

        <inquiry>
            <title>Purchase Order Quote List Inquiry</title>

            <inquiryFields>
                <field attributeName="purchaseOrderQuoteListIdentifier" />
                <field attributeName="purchaseOrderQuoteListName" />
                <field attributeName="contractManagerCode" />
            </inquiryFields>
        </inquiry>

        <lookup>
            <title>Purchase Order Quote List Lookup</title>

			<menubar><![CDATA[<a href="maintenance.do?methodToCall=start&businessObjectClassName=org.kuali.module.purap.bo.PurchaseOrderQuoteList">Create New</a>]]></menubar>

            <instructions>Lookup an Purchase Order Quote List</instructions>

            <defaultSort sortAscending="true">
                <sortAttributes>
                    <sortAttribute attributeName="contractManagerCode" />
                </sortAttributes>
            </defaultSort>

            <lookupFields>
                <lookupField attributeName="purchaseOrderQuoteListIdentifier" required="false" />
                <lookupField attributeName="purchaseOrderQuoteListName" required="false" />
                <lookupField attributeName="contractManagerCode" required="false" />
            </lookupFields>

            <resultFields>
                <field attributeName="purchaseOrderQuoteListIdentifier" />
                <field attributeName="purchaseOrderQuoteListName" />
                <field attributeName="contractManagerCode" />
            </resultFields>
        </lookup>

        <attributes>
			
            <attribute name="purchaseOrderQuoteListIdentifier" forceUppercase="true">
                <label>Purchase Order Quote List Identifier</label>
                <shortLabel>Identifier</shortLabel>

                <maxLength>9</maxLength>
                <validationPattern><numeric /></validationPattern>

                <control>
                    <text size="11" />
                </control>

                <summary>purchaseOrderQuoteListIdentifier</summary>
                <description>purchaseOrderQuoteListIdentifier description...</description>
			</attribute>

            <attribute name="purchaseOrderQuoteListName" forceUppercase="false">
                <label>Purchase Order Quote List Name</label>
                <shortLabel>Name</shortLabel>

                <maxLength>100</maxLength>
                <validationPattern><anyCharacter allowWhitespace="true"/></validationPattern>

                <control>
                    <text size="102" />
                </control>

                <summary>purchaseOrderQuoteListName</summary>
                <description>purchaseOrderQuoteListName description...</description>
			</attribute>
			
			<attributeReference name="contractManagerCode" sourceClassName="org.kuali.module.purap.bo.ContractManager" sourceAttributeName="contractManagerCode">
                <control>
                    <select valuesFinderClass="org.kuali.core.lookup.keyvalues.ContractManagerValuesFinder" />
                </control>
            </attributeReference>			
			<attributeReference name="versionNumber" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="versionNumber" />
			
        </attributes>
    </businessObject>
</dictionaryEntry>
