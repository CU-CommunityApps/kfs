<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "../dataDictionary-1_0.dtd">
<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.module.purap.bo.PurchaseOrderQuoteLanguage">
 
        <inquiry>
            <title>Purchase Order Quote Language Inquiry</title>
             
            <inquirySections>
                <inquirySection title="Header Text">
                    <inquiryFields>
                        <field attributeName="active" />
                        <field attributeName="purchaseOrderQuoteLanguageCreateDate" />
                        <field attributeName="purchaseOrderQuoteLanguageDescription" />
                        <field attributeName="purchaseOrderQuoteLanguageIdentifier" />
                    </inquiryFields>
                </inquirySection>
            </inquirySections>  
        </inquiry>

        <lookup>
            <title>Purchase Order Quote Language Lookup</title>

            <instructions>Look up a Purchase Order Quote Language</instructions>

            <defaultSort sortAscending="true">
                <sortAttributes>                
	                <sortAttribute attributeName="purchaseOrderQuoteLanguageIdentifier" />
                    <sortAttribute attributeName="purchaseOrderQuoteLanguageCreateDate" />
                </sortAttributes>
            </defaultSort>

            <lookupFields>                                
                <lookupField attributeName="purchaseOrderQuoteLanguageIdentifier" required="false" />
                <lookupField attributeName="purchaseOrderQuoteLanguageDescription" required="false" />
                <lookupField attributeName="purchaseOrderQuoteLanguageCreateDate" required="false" />
				<lookupField attributeName="active" required="false" />                
            </lookupFields>

            <resultFields>
                <field attributeName="purchaseOrderQuoteLanguageIdentifier" />	
                <field attributeName="purchaseOrderQuoteLanguageDescription" />                
				<field attributeName="purchaseOrderQuoteLanguageCreateDate" />			                
				<field attributeName="active" />
            </resultFields>
        </lookup>

        <attributes>

            <attribute name="active" forceUppercase="true">
                <label>Active</label>
                <shortLabel>Active</shortLabel>

                <maxLength>1</maxLength>

                <control>
                    <checkbox />
                </control>
			</attribute>

            <attributeReference name="purchaseOrderQuoteLanguageCreateDate" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="genericTimestamp">
                <label>Purchase Order Quote Language Create Date</label>
                <shortLabel>Date</shortLabel>
                <required>true</required>
            </attributeReference>

            <attribute name="purchaseOrderQuoteLanguageDescription" forceUppercase="false">
                <label>Purchase Order Quote Language Description</label>
                <shortLabel>Description</shortLabel>

                <maxLength>2000</maxLength>
                <validationPattern><anyCharacter allowWhitespace="true" /></validationPattern>
                
                <control>
                    <textarea rows="5" cols="80"/>
                </control>
			</attribute>

            <attribute name="purchaseOrderQuoteLanguageIdentifier" forceUppercase="true">
                <label>Purchase Order Quote Language Identifier</label>
                <shortLabel>Identifier</shortLabel>

                <maxLength>10</maxLength>
                <validationPattern><numeric /></validationPattern>

                <control>
                    <hidden/>
                </control>
			</attribute>

            <attributeReference name="versionNumber" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="versionNumber" />

        </attributes>
        <objectLabel>Purchase Order Quote Language</objectLabel>
    </businessObject>
</dictionaryEntry>
