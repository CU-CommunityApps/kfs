                <summary>purchaseOrderQuoteLanguageCreateDate</summary>
                <description>purchaseOrderQuoteLanguageCreateDate description...</description>
			</attribute>

            <attribute name="purchaseOrderQuoteLanguageDescription" forceUppercase="true">
                <label>Purchase Order Quote Language Description</label>
                <shortLabel>Description</shortLabel>

                <maxLength>2000</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <textarea rows="5" cols="80"/>
                </control>

                <summary>purchaseOrderQuoteLanguageDescription</summary>
                <description>purchaseOrderQuoteLanguageDescription description...</description>
			</attribute>

            <attribute name="purchaseOrderQuoteLanguageIdentifier" forceUppercase="true">
                <label>Purchase Order Quote Language Identifier</label>
                <shortLabel>Identifier</shortLabel>

                <maxLength>10</maxLength>
                <validationPattern><numeric /></validationPattern>

                <control>
                    <text size="12" />
                </control>

                <summary>purchaseOrderQuoteLanguageIdentifier</summary>
