  <bean id="PurchaseOrderCapitalAssetLocation-capitalAssetLocationIdentifier-parentBean" parent="AttributeDefinition" abstract="true"
        p:name="capitalAssetLocationIdentifier"
        p:forceUppercase="false"
        p:label="Capital Asset Location Identifier"
        p:shortLabel="Capital Asset Location Identifier"
        p:maxLength="10"
        p:required="false" >
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern"
            p:allowWhitespace="true" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
