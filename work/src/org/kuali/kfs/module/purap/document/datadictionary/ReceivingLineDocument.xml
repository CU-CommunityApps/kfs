  <bean id="ReceivingLineDocument" parent="ReceivingLineDocument-parentBean" />

  <bean id="ReceivingLineDocument-parentBean" abstract="true" parent="TransactionalDocumentEntry">
    <property name="label" value="Receiving Line" />
    <property name="shortLabel" value="Receiving Line" />
    <property name="documentTypeName" value="ReceivingLineDocument" />
    <property name="documentClass" value="org.kuali.module.purap.document.ReceivingLineDocument" />
    <property name="businessRulesClass" value="org.kuali.module.purap.rules.ReceivingLineDocumentRule" />
    <property name="documentAuthorizerClass" value="org.kuali.module.purap.document.authorization.ReceivingLineDocumentAuthorizer" />
    <property name="preRulesCheckClass" value="org.kuali.module.purap.rules.ReceivingLineDocumentPreRules" />
    <property name="documentTypeCode" value="RCVL" />
    <property name="authorizations" >
      <list>
        <bean parent="AuthorizationDefinition">
          <property name="action" value="initiate" />
          <property name="authorizedGroups" >
            <set>
              <value>PA_AP_USERS</value>
            </set>
          </property>
        </bean>
      </list>
    </property>
    <property name="attributes" >
      <list>
        <ref bean="ReceivingLineDocument-documentNumber" />
        <ref bean="ReceivingLineDocument-purchaseOrderIdentifier" />
        <ref bean="ReceivingLineDocument-carrierCode" />
        <ref bean="ReceivingLineDocument-deliveryBuildingCode" />
        <ref bean="ReceivingLineDocument-deliveryBuildingLine1Address" />
        <ref bean="ReceivingLineDocument-deliveryBuildingLine2Address" />
        <ref bean="ReceivingLineDocument-deliveryBuildingName" />
        <ref bean="ReceivingLineDocument-deliveryBuildingOther" />
        <ref bean="ReceivingLineDocument-deliveryBuildingRoomNumber" />
        <ref bean="ReceivingLineDocument-deliveryCampusCode" />
        <ref bean="ReceivingLineDocument-deliveryCityName" />
        <ref bean="ReceivingLineDocument-deliveryCountryCode" />
        <ref bean="ReceivingLineDocument-deliveryInstructionText" />
        <ref bean="ReceivingLineDocument-deliveryPostalCode" />
        <ref bean="ReceivingLineDocument-deliveryRequiredDate" />
        <ref bean="ReceivingLineDocument-deliveryRequiredDateReasonCode" />
        <ref bean="ReceivingLineDocument-deliveryStateCode" />
        <ref bean="ReceivingLineDocument-deliveryToEmailAddress" />
        <ref bean="ReceivingLineDocument-deliveryToName" />
        <ref bean="ReceivingLineDocument-deliveryToPhoneNumber" />
        <ref bean="ReceivingLineDocument-shipmentBillOfLadingNumber" />
        <ref bean="ReceivingLineDocument-shipmentPackingSlipNumber" />
        <ref bean="ReceivingLineDocument-shipmentReceivedDate" />
        <ref bean="ReceivingLineDocument-shipmentReferenceNumber" />
        <ref bean="ReceivingLineDocument-vendorHeaderGeneratedIdentifier" />
        <ref bean="ReceivingLineDocument-vendorName" />
        <ref bean="ReceivingLineDocument-vendorNumber" />
        <ref bean="ReceivingLineDocument-vendorDetailAssignedIdentifier" />
        <ref bean="ReceivingLineDocument-vendorCityName" />
        <ref bean="ReceivingLineDocument-vendorCountryCode" />
        <ref bean="ReceivingLineDocument-vendorLine1Address" />
        <ref bean="ReceivingLineDocument-vendorLine2Address" />
        <ref bean="ReceivingLineDocument-vendorStateCode" />
        <ref bean="ReceivingLineDocument-vendorPostalCode" />
        <ref bean="ReceivingLineDocument-versionNumber" />
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->


  <bean id="ReceivingLineDocument-documentNumber" parent="ReceivingLineDocument-documentNumber-parentBean" />

  <bean id="ReceivingLineDocument-documentNumber-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="documentNumber" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Document Number" />
    <property name="shortLabel" value="Number" />
    <property name="maxLength" value="14" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="16" />
    </property>
    <property name="summary" value="documentNumber" />
    <property name="description" value="documentNumber description..." />
  </bean>
  <bean id="ReceivingLineDocument-purchaseOrderIdentifier" parent="ReceivingLineDocument-purchaseOrderIdentifier-parentBean" />

