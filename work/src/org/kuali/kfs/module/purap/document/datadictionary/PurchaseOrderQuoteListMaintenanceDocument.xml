<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="PurchaseOrderQuoteListMaintenanceDocument" parent="PurchaseOrderQuoteListMaintenanceDocument-parentBean" />

  <bean id="PurchaseOrderQuoteListMaintenanceDocument-parentBean" abstract="true" parent="MaintenanceDocumentEntry">
    <property name="businessObjectClass" value="org.kuali.module.purap.bo.PurchaseOrderQuoteList" />
    <property name="maintainableClass" value="org.kuali.core.maintenance.KualiMaintainableImpl" />
    <property name="maintainableSections" >
      <list>
        <ref bean="PurchaseOrderQuoteListMaintenanceDocument-EditQuoteList" />
        <ref bean="PurchaseOrderQuoteListMaintenanceDocument-Vendors" />
      </list>
    </property>
    <property name="lockingKeys" >
      <list>
        <value>purchaseOrderQuoteListIdentifier</value>
      </list>
    </property>
    <property name="label" value="Purchase Order Quote List" />
    <property name="shortLabel" value="Purchase Order Quote List" />
    <property name="helpDefinition" >
      <bean parent="HelpDefinition"
            p:parameterNamespace="KFS-PA"
            p:parameterName="PURCHASE_ORDER_QUOTE_LIST"
            p:parameterDetailType="PurchaseOrderQuoteList" />
    </property>
    <property name="documentTypeName" value="PurchaseOrderQuoteListMaintenanceDocument" />
    <property name="businessRulesClass" value="org.kuali.module.purap.rules.PurchaseOrderQuoteListRule" />
    <property name="documentAuthorizerClass" value="org.kuali.kfs.authorization.FinancialSystemMaintenanceDocumentAuthorizerBase" />
    <property name="documentTypeCode" value="PMQT" />
    <property name="workflowProperties" >
      <ref bean="PurchaseOrderQuoteListMaintenanceDocument-workflowProperties" />
    </property>
    <property name="authorizations" >
      <list>
        <bean parent="AuthorizationDefinition">
          <property name="action" value="initiate" />
          <property name="authorizedGroups" >
            <set>
              <value>SY_MAINTENANCE_USERS</value>
            </set>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Maintenance Section Definitions -->


  <bean id="PurchaseOrderQuoteListMaintenanceDocument-EditQuoteList" parent="PurchaseOrderQuoteListMaintenanceDocument-EditQuoteList-parentBean" />

  <bean id="PurchaseOrderQuoteListMaintenanceDocument-EditQuoteList-parentBean" abstract="true" parent="MaintainableSectionDefinition">
    <property name="id" value="Edit Quote List" />
    <property name="title" value="Edit Quote List" />
    <property name="maintainableItems" >
      <list>
        <bean parent="MaintainableFieldDefinition"
              p:name="purchaseOrderQuoteListIdentifier" />
        <bean parent="MaintainableFieldDefinition"
              p:name="purchaseOrderQuoteListName"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="contractManagerCode"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="versionNumber" />
      </list>
    </property>
  </bean>
  <bean id="PurchaseOrderQuoteListMaintenanceDocument-Vendors" parent="PurchaseOrderQuoteListMaintenanceDocument-Vendors-parentBean" />

  <bean id="PurchaseOrderQuoteListMaintenanceDocument-Vendors-parentBean" abstract="true" parent="MaintainableSectionDefinition">
    <property name="id" value="Vendors" />
    <property name="title" value="Vendors" />
    <property name="maintainableItems" >
      <list>
        <bean parent="MaintainableCollectionDefinition">
          <property name="name" value="quoteListVendors" />
          <property name="businessObjectClass" value="org.kuali.module.purap.bo.PurchaseOrderQuoteListVendor" />
          <property name="summaryTitle" value="Vendor" />
          <property name="summaryFields" >
            <list>
              <bean parent="MaintainableFieldDefinition"
                    p:name="vendorHeaderGeneratedIdentifier" />
              <bean parent="MaintainableFieldDefinition"
                    p:name="vendorDetailAssignedIdentifier" />
            </list>
          </property>
          <property name="maintainableFields" >
            <list>
              <bean parent="MaintainableFieldDefinition"
                    p:name="vendorDetail.vendorName"
                    p:readOnlyAfterAdd="true"
                    p:template="vendorName" 
                    p:required="true" />
              <bean parent="MaintainableFieldDefinition"
                    p:name="vendorDetail.activeIndicator"
                    p:readOnlyAfterAdd="true" />
              <bean parent="MaintainableFieldDefinition"
                    p:name="vendorHeaderGeneratedIdentifier"
                    p:template="vendorHeaderGeneratedIdentifier" />
              <bean parent="MaintainableFieldDefinition"
                    p:name="vendorDetailAssignedIdentifier"
                    p:template="vendorDetailAssignedIdentifier" />
              <bean parent="MaintainableFieldDefinition"
                    p:name="versionNumber" />                    
            </list>
          </property>
          <property name="duplicateIdentificationFields">
            <list>
                <bean parent="MaintainableFieldDefinition" p:name="vendorHeaderGeneratedIdentifier" />
                <bean parent="MaintainableFieldDefinition" p:name="vendorDetailAssignedIdentifier" />
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Exported Workflow Properties -->


  <bean id="PurchaseOrderQuoteListMaintenanceDocument-workflowProperties" parent="PurchaseOrderQuoteListMaintenanceDocument-workflowProperties-parentBean" />

  <bean id="PurchaseOrderQuoteListMaintenanceDocument-workflowProperties-parentBean" abstract="true" parent="WorkflowProperties">
  </bean>
</beans>
