<?xml version="1.0" encoding="UTF-8"?><beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p" xmlns:dd="http://rice.kuali.org/dd" xsi:schemaLocation="http://www.springframework.org/schema/beans         http://www.springframework.org/schema/beans/spring-beans-2.0.xsd         http://rice.kuali.org/dd         http://rice.kuali.org/dd/dd.xsd">

  <bean id="CarrierMaintenanceDocument" parent="CarrierMaintenanceDocument-parentBean"/>

  <bean id="CarrierMaintenanceDocument-parentBean" abstract="true" parent="MaintenanceDocumentEntry">
    <property name="businessObjectClass" value="org.kuali.kfs.module.purap.businessobject.Carrier"/>
    <property name="maintainableClass" value="org.kuali.kfs.sys.document.FinancialSystemMaintainable"/>
    <property name="maintainableSections">
      <list>
        <ref bean="CarrierMaintenanceDocument-EditCarrier"/>
      </list>
    </property>
    <property name="lockingKeys">
      <list>
        <value>carrierCode</value>
      </list>
    </property>
    
    
    
    <property name="documentTypeName" value="PMCA"/>
    <property name="documentAuthorizerClass" value="org.kuali.kfs.sys.document.authorization.FinancialSystemMaintenanceDocumentAuthorizerBase"/>
    <property name="workflowAttributes">
    	<ref bean="CarrierMaintenanceDocument-workflowAttributes"/>
    </property>
  </bean>

<!-- Maintenance Section Definitions -->

  <bean id="CarrierMaintenanceDocument-EditCarrier" parent="CarrierMaintenanceDocument-EditCarrier-parentBean"/>

  <bean id="CarrierMaintenanceDocument-EditCarrier-parentBean" abstract="true" parent="MaintainableSectionDefinition">
    <property name="id" value="Edit Carrier"/>
    <property name="title" value="Edit Carrier"/>
    <property name="maintainableItems">
      <list>
        <bean parent="MaintainableFieldDefinition" p:name="carrierCode" p:required="true"/>
        <bean parent="MaintainableFieldDefinition" p:name="carrierDescription" p:required="true"/>
        <bean parent="MaintainableFieldDefinition" p:name="active" p:defaultValue="true"/>
      </list>
    </property>
  </bean>
  
  <!-- workflow attributes -->
  <bean id="CarrierMaintenanceDocument-workflowAttributes" parent="CarrierMaintenanceDocument-workflowAttributes-parentBean"/>
  
  <bean id="CarrierMaintenanceDocument-workflowAttributes-parentBean" class="org.kuali.rice.kns.datadictionary.WorkflowAttributes" abstract="true"/>
</beans>