<?xml version="1.0" encoding="UTF-8"?><beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p" xmlns:dd="http://rice.kuali.org/dd" xsi:schemaLocation="http://www.springframework.org/schema/beans         http://www.springframework.org/schema/beans/spring-beans-2.0.xsd         http://rice.kuali.org/dd         http://rice.kuali.org/dd/dd.xsd">

  <bean id="PurchaseOrderReopenDocument" parent="PurchaseOrderReopenDocument-parentBean"/>

  <bean id="PurchaseOrderReopenDocument-parentBean" abstract="true" parent="TransactionalDocumentEntry">
    <property name="documentTypeName" value="POR"/>
    <property name="sessionDocument" value="true"/>
    <property name="documentClass" value="org.kuali.kfs.module.purap.document.PurchaseOrderReopenDocument"/>
    <property name="businessRulesClass" value="org.kuali.kfs.sys.document.validation.impl.AccountingRuleEngineRuleBase"/>
    <property name="documentAuthorizerClass" value="org.kuali.kfs.module.purap.document.authorization.PurchasingAccountsPayableTransactionalDocumentAuthorizerBase"/>
    <property name="documentPresentationControllerClass" value="org.kuali.kfs.module.purap.document.authorization.PurchaseOrderReopenDocumentPresentationController"/>
    <property name="potentiallySensitive" value="true"/>
    <property name="workflowAttributes">
    	<ref bean="PurchaseOrderReopenDocument-workflowAttributes"/>
    </property>
    <property name="accountingLineGroups">
    	<map>
    		<entry>
    			<key><value>source</value></key>
	    		<ref bean="PurchaseOrderDocument-sourceAccountingLineGroup" parent="AccountingLineGroup"/>
	    	</entry>
    	</map>
    </property>
    <property name="validationMap" ref="PurchaseOrderReopenDocument-validations"/>
	<property name="relationships">
    	<list>
			<bean parent="RelationshipDefinition">
              <property name="objectAttributeName" value="deliveryCampus" />
              <property name="primitiveAttributes" >
                <list>
                  <bean parent="PrimitiveAttributeDefinition"
                        p:sourceName="deliveryCampusCode"
                        p:targetName="campusCode" />
                </list>
              </property>
    		</bean>
    	</list>
    </property>
  </bean>

	<bean id="PurchaseOrderReopenDocument-validations" parent="PurchaseOrderReopenDocument-validations-parentBean"/>
	 
	   <bean id="PurchaseOrderReopenDocument-validations-parentBean" class="org.springframework.beans.factory.config.MapFactoryBean">
	   	<property name="sourceMap">
	    	<map>			
				<entry>
					<key><value>org.kuali.kfs.sys.document.validation.event.AttributedRouteDocumentEvent</value></key>
					<value>PurchaseOrderReopen-routeDocumentValidation</value>
				</entry>
				<entry>
					<key><value>org.kuali.kfs.sys.document.validation.event.AttributedApproveDocumentEvent</value></key>
					<value>PurchaseOrderReopen-approveDocumentValidation</value>
				</entry>		
				<entry>
					<key><value>org.kuali.kfs.sys.document.validation.event.AddAccountingLineEvent</value></key>
					<value>PurchaseOrderReopen-addAccountingLineValidation</value>
				</entry>
				<entry>
					<key><value>org.kuali.kfs.sys.document.validation.event.UpdateAccountingLineEvent</value></key>
					<value>PurchaseOrderReopen-updateAccountingLineValidation</value>
				</entry>	
				<entry>
					<key><value>org.kuali.kfs.sys.document.validation.event.ReviewAccountingLineEvent</value></key>
					<value>PurchaseOrderReopen-reviewAccountingLineValidation</value>
				</entry>
				<entry>
					<key><value>org.kuali.kfs.module.purap.document.validation.event.AttributedAddPurchasingAccountsPayableItemEvent</value></key>
					<value>PurchaseOrderReopen-addPurchasingAccountsPayableItemValidation</value>				
				</entry>
				<entry>
					<key><value>org.kuali.kfs.module.purap.document.validation.event.AttributedImportPurchasingAccountsPayableItemEvent</value></key>
					<value>PurchaseOrderReopen-importPurchasingAccountsPayableItemValidation</value>				
				</entry>																									
				<entry>
					<key><value>org.kuali.kfs.sys.document.validation.event.AttributedSaveDocumentEvent</value></key>
					<value>PurchaseOrderReopen-saveDocumentValidation</value>				
				</entry>												
		    </map>
	    </property>	  
	   </bean>

    <!-- workflow attributes -->
    <bean id="PurchaseOrderReopenDocument-workflowAttributes" parent="PurchaseOrderReopenDocument-workflowAttributes-parentBean"/>
  
    <bean id="PurchaseOrderReopenDocument-workflowAttributes-parentBean" class="org.kuali.rice.kns.datadictionary.WorkflowAttributes" abstract="true">
    	<property name="searchingTypeDefinitions">
    		<ref bean="SearchingTypeDefinitions-PurchaseOrderDocumentFamily" />
    	</property> 
    </bean>
      
</beans>