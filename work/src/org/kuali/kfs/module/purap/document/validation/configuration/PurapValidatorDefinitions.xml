	<bean id="PaymentRequest-purchaseOrderIdValidation" class="org.kuali.kfs.module.purap.document.validation.impl.PaymentRequestPurchaseOrderIdValidation" abstract="true"/>

	<bean id="PaymentRequest-invoiceValidation" class="org.kuali.kfs.module.purap.document.validation.impl.PaymentRequestInvoiceValidation" abstract="true"/>

	<bean id="PaymentRequest-dateForContinueValidation" class="org.kuali.kfs.module.purap.document.validation.impl.PaymentRequestDateForContinueValidation" abstract="true">
		<property name="paymentRequestService" ref="paymentRequestService" />
	</bean>

	<bean id="PaymentRequest-totalsValidation" class="org.kuali.kfs.module.purap.document.validation.impl.PaymentRequestTotalsValidation" abstract="true"/>

	<bean id="PaymentRequest-payDateNotOverThresholdDaysAwayValidation" class="org.kuali.kfs.module.purap.document.validation.impl.PaymentRequestPayDateNotOverThresholdDaysAwayValidation" abstract="true">
		<property name="purapService" ref="purapService" />
	</bean>

	<bean id="PaymentRequest-grandTotalOverZeroValidation" class="org.kuali.kfs.module.purap.document.validation.impl.PaymentRequestGrandTotalOverZeroValidation" abstract="true"/>

	<bean id="PaymentRequest-payDateNotPastValidation" class="org.kuali.kfs.module.purap.document.validation.impl.PaymentRequestPayDateNotPastValidation" abstract="true">
		<property name="purapService" ref="purapService" />
		<property name="persistenceService" ref="persistenceService"/>
		<property name="businessObjectService" ref="businessObjectService" />
	</bean>

	<bean id="PaymentRequest-cancelValidation" class="org.kuali.kfs.module.purap.document.validation.impl.PaymentRequestCancelValidation" abstract="true"/>
	
	<bean id="PaymentRequest-vendorValidation" class="org.kuali.kfs.module.purap.document.validation.impl.PaymentRequestVendorValidation" abstract="true"/>

	<bean id="PaymentRequest-reviewValidation" class="org.kuali.kfs.module.purap.document.validation.impl.PaymentRequestReviewValidation" abstract="true">
		<property name="universityDateService" ref="universityDateService" />
	</bean>	

	<bean id="PaymentRequest-checkNegativeAccountsValidation" class="org.kuali.kfs.module.purap.document.validation.impl.PaymentRequestCheckNegativeAccountsValidation" abstract="true">		
		<property name="purapAccountingService" ref="purapAccountingService"/>
	</bean>	

	<bean id="PaymentRequest-reviewAccountingLine" parent="AccountingDocument-ReviewAccountingLine-DefaultValidation" scope="prototype"/>
	
	
	<bean id="PaymentRequest-processItemValidation" class="org.kuali.kfs.module.purap.document.validation.impl.PaymentRequestProcessItemValidation" abstract="true">
		<property name="purapService" ref="purapService" />
		<property name="reviewAccountingLineValidation" ref="PaymentRequest-reviewAccountingLine" />		
	</bean>        
    
    <!-- New Process Item Validation Definitions -->
    <bean id="PurchasingAccountsPayable-newProcessItemValidation-parentBean" class="org.kuali.kfs.sys.document.validation.impl.CompositeValidation" scope="prototype" abstract="true"> 								
        <property name="quitOnFail" value="false" />
        <property name="validations">
            <list>
                <!-- These are the validations from within the loop of items in newProcessItemValidation in
                     PurchasingAccountsPayableDocumentRuleBase -->
                <bean class="org.kuali.kfs.sys.document.validation.impl.CollectionValidation" scope="prototype">
                    <property name="collectionProperty" value="document.items" />
                    <property name="validations">
                        <list>
                            <bean parent="AccountingDocument-businessObjectDataDictionaryValidation" scope="prototype">
                                <property name="parameterProperties">
                                    <list>
                                        <bean parent="validationFieldConversion">
                                            <property name="sourceEventProperty" value="iterationSubject" />
                                            <property name="targetValidationProperty" value="businessObjectForValidation" />
                                        </bean>
                                    </list>
                                </property>
                            </bean>
                            
                            <!-- Need to do the itemConsideredEnteredBranchingValidation to determine whether
                                 we need to call the newIndividualItemValidation (not needed if item is not 
                                 considered entered -->
                            <bean parent="PurchasingAccountsPayable-itemConsideredEnteredBranchingValidation" scope="prototype">
			                    <property name="branchMap">
				                    <map>
					                    <entry>
						                    <key><value>needsIndividualItemValidation</value></key>
                                            <bean parent="PurchasingAccountsPayable-itemConsideredEnteredCompositeValidation-parentBean" scope="prototype"/>
					                    </entry>
				                    </map>
					            </property>
		                        <property name="parameterProperties">
						            <list>
				  	                    <bean parent="validationFieldConversion">
					   	                    <property name="sourceEventProperty" value="iterationSubject" />
								            <property name="targetValidationProperty" value="itemForValidation" />
							            </bean>
						            </list>
					            </property>
				            </bean>
				            <!-- End of itemConsideredEnteredBranchingValidation -->	
                        </list>
                    </property>
                </bean>	  		
                <!-- End of the newProcessItemValidation in PurchasingAccountsPayableDocumentRuleBase -->			                
            </list>
        </property>					
    </bean>  

    <bean id="PurchasingAccountsPayable-itemConsideredEnteredCompositeValidation-parentBean" class="org.kuali.kfs.sys.document.validation.impl.CompositeValidation" scope="prototype" abstract="true"> 								
        <property name="quitOnFail" value="false" />
        <property name="validations">
            <list>    
			  
                <bean parent="PurchasingAccountsPayable-newIndividualItemValidation" scope="prototype">
                    <property name="parameterProperties">
                        <list>
                            <bean parent="validationFieldConversion">
                                <property name="sourceEventProperty" value="iterationSubject" />
                                <property name="targetValidationProperty" value="itemForValidation" />
                            </bean>
                        </list>
                    </property>
                </bean>
				
				<!-- Need to do the purchasingAccountsPayableRequiresAccountValidationBranchingValidation to determine
				     whether we need to call the processAccountValidation -->
                <bean parent="PurchasingAccountsPayable-requiresAccountValidationBranchingValidation" scope="prototype">
			        <property name="branchMap">
				        <map>
					        <entry>
						        <key><value>needsAccountValidation</value></key>
                                <bean parent="PurchasingAccountsPayable-processAccountValidation" scope="prototype">
                                    <property name="parameterProperties">
                                        <list>
                                            <bean parent="validationFieldConversion">
                                                <property name="sourceEventProperty" value="iterationSubject" />
                                                <property name="targetValidationProperty" value="itemForValidation" />
                                            </bean>
                                        </list>
                                    </property>
                                </bean>
							</entry>
						</map>
					</property>
					<property name="parameterProperties">
			            <list>
					        <bean parent="validationFieldConversion">
					            <property name="sourceEventProperty" value="iterationSubject" />
								<property name="targetValidationProperty" value="itemForValidation" />
					        </bean>
						</list>
					</property>
				</bean>		
				<!-- End of purchasingAccountsPayableRequiresAccountValidationBranchingValidation -->				            				                        
            </list>
        </property>
    </bean>    	

</beans>
