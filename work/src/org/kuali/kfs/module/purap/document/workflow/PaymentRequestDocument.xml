<?xml version='1.0' encoding='UTF-8'?>
<data xmlns="ns:workflow" xmlns:fo="http://www.w3.org/1999/XSL/Format" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="ns:workflow resource:WorkflowData">
  <documentTypes xmlns="ns:workflow/DocumentType" xsi:schemaLocation="ns:workflow/DocumentType resource:DocumentType">
    <documentType>
      <name>PREQ</name>
      <parent>AccountsPayableTransactionalDocument</parent>
      <docHandler>${application.url}/purapPaymentRequest.do?methodToCall=docHandler</docHandler>
      <routePaths>
        <routePath>
          <start name="AdHoc" nextNode="RequiresImageAttachment"/>
          <split name="RequiresImageAttachment" nextNode="PurchaseWasReceived">
            <branch name="True">
              <requests name="ImageAttachment" nextNode="JoinRequiresImageAttachment"/>
            </branch>
            <branch name="False">
              <simple name="NoOpRequiresImageAttachment" nextNode="JoinRequiresImageAttachment"/>
            </branch>
            <join name="JoinRequiresImageAttachment"/>
          </split>
          <split name="PurchaseWasReceived" nextNode="SubAccount">
            <branch name="True">
              <requests name="Receiving" nextNode="JoinReceiving"/>
            </branch>
            <branch name="False">
              <simple name="NoOpReceiving" nextNode="JoinReceiving"/>
            </branch>
            <join name="JoinReceiving"/>
          </split>
          <requests name="SubAccount" nextNode="Account"/>
          <requests name="Account" nextNode="AccountingOrganizationHierarchy"/>
          <requests name="AccountingOrganizationHierarchy" nextNode="VendorIsEmployeeOrNonResidentAlien"/>
          <split name="VendorIsEmployeeOrNonResidentAlien">
            <branch name="True">
              <requests name="Tax" nextNode="JoinVendorIsEmployeeOrNonResidentAlien"/>
            </branch>
            <branch name="False">
              <simple name="NoOpVendorIsEmployeeOrNonResidentAlien" nextNode="JoinVendorIsEmployeeOrNonResidentAlien"/>
            </branch>
            <join name="JoinVendorIsEmployeeOrNonResidentAlien"/>
          </split>
        </routePath>
      </routePaths>
      <routeNodes>
        <start name="AdHoc"/>
        <role name="ImageAttachment">
          <qualifierResolverClass>org.kuali.rice.kns.workflow.attribute.DataDictionaryQualifierResolver</qualifierResolverClass>
        </role>
        <role name="Receiving">
          <qualifierResolverClass>org.kuali.rice.kns.workflow.attribute.DataDictionaryQualifierResolver</qualifierResolverClass>
        </role>
        <role name="SubAccount">
          <qualifierResolverClass>org.kuali.rice.kns.workflow.attribute.DataDictionaryQualifierResolver</qualifierResolverClass>
        </role>
        <role name="Account">
          <qualifierResolverClass>org.kuali.rice.kns.workflow.attribute.DataDictionaryQualifierResolver</qualifierResolverClass>
          <mandatoryRoute>true</mandatoryRoute>
        </role>
        <role name="AccountingOrganizationHierarchy">
          <qualifierResolverClass>org.kuali.rice.kns.workflow.attribute.DataDictionaryQualifierResolver</qualifierResolverClass>
        </role>
        <role name="Tax">
          <qualifierResolverClass>org.kuali.rice.kns.workflow.attribute.DataDictionaryQualifierResolver</qualifierResolverClass>
        </role>
        <split name="PurchaseWasReceived">
          <type>org.kuali.kfs.sys.document.workflow.SimpleBooleanSplitNode</type>
        </split>
        <split name="VendorIsEmployeeOrNonResidentAlien">
          <type>org.kuali.kfs.sys.document.workflow.SimpleBooleanSplitNode</type>
        </split>
        <simple name="NoOpVendorIsEmployeeOrNonResidentAlien">
          <type>org.kuali.rice.kew.engine.node.NoOpNode</type>
        </simple>
        <simple name="NoOpReceiving">
          <type>org.kuali.rice.kew.engine.node.NoOpNode</type>
        </simple>
        <join name="JoinReceiving"/>
        <split name="RequiresImageAttachment">
          <type>org.kuali.kfs.sys.document.workflow.SimpleBooleanSplitNode</type>
        </split>
        <simple name="NoOpRequiresImageAttachment">
          <type>org.kuali.rice.kew.engine.node.NoOpNode</type>
        </simple>
        <join name="JoinRequiresImageAttachment"/>
        <join name="JoinVendorIsEmployeeOrNonResidentAlien"/>
      </routeNodes>
    </documentType>
  </documentTypes>
</data>