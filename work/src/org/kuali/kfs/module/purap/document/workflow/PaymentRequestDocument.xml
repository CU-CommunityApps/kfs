<?xml version="1.0" encoding="UTF-8"?>
<data xmlns="ns:workflow" xmlns:fo="http://www.w3.org/1999/XSL/Format" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="ns:workflow resource:WorkflowData">
<documentTypes xmlns="ns:workflow/DocumentType" xsi:schemaLocation="ns:workflow/DocumentType resource:DocumentType">
<documentType>
<name>PaymentRequestDocument</name>
<parent>AccountsPayableTransactionalDocument</parent>
<docHandler>${application.url}/purapPaymentRequest.do?methodToCall=docHandler</docHandler>
<routePaths>
        <routePath>
          <start name="AdHoc" nextNode="RequiresInvoiceAttachment"/>
          <split name="RequiresInvoiceAttachment" nextNode="PurchaseWasReceived">
			<branch name="True">
				<requests name="InvoiceAttachment" nextNode="JoinRequiresInvoiceAttachment"/>
			</branch>
			<branch name="False">
				<simple name="NoOpRequiresInvoiceAttachment" nextNode="JoinRequiresInvoiceAttachment"/>
			</branch>
			<join name="JoinRequiresInvoiceAttachment"/>
		  </split> 
          <split name="PurchaseWasReceived" nextNode="SubAccount">
						<branch name="True">
							<requests name="Receiving" nextNode="Join"/>
						</branch>
						<branch name="False">
							<simple name="NoOpReceiving" nextNode="Join"/>
						</branch>
						<join name="Join"/>
					</split>
          <requests name="SubAccount" nextNode="Account"/>
          <requests name="Account" nextNode="OrganizationHierarchy"/>
          <requests name="OrganizationHierarchy" nextNode="Tax"/>
          <split name="VendorIsEmployeeOrNonResidentAlien">
						<branch name="True">
							<requests name="Tax" nextNode="Join"/>
						</branch>
						<branch name="False">
							<simple name="NoOp" nextNode="Join"/>
						</branch>
						<join name="Join"/>
					</split>
        </routePath>
      </routePaths>
<routeNodes>
<start name="AdHoc"/>
<role name="InvoiceAttachment">
<!--<ruleTemplate>KualiAccountsPayableTemplate</ruleTemplate>-->
<qualifierResolverClass>org.kuali.rice.kns.workflow.attribute.DataDictionaryQualifierResolver</qualifierResolverClass>
</role>
<role name="Receiving">
<!--<ruleTemplate>AwaitingReceivingStatusTemplate</ruleTemplate>-->
<qualifierResolverClass>org.kuali.rice.kns.workflow.attribute.DataDictionaryQualifierResolver</qualifierResolverClass>
</role>
<role name="SubAccount">
<!--<ruleTemplate>KualiSubAccountTemplate</ruleTemplate>-->
<qualifierResolverClass>org.kuali.rice.kns.workflow.attribute.DataDictionaryQualifierResolver</qualifierResolverClass>
</role>
<role name="Account">
<!--<ruleTemplate>KualiAccountTemplate</ruleTemplate>-->
<qualifierResolverClass>org.kuali.rice.kns.workflow.attribute.DataDictionaryQualifierResolver</qualifierResolverClass>
<mandatoryRoute>true</mandatoryRoute>
</role>
<role name="OrganizationHierarchy">
<!--<ruleTemplate>KualiOrgReviewTemplate</ruleTemplate>-->
<qualifierResolverClass>org.kuali.rice.kns.workflow.attribute.DataDictionaryQualifierResolver</qualifierResolverClass>
</role>
<role name="Tax">
<ruleTemplate>KualiVendorTaxReviewTemplate</ruleTemplate>
<qualifierResolverClass>org.kuali.rice.kns.workflow.attribute.DataDictionaryQualifierResolver</qualifierResolverClass>
</role>

<split name="PurchaseWasReceived">
<type>org.kuali.kfs.sys.document.routing.SimpleBooleanSplitNode</type>
</split>
<split name="VendorIsEmployeeOrNonResidentAlien">
<type>org.kuali.kfs.sys.document.routing.SimpleBooleanSplitNode</type>
</split>
<simple name="NoOp">
<type>org.kuali.rice.kew.engine.node.NoOpNode</type>
</simple>
<simple name="NoOpReceiving">
<type>org.kuali.rice.kew.engine.node.NoOpNode</type>
</simple>
<join name="Join"/>
<split name="RequiresInvoiceAttachment">
<type>org.kuali.kfs.sys.document.routing.SimpleBooleanSplitNode</type>
</split>
<simple name="NoOpRequiresInvoiceAttachment">
<type>org.kuali.rice.kew.engine.node.NoOpNode</type>
</simple>
<join name="JoinRequiresInvoiceAttachment"/>
</routeNodes>
</documentType>
</documentTypes>
</data>
