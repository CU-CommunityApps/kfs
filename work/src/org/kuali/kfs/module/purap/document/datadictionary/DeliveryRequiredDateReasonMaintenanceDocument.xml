<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="DeliveryRequiredDateReasonMaintenanceDocument" parent="DeliveryRequiredDateReasonMaintenanceDocument-parentBean" />

  <bean id="DeliveryRequiredDateReasonMaintenanceDocument-parentBean" abstract="true" parent="MaintenanceDocumentEntry">
    <property name="businessObjectClass" value="org.kuali.module.purap.bo.DeliveryRequiredDateReason" />
    <property name="maintainableClass" value="org.kuali.core.maintenance.KualiMaintainableImpl" />
    <property name="maintainableSections" >
      <list>
        <ref bean="DeliveryRequiredDateReasonMaintenanceDocument-EditDeliveryRequiredDateReason" />
      </list>
    </property>
    <property name="lockingKeys" >
      <list>
        <value>deliveryRequiredDateReasonCode</value>
      </list>
    </property>
    <property name="label" value="Delivery Required Date Reason" />
    <property name="shortLabel" value="Delivery Required Date Reason" />
    <property name="helpDefinition" >
      <bean parent="HelpDefinition"
            p:parameterNamespace="KFS-PA"
            p:parameterName="DELIVERY_REQUIRED_DATE_REASON"
            p:parameterDetailType="DeliveryRequiredDateReason" />
    </property>
    <property name="documentTypeName" value="DeliveryRequiredDateReasonMaintenanceDocument" />
    <property name="documentAuthorizerClass" value="org.kuali.core.document.authorization.MaintenanceDocumentAuthorizerBase" />
    <property name="documentTypeCode" value="PMDR" />
    <property name="workflowProperties" >
      <ref bean="DeliveryRequiredDateReasonMaintenanceDocument-workflowProperties" />
    </property>
    <property name="authorizations" >
      <list>
        <bean parent="AuthorizationDefinition">
          <property name="action" value="initiate" />
          <property name="authorizedGroups" >
            <set>
              <value>PA_PUR_MAINTENANCE_USERS</value>
            </set>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Maintenance Section Definitions -->


  <bean id="DeliveryRequiredDateReasonMaintenanceDocument-EditDeliveryRequiredDateReason" parent="DeliveryRequiredDateReasonMaintenanceDocument-EditDeliveryRequiredDateReason-parentBean" />

  <bean id="DeliveryRequiredDateReasonMaintenanceDocument-EditDeliveryRequiredDateReason-parentBean" abstract="true" parent="MaintainableSectionDefinition">
    <property name="id" value="Edit Delivery Required Date Reason" />
    <property name="title" value="Edit Delivery Required Date Reason" />
    <property name="maintainableItems" >
      <list>
        <bean parent="MaintainableFieldDefinition"
              p:name="deliveryRequiredDateReasonCode"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="deliveryRequiredDateReasonDescription"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="active" />
        <bean parent="MaintainableFieldDefinition"
              p:name="versionNumber" />
      </list>
    </property>
  </bean>

<!-- Exported Workflow Properties -->


  <bean id="DeliveryRequiredDateReasonMaintenanceDocument-workflowProperties" parent="DeliveryRequiredDateReasonMaintenanceDocument-workflowProperties-parentBean" />

  <bean id="DeliveryRequiredDateReasonMaintenanceDocument-workflowProperties-parentBean" abstract="true" parent="WorkflowProperties">
  </bean>
</beans>
