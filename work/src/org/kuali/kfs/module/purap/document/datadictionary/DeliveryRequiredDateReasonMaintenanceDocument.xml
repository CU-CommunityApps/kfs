<?xml version="1.0" encoding="UTF-8"?><beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p" xmlns:dd="http://rice.kuali.org/dd" xsi:schemaLocation="http://www.springframework.org/schema/beans         http://www.springframework.org/schema/beans/spring-beans-2.0.xsd         http://rice.kuali.org/dd         http://rice.kuali.org/dd/dd.xsd">

  <bean id="DeliveryRequiredDateReasonMaintenanceDocument" parent="DeliveryRequiredDateReasonMaintenanceDocument-parentBean"/>

  <bean id="DeliveryRequiredDateReasonMaintenanceDocument-parentBean" abstract="true" parent="MaintenanceDocumentEntry">
    <property name="businessObjectClass" value="org.kuali.kfs.module.purap.businessobject.DeliveryRequiredDateReason"/>
    <property name="maintainableClass" value="org.kuali.rice.kns.maintenance.KualiMaintainableImpl"/>
    <property name="maintainableSections">
      <list>
        <ref bean="DeliveryRequiredDateReasonMaintenanceDocument-EditDeliveryRequiredDateReason"/>
      </list>
    </property>
    <property name="lockingKeys">
      <list>
        <value>deliveryRequiredDateReasonCode</value>
      </list>
    </property>
    
    
    
    <property name="documentTypeName" value="PMDR"/>
    <property name="documentAuthorizerClass" value="org.kuali.kfs.sys.document.authorization.FinancialSystemMaintenanceDocumentAuthorizerBase"/>
    <property name="workflowAttributes">
    	<ref bean="DeliveryRequiredDateReasonMaintenanceDocument-workflowAttributes"/>
    </property>
  </bean>

<!-- Maintenance Section Definitions -->

  <bean id="DeliveryRequiredDateReasonMaintenanceDocument-EditDeliveryRequiredDateReason" parent="DeliveryRequiredDateReasonMaintenanceDocument-EditDeliveryRequiredDateReason-parentBean"/>

  <bean id="DeliveryRequiredDateReasonMaintenanceDocument-EditDeliveryRequiredDateReason-parentBean" abstract="true" parent="MaintainableSectionDefinition">
    <property name="id" value="Edit Delivery Required Date Reason"/>
    <property name="title" value="Edit Delivery Required Date Reason"/>
    <property name="maintainableItems">
      <list>
        <bean parent="MaintainableFieldDefinition" p:name="deliveryRequiredDateReasonCode" p:required="true"/>
        <bean parent="MaintainableFieldDefinition" p:name="deliveryRequiredDateReasonDescription" p:required="true"/>
        <bean parent="MaintainableFieldDefinition" p:name="active" p:defaultValue="true"/>
      </list>
    </property>
  </bean>
  
  <!-- workflow attributes -->
  <bean id="DeliveryRequiredDateReasonMaintenanceDocument-workflowAttributes" parent="DeliveryRequiredDateReasonMaintenanceDocument-workflowAttributes-parentBean"/>
  
  <bean id="DeliveryRequiredDateReasonMaintenanceDocument-workflowAttributes-parentBean" class="org.kuali.rice.kns.datadictionary.WorkflowAttributes" abstract="true"/>
</beans>