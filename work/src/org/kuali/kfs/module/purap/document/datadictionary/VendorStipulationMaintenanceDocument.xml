<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="VendorStipulationMaintenanceDocument" parent="VendorStipulationMaintenanceDocument-parentBean" />

  <bean id="VendorStipulationMaintenanceDocument-parentBean" abstract="true" parent="MaintenanceDocumentEntry">
    <property name="businessObjectClass" value="org.kuali.kfs.module.purap.businessobject.VendorStipulation" />
    <property name="maintainableClass" value="org.kuali.rice.kns.maintenance.KualiMaintainableImpl" />
    <property name="maintainableSections" >
      <list>
        <ref bean="VendorStipulationMaintenanceDocument-EditVendorStipulation" />
      </list>
    </property>
    <property name="lockingKeys" >
      <list>
        <value>vendorStipulationIdentifier</value>
      </list>
    </property>
    <property name="label" value="Vendor Stipulation" />
    <property name="shortLabel" value="Vendor Stipulation" />
    <property name="helpDefinition" >
      <bean parent="HelpDefinition" p:parameterClass="org.kuali.kfs.module.purap.businessobject.VendorStipulation" p:parameterName="VENDOR_STIPULATION" />
    </property>
    <property name="documentTypeName" value="VendorStipulationMaintenanceDocument" />
    <property name="businessRulesClass" value="org.kuali.kfs.module.purap.document.validation.impl.VendorStipulationRule" />
    <property name="documentAuthorizerClass" value="org.kuali.kfs.sys.document.authorization.FinancialSystemMaintenanceDocumentAuthorizerBase" />
    <property name="documentTypeCode" value="PMSI" />
    <property name="workflowProperties" >
      <ref bean="VendorStipulationMaintenanceDocument-workflowProperties" />
    </property>
    <property name="authorizations" >
      <list>
        <bean parent="AuthorizationDefinition">
          <property name="action" value="initiate" />
          <property name="authorizedGroups" >
            <set>
              <value>SY_MAINTENANCE_USERS</value>
            </set>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Maintenance Section Definitions -->


  <bean id="VendorStipulationMaintenanceDocument-EditVendorStipulation" parent="VendorStipulationMaintenanceDocument-EditVendorStipulation-parentBean" />

  <bean id="VendorStipulationMaintenanceDocument-EditVendorStipulation-parentBean" abstract="true" parent="MaintainableSectionDefinition">
    <property name="id" value="Edit Vendor Stipulation" />
    <property name="title" value="Edit Vendor Stipulation" />
    <property name="maintainableItems" >
      <list>
        <bean parent="MaintainableFieldDefinition"
              p:name="vendorStipulationIdentifier" />
        <bean parent="MaintainableFieldDefinition"
              p:name="vendorStipulationName"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="vendorStipulationDescription"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="active" />
      </list>
    </property>
  </bean>

<!-- Exported Workflow Properties -->


  <bean id="VendorStipulationMaintenanceDocument-workflowProperties" parent="VendorStipulationMaintenanceDocument-workflowProperties-parentBean" />

  <bean id="VendorStipulationMaintenanceDocument-workflowProperties-parentBean" abstract="true" parent="WorkflowProperties">
  </bean>
</beans>
