<?xml version="1.0" encoding="UTF-8"?><beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p" xmlns:dd="http://rice.kuali.org/dd" xsi:schemaLocation="http://www.springframework.org/schema/beans         http://www.springframework.org/schema/beans/spring-beans-2.0.xsd         http://rice.kuali.org/dd         http://rice.kuali.org/dd/dd.xsd">

  <bean id="PurchaseOrderCloseDocument" parent="PurchaseOrderCloseDocument-parentBean"/>

  <bean id="PurchaseOrderCloseDocument-parentBean" abstract="true" parent="TransactionalDocumentEntry">
    
    
    
    <property name="documentTypeName" value="POC"/>
    <property name="sessionDocument" value="true"/>
    <property name="documentClass" value="org.kuali.kfs.module.purap.document.PurchaseOrderCloseDocument"/>
    <property name="businessRulesClass" value="org.kuali.kfs.sys.document.validation.impl.AccountingRuleEngineRuleBase"/>
    <property name="documentAuthorizerClass" value="org.kuali.kfs.module.purap.document.authorization.PurchasingAccountsPayableTransactionalDocumentAuthorizerBase"/>
    <property name="documentPresentationControllerClass" value="org.kuali.kfs.module.purap.document.authorization.PurchaseOrderCloseDocumentPresentationController"/>
    <property name="workflowAttributes">
    	<ref bean="PurchaseOrderCloseDocument-workflowAttributes"/>
    </property>
    <property name="accountingLineGroups">
    	<map>
    		<entry>
    			<key><value>source</value></key>
	    		<ref bean="PurchaseOrderDocument-sourceAccountingLineGroup" parent="AccountingLineGroup"/>
	    	</entry>
    	</map>
    </property>
    <property name="validationMap" ref="PurchaseOrderCloseDocument-validations"/>
  </bean>

  
	<bean id="PurchaseOrderCloseDocument-validations" parent="PurchaseOrderCloseDocument-validations-parentBean"/>
  
    <bean id="PurchaseOrderCloseDocument-validations-parentBean" class="org.springframework.beans.factory.config.MapFactoryBean">
    	<property name="sourceMap">
	    	<map>			
				<entry>
					<key><value>org.kuali.kfs.sys.document.validation.event.AttributedRouteDocumentEvent</value></key>
					<value>PurchaseOrderClose-routeDocumentValidation</value>
				</entry>
				<entry>
					<key><value>org.kuali.kfs.sys.document.validation.event.AttributedApproveDocumentEvent</value></key>
					<value>PurchaseOrderClose-approveDocumentValidation</value>
				</entry>		
				<entry>
					<key><value>org.kuali.kfs.sys.document.validation.event.AddAccountingLineEvent</value></key>
					<value>PurchaseOrderClose-addAccountingLineValidation</value>
				</entry>
				<entry>
					<key><value>org.kuali.kfs.sys.document.validation.event.UpdateAccountingLineEvent</value></key>
					<value>PurchaseOrderClose-updateAccountingLineValidation</value>
				</entry>	
				<entry>
					<key><value>org.kuali.kfs.sys.document.validation.event.ReviewAccountingLineEvent</value></key>
					<value>PurchaseOrderClose-reviewAccountingLineValidation</value>
				</entry>
				<entry>
					<key><value>org.kuali.kfs.module.purap.document.validation.event.AttributedAddPurchasingAccountsPayableItemEvent</value></key>
					<value>PurchaseOrderClose-addPurchasingAccountsPayableItemValidation</value>				
				</entry>
				<entry>
					<key><value>org.kuali.kfs.module.purap.document.validation.event.AttributedImportPurchasingAccountsPayableItemEvent</value></key>
					<value>PurchaseOrderClose-importPurchasingAccountsPayableItemValidation</value>				
				</entry>																									
				<entry>
					<key><value>org.kuali.kfs.sys.document.validation.event.AttributedSaveDocumentEvent</value></key>
					<value>PurchaseOrderClose-saveDocumentValidation</value>				
				</entry>												
		    </map>
	    </property>	  
    </bean>
  
    <!-- workflow attributes -->
    <bean id="PurchaseOrderCloseDocument-workflowAttributes" parent="PurchaseOrderCloseDocument-workflowAttributes-parentBean"/>
  
    <bean id="PurchaseOrderCloseDocument-workflowAttributes-parentBean" class="org.kuali.rice.kns.datadictionary.WorkflowAttributes" abstract="true">
    	<property name="searchingTypeDefinitions">
    		<ref bean="SearchingTypeDefinitions-PurchaseOrderDocumentFamily" />
    	</property>
    </bean>

  
</beans>