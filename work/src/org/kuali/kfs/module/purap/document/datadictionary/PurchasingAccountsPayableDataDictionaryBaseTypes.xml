<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xmlns:dd="http://rice.kuali.org/dd" 
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd
        http://rice.kuali.org/dd
        http://rice.kuali.org/dd/dd.xsd">
      
   	<bean id="RoutingType-PurchasingDocument-Commodity" class="org.kuali.rice.kns.datadictionary.RoutingTypeDefinition">
    	<property name="routingAttributes">
    		<list>
				<ref bean="RoutingAttribute-campusCode" />
		        <bean class="org.kuali.rice.kns.datadictionary.RoutingAttribute">
			    	<property name="qualificationAttributeName" value="purchasingCommodityCode" />
			    </bean>			    
    		</list>
    	</property>
    	<property name="documentValuePathGroups">
    		<list>
				<bean class="org.kuali.rice.kns.datadictionary.DocumentValuePathGroup">
					<property name="documentValues">
						<list>
							<value>deliveryCampusCode</value>
    					</list>
    				</property>
    				<property name="documentCollectionPath">
						<bean class="org.kuali.rice.kns.datadictionary.DocumentCollectionPath">
							<property name="collectionPath" value="items" />
   							<property name="documentValues">
	   							<list>
	   								<value>purchasingCommodityCode</value>
	   							</list>
   							</property>
   						</bean>   
					</property>
				</bean>
    		</list>
    	</property>
   	</bean>
   	   	    
    <bean id="RoutingType-RequisitionDocument-Organization" class="org.kuali.rice.kns.datadictionary.RoutingTypeDefinition">
    	<property name="routingAttributes">
    		<list>
				<ref bean="RoutingAttribute-chartOfAccountsCode" />
				<ref bean="RoutingAttribute-organizationCode" />
    		</list>
    	</property>
    	<property name="documentValuePathGroups">
    		<list>
    		    <bean class="org.kuali.rice.kns.datadictionary.DocumentValuePathGroup">
    		        <property name="documentValues">
    					<list>
    			            <value>chartOfAccountsCode</value>
    			            <value>organizationCode</value>
    			        </list>
    			    </property>
    			</bean>
    		</list>
    	</property>
    </bean>
    
    <bean id="RoutingType-PurchasingAccountsPayableDocument-Account" class="org.kuali.rice.kns.datadictionary.RoutingTypeDefinition">
    	<property name="routingAttributes">
    		<list>
		        <ref bean="RoutingAttribute-financialDocumentTotalAmount" />
		        <ref bean="RoutingAttribute-chartOfAccountsCode" />
		        <ref bean="RoutingAttribute-accountNumber" />
    		</list>
    	</property>
    	<property name="documentValuePathGroups">
    		<list>
    			<ref bean="DocumentValuePathGroup-PurchasingAccountsPayableDocument-Account-accountingLines" />
    		</list>
    	</property>
    </bean>
    
    <bean id="DocumentValuePathGroup-PurchasingAccountsPayableDocument-Account-accountingLines" parent="DocumentValuePathGroup-AccountingDocument-financialDocumentTotalAmount">
    	<property name="documentCollectionPath">
			<bean class="org.kuali.rice.kns.datadictionary.DocumentCollectionPath">
				<property name="collectionPath" value="items" />
				<property name="nestedCollection">
    		<bean parent="DocumentCollectionPath-AccountingLine-chartAndAccountNumber">
						<property name="collectionPath" value="sourceAccountingLines" />
					</bean>
				</property>
    		</bean>
    	</property>
    </bean>

	<bean id="RoutingType-PurchasingAccountsPayableDocument-AccountingOrganizationHierarchy" class="org.kuali.rice.kns.datadictionary.RoutingTypeDefinition">
    	<property name="routingAttributes">
    		<list>
		        <ref bean="RoutingAttribute-financialDocumentTotalAmount" />
		        <ref bean="RoutingAttribute-chartOfAccountsCode" />
		        <ref bean="RoutingAttribute-organizationCode" />
		        <ref bean="RoutingAttribute-overrideCode" />
    		</list>
    	</property>
    	<property name="documentValuePathGroups">
    		<list>
    			<ref bean="DocumentValuePathGroup-PurchasingAccountsPayableDocument-AccountingOrganizationHierarchy-accountingLines" />
    		</list>
    	</property>
    </bean>

	<bean id="DocumentValuePathGroup-PurchasingAccountsPayableDocument-AccountingOrganizationHierarchy-accountingLines" parent="DocumentValuePathGroup-AccountingDocument-financialDocumentTotalAmount">
		<property name="documentCollectionPath">
			<bean class="org.kuali.rice.kns.datadictionary.DocumentCollectionPath">
				<property name="collectionPath" value="items" />
				<property name="nestedCollection">
					<bean parent="DocumentCollectionPath-AccountingLine-chartOrganizationAndOverrideCode">
						<property name="collectionPath" value="sourceAccountingLines" />
					</bean>
				</property>
			</bean>
		</property>
	</bean>

    <bean id="RoutingType-PurchaseOrderDocument-ContractManagement" class="org.kuali.rice.kns.datadictionary.RoutingTypeDefinition">
    	<property name="routingAttributes">
    		<list>
    		    <bean class="org.kuali.rice.kns.datadictionary.RoutingAttribute">
    		        <property name="qualificationAttributeName" value="contractManagerCode" />
    		    </bean>
    		</list>
    	</property>
    	<property name="documentValuePathGroups">
    		<list>
    		    <bean class="org.kuali.rice.kns.datadictionary.DocumentValuePathGroup">
    		        <property name="documentValues">
    					<list>
    			            <value>contractManagerCode</value>
    			        </list>
    			    </property>
    			</bean>
    		</list>
    	</property>
    </bean>
    
    <bean id="RoutingType-PurchasingAccountsPayableDocument-SubAccount" class="org.kuali.rice.kns.datadictionary.RoutingTypeDefinition">
    	<property name="routingAttributes">
    		<list>
		        <ref bean="RoutingAttribute-chartOfAccountsCode" />
		        <ref bean="RoutingAttribute-accountNumber" />
		        <bean class="org.kuali.rice.kns.datadictionary.RoutingAttribute">
    		        <property name="qualificationAttributeName" value="subAccountNumber" />
    		    </bean>
    		</list>
    	</property>
    	<property name="documentValuePathGroups">
    		<list>
    			<ref bean="DocumentValuePathGroup-PurchasingAccountsPayableDocument-SubAccount-accountingLines" />
    		</list>
    	</property>
    </bean>

    <bean id="DocumentValuePathGroup-PurchasingAccountsPayableDocument-SubAccount-accountingLines"  class="org.kuali.rice.kns.datadictionary.DocumentValuePathGroup">
    	<property name="documentCollectionPath">
			<bean class="org.kuali.rice.kns.datadictionary.DocumentCollectionPath">
				<property name="collectionPath" value="items" />
				<property name="nestedCollection">
    		        <bean parent="DocumentCollectionPath-AccountingLine-chartAndAccountNumberAndSubAccountNumber">
				        <property name="collectionPath" value="sourceAccountingLines" />
				    </bean>
				</property>
    		</bean>
    	</property>
    </bean>

   	<bean id="DocumentCollectionPath-AccountingLine-chartAndAccountNumberAndSubAccountNumber" class="org.kuali.rice.kns.datadictionary.DocumentCollectionPath" abstract="true">
   		<property name="documentValues">
	   		<list>
	   			<value>chartOfAccountsCode</value>
	   			<value>accountNumber</value>
	   			<value>subAccountNumber</value>
	   		</list>
   		</property>
   	</bean>
   	            
<!-- All of the Search Type Attributes -->
        
	<bean id="SearchingType-PurchasingAccountsPayableDocument-organizationCode" class="org.kuali.rice.kns.datadictionary.SearchingTypeDefinition">
        <property name="searchingAttribute" ref="SearchingAttribute-organizationCode" />
        <property name="documentValues">
        	<list>
        		<value>organizationCode</value>
        	</list>
        </property>
    </bean>
    
	<bean id="SearchingType-PurchasingAccountsPayableDocument-financialDocumentTotalAmount" class="org.kuali.rice.kns.datadictionary.SearchingTypeDefinition">
        <property name="searchingAttribute" ref="SearchingAttribute-financialDocumentTotalAmount" />
        <property name="documentValues">
        	<list>
        		<value>documentHeader.financialDocumentTotalAmount</value>
        	</list>
        </property>
    </bean>
    <bean id="SearchingAttribute-financialDocumentTotalAmount" parent="WorkflowAttributeMetaData-financialDocumentTotalAmount" class="org.kuali.rice.kns.datadictionary.SearchingAttribute" />
  
</beans>
