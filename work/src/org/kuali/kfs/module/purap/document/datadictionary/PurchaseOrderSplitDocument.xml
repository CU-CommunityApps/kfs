    <property name="shortLabel" value="Purchase Order Split" />
    <property name="helpDefinition" >
      <bean parent="HelpDefinition"
            p:parameterNamespace="KFS-PURAP"
            p:parameterName="PURCHASE_ORDER"
            p:parameterDetailType="PurchaseOrder" />
    </property>
    <property name="documentTypeName" value="PurchaseOrderSplitDocument" />
    <property name="sessionDocument" value="true" />
    <property name="documentClass" value="org.kuali.kfs.module.purap.document.PurchaseOrderSplitDocument" />
