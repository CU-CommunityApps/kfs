<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="RecurringPaymentFrequencyMaintenanceDocument" parent="RecurringPaymentFrequencyMaintenanceDocument-parentBean" />

  <bean id="RecurringPaymentFrequencyMaintenanceDocument-parentBean" abstract="true" parent="MaintenanceDocumentEntry">
    <property name="businessObjectClass" value="org.kuali.kfs.module.purap.businessobject.RecurringPaymentFrequency" />
    <property name="maintainableClass" value="org.kuali.rice.kns.maintenance.KualiMaintainableImpl" />
    <property name="maintainableSections" >
      <list>
        <ref bean="RecurringPaymentFrequencyMaintenanceDocument-EditRecurringPmtFrequency" />
      </list>
    </property>
    <property name="lockingKeys" >
      <list>
        <value>recurringPaymentFrequencyCode</value>
      </list>
    </property>
    <property name="label" value="Recurring Payment Frequency" />
    <property name="shortLabel" value="Recurring Payment Frequency" />
    <property name="helpDefinition" >
      <bean parent="HelpDefinition" p:parameterClass="org.kuali.kfs.module.purap.businessobject.RecurringPaymentFrequency" p:parameterName="RECURRING_PAYMENT_FREQUENCY" />
    </property>
    <property name="documentTypeName" value="RecurringPaymentFrequencyMaintenanceDocument" />
    <property name="documentAuthorizerClass" value="org.kuali.kfs.sys.document.authorization.FinancialSystemMaintenanceDocumentAuthorizerBase" />
    <property name="documentTypeCode" value="PMRF" />
    <property name="workflowProperties" >
      <ref bean="RecurringPaymentFrequencyMaintenanceDocument-workflowProperties" />
    </property>
    <property name="authorizations" >
      <list>
        <bean parent="AuthorizationDefinition">
          <property name="action" value="initiate" />
          <property name="authorizedGroups" >
            <set>
              <value>SY_MAINTENANCE_USERS</value>
            </set>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Maintenance Section Definitions -->


  <bean id="RecurringPaymentFrequencyMaintenanceDocument-EditRecurringPmtFrequency" parent="RecurringPaymentFrequencyMaintenanceDocument-EditRecurringPmtFrequency-parentBean" />

  <bean id="RecurringPaymentFrequencyMaintenanceDocument-EditRecurringPmtFrequency-parentBean" abstract="true" parent="MaintainableSectionDefinition">
    <property name="id" value="Edit Recurring Pmt Frequency" />
    <property name="title" value="Edit Recurring Pmt Frequency" />
    <property name="maintainableItems" >
      <list>
        <bean parent="MaintainableFieldDefinition"
              p:name="recurringPaymentFrequencyCode"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="recurringPaymentFrequencyDescription"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="active" />
      </list>
    </property>
  </bean>

<!-- Exported Workflow Properties -->


  <bean id="RecurringPaymentFrequencyMaintenanceDocument-workflowProperties" parent="RecurringPaymentFrequencyMaintenanceDocument-workflowProperties-parentBean" />

  <bean id="RecurringPaymentFrequencyMaintenanceDocument-workflowProperties-parentBean" abstract="true" parent="WorkflowProperties">
  </bean>
</beans>
