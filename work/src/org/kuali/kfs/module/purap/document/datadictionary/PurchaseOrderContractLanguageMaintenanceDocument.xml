<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="PurchaseOrderContractLanguageMaintenanceDocument" parent="PurchaseOrderContractLanguageMaintenanceDocument-parentBean" />

  <bean id="PurchaseOrderContractLanguageMaintenanceDocument-parentBean" abstract="true" parent="MaintenanceDocumentEntry">
    <property name="businessObjectClass" value="org.kuali.module.purap.bo.PurchaseOrderContractLanguage" />
    <property name="maintainableClass" value="org.kuali.module.purap.maintenance.PurchaseOrderContractLanguageMaintainableImpl" />
    <property name="maintainableSections" >
      <list>
        <ref bean="PurchaseOrderContractLanguageMaintenanceDocument-EditPOContractLanguage" />
      </list>
    </property>
    <property name="lockingKeys" >
      <list>
        <value>purchaseOrderContractLanguageIdentifier</value>
      </list>
    </property>
    <property name="label" value="Purchase Order Contract Language" />
    <property name="shortLabel" value="Purchase Order Contract Language" />
    <property name="helpDefinition" >
      <bean parent="HelpDefinition"
            p:parameterNamespace="KFS-PA"
            p:parameterName="PURCHASE_ORDER_CONTRACT_LANGUAGE"
            p:parameterDetailType="PurchaseOrderContractLanguage" />
    </property>
    <property name="documentTypeName" value="PurchaseOrderContractLanguageMaintenanceDocument" />
    <property name="businessRulesClass" value="org.kuali.module.purap.rules.PurchaseOrderContractLanguageRule" />
    <property name="documentAuthorizerClass" value="org.kuali.core.document.authorization.MaintenanceDocumentAuthorizerBase" />
    <property name="documentTypeCode" value="PMCL" />
    <property name="workflowProperties" >
      <ref bean="PurchaseOrderContractLanguageMaintenanceDocument-workflowProperties" />
    </property>
    <property name="authorizations" >
      <list>
        <bean parent="AuthorizationDefinition">
          <property name="action" value="initiate" />
          <property name="authorizedGroups" >
            <set>
              <value>SY_MAINTENANCE_USERS</value>
            </set>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Maintenance Section Definitions -->


  <bean id="PurchaseOrderContractLanguageMaintenanceDocument-EditPOContractLanguage" parent="PurchaseOrderContractLanguageMaintenanceDocument-EditPOContractLanguage-parentBean" />

  <bean id="PurchaseOrderContractLanguageMaintenanceDocument-EditPOContractLanguage-parentBean" abstract="true" parent="MaintainableSectionDefinition">
    <property name="id" value="Edit PO Contract Language" />
    <property name="title" value="Edit PO Contract Language" />
    <property name="maintainableItems" >
      <list>
        <bean parent="MaintainableFieldDefinition"
              p:name="versionNumber" />
        <bean parent="MaintainableFieldDefinition"
              p:name="purchaseOrderContractLanguageIdentifier" />
        <bean parent="MaintainableFieldDefinition"
              p:name="campusCode"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="purchaseOrderContractLanguageDescription"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="contractLanguageCreateDate"
              p:readOnly="true"
              p:defaultValueFinderClass="org.kuali.kfs.lookup.valuefinder.CurrentDateMMDDYYYYFinder" />
        <bean parent="MaintainableFieldDefinition"
              p:name="active" />
      </list>
    </property>
  </bean>

<!-- Exported Workflow Properties -->


  <bean id="PurchaseOrderContractLanguageMaintenanceDocument-workflowProperties" parent="PurchaseOrderContractLanguageMaintenanceDocument-workflowProperties-parentBean" />

  <bean id="PurchaseOrderContractLanguageMaintenanceDocument-workflowProperties-parentBean" abstract="true" parent="WorkflowProperties">
  </bean>
</beans>
