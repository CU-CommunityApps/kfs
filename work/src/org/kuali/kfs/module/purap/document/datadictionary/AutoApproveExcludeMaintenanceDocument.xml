<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "../../../../core/datadictionary/dataDictionary-1_0.dtd">

<dictionaryEntry>
	<maintenanceDocument>
		<businessObjectClass>
			org.kuali.module.purap.bo.AutoApproveExclude
		</businessObjectClass>
		<maintainableClass>
			org.kuali.core.maintenance.KualiMaintainableImpl
		</maintainableClass>

		<documentAuthorizerClass>
			org.kuali.core.document.authorization.MaintenanceDocumentAuthorizerBase
		</documentAuthorizerClass>
		<authorizations>
			<authorization action="initiate">
				<workgroups>
					<workgroup>PURAP_AP_ADMIN</workgroup>
				</workgroups>
			</authorization>
		</authorizations>

		<documentTypeName>
			AutoApproveExcludeMaintenanceDocument
		</documentTypeName>
		<documentTypeCode>PMAA</documentTypeCode>

		<label>Auto Approve Exclude Maintenance Document</label>
		<shortLabel>Auto Approve Exclude Maint Doc</shortLabel>

		<lockingKeys>
			<field attributeName="accountNumber" />
		</lockingKeys>

		<defaultExistenceChecks>
			<reference attributeName="chartOfAccounts"
				attributeToHighlightOnFail="chartOfAccountsCode" />
			<reference attributeName="account"
				attributeToHighlightOnFail="accountNumber" />
		</defaultExistenceChecks>

		<maintainableSections>
			<maintainableSection title="Edit Auto Approve Exclude">
				<maintainableItems>
					<maintainableField name="chartOfAccountsCode"
						required="true" />
					<maintainableField name="accountNumber"
						required="true" />
					<maintainableField name="active" />
					<maintainableField name="versionNumber" />
				</maintainableItems>
			</maintainableSection>
		</maintainableSections>
	</maintenanceDocument>
</dictionaryEntry>
