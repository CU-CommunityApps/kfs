<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "../dataDictionary-1_0.dtd">
<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.module.budget.bo.CalculatedSalaryFoundationTrackerOverride">
        <customAttributeEnabled>false</customAttributeEnabled>

        <inquiry>
            <title>Calculated Salary Foundation Tracker Override Inquiry</title>

            <inquirySections><inquirySection title="Header Text"><inquiryFields>
                <field attributeName="universityFiscalYear" />
                <field attributeName="chartOfAccountsCode" />
                <field attributeName="accountNumber" />
                <field attributeName="subAccountNumber" />
                <field attributeName="financialObjectCode" />
                <field attributeName="financialSubObjectCode" />
                <field attributeName="positionNumber" />
                <field attributeName="emplid" />
                <field attributeName="csfAmount" />
                <field attributeName="csfFullTimeEmploymentQuantity" />
                <field attributeName="csfTimePercent" />
                <field attributeName="csfFundingStatusCode" />
                <field attributeName="csfDeleteCode" />
            </inquiryFields></inquirySection></inquirySections>
        </inquiry>

        <lookup>
            <title>Calculated Salary Foundation Tracker Override Lookup</title>

            <instructions>Look up a Calculated Salary Foundation Tracker Override</instructions>

            <defaultSort sortAscending="true">
                <sortAttributes>
                    <sortAttribute attributeName="universityFiscalYear" />
                    <sortAttribute attributeName="chartOfAccountsCode" />
                    <sortAttribute attributeName="accountNumber" />
                </sortAttributes>
            </defaultSort>

            <lookupFields>
                <lookupField attributeName="universityFiscalYear" required="false"  />
                <lookupField attributeName="chartOfAccountsCode" required="false" />
                <lookupField attributeName="accountNumber" required="false" />
                <lookupField attributeName="subAccountNumber" required="false" />
                <lookupField attributeName="financialObjectCode" required="false" />
                <lookupField attributeName="financialSubObjectCode" required="false" />
                <lookupField attributeName="positionNumber" required="false" />
                <lookupField attributeName="emplid" required="false" />
                <lookupField attributeName="csfAmount" required="false" />
                <lookupField attributeName="csfFullTimeEmploymentQuantity" required="false" />
                <lookupField attributeName="csfTimePercent" required="false" />
                <lookupField attributeName="csfFundingStatusCode" required="false" />
                <lookupField attributeName="csfDeleteCode" required="false" />
            </lookupFields>

            <resultFields>
                <field attributeName="universityFiscalYear" noInquiry="true" />
                <field attributeName="chartOfAccountsCode" />
                <field attributeName="accountNumber" />
                <field attributeName="subAccountNumber" />
                <field attributeName="financialObjectCode" />
                <field attributeName="financialSubObjectCode" />
                <field attributeName="positionNumber" />
                <field attributeName="emplid" />
                <field attributeName="csfAmount" />
                <field attributeName="csfFullTimeEmploymentQuantity" />
                <field attributeName="csfTimePercent" />
                <field attributeName="csfFundingStatusCode" />
                <field attributeName="csfDeleteCode" />
            </resultFields>
        </lookup>

        <attributes>

            <attributeReference name="accountNumber" sourceClassName="org.kuali.module.chart.bo.Account" sourceAttributeName="accountNumber">
                <required>true</required>
                <description>Account Number for the override.</description>
            </attributeReference>


            <attributeReference name="chartOfAccountsCode" sourceClassName="org.kuali.module.chart.bo.Chart" sourceAttributeName="chartOfAccountsCode">
                <required>true</required>
			  <control>
				 <select valuesFinderClass="org.kuali.module.chart.lookup.keyvalues.ChartValuesFinder" />
			  </control>
                <description>Chart of Accounts Code for the override.</description>
            </attributeReference>


            <attribute name="csfAmount" forceUppercase="true">
                <label>CSF Amount</label>
                <shortLabel>CSF Amount</shortLabel>

                <maxLength>19</maxLength>

                <validationPattern>
                    <fixedPoint allowNegative="false" precision="19" scale="2" />
                </validationPattern>
                <required>true</required>

                <control>
                    <text size="21" />
                </control>

                <summary>Position CSF Amount</summary>
                <description>Position CSF Amount for the override.</description>
			</attribute>

            <attribute name="csfDeleteCode" forceUppercase="true">
                <label>Csf Delete Code</label>
                <shortLabel>Delete Code</shortLabel>

                <maxLength>1</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="3" />
                </control>

                <summary>Position CSF Delete Code</summary>
                <description>Position CSF Delete Code for the override.</description>
			</attribute>

            <attribute name="csfFullTimeEmploymentQuantity" forceUppercase="true">
                <label>CSF FTE</label>
                <shortLabel>CSF FTE</shortLabel>

                <maxLength>9</maxLength>
                 <validationPattern>
                    <floatingPoint allowNegative="false" />
                </validationPattern>

                <control>
                    <text size="11" />
                </control>

                <summary>Position CSF Full Time Equivalent</summary>
                <description>Position CSF Full Time Equivalent for the override.</description>
			</attribute>

            <attribute name="csfFundingStatusCode" forceUppercase="true">
                <label>CSF Funding Status Code</label>
                <shortLabel>CSF Funding Status</shortLabel>

                <maxLength>1</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="3" />
                </control>

                <summary>Position CSF Funding Status Code</summary>
                <description>Position CSF Funding Status Code for the override.</description>
			</attribute>

            <attribute name="csfTimePercent" forceUppercase="true">
                <label>CSF Time Percent Time</label>
                <shortLabel>Percent Time</shortLabel>

                <maxLength>9</maxLength>

                <validationPattern>
                    <floatingPoint allowNegative="false" />
                </validationPattern>

                <control>
                    <text size="11" />
                </control>

                <summary>Position CSF Time Percent Time</summary>
                <description>Position CSF Time Percent Time for the override</description>
			</attribute>

            <attribute name="emplid" forceUppercase="true">
                <label>Employee ID</label>
                <shortLabel>Employee ID</shortLabel>

                <maxLength>11</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="13" />
                </control>

                <summary>Employee ID</summary>
                <description>Employee ID</description>
			</attribute>

             <attributeReference name="financialObjectCode" sourceClassName="org.kuali.module.chart.bo.ObjectCode" sourceAttributeName="financialObjectCode">
                <required>true</required>
                 <description>Financial Object Code for the override</description>                
            </attributeReference>


            <attributeReference name="financialSubObjectCode" sourceClassName="org.kuali.module.chart.bo.SubObjCd" sourceAttributeName="financialSubObjectCode">
                <required>false</required>
                    <description>Sub-Object Code for the override.</description>
            </attributeReference>


            <attribute name="positionNumber" forceUppercase="true">
                <label>Position Number</label>
                <shortLabel>Position Number</shortLabel>

                <maxLength>8</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="10" />
                </control>

                <summary>Position Number</summary>
                <description>Position Number for the override.</description>
			</attribute>

            <attributeReference name="subAccountNumber" sourceClassName="org.kuali.module.chart.bo.SubAccount" sourceAttributeName="subAccountNumber">
                <description>Sub-Account number for the override</description>
            </attributeReference>

            <attribute name="universityFiscalYear" forceUppercase="true" >
                <label>University Fiscal Year</label>
                <shortLabel>Year</shortLabel>

                <maxLength>4</maxLength>
                <validationPattern><numeric /></validationPattern>

                <control>
                    <text size="6" />
                </control>

                <summary>University Fiscal Year</summary>
                <description>University Fiscal Year</description>
			</attribute>

            <attributeReference name="versionNumber" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="versionNumber" />

        </attributes>
    </businessObject>
</dictionaryEntry>
