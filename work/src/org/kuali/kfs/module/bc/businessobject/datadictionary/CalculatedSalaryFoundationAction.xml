<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="CalculatedSalaryFoundationAction" parent="CalculatedSalaryFoundationAction-parentBean" />

  <bean id="CalculatedSalaryFoundationAction-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.kfs.module.bc.businessobject.CalculatedSalaryFoundationAction" />
    <property name="inquiryDefinition" >
      <ref bean="CalculatedSalaryFoundationAction-inquiryDefinition" />
    </property>
    <property name="lookupDefinition" >
      <ref bean="CalculatedSalaryFoundationAction-lookupDefinition" />
    </property>
    <property name="objectLabel" value="Calculated Salary Foundation Action" />
    <property name="attributes" >
      <list>
        <ref bean="CalculatedSalaryFoundationAction-action" />
        <ref bean="CalculatedSalaryFoundationAction-actionReason" />
        <ref bean="CalculatedSalaryFoundationAction-csfFundingStatusCode" />
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->


  <bean id="CalculatedSalaryFoundationAction-action" parent="CalculatedSalaryFoundationAction-action-parentBean" />

  <bean id="CalculatedSalaryFoundationAction-action-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="action" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Action" />
    <property name="shortLabel" value="Action" />
    <property name="maxLength" value="3" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="5" />
    </property>
    <property name="summary" value="action" />
    <property name="description" value="action description..." />
  </bean>
  <bean id="CalculatedSalaryFoundationAction-actionReason" parent="CalculatedSalaryFoundationAction-actionReason-parentBean" />

  <bean id="CalculatedSalaryFoundationAction-actionReason-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="actionReason" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Action Reason" />
    <property name="shortLabel" value="Reason" />
    <property name="maxLength" value="3" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="5" />
    </property>
    <property name="summary" value="actionReason" />
    <property name="description" value="actionReason description..." />
  </bean>
  <bean id="CalculatedSalaryFoundationAction-csfFundingStatusCode" parent="CalculatedSalaryFoundationAction-csfFundingStatusCode-parentBean" />

  <bean id="CalculatedSalaryFoundationAction-csfFundingStatusCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="csfFundingStatusCode" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Csf Funding Status Code" />
    <property name="shortLabel" value="Code" />
    <property name="maxLength" value="1" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="3" />
    </property>
    <property name="summary" value="csfFundingStatusCode" />
    <property name="description" value="csfFundingStatusCode description..." />
  </bean>

<!-- Business Object Inquiry Definition -->


  <bean id="CalculatedSalaryFoundationAction-inquiryDefinition" parent="CalculatedSalaryFoundationAction-inquiryDefinition-parentBean" />

  <bean id="CalculatedSalaryFoundationAction-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
    <property name="title" value="Calculated Salary Foundation Action Inquiry" />
    <property name="inquirySections" >
      <list>
        <bean parent="InquirySectionDefinition">
          <property name="title" value="" />
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields" >
            <list>
              <bean parent="FieldDefinition"
                    p:attributeName="action" />
              <bean parent="FieldDefinition"
                    p:attributeName="actionReason" />
              <bean parent="FieldDefinition"
                    p:attributeName="csfFundingStatusCode" />
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Business Object Lookup Definition -->


  <bean id="CalculatedSalaryFoundationAction-lookupDefinition" parent="CalculatedSalaryFoundationAction-lookupDefinition-parentBean" />

  <bean id="CalculatedSalaryFoundationAction-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
    <property name="title" value="Calculated Salary Foundation Action Lookup" />
    <property name="instructions" value="Look up a Calculated Salary Foundation Action" />
    <property name="defaultSort" >
      <bean parent="SortDefinition">
      </bean>
    </property>
    <property name="lookupFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="action" />
        <bean parent="FieldDefinition"
              p:attributeName="actionReason" />
        <bean parent="FieldDefinition"
              p:attributeName="csfFundingStatusCode" />
      </list>
    </property>
    <property name="resultFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="action" />
        <bean parent="FieldDefinition"
              p:attributeName="actionReason" />
        <bean parent="FieldDefinition"
              p:attributeName="csfFundingStatusCode" />
      </list>
    </property>
  </bean>
</beans>
