<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="BudgetConstructionReasonCodePick" parent="BudgetConstructionReasonCodePick-parentBean" />

  <bean id="BudgetConstructionReasonCodePick-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.module.budget.bo.BudgetConstructionReasonCodePick" />
    <property name="inquiryDefinition" >
      <ref bean="BudgetConstructionReasonCodePick-inquiryDefinition" />
    </property>
    <property name="lookupDefinition" >
      <ref bean="BudgetConstructionReasonCodePick-lookupDefinition" />
    </property>
    <property name="attributes" >
      <list>
        <ref bean="BudgetConstructionReasonCodePick-appointmentFundingReasonCode" />
        <ref bean="BudgetConstructionReasonCodePick-personUniversalIdentifier" />
        <ref bean="BudgetConstructionReasonCodePick-selectFlag" />
        <ref bean="BudgetConstructionReasonCodePick-appointmentFundingReason.appointmentFundingReasonDescription" />
        <ref bean="BudgetConstructionReasonCodePick-versionNumber" />
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->


  <bean id="BudgetConstructionReasonCodePick-appointmentFundingReasonCode" parent="BudgetConstructionReasonCodePick-appointmentFundingReasonCode-parentBean" />

  <bean id="BudgetConstructionReasonCodePick-appointmentFundingReasonCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="appointmentFundingReasonCode" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Appointment Funding Reason Code" />
    <property name="shortLabel" value="Code" />
    <property name="maxLength" value="3" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="5" />
    </property>
    <property name="summary" value="appointmentFundingReasonCode" />
    <property name="description" value="appointmentFundingReasonCode description..." />
  </bean>
  <bean id="BudgetConstructionReasonCodePick-personUniversalIdentifier" parent="BudgetConstructionReasonCodePick-personUniversalIdentifier-parentBean" />

  <bean id="BudgetConstructionReasonCodePick-personUniversalIdentifier-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="personUniversalIdentifier" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Person Universal Identifier" />
    <property name="shortLabel" value="Identifier" />
    <property name="maxLength" value="10" />
    <property name="validationPattern" >
      <bean parent="NumericValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="12" />
    </property>
    <property name="summary" value="personUniversalIdentifier" />
    <property name="description" value="personUniversalIdentifier description..." />
  </bean>
  <bean id="BudgetConstructionReasonCodePick-selectFlag" parent="BudgetConstructionReasonCodePick-selectFlag-parentBean" />

  <bean id="BudgetConstructionReasonCodePick-selectFlag-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="selectFlag" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Select Flag" />
    <property name="shortLabel" value="Flag" />
    <property name="maxLength" value="7" />
    <property name="validationPattern" >
      <bean parent="NumericValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="9" />
    </property>
    <property name="summary" value="selectFlag" />
    <property name="description" value="selectFlag description..." />
  </bean>
  <bean id="BudgetConstructionReasonCodePick-appointmentFundingReason.appointmentFundingReasonDescription" parent="BudgetConstructionReasonCodePick-appointmentFundingReason.appointmentFundingReasonDescription-parentBean" />

  <bean id="BudgetConstructionReasonCodePick-appointmentFundingReason.appointmentFundingReasonDescription-parentBean" abstract="true" parent="BudgetConstructionAppointmentFundingReasonCode-appointmentFundingReasonDescription">
    <property name="name" value="appointmentFundingReason.appointmentFundingReasonDescription" />
  </bean>
  <bean id="BudgetConstructionReasonCodePick-versionNumber" parent="BudgetConstructionReasonCodePick-versionNumber-parentBean" />

  <bean id="BudgetConstructionReasonCodePick-versionNumber-parentBean" abstract="true" parent="GenericAttributes-versionNumber">
  </bean>

<!-- Business Object Inquiry Definition -->


  <bean id="BudgetConstructionReasonCodePick-inquiryDefinition" parent="BudgetConstructionReasonCodePick-inquiryDefinition-parentBean" />

  <bean id="BudgetConstructionReasonCodePick-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
    <property name="title" value="Budget Construction Reason Code Pick Inquiry" />
    <property name="inquirySections" >
      <list>
        <bean parent="InquirySectionDefinition">
          <property name="title" value="Header Text" />
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields" >
            <list>
              <bean parent="FieldDefinition"
                    p:attributeName="appointmentFundingReasonCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="personUniversalIdentifier" />
              <bean parent="FieldDefinition"
                    p:attributeName="selectFlag" />
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Business Object Lookup Definition -->


  <bean id="BudgetConstructionReasonCodePick-lookupDefinition" parent="BudgetConstructionReasonCodePick-lookupDefinition-parentBean" />

  <bean id="BudgetConstructionReasonCodePick-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
    <property name="title" value="Budget Construction Reason Code Pick Lookup" />
    <property name="instructions" value="Look up a Budget Construction Reason Code Pick" />
    <property name="defaultSort" >
      <bean parent="SortDefinition">
      </bean>
    </property>
    <property name="lookupFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="appointmentFundingReasonCode" />
        <bean parent="FieldDefinition"
              p:attributeName="personUniversalIdentifier" />
        <bean parent="FieldDefinition"
              p:attributeName="selectFlag" />
      </list>
    </property>
    <property name="resultFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="appointmentFundingReasonCode" />
        <bean parent="FieldDefinition"
              p:attributeName="personUniversalIdentifier" />
        <bean parent="FieldDefinition"
              p:attributeName="selectFlag" />
      </list>
    </property>
  </bean>
</beans>
