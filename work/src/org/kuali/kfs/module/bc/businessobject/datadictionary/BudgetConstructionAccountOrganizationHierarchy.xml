<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="BudgetConstructionAccountOrganizationHierarchy" parent="BudgetConstructionAccountOrganizationHierarchy-parentBean" />

  <bean id="BudgetConstructionAccountOrganizationHierarchy-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.module.budget.bo.BudgetConstructionAccountOrganizationHierarchy" />
    <property name="inquiryDefinition" >
      <ref bean="BudgetConstructionAccountOrganizationHierarchy-inquiryDefinition" />
    </property>
    <property name="lookupDefinition" >
      <ref bean="BudgetConstructionAccountOrganizationHierarchy-lookupDefinition" />
    </property>
    <property name="attributes" >
      <list>
        <ref bean="BudgetConstructionAccountOrganizationHierarchy-accountNumber" />
        <ref bean="BudgetConstructionAccountOrganizationHierarchy-chartOfAccountsCode" />
        <ref bean="BudgetConstructionAccountOrganizationHierarchy-organizationChartOfAccountsCode" />
        <ref bean="BudgetConstructionAccountOrganizationHierarchy-organizationCode" />
        <ref bean="BudgetConstructionAccountOrganizationHierarchy-organizationLevelCode" />
        <ref bean="BudgetConstructionAccountOrganizationHierarchy-universityFiscalYear" />
        <ref bean="BudgetConstructionAccountOrganizationHierarchy-versionNumber" />
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->


  <bean id="BudgetConstructionAccountOrganizationHierarchy-accountNumber" parent="BudgetConstructionAccountOrganizationHierarchy-accountNumber-parentBean" />

  <bean id="BudgetConstructionAccountOrganizationHierarchy-accountNumber-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="accountNumber" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Account Number" />
    <property name="shortLabel" value="Number" />
    <property name="maxLength" value="7" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="9" />
    </property>
    <property name="summary" value="accountNumber" />
    <property name="description" value="accountNumber description..." />
  </bean>
  <bean id="BudgetConstructionAccountOrganizationHierarchy-chartOfAccountsCode" parent="BudgetConstructionAccountOrganizationHierarchy-chartOfAccountsCode-parentBean" />

  <bean id="BudgetConstructionAccountOrganizationHierarchy-chartOfAccountsCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="chartOfAccountsCode" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Chart Of Accounts Code" />
    <property name="shortLabel" value="Code" />
    <property name="maxLength" value="2" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="4" />
    </property>
    <property name="summary" value="chartOfAccountsCode" />
    <property name="description" value="chartOfAccountsCode description..." />
  </bean>
  <bean id="BudgetConstructionAccountOrganizationHierarchy-organizationChartOfAccountsCode" parent="BudgetConstructionAccountOrganizationHierarchy-organizationChartOfAccountsCode-parentBean" />

  <bean id="BudgetConstructionAccountOrganizationHierarchy-organizationChartOfAccountsCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="organizationChartOfAccountsCode" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Organization Chart Of Accounts Code" />
    <property name="shortLabel" value="Code" />
    <property name="maxLength" value="2" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="4" />
    </property>
    <property name="summary" value="organizationChartOfAccountsCode" />
    <property name="description" value="organizationChartOfAccountsCode description..." />
  </bean>
  <bean id="BudgetConstructionAccountOrganizationHierarchy-organizationCode" parent="BudgetConstructionAccountOrganizationHierarchy-organizationCode-parentBean" />

  <bean id="BudgetConstructionAccountOrganizationHierarchy-organizationCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="organizationCode" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Organization Code" />
    <property name="shortLabel" value="Code" />
    <property name="maxLength" value="4" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="6" />
    </property>
    <property name="summary" value="organizationCode" />
    <property name="description" value="organizationCode description..." />
  </bean>
  <bean id="BudgetConstructionAccountOrganizationHierarchy-organizationLevelCode" parent="BudgetConstructionAccountOrganizationHierarchy-organizationLevelCode-parentBean" />

  <bean id="BudgetConstructionAccountOrganizationHierarchy-organizationLevelCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="organizationLevelCode" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Organization Level Code" />
    <property name="shortLabel" value="Code" />
    <property name="maxLength" value="7" />
    <property name="validationPattern" >
      <bean parent="NumericValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="9" />
    </property>
    <property name="summary" value="organizationLevelCode" />
    <property name="description" value="organizationLevelCode description..." />
  </bean>
  <bean id="BudgetConstructionAccountOrganizationHierarchy-universityFiscalYear" parent="BudgetConstructionAccountOrganizationHierarchy-universityFiscalYear-parentBean" />

  <bean id="BudgetConstructionAccountOrganizationHierarchy-universityFiscalYear-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="universityFiscalYear" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="University Fiscal Year" />
    <property name="shortLabel" value="Year" />
    <property name="maxLength" value="4" />
    <property name="validationPattern" >
      <bean parent="NumericValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="6" />
    </property>
    <property name="summary" value="universityFiscalYear" />
    <property name="description" value="universityFiscalYear description..." />
  </bean>
  <bean id="BudgetConstructionAccountOrganizationHierarchy-versionNumber" parent="BudgetConstructionAccountOrganizationHierarchy-versionNumber-parentBean" />

  <bean id="BudgetConstructionAccountOrganizationHierarchy-versionNumber-parentBean" abstract="true" parent="GenericAttributes-versionNumber">
  </bean>

<!-- Business Object Inquiry Definition -->


  <bean id="BudgetConstructionAccountOrganizationHierarchy-inquiryDefinition" parent="BudgetConstructionAccountOrganizationHierarchy-inquiryDefinition-parentBean" />

  <bean id="BudgetConstructionAccountOrganizationHierarchy-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
    <property name="title" value="Budget Construction Account Organization Hierarchy Inquiry" />
    <property name="inquirySections" >
      <list>
        <bean parent="InquirySectionDefinition">
          <property name="title" value="Header Text" />
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields" >
            <list>
              <bean parent="FieldDefinition"
                    p:attributeName="accountNumber" />
              <bean parent="FieldDefinition"
                    p:attributeName="chartOfAccountsCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="organizationChartOfAccountsCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="organizationCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="organizationLevelCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="universityFiscalYear" />
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Business Object Lookup Definition -->


  <bean id="BudgetConstructionAccountOrganizationHierarchy-lookupDefinition" parent="BudgetConstructionAccountOrganizationHierarchy-lookupDefinition-parentBean" />

  <bean id="BudgetConstructionAccountOrganizationHierarchy-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
    <property name="title" value="Budget Construction Account Organization Hierarchy Lookup" />
    <property name="instructions" value="Look up a Budget Construction Account Organization Hierarchy" />
    <property name="defaultSort" >
      <bean parent="SortDefinition">
      </bean>
    </property>
    <property name="lookupFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="accountNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="chartOfAccountsCode" />
        <bean parent="FieldDefinition"
              p:attributeName="organizationChartOfAccountsCode" />
        <bean parent="FieldDefinition"
              p:attributeName="organizationCode" />
        <bean parent="FieldDefinition"
              p:attributeName="organizationLevelCode" />
        <bean parent="FieldDefinition"
              p:attributeName="universityFiscalYear" />
      </list>
    </property>
    <property name="resultFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="accountNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="chartOfAccountsCode" />
        <bean parent="FieldDefinition"
              p:attributeName="organizationChartOfAccountsCode" />
        <bean parent="FieldDefinition"
              p:attributeName="organizationCode" />
        <bean parent="FieldDefinition"
              p:attributeName="organizationLevelCode" />
        <bean parent="FieldDefinition"
              p:attributeName="universityFiscalYear" />
      </list>
    </property>
  </bean>
</beans>
