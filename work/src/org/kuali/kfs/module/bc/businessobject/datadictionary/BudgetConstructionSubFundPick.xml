<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="BudgetConstructionSubFundPick" parent="BudgetConstructionSubFundPick-parentBean" />

  <bean id="BudgetConstructionSubFundPick-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.kfs.module.bc.businessobject.BudgetConstructionSubFundPick" />
    <property name="inquiryDefinition" >
      <ref bean="BudgetConstructionSubFundPick-inquiryDefinition" />
    </property>
    <property name="lookupDefinition" >
      <ref bean="BudgetConstructionSubFundPick-lookupDefinition" />
    </property>
    <property name="objectLabel" value="Budget Construction Sub Fund Pick" />
    <property name="attributes" >
      <list>
        <ref bean="BudgetConstructionSubFundPick-personUniversalIdentifier" />
        <ref bean="BudgetConstructionSubFundPick-reportFlag" />
        <ref bean="BudgetConstructionSubFundPick-subFundGroupCode" />
        <ref bean="BudgetConstructionSubFundPick-subFundGroup.subFundGroupDescription" />
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->


  <bean id="BudgetConstructionSubFundPick-personUniversalIdentifier" parent="BudgetConstructionSubFundPick-personUniversalIdentifier-parentBean" />

  <bean id="BudgetConstructionSubFundPick-personUniversalIdentifier-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="personUniversalIdentifier" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Person Universal Identifier" />
    <property name="shortLabel" value="Identifier" />
    <property name="maxLength" value="10" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="12" />
    </property>
    <property name="summary" value="personUniversalIdentifier" />
    <property name="description" value="personUniversalIdentifier description..." />
  </bean>
  <bean id="BudgetConstructionSubFundPick-reportFlag" parent="BudgetConstructionSubFundPick-reportFlag-parentBean" />

  <bean id="BudgetConstructionSubFundPick-reportFlag-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="reportFlag" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Report Flag" />
    <property name="shortLabel" value="Flag" />
    <property name="maxLength" value="7" />
    <property name="validationPattern" >
      <bean parent="NumericValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="9" />
    </property>
    <property name="summary" value="reportFlag" />
    <property name="description" value="reportFlag description..." />
  </bean>
  <bean id="BudgetConstructionSubFundPick-subFundGroupCode" parent="BudgetConstructionSubFundPick-subFundGroupCode-parentBean" />

  <bean id="BudgetConstructionSubFundPick-subFundGroupCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="subFundGroupCode" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Sub Fund Group Code" />
    <property name="shortLabel" value="Code" />
    <property name="maxLength" value="6" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="8" />
    </property>
    <property name="summary" value="subFundGroupCode" />
    <property name="description" value="subFundGroupCode description..." />
  </bean>
  <bean id="BudgetConstructionSubFundPick-subFundGroup.subFundGroupDescription" parent="BudgetConstructionSubFundPick-subFundGroup.subFundGroupDescription-parentBean" />

  <bean id="BudgetConstructionSubFundPick-subFundGroup.subFundGroupDescription-parentBean" abstract="true" parent="SubFundGroup-subFundGroupDescription">
    <property name="name" value="subFundGroup.subFundGroupDescription" />
    <property name="required" value="false" />
  </bean>

<!-- Business Object Inquiry Definition -->


  <bean id="BudgetConstructionSubFundPick-inquiryDefinition" parent="BudgetConstructionSubFundPick-inquiryDefinition-parentBean" />

  <bean id="BudgetConstructionSubFundPick-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
    <property name="title" value="Budget Construction Sub Fund Pick Inquiry" />
    <property name="inquirySections" >
      <list>
        <bean parent="InquirySectionDefinition">
          <property name="title" value="" />
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields" >
            <list>
              <bean parent="FieldDefinition"
                    p:attributeName="personUniversalIdentifier" />
              <bean parent="FieldDefinition"
                    p:attributeName="reportFlag" />
              <bean parent="FieldDefinition"
                    p:attributeName="subFundGroupCode" />
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Business Object Lookup Definition -->


  <bean id="BudgetConstructionSubFundPick-lookupDefinition" parent="BudgetConstructionSubFundPick-lookupDefinition-parentBean" />

  <bean id="BudgetConstructionSubFundPick-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
    <property name="title" value="Budget Construction Sub Fund Pick Lookup" />
    <property name="instructions" value="Look up a Budget Construction Sub Fund Pick" />
    <property name="defaultSort" >
      <bean parent="SortDefinition">
      </bean>
    </property>
    <property name="lookupFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="personUniversalIdentifier" />
        <bean parent="FieldDefinition"
              p:attributeName="reportFlag" />
        <bean parent="FieldDefinition"
              p:attributeName="subFundGroupCode" />
      </list>
    </property>
    <property name="resultFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="personUniversalIdentifier" />
        <bean parent="FieldDefinition"
              p:attributeName="reportFlag" />
        <bean parent="FieldDefinition"
              p:attributeName="subFundGroupCode" />
      </list>
    </property>
  </bean>
</beans>
