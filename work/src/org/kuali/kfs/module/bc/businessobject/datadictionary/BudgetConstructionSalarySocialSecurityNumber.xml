<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="BudgetConstructionSalarySocialSecurityNumber" parent="BudgetConstructionSalarySocialSecurityNumber-parentBean" />

  <bean id="BudgetConstructionSalarySocialSecurityNumber-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.kfs.module.bc.businessobject.BudgetConstructionSalarySocialSecurityNumber" />
    <property name="inquiryDefinition" >
      <ref bean="BudgetConstructionSalarySocialSecurityNumber-inquiryDefinition" />
    </property>
    <property name="lookupDefinition" >
      <ref bean="BudgetConstructionSalarySocialSecurityNumber-lookupDefinition" />
    </property>
    <property name="objectLabel" value="Budget Construction Salary Social Security Number" />
    <property name="attributes" >
      <list>
        <ref bean="BudgetConstructionSalarySocialSecurityNumber-emplid" />
        <ref bean="BudgetConstructionSalarySocialSecurityNumber-organizationChartOfAccountsCode" />
        <ref bean="BudgetConstructionSalarySocialSecurityNumber-organizationCode" />
        <ref bean="BudgetConstructionSalarySocialSecurityNumber-personName" />
        <ref bean="BudgetConstructionSalarySocialSecurityNumber-personUniversalIdentifier" />
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->


  <bean id="BudgetConstructionSalarySocialSecurityNumber-emplid" parent="BudgetConstructionSalarySocialSecurityNumber-emplid-parentBean" />

  <bean id="BudgetConstructionSalarySocialSecurityNumber-emplid-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="emplid" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Emplid" />
    <property name="shortLabel" value="Emplid" />
    <property name="maxLength" value="11" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="13" />
    </property>
    <property name="summary" value="emplid" />
    <property name="description" value="emplid description..." />
  </bean>
  <bean id="BudgetConstructionSalarySocialSecurityNumber-organizationChartOfAccountsCode" parent="BudgetConstructionSalarySocialSecurityNumber-organizationChartOfAccountsCode-parentBean" />

  <bean id="BudgetConstructionSalarySocialSecurityNumber-organizationChartOfAccountsCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="organizationChartOfAccountsCode" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Organization Chart Of Accounts Code" />
    <property name="shortLabel" value="Code" />
    <property name="maxLength" value="2" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="4" />
    </property>
    <property name="summary" value="organizationChartOfAccountsCode" />
    <property name="description" value="organizationChartOfAccountsCode description..." />
  </bean>
  <bean id="BudgetConstructionSalarySocialSecurityNumber-organizationCode" parent="BudgetConstructionSalarySocialSecurityNumber-organizationCode-parentBean" />

  <bean id="BudgetConstructionSalarySocialSecurityNumber-organizationCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="organizationCode" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Organization Code" />
    <property name="shortLabel" value="Code" />
    <property name="maxLength" value="4" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="6" />
    </property>
    <property name="summary" value="organizationCode" />
    <property name="description" value="organizationCode description..." />
  </bean>
  <bean id="BudgetConstructionSalarySocialSecurityNumber-personName" parent="BudgetConstructionSalarySocialSecurityNumber-personName-parentBean" />

  <bean id="BudgetConstructionSalarySocialSecurityNumber-personName-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="personName" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Person Name" />
    <property name="shortLabel" value="Name" />
    <property name="maxLength" value="50" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="52" />
    </property>
    <property name="summary" value="personName" />
    <property name="description" value="personName description..." />
  </bean>
  <bean id="BudgetConstructionSalarySocialSecurityNumber-personUniversalIdentifier" parent="BudgetConstructionSalarySocialSecurityNumber-personUniversalIdentifier-parentBean" />

  <bean id="BudgetConstructionSalarySocialSecurityNumber-personUniversalIdentifier-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="personUniversalIdentifier" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Person Universal Identifier" />
    <property name="shortLabel" value="Identifier" />
    <property name="maxLength" value="10" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="12" />
    </property>
    <property name="summary" value="personUniversalIdentifier" />
    <property name="description" value="personUniversalIdentifier description..." />
  </bean>

<!-- Business Object Inquiry Definition -->


  <bean id="BudgetConstructionSalarySocialSecurityNumber-inquiryDefinition" parent="BudgetConstructionSalarySocialSecurityNumber-inquiryDefinition-parentBean" />

  <bean id="BudgetConstructionSalarySocialSecurityNumber-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
    <property name="title" value="Budget Construction Salary Social Security Number Inquiry" />
    <property name="inquirySections" >
      <list>
        <bean parent="InquirySectionDefinition">
          <property name="title" value="" />
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields" >
            <list>
              <bean parent="FieldDefinition"
                    p:attributeName="emplid" />
              <bean parent="FieldDefinition"
                    p:attributeName="organizationChartOfAccountsCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="organizationCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="personName" />
              <bean parent="FieldDefinition"
                    p:attributeName="personUniversalIdentifier" />
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Business Object Lookup Definition -->


  <bean id="BudgetConstructionSalarySocialSecurityNumber-lookupDefinition" parent="BudgetConstructionSalarySocialSecurityNumber-lookupDefinition-parentBean" />

  <bean id="BudgetConstructionSalarySocialSecurityNumber-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
    <property name="title" value="Budget Construction Salary Social Security Number Lookup" />
    <property name="instructions" value="Look up a Budget Construction Salary Social Security Number" />
    <property name="defaultSort" >
      <bean parent="SortDefinition">
      </bean>
    </property>
    <property name="lookupFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="emplid" />
        <bean parent="FieldDefinition"
              p:attributeName="organizationChartOfAccountsCode" />
        <bean parent="FieldDefinition"
              p:attributeName="organizationCode" />
        <bean parent="FieldDefinition"
              p:attributeName="personName" />
        <bean parent="FieldDefinition"
              p:attributeName="personUniversalIdentifier" />
      </list>
    </property>
    <property name="resultFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="emplid" />
        <bean parent="FieldDefinition"
              p:attributeName="organizationChartOfAccountsCode" />
        <bean parent="FieldDefinition"
              p:attributeName="organizationCode" />
        <bean parent="FieldDefinition"
              p:attributeName="personName" />
        <bean parent="FieldDefinition"
              p:attributeName="personUniversalIdentifier" />
      </list>
    </property>
  </bean>
</beans>
