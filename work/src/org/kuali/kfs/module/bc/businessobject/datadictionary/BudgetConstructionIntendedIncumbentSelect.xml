<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "../dataDictionary-1_0.dtd">
<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.module.budget.bo.BudgetConstructionIntendedIncumbentSelect">


        <inquiry>
            <title>Budget Construction Intended Incumbent Select Inquiry</title>

            <inquirySections><inquirySection title="Header Text"><inquiryFields>
                <field attributeName="emplid" />
                <field attributeName="financialObjectCode" />
                <field attributeName="personName" />
                <field attributeName="personUniversalIdentifier" />
            </inquiryFields></inquirySection></inquirySections>
        </inquiry>

        <lookup>
            <!-- Use this to include the hidden field personUniversalIdentifier in the search criteria -->
            <lookupableID>intendedIncumbentSelectLookupable</lookupableID>	        

            <title>Budget Construction Intended Incumbent Select Lookup</title>

            <instructions>Look up a Budget Construction Intended Incumbent Select</instructions>

            <defaultSort sortAscending="true">
                <sortAttributes>
                    <sortAttribute attributeName="personName" />
                </sortAttributes>
            </defaultSort>

            <lookupFields>
                <lookupField attributeName="emplid" required="false" />
                <lookupField attributeName="financialObjectCode" required="false" />
                <lookupField attributeName="personName" required="false" />
                <lookupField attributeName="personUniversalIdentifier" required="false" />
            </lookupFields>

            <resultFields>
                <field attributeName="emplid" />
                <field attributeName="financialObjectCode" />
                <field attributeName="personName" />
                <field attributeName="dummyBusinessObject.linkButtonOption" />
            </resultFields>
        </lookup>

        <attributes>

            <attribute name="emplid" forceUppercase="true">
                <label>Emplid</label>
                <shortLabel>Emplid</shortLabel>

                <maxLength>11</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="13" />
                </control>

                <summary>emplid</summary>
                <description>emplid description...</description>
			</attribute>

            <attribute name="financialObjectCode" forceUppercase="true">
                <label>Financial Object Code</label>
                <shortLabel>Code</shortLabel>

                <maxLength>4</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="6" />
                </control>

                <summary>financialObjectCode</summary>
                <description>financialObjectCode description...</description>
			</attribute>

            <attribute name="personName" forceUppercase="true">
                <label>Person Name</label>
                <shortLabel>Name</shortLabel>

                <maxLength>50</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="52" />
                </control>

                <summary>personName</summary>
                <description>personName description...</description>
			</attribute>

            <!-- marked hidden and used in the lookup criteria to operate on rows associated with the user -->
            <attribute name="personUniversalIdentifier" forceUppercase="true">
                <label>Person Universal Identifier</label>
                <shortLabel>Identifier</shortLabel>

                <maxLength>10</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <hidden />
                </control>

                <summary>personUniversalIdentifier</summary>
                <description>personUniversalIdentifier description...</description>
			</attribute>

			<attributeReference name="dummyBusinessObject.linkButtonOption" sourceClassName="org.kuali.module.gl.bo.TransientBalanceInquiryAttributes" sourceAttributeName="linkButtonOption">
                <label>Incumbent Salary Setting</label>
                <shortLabel>Incumbent SalSet</shortLabel>
                <summary>Incumbent Salary Setting</summary>
                <description>Incumbent Salary Setting ...</description>
            </attributeReference>

            <attributeReference name="versionNumber" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="versionNumber" />

        </attributes>
    </businessObject>
</dictionaryEntry>
