<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "../dataDictionary-1_0.dtd">
<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.module.budget.bo.BudgetConstructionAccountSelect">


        <inquiry>
            <title>Budget Construction Account Select Inquiry</title>

            <inquirySections><inquirySection title="Header Text"><inquiryFields>
                <field attributeName="chartOfAccountsCode" />
                <field attributeName="accountNumber" />
                <field attributeName="subAccountNumber" />
                <field attributeName="documentNumber" />
                <field attributeName="financialDocumentCreateDate" />
                <field attributeName="financialDocumentInitiatorIdentifier" />
                <field attributeName="financialDocumentStatusCode" />
                <field attributeName="organizationLevelCode" />
                <field attributeName="organizationChartOfAccountsCode" />
                <field attributeName="organizationCode" />
                <field attributeName="personUniversalIdentifier" />
                <field attributeName="universityFiscalYear" />
            </inquiryFields></inquirySection></inquirySections>
        </inquiry>

        <lookup>
            <!-- Use this to include the hidden field personUniversalIdentifier in the search criteria -->
            <lookupableID>accountSelectLookupable</lookupableID>	        

            <title>Budget Construction Account Select Lookup</title>

            <instructions>Look up a Budget Construction Account Select</instructions>

            <defaultSort sortAscending="true">
                <sortAttributes>
                    <sortAttribute attributeName="chartOfAccountsCode" />
                    <sortAttribute attributeName="accountNumber" />                    
                    <sortAttribute attributeName="subAccountNumber" />
                </sortAttributes>
            </defaultSort>

            <lookupFields>
                <lookupField attributeName="chartOfAccountsCode" required="false" />
                <lookupField attributeName="accountNumber" required="false" />
                <lookupField attributeName="subAccountNumber" required="false" />
                <lookupField attributeName="financialDocumentStatusCode" required="false" />
                <lookupField attributeName="financialDocumentCreateDate" required="false" />
                <lookupField attributeName="financialDocumentInitiatorIdentifier" required="false" />
                <lookupField attributeName="organizationLevelCode" required="false" />
                <lookupField attributeName="organizationChartOfAccountsCode" required="false" />
                <lookupField attributeName="organizationCode" required="false" />
                <lookupField attributeName="personUniversalIdentifier" required="false" />
            </lookupFields>

            <resultFields>
                <field attributeName="chartOfAccountsCode" />
                <field attributeName="accountNumber" />
                <field attributeName="subAccountNumber" />
                <field attributeName="financialDocumentStatusCode" />
                <field attributeName="financialDocumentCreateDate" />
                <field attributeName="financialDocumentInitiatorIdentifier" />
                <field attributeName="organizationLevelCode" />
                <field attributeName="organizationChartOfAccountsCode" />
                <field attributeName="organizationCode" />
                <field attributeName="dummyBusinessObject.linkButtonOption" />
            </resultFields>
        </lookup>

        <attributes>

            <attribute name="accountNumber" forceUppercase="true">
                <label>Account</label>
                <shortLabel>Account</shortLabel>

                <maxLength>7</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="9" />
                </control>

                <summary>accountNumber</summary>
                <description>accountNumber description...</description>
			</attribute>

            <attribute name="chartOfAccountsCode" forceUppercase="true">
                <label>Chart</label>
                <shortLabel>Chart</shortLabel>

                <maxLength>2</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="4" />
                </control>

                <summary>chartOfAccountsCode</summary>
                <description>chartOfAccountsCode description...</description>
			</attribute>

            <attribute name="financialDocumentCreateDate" forceUppercase="true">
                <label>Last Upd</label>
                <shortLabel>Last Upd</shortLabel>

                <maxLength>7</maxLength>
                <validationPattern><date /></validationPattern>

                <control>
                    <text size="9" />
                </control>

                <summary>financialDocumentCreateDate</summary>
                <description>financialDocumentCreateDate description...</description>
			</attribute>

            <attribute name="financialDocumentInitiatorIdentifier" forceUppercase="true">
                <label>Upd by</label>
                <shortLabel>Upd by</shortLabel>

                <maxLength>10</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="12" />
                </control>

                <summary>financialDocumentInitiatorIdentifier</summary>
                <description>financialDocumentInitiatorIdentifier description...</description>
			</attribute>

            <attribute name="documentNumber" forceUppercase="true">
                <label>Document Number</label>
                <shortLabel>Number</shortLabel>

                <maxLength>14</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="16" />
                </control>

                <summary>documentNumber</summary>
                <description>documentNumber description...</description>
			</attribute>

            <attribute name="financialDocumentStatusCode" forceUppercase="true">
                <label>Status</label>
                <shortLabel>Status</shortLabel>

                <maxLength>2</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="4" />
                </control>

                <summary>financialDocumentStatusCode</summary>
                <description>financialDocumentStatusCode description...</description>
			</attribute>

            <attribute name="organizationChartOfAccountsCode" forceUppercase="true">
                <label>Org Chart</label>
                <shortLabel>Org Chart</shortLabel>

                <maxLength>2</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="4" />
                </control>

                <summary>organizationChartOfAccountsCode</summary>
                <description>organizationChartOfAccountsCode description...</description>
			</attribute>

            <attribute name="organizationCode" forceUppercase="true">
                <label>Org</label>
                <shortLabel>Org</shortLabel>

                <maxLength>4</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="6" />
                </control>

                <summary>organizationCode</summary>
                <description>organizationCode description...</description>
			</attribute>

            <attribute name="organizationLevelCode" forceUppercase="true">
                <label>Org Level</label>
                <shortLabel>Org Level</shortLabel>

                <maxLength>7</maxLength>
                <validationPattern><numeric /></validationPattern>

                <control>
                    <text size="9" />
                </control>

                <summary>organizationLevelCode</summary>
                <description>organizationLevelCode description...</description>
			</attribute>

            <attribute name="personUniversalIdentifier" forceUppercase="true">
                <label>Person Universal Identifier</label>
                <shortLabel>Identifier</shortLabel>

                <maxLength>10</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <hidden />
                </control>

                <summary>personUniversalIdentifier</summary>
                <description>personUniversalIdentifier description...</description>
			</attribute>

            <attribute name="subAccountNumber" forceUppercase="true">
                <label>Sub Account</label>
                <shortLabel>Sub Acct</shortLabel>

                <maxLength>5</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="7" />
                </control>

                <summary>subAccountNumber</summary>
                <description>subAccountNumber description...</description>
			</attribute>

            <attribute name="universityFiscalYear" forceUppercase="true">
                <label>University Fiscal Year</label>
                <shortLabel>Year</shortLabel>

                <maxLength>4</maxLength>
                <validationPattern><numeric /></validationPattern>

                <control>
                    <text size="6" />
                </control>

                <summary>universityFiscalYear</summary>
                <description>universityFiscalYear description...</description>
			</attribute>

			<attributeReference name="dummyBusinessObject.linkButtonOption" sourceClassName="org.kuali.module.gl.bo.TransientBalanceInquiryAttributes" sourceAttributeName="linkButtonOption">
                <label>Load Document</label>
                <shortLabel>Load Doc</shortLabel>
                <summary>Load Document</summary>
                <description>Load Document ...</description>
            </attributeReference>

            <attributeReference name="versionNumber" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="versionNumber" />

        </attributes>
    </businessObject>
</dictionaryEntry>
