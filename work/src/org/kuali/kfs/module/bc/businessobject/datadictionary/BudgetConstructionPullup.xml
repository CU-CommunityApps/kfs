<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "../dataDictionary-1_0.dtd">
<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.module.budget.bo.BudgetConstructionPullup">


        <inquiry>
            <title>Budget Construction Pullup Inquiry</title>

            <inquirySections><inquirySection title="Header Text"><inquiryFields>
                <field attributeName="chartOfAccountsCode" />
                <field attributeName="organizationCode" />
                <field attributeName="personUniversalIdentifier" />
                <field attributeName="pullFlag" />
                <field attributeName="reportsToChartOfAccountsCode" />
                <field attributeName="reportsToOrganizationCode" />
            </inquiryFields></inquirySection></inquirySections>
        </inquiry>

        <lookup>
            <title>Budget Construction Pullup Lookup</title>

            <instructions>Look up a Budget Construction Pullup</instructions>

            <defaultSort sortAscending="true">
                <sortAttributes>
                    <sortAttribute attributeName="chartOfAccountsCode" />
                    <sortAttribute attributeName="organizationCode" />
                </sortAttributes>
            </defaultSort>

            <lookupFields>
                <lookupField attributeName="chartOfAccountsCode" required="false" />
                <lookupField attributeName="organizationCode" required="false" />
                <lookupField attributeName="personUniversalIdentifier" required="false" />
                <lookupField attributeName="pullFlag" required="false" />
                <lookupField attributeName="reportsToChartOfAccountsCode" required="false" />
                <lookupField attributeName="reportsToOrganizationCode" required="false" />
            </lookupFields>

            <resultFields>
                <field attributeName="chartOfAccountsCode" />
                <field attributeName="organizationCode" />
                <field attributeName="personUniversalIdentifier" />
                <field attributeName="pullFlag" />
                <field attributeName="reportsToChartOfAccountsCode" />
                <field attributeName="reportsToOrganizationCode" />
            </resultFields>
        </lookup>

        <attributes>

            <attribute name="chartOfAccountsCode" forceUppercase="true">
                <label>Chart Of Accounts Code</label>
                <shortLabel>Code</shortLabel>

                <maxLength>2</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="4" />
                </control>

                <summary>chartOfAccountsCode</summary>
                <description>chartOfAccountsCode description...</description>
			</attribute>

            <attribute name="organizationCode" forceUppercase="true">
                <label>Organization Code</label>
                <shortLabel>Code</shortLabel>

                <maxLength>4</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="6" />
                </control>

                <summary>organizationCode</summary>
                <description>organizationCode description...</description>
			</attribute>

            <attribute name="personUniversalIdentifier" forceUppercase="true">
                <label>Person Universal Identifier</label>
                <shortLabel>Identifier</shortLabel>

                <maxLength>10</maxLength>
                <validationPattern><numeric /></validationPattern>

                <control>
                    <text size="12" />
                </control>

                <summary>personUniversalIdentifier</summary>
                <description>personUniversalIdentifier description...</description>
			</attribute>

            <attribute name="pullFlag" forceUppercase="true">
                <label>Pull Flag</label>
                <shortLabel>Flag</shortLabel>

                <maxLength>5</maxLength>
                <validationPattern><numeric /></validationPattern>

                <control>
                    <text size="7" />
                </control>

                <summary>pullFlag</summary>
                <description>pullFlag description...</description>
			</attribute>

            <attribute name="reportsToChartOfAccountsCode" forceUppercase="true">
                <label>Reports To Chart Of Accounts Code</label>
                <shortLabel>Code</shortLabel>

                <maxLength>2</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="4" />
                </control>

                <summary>reportsToChartOfAccountsCode</summary>
                <description>reportsToChartOfAccountsCode description...</description>
			</attribute>

            <attribute name="reportsToOrganizationCode" forceUppercase="true">
                <label>Reports To Organization Code</label>
                <shortLabel>Code</shortLabel>

                <maxLength>4</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="6" />
                </control>

                <summary>reportsToOrganizationCode</summary>
                <description>reportsToOrganizationCode description...</description>
			</attribute>

            <attributeReference name="versionNumber" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="versionNumber" />

