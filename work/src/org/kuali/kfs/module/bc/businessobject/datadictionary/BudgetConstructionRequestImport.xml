<!DOCTYPE dictionaryEntry PUBLIC "-//Kuali Project//DTD Data Dictionary 1.0//EN" "../../../../../../../../rice/kns/src/main/resources/org/kuali/core/datadictionary/dataDictionary-1_0.dtd">
<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.module.budget.bo.BudgetConstructionRequestImport">
       <lookup>
       		<title>Budget Construction Import Tool</title>

            <instructions>Budget Construction Import Tool</instructions>

            <defaultSort sortAscending="true">
                <sortAttributes>
                    <sortAttribute attributeName="fileName" />
                </sortAttributes>
            </defaultSort>

            <lookupFields>
            	<lookupField attributeName="fileName" />
			    <lookupField attributeName="fileType" />
			    <lookupField attributeName="fieldDelimiter" />
			    <lookupField attributeName="textFieldDelimiter" />
            </lookupFields>

            <resultFields>
            	<field attributeName="fileName" />
			    <field attributeName="fileType" />
			    <field attributeName="fieldDelimiter" />
			    <field attributeName="textFieldDelimiter" /> 
            </resultFields>
        </lookup>

        <attributes>
			
			<attribute name="fileName" forceUppercase="true">
                <label>Import File</label>
                <shortLabel>Import File</shortLabel>

                <maxLength>50</maxLength>
                
                <control>
                    <text size="50" />
                </control>

                <summary>Import File</summary>
                <description>Import File</description>
			</attribute>
			
			<attribute name="otherFieldDelimiter" forceUppercase="true">
                <label>Field Delimiter</label>
                <shortLabel>Field Delimiter</shortLabel>

                <maxLength>1</maxLength>
                
                <control>
                    <text size="1" />
                </control>

                <summary>Field Delimiter</summary>
                <description>Field Delimiter</description>
			</attribute>
			
			<attribute name="otherTextFieldDelimiter" forceUppercase="true">
                <label>Text Field Delimiter</label>
                <shortLabel>Text Field Delimiter</shortLabel>

                <maxLength>1</maxLength>
                
                <control>
                    <text size="1" />
                </control>

                <summary>Field Delimiter</summary>
                <description>Field Delimiter</description>
			</attribute>
			
			<attribute name="fileType" forceUppercase="true">
                <label>Import File Type</label>
                <shortLabel>Import File Type</shortLabel>
				<maxLength>50</maxLength>
                <control>
                    <radio valuesFinderClass="org.kuali.module.budget.lookup.keyvalues.FileTypeValuesFinder" />
                </control>

                <summary>Import File  Type</summary>
                <description>Import File  Type</description>
			</attribute>
			
			<attribute name="fieldDelimiter" forceUppercase="true">
                <label>Fields Separated By</label>
                <shortLabel>Fields Separated By</shortLabel>
				<maxLength>50</maxLength>
                <control>
                    <radio valuesFinderClass="org.kuali.module.budget.lookup.keyvalues.FieldDelimiterValuesFinder" />
                </control>

                <summary>Fields Separated By</summary>
                <description>Fields Separated By</description>
			</attribute>
			
			<attribute name="textFieldDelimiter" forceUppercase="true">
                <label>Text Fields Delimited By</label>
                <shortLabel>Text Field Delimiter</shortLabel>
				<maxLength>50</maxLength>
                <control>
					<radio valuesFinderClass="org.kuali.module.budget.lookup.keyvalues.TextFieldDelimiterValuesFinder" />
				</control>

                <summary>Text Field Delimiter</summary>
                <description>Text Field Delimiter</description>
			</attribute>
			
        </attributes>
          
    </businessObject>
</dictionaryEntry>
