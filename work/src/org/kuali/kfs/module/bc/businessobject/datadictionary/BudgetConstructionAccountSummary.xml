                <field attributeName="accountNumber" />
                <field attributeName="chartOfAccountsCode" />
                <field attributeName="financialBeginningBalanceLineAmount" />
                <field attributeName="fundGroupCode" />
                <field attributeName="incomeExpenseCode" />
                <field attributeName="organizationChartOfAccountsCode" />
                <field attributeName="organizationCode" />
                <field attributeName="personUniversalIdentifier" />
