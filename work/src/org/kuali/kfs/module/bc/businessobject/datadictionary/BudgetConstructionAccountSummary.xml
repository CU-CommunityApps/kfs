                <field attributeName="personUniversalIdentifier" />
                <field attributeName="subAccountNumber" />
                <field attributeName="subFundGroupCode" />
                <field attributeName="subFundSortCode" />
            </inquiryFields></inquirySection></inquirySections>
        </inquiry>

        <lookup>
            <title>Budget Construction Account Summary Lookup</title>

            <instructions>Look up a Budget Construction Account Summary</instructions>

            <defaultSort sortAscending="true">
                <sortAttributes>
                    <sortAttribute attributeName="organizationChartOfAccountsCode" />
                    <sortAttribute attributeName="organizationCode" />
                </sortAttributes>
            </defaultSort>

            <lookupFields>
                <lookupField attributeName="accountLineAnnualBalanceAmount" required="false" />
                <lookupField attributeName="accountNumber" required="false" />
                <lookupField attributeName="chartOfAccountsCode" required="false" />
                <lookupField attributeName="financialBeginningBalanceLineAmount" required="false" />
                <lookupField attributeName="fundGroupCode" required="false" />
