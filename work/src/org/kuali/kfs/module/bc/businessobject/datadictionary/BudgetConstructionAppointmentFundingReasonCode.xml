<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="BudgetConstructionAppointmentFundingReasonCode" parent="BudgetConstructionAppointmentFundingReasonCode-parentBean" />

  <bean id="BudgetConstructionAppointmentFundingReasonCode-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.module.budget.bo.BudgetConstructionAppointmentFundingReasonCode" />
    <property name="inquiryDefinition" >
      <ref bean="BudgetConstructionAppointmentFundingReasonCode-inquiryDefinition" />
    </property>
    <property name="lookupDefinition" >
      <ref bean="BudgetConstructionAppointmentFundingReasonCode-lookupDefinition" />
    </property>
    <property name="titleAttribute" value="appointmentFundingReasonCode" />
    <property name="attributes" >
      <list>
        <ref bean="BudgetConstructionAppointmentFundingReasonCode-appointmentFundingReasonCode" />
        <ref bean="BudgetConstructionAppointmentFundingReasonCode-appointmentFundingReasonDescription" />
        <ref bean="BudgetConstructionAppointmentFundingReasonCode-rowActiveIndicator" />
        <ref bean="BudgetConstructionAppointmentFundingReasonCode-versionNumber" />
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->


  <bean id="BudgetConstructionAppointmentFundingReasonCode-appointmentFundingReasonCode" parent="BudgetConstructionAppointmentFundingReasonCode-appointmentFundingReasonCode-parentBean" />

  <bean id="BudgetConstructionAppointmentFundingReasonCode-appointmentFundingReasonCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="appointmentFundingReasonCode" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Appointment Funding Reason Code" />
    <property name="shortLabel" value="Code" />
    <property name="maxLength" value="3" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="5" />
    </property>
    <property name="summary" value="Appointment Funding Reason Code" />
    <property name="description" value="Reason Code for a request salary." />
  </bean>
  <bean id="BudgetConstructionAppointmentFundingReasonCode-appointmentFundingReasonDescription" parent="BudgetConstructionAppointmentFundingReasonCode-appointmentFundingReasonDescription-parentBean" />

  <bean id="BudgetConstructionAppointmentFundingReasonCode-appointmentFundingReasonDescription-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="appointmentFundingReasonDescription" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Appointment Funding Reason Description" />
    <property name="shortLabel" value="Description" />
    <property name="maxLength" value="40" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern"
            p:allowWhitespace="true" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="42" />
    </property>
    <property name="summary" value="Appointment Funding Reason Description" />
    <property name="description" value="Description of Reason Code" />
  </bean>
  <bean id="BudgetConstructionAppointmentFundingReasonCode-rowActiveIndicator" parent="BudgetConstructionAppointmentFundingReasonCode-rowActiveIndicator-parentBean" />

  <bean id="BudgetConstructionAppointmentFundingReasonCode-rowActiveIndicator-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="rowActiveIndicator" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Row Active Indicator" />
    <property name="shortLabel" value="Indicator" />
    <property name="maxLength" value="1" />
    <property name="control" >
      <bean parent="CheckboxControlDefinition" />
    </property>
    <property name="summary" value="Active Indicator" />
    <property name="description" value="Reason Code Active Indicator" />
  </bean>
  <bean id="BudgetConstructionAppointmentFundingReasonCode-versionNumber" parent="BudgetConstructionAppointmentFundingReasonCode-versionNumber-parentBean" />

  <bean id="BudgetConstructionAppointmentFundingReasonCode-versionNumber-parentBean" abstract="true" parent="AttributeReferenceDummy-versionNumber">
  </bean>

<!-- Business Object Inquiry Definition -->


  <bean id="BudgetConstructionAppointmentFundingReasonCode-inquiryDefinition" parent="BudgetConstructionAppointmentFundingReasonCode-inquiryDefinition-parentBean" />

  <bean id="BudgetConstructionAppointmentFundingReasonCode-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
    <property name="title" value="Budget Construction Appointment Funding Reason Code Inquiry" />
    <property name="inquirySections" >
      <list>
        <bean parent="InquirySectionDefinition">
          <property name="title" value="Header Text" />
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields" >
            <list>
              <bean parent="FieldDefinition"
                    p:attributeName="appointmentFundingReasonCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="appointmentFundingReasonDescription" />
              <bean parent="FieldDefinition"
                    p:attributeName="rowActiveIndicator" />
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Business Object Lookup Definition -->


  <bean id="BudgetConstructionAppointmentFundingReasonCode-lookupDefinition" parent="BudgetConstructionAppointmentFundingReasonCode-lookupDefinition-parentBean" />

  <bean id="BudgetConstructionAppointmentFundingReasonCode-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
    <property name="title" value="Budget Construction Appointment Funding Reason Code Lookup" />
    <property name="instructions" value="Look up a Budget Construction Appointment Funding Reason Code" />
    <property name="defaultSort" >
      <bean parent="SortDefinition">
      </bean>
    </property>
    <property name="lookupFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="appointmentFundingReasonCode" />
        <bean parent="FieldDefinition"
              p:attributeName="appointmentFundingReasonDescription" />
        <bean parent="FieldDefinition"
              p:attributeName="rowActiveIndicator" />
      </list>
    </property>
    <property name="resultFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="appointmentFundingReasonCode"
              p:forceInquiry="true" />
        <bean parent="FieldDefinition"
              p:attributeName="appointmentFundingReasonDescription" />
        <bean parent="FieldDefinition"
              p:attributeName="rowActiveIndicator" />
      </list>
    </property>
  </bean>
</beans>
