<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "../dataDictionary-1_0.dtd">
<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.module.budget.bo.BudgetConstructionObjectPick">


        <inquiry>
            <title>Budget Construction Object Pick Inquiry</title>

            <inquirySections><inquirySection title="Header Text"><inquiryFields>
                <field attributeName="financialObjectCode" />
                <field attributeName="personUniversalIdentifier" />
                <field attributeName="selectFlag" />
            </inquiryFields></inquirySection></inquirySections>
        </inquiry>

        <lookup>
            <title>Budget Construction Object Pick Lookup</title>

            <instructions>Look up a Budget Construction Object Pick</instructions>

            <defaultSort sortAscending="true">
                <sortAttributes>
                    <sortAttribute attributeName="financialObjectCode" />
                </sortAttributes>
            </defaultSort>

            <lookupFields>
                <lookupField attributeName="financialObjectCode" required="false" />
                <lookupField attributeName="personUniversalIdentifier" required="false" />
                <lookupField attributeName="selectFlag" required="false" />
            </lookupFields>

            <resultFields>
                <field attributeName="financialObjectCode" />
                <field attributeName="personUniversalIdentifier" />
                <field attributeName="selectFlag" />
            </resultFields>
        </lookup>

        <attributes>

            <attribute name="financialObjectCode" forceUppercase="true">
                <label>Object Code</label>
                <shortLabel>Code</shortLabel>

                <maxLength>4</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="6" />
                </control>

                <summary>financialObjectCode</summary>
                <description>financialObjectCode description...</description>
			</attribute>

            <attribute name="personUniversalIdentifier" forceUppercase="true">
                <label>Person Universal Identifier</label>
                <shortLabel>Identifier</shortLabel>

                <maxLength>10</maxLength>
                <validationPattern><numeric /></validationPattern>

                <control>
                    <text size="12" />
                </control>

                <summary>personUniversalIdentifier</summary>
                <description>personUniversalIdentifier description...</description>
			</attribute>

            <attribute name="selectFlag" forceUppercase="true">
                <label>Select Flag</label>
                <shortLabel>Flag</shortLabel>

                <maxLength>7</maxLength>
                <validationPattern><numeric /></validationPattern>

                <control>
                    <text size="9" />
                </control>

                <summary>selectFlag</summary>
                <description>selectFlag description...</description>
			</attribute>
			
			<attributeReference name="objectCodeDescription" 
			                    sourceClassName="org.kuali.module.chart.bo.ObjectCode" 
			                    sourceAttributeName="financialObjectCodeName" >
			     <label>Object Description</label>
			     <required>false</required>               
			</attributeReference>                    

            <attributeReference name="versionNumber" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="versionNumber" />

        </attributes>
    </businessObject>
</dictionaryEntry>
