<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="BudgetConstructionHeader" parent="BudgetConstructionHeader-parentBean" />

  <bean id="BudgetConstructionHeader-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.module.budget.bo.BudgetConstructionHeader" />
    <property name="inquiryDefinition" >
      <ref bean="BudgetConstructionHeader-inquiryDefinition" />
    </property>
    <property name="lookupDefinition" >
      <ref bean="BudgetConstructionHeader-lookupDefinition" />
    </property>
    <property name="attributes" >
      <list>
        <ref bean="BudgetConstructionHeader-accountNumber" />
        <ref bean="BudgetConstructionHeader-budgetLockUserIdentifier" />
        <ref bean="BudgetConstructionHeader-budgetTransactionLockUserIdentifier" />
        <ref bean="BudgetConstructionHeader-chartOfAccountsCode" />
        <ref bean="BudgetConstructionHeader-documentNumber" />
        <ref bean="BudgetConstructionHeader-organizationLevelChartOfAccountsCode" />
        <ref bean="BudgetConstructionHeader-organizationLevelCode" />
        <ref bean="BudgetConstructionHeader-organizationLevelOrganizationCode" />
        <ref bean="BudgetConstructionHeader-subAccountNumber" />
        <ref bean="BudgetConstructionHeader-universityFiscalYear" />
        <ref bean="BudgetConstructionHeader-versionNumber" />
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->


  <bean id="BudgetConstructionHeader-accountNumber" parent="BudgetConstructionHeader-accountNumber-parentBean" />

  <bean id="BudgetConstructionHeader-accountNumber-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="accountNumber" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Account Number" />
    <property name="shortLabel" value="Account" />
    <property name="maxLength" value="7" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="required" value="true" />
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="9" />
    </property>
    <property name="summary" value="accountNumber" />
    <property name="description" value="accountNumber description..." />
  </bean>
  <bean id="BudgetConstructionHeader-budgetLockUserIdentifier" parent="BudgetConstructionHeader-budgetLockUserIdentifier-parentBean" />

  <bean id="BudgetConstructionHeader-budgetLockUserIdentifier-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="budgetLockUserIdentifier" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Budget Lock User Identifier" />
    <property name="shortLabel" value="Identifier" />
    <property name="maxLength" value="10" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="11" />
    </property>
    <property name="summary" value="budgetLockUserIdentifier" />
    <property name="description" value="budgetLockUserIdentifier description..." />
  </bean>
  <bean id="BudgetConstructionHeader-budgetTransactionLockUserIdentifier" parent="BudgetConstructionHeader-budgetTransactionLockUserIdentifier-parentBean" />

  <bean id="BudgetConstructionHeader-budgetTransactionLockUserIdentifier-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="budgetTransactionLockUserIdentifier" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Budget Transaction Lock User Identifier" />
    <property name="shortLabel" value="Identifier" />
    <property name="maxLength" value="10" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="11" />
    </property>
    <property name="summary" value="budgetTransactionLockUserIdentifier" />
    <property name="description" value="budgetTransactionLockUserIdentifier description..." />
  </bean>
  <bean id="BudgetConstructionHeader-chartOfAccountsCode" parent="BudgetConstructionHeader-chartOfAccountsCode-parentBean" />

  <bean id="BudgetConstructionHeader-chartOfAccountsCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="chartOfAccountsCode" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Chart Of Accounts Code" />
    <property name="shortLabel" value="Chart" />
    <property name="maxLength" value="2" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="required" value="true" />
    <property name="control" >
      <bean parent="SelectControlDefinition"
            p:valuesFinderClass="org.kuali.module.chart.lookup.keyvalues.SimpleChartValuesFinder"
            p:includeKeyInLabel="false" />
    </property>
    <property name="summary" value="chartOfAccountsCode" />
    <property name="description" value="chartOfAccountsCode description..." />
  </bean>
  <bean id="BudgetConstructionHeader-documentNumber" parent="BudgetConstructionHeader-documentNumber-parentBean" />

  <bean id="BudgetConstructionHeader-documentNumber-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="documentNumber" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Document Number" />
    <property name="shortLabel" value="Doc#" />
    <property name="maxLength" value="14" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="required" value="true" />
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="15" />
    </property>
    <property name="summary" value="documentNumber" />
    <property name="description" value="documentNumber description..." />
  </bean>
  <bean id="BudgetConstructionHeader-organizationLevelChartOfAccountsCode" parent="BudgetConstructionHeader-organizationLevelChartOfAccountsCode-parentBean" />

  <bean id="BudgetConstructionHeader-organizationLevelChartOfAccountsCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="organizationLevelChartOfAccountsCode" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Organization Level Chart Of Accounts Code" />
    <property name="shortLabel" value="Code" />
    <property name="maxLength" value="2" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="3" />
    </property>
    <property name="summary" value="organizationLevelChartOfAccountsCode" />
    <property name="description" value="organizationLevelChartOfAccountsCode description..." />
  </bean>
  <bean id="BudgetConstructionHeader-organizationLevelCode" parent="BudgetConstructionHeader-organizationLevelCode-parentBean" />

  <bean id="BudgetConstructionHeader-organizationLevelCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="organizationLevelCode" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Organization Level Code" />
    <property name="shortLabel" value="Code" />
    <property name="maxLength" value="7" />
    <property name="validationPattern" >
      <bean parent="NumericValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="8" />
    </property>
    <property name="summary" value="organizationLevelCode" />
    <property name="description" value="organizationLevelCode description..." />
  </bean>
  <bean id="BudgetConstructionHeader-organizationLevelOrganizationCode" parent="BudgetConstructionHeader-organizationLevelOrganizationCode-parentBean" />

  <bean id="BudgetConstructionHeader-organizationLevelOrganizationCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="organizationLevelOrganizationCode" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Organization Level Organization Code" />
    <property name="shortLabel" value="Code" />
    <property name="maxLength" value="4" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="5" />
    </property>
    <property name="summary" value="organizationLevelOrganizationCode" />
    <property name="description" value="organizationLevelOrganizationCode description..." />
  </bean>
  <bean id="BudgetConstructionHeader-subAccountNumber" parent="BudgetConstructionHeader-subAccountNumber-parentBean" />

  <bean id="BudgetConstructionHeader-subAccountNumber-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="subAccountNumber" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Sub Account Number" />
    <property name="shortLabel" value="Sub Account" />
    <property name="maxLength" value="5" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="6" />
    </property>
    <property name="summary" value="subAccountNumber" />
    <property name="description" value="subAccountNumber description..." />
  </bean>
  <bean id="BudgetConstructionHeader-universityFiscalYear" parent="BudgetConstructionHeader-universityFiscalYear-parentBean" />

  <bean id="BudgetConstructionHeader-universityFiscalYear-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="universityFiscalYear" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="University Fiscal Year" />
    <property name="shortLabel" value="Year" />
    <property name="maxLength" value="4" />
    <property name="validationPattern" >
      <bean parent="NumericValidationPattern" />
    </property>
    <property name="required" value="true" />
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="5" />
    </property>
    <property name="summary" value="universityFiscalYear" />
    <property name="description" value="universityFiscalYear description..." />
  </bean>
  <bean id="BudgetConstructionHeader-versionNumber" parent="BudgetConstructionHeader-versionNumber-parentBean" />

  <bean id="BudgetConstructionHeader-versionNumber-parentBean" abstract="true" parent="GenericAttributes-versionNumber">
  </bean>

<!-- Business Object Inquiry Definition -->


  <bean id="BudgetConstructionHeader-inquiryDefinition" parent="BudgetConstructionHeader-inquiryDefinition-parentBean" />

  <bean id="BudgetConstructionHeader-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
    <property name="title" value="Budget Construction Header Inquiry" />
    <property name="inquirySections" >
      <list>
        <bean parent="InquirySectionDefinition">
          <property name="title" value="Header Text" />
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields" >
            <list>
              <bean parent="FieldDefinition"
                    p:attributeName="accountNumber" />
              <bean parent="FieldDefinition"
                    p:attributeName="budgetLockUserIdentifier" />
              <bean parent="FieldDefinition"
                    p:attributeName="budgetTransactionLockUserIdentifier" />
              <bean parent="FieldDefinition"
                    p:attributeName="chartOfAccountsCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="documentNumber" />
              <bean parent="FieldDefinition"
                    p:attributeName="organizationLevelChartOfAccountsCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="organizationLevelCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="organizationLevelOrganizationCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="subAccountNumber" />
              <bean parent="FieldDefinition"
                    p:attributeName="universityFiscalYear" />
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Business Object Lookup Definition -->


  <bean id="BudgetConstructionHeader-lookupDefinition" parent="BudgetConstructionHeader-lookupDefinition-parentBean" />

  <bean id="BudgetConstructionHeader-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
    <property name="title" value="Budget Construction Header Lookup" />
    <property name="instructions" value="Look up a Budget Construction Header" />
    <property name="defaultSort" >
      <bean parent="SortDefinition">
      </bean>
    </property>
    <property name="lookupFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="accountNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="budgetLockUserIdentifier" />
        <bean parent="FieldDefinition"
              p:attributeName="budgetTransactionLockUserIdentifier" />
        <bean parent="FieldDefinition"
              p:attributeName="chartOfAccountsCode" />
        <bean parent="FieldDefinition"
              p:attributeName="documentNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="organizationLevelChartOfAccountsCode" />
        <bean parent="FieldDefinition"
              p:attributeName="organizationLevelCode" />
        <bean parent="FieldDefinition"
              p:attributeName="organizationLevelOrganizationCode" />
        <bean parent="FieldDefinition"
              p:attributeName="subAccountNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="universityFiscalYear" />
      </list>
    </property>
    <property name="resultFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="accountNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="budgetLockUserIdentifier" />
        <bean parent="FieldDefinition"
              p:attributeName="budgetTransactionLockUserIdentifier" />
        <bean parent="FieldDefinition"
              p:attributeName="chartOfAccountsCode" />
        <bean parent="FieldDefinition"
              p:attributeName="documentNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="organizationLevelChartOfAccountsCode" />
        <bean parent="FieldDefinition"
              p:attributeName="organizationLevelCode" />
        <bean parent="FieldDefinition"
              p:attributeName="organizationLevelOrganizationCode" />
        <bean parent="FieldDefinition"
              p:attributeName="subAccountNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="universityFiscalYear" />
      </list>
    </property>
  </bean>
</beans>
