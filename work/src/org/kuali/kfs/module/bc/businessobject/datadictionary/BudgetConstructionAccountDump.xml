<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="BudgetConstructionAccountDump" parent="BudgetConstructionAccountDump-parentBean" />

  <bean id="BudgetConstructionAccountDump-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.kfs.module.bc.businessobject.BudgetConstructionAccountDump" />
    <property name="inquiryDefinition" >
      <ref bean="BudgetConstructionAccountDump-inquiryDefinition" />
    </property>
    <property name="lookupDefinition" >
      <ref bean="BudgetConstructionAccountDump-lookupDefinition" />
    </property>
    <property name="objectLabel" value="Budget Construction Account Dump" />
    <property name="attributes" >
      <list>
        <ref bean="BudgetConstructionAccountDump-accountNumber" />
        <ref bean="BudgetConstructionAccountDump-chartOfAccountsCode" />
        <ref bean="BudgetConstructionAccountDump-principalId" />
        <ref bean="BudgetConstructionAccountDump-subAccountNumber" />
        <ref bean="BudgetConstructionAccountDump-universityFiscalYear" />
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->


  <bean id="BudgetConstructionAccountDump-accountNumber" parent="BudgetConstructionAccountDump-accountNumber-parentBean" />

  <bean id="BudgetConstructionAccountDump-accountNumber-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="accountNumber" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Account Number" />
    <property name="shortLabel" value="Number" />
    <property name="maxLength" value="7" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="9" />
    </property>
    <property name="summary" value="accountNumber" />
    <property name="description" value="accountNumber description..." />
  </bean>
  <bean id="BudgetConstructionAccountDump-chartOfAccountsCode" parent="BudgetConstructionAccountDump-chartOfAccountsCode-parentBean" />

  <bean id="BudgetConstructionAccountDump-chartOfAccountsCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="chartOfAccountsCode" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Chart Of Accounts Code" />
    <property name="shortLabel" value="Code" />
    <property name="maxLength" value="2" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="4" />
    </property>
    <property name="summary" value="chartOfAccountsCode" />
    <property name="description" value="chartOfAccountsCode description..." />
  </bean>
  <bean id="BudgetConstructionAccountDump-principalId" parent="BudgetConstructionAccountDump-principalId-parentBean" />

  <bean id="BudgetConstructionAccountDump-principalId-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="principalId" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Person Universal Identifier" />
    <property name="shortLabel" value="Identifier" />
    <property name="maxLength" value="10" />
    <property name="validationPattern" >
      <bean parent="NumericValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="12" />
    </property>
    <property name="summary" value="principalId" />
    <property name="description" value="principalId description..." />
  </bean>
  <bean id="BudgetConstructionAccountDump-subAccountNumber" parent="BudgetConstructionAccountDump-subAccountNumber-parentBean" />

  <bean id="BudgetConstructionAccountDump-subAccountNumber-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="subAccountNumber" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Sub Account Number" />
    <property name="shortLabel" value="Number" />
    <property name="maxLength" value="5" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="7" />
    </property>
    <property name="summary" value="subAccountNumber" />
    <property name="description" value="subAccountNumber description..." />
  </bean>
  <bean id="BudgetConstructionAccountDump-universityFiscalYear" parent="BudgetConstructionAccountDump-universityFiscalYear-parentBean" />

  <bean id="BudgetConstructionAccountDump-universityFiscalYear-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="universityFiscalYear" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="University Fiscal Year" />
    <property name="shortLabel" value="Year" />
    <property name="maxLength" value="4" />
    <property name="validationPattern" >
      <bean parent="NumericValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="6" />
    </property>
    <property name="summary" value="universityFiscalYear" />
    <property name="description" value="universityFiscalYear description..." />
  </bean>

<!-- Business Object Inquiry Definition -->


  <bean id="BudgetConstructionAccountDump-inquiryDefinition" parent="BudgetConstructionAccountDump-inquiryDefinition-parentBean" />

  <bean id="BudgetConstructionAccountDump-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
    <property name="title" value="Budget Construction Account Dump Inquiry" />
    <property name="inquirySections" >
      <list>
        <bean parent="InquirySectionDefinition">
          <property name="title" value="" />
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields" >
            <list>
              <bean parent="FieldDefinition"
                    p:attributeName="accountNumber" />
              <bean parent="FieldDefinition"
                    p:attributeName="chartOfAccountsCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="principalId" />
              <bean parent="FieldDefinition"
                    p:attributeName="subAccountNumber" />
              <bean parent="FieldDefinition"
                    p:attributeName="universityFiscalYear" />
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Business Object Lookup Definition -->


  <bean id="BudgetConstructionAccountDump-lookupDefinition" parent="BudgetConstructionAccountDump-lookupDefinition-parentBean" />

  <bean id="BudgetConstructionAccountDump-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
    <property name="title" value="Budget Construction Account Dump Lookup" />
    <property name="instructions" value="Look up a Budget Construction Account Dump" />
    <property name="defaultSort" >
      <bean parent="SortDefinition">
      </bean>
    </property>
    <property name="lookupFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="accountNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="chartOfAccountsCode" />
        <bean parent="FieldDefinition"
              p:attributeName="principalId" />
        <bean parent="FieldDefinition"
              p:attributeName="subAccountNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="universityFiscalYear" />
      </list>
    </property>
    <property name="resultFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="accountNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="chartOfAccountsCode" />
        <bean parent="FieldDefinition"
              p:attributeName="principalId" />
        <bean parent="FieldDefinition"
              p:attributeName="subAccountNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="universityFiscalYear" />
      </list>
    </property>
  </bean>
</beans>

