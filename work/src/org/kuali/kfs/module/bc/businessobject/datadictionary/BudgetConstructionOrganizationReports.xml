<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="BudgetConstructionOrganizationReports" parent="BudgetConstructionOrganizationReports-parentBean" />

  <bean id="BudgetConstructionOrganizationReports-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.kfs.module.bc.businessobject.BudgetConstructionOrganizationReports" />
    <property name="inquiryDefinition" >
      <ref bean="BudgetConstructionOrganizationReports-inquiryDefinition" />
    </property>
    <property name="lookupDefinition" >
      <ref bean="BudgetConstructionOrganizationReports-lookupDefinition" />
    </property>
    <property name="attributes" >
      <list>
        <ref bean="BudgetConstructionOrganizationReports-chartOfAccountsCode" />
        <ref bean="BudgetConstructionOrganizationReports-chartOfAccounts.codeAndDescription" />
        <ref bean="BudgetConstructionOrganizationReports-organizationCode" />
        <ref bean="BudgetConstructionOrganizationReports-reportsToChartOfAccountsCode" />
        <ref bean="BudgetConstructionOrganizationReports-reportsToChartOfAccounts.codeAndDescription" />
        <ref bean="BudgetConstructionOrganizationReports-reportsToOrganizationCode" />
        <ref bean="BudgetConstructionOrganizationReports-responsibilityCenterCode" />
        <ref bean="BudgetConstructionOrganizationReports-selectionKeyCode" />
        <ref bean="BudgetConstructionOrganizationReports-versionNumber" />
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->


  <bean id="BudgetConstructionOrganizationReports-chartOfAccountsCode" parent="BudgetConstructionOrganizationReports-chartOfAccountsCode-parentBean" />

  <bean id="BudgetConstructionOrganizationReports-chartOfAccountsCode-parentBean" abstract="true" parent="Chart-chartOfAccountsCode">
    <property name="control" >
      <bean parent="SelectControlDefinition"
            p:valuesFinderClass="org.kuali.kfs.coa.businessobject.options.ChartValuesFinder"
            p:includeKeyInLabel="false" />
    </property>
  </bean>
  <bean id="BudgetConstructionOrganizationReports-chartOfAccounts.codeAndDescription" parent="BudgetConstructionOrganizationReports-chartOfAccounts.codeAndDescription-parentBean" />

  <bean id="BudgetConstructionOrganizationReports-chartOfAccounts.codeAndDescription-parentBean" abstract="true" parent="Chart-codeAndDescription">
    <property name="name" value="chartOfAccounts.codeAndDescription" />
    <property name="required" value="false" />
    <property name="control" >
      <bean parent="HiddenControlDefinition" />
    </property>
    <property name="summary" value="&amp;nbsp;" />
    <property name="description" value="Chart Code and Description" />
  </bean>
  <bean id="BudgetConstructionOrganizationReports-organizationCode" parent="BudgetConstructionOrganizationReports-organizationCode-parentBean" />

  <bean id="BudgetConstructionOrganizationReports-organizationCode-parentBean" abstract="true" parent="Org-organizationCode">
  </bean>
  <bean id="BudgetConstructionOrganizationReports-reportsToChartOfAccountsCode" parent="BudgetConstructionOrganizationReports-reportsToChartOfAccountsCode-parentBean" />

  <bean id="BudgetConstructionOrganizationReports-reportsToChartOfAccountsCode-parentBean" abstract="true" parent="Chart-chartOfAccountsCode">
    <property name="name" value="reportsToChartOfAccountsCode" />
    <property name="label" value="Reports To Chart" />
    <property name="shortLabel" value="Repts to Cht" />
    <property name="control" >
      <bean parent="SelectControlDefinition"
            p:valuesFinderClass="org.kuali.kfs.coa.businessobject.options.ChartValuesFinder"
            p:includeKeyInLabel="false" />
    </property>
    <property name="description" value="The Chart for the Reports To organization." />
  </bean>
  <bean id="BudgetConstructionOrganizationReports-reportsToChartOfAccounts.codeAndDescription" parent="BudgetConstructionOrganizationReports-reportsToChartOfAccounts.codeAndDescription-parentBean" />

  <bean id="BudgetConstructionOrganizationReports-reportsToChartOfAccounts.codeAndDescription-parentBean" abstract="true" parent="Chart-codeAndDescription">
    <property name="name" value="reportsToChartOfAccounts.codeAndDescription" />
    <property name="label" value="Reports To Chart" />
    <property name="shortLabel" value="Repts to Cht" />
    <property name="required" value="false" />
    <property name="control" >
      <bean parent="HiddenControlDefinition" />
    </property>
    <property name="summary" value="&amp;nbsp;" />
    <property name="description" value="The Chart for the Reports To organization." />
  </bean>
  <bean id="BudgetConstructionOrganizationReports-reportsToOrganizationCode" parent="BudgetConstructionOrganizationReports-reportsToOrganizationCode-parentBean" />

  <bean id="BudgetConstructionOrganizationReports-reportsToOrganizationCode-parentBean" abstract="true" parent="Org-organizationCode">
    <property name="name" value="reportsToOrganizationCode" />
    <property name="label" value="Reports To Organization" />
    <property name="shortLabel" value="Rpts To Org" />
    <property name="description" value="The Reports To organization for the account." />
  </bean>
  <bean id="BudgetConstructionOrganizationReports-responsibilityCenterCode" parent="BudgetConstructionOrganizationReports-responsibilityCenterCode-parentBean" />

  <bean id="BudgetConstructionOrganizationReports-responsibilityCenterCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="responsibilityCenterCode" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Responsibility Center Code" />
    <property name="shortLabel" value="Code" />
    <property name="maxLength" value="2" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="4" />
    </property>
    <property name="summary" value="Responsibility Center Code" />
    <property name="description" value="The code uniquely identifying a particular Responsibility Center." />
  </bean>
  <bean id="BudgetConstructionOrganizationReports-selectionKeyCode" parent="BudgetConstructionOrganizationReports-selectionKeyCode-parentBean" />

  <bean id="BudgetConstructionOrganizationReports-selectionKeyCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="selectionKeyCode" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Point Of View Key Code" />
    <property name="shortLabel" value="Code" />
    <property name="maxLength" value="7" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="SelectControlDefinition"
            p:valuesFinderClass="org.kuali.kfs.module.bc.businessobject.options.PointOfViewOrgValuesFinder"
            p:includeKeyInLabel="false" />
    </property>
    <property name="summary" value="Point Of View Organization Selection Key" />
    <property name="description" value="The composite key uniquely identifying a point of view organization." />
  </bean>
  <bean id="BudgetConstructionOrganizationReports-versionNumber" parent="BudgetConstructionOrganizationReports-versionNumber-parentBean" />

  <bean id="BudgetConstructionOrganizationReports-versionNumber-parentBean" abstract="true" parent="GenericAttributes-versionNumber">
  </bean>

<!-- Business Object Inquiry Definition -->


  <bean id="BudgetConstructionOrganizationReports-inquiryDefinition" parent="BudgetConstructionOrganizationReports-inquiryDefinition-parentBean" />

  <bean id="BudgetConstructionOrganizationReports-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
    <property name="title" value="Budget Construction Organization Reports Inquiry" />
    <property name="inquirySections" >
      <list>
        <bean parent="InquirySectionDefinition">
          <property name="title" value="" />
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields" >
            <list>
              <bean parent="FieldDefinition"
                    p:attributeName="chartOfAccounts.codeAndDescription" />
              <bean parent="FieldDefinition"
                    p:attributeName="organizationCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="reportsToChartOfAccounts.codeAndDescription" />
              <bean parent="FieldDefinition"
                    p:attributeName="reportsToOrganizationCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="responsibilityCenterCode" />
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Business Object Lookup Definition -->


  <bean id="BudgetConstructionOrganizationReports-lookupDefinition" parent="BudgetConstructionOrganizationReports-lookupDefinition-parentBean" />

  <bean id="BudgetConstructionOrganizationReports-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
    <property name="title" value="Budget Construction Organization Reports Lookup" />
    <property name="instructions" value="Look up Budget Construction Organization Reports" />
    <property name="defaultSort" >
      <bean parent="SortDefinition">
      </bean>
    </property>
    <property name="lookupFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="chartOfAccountsCode" />
        <bean parent="FieldDefinition"
              p:attributeName="organizationCode" />
        <bean parent="FieldDefinition"
              p:attributeName="reportsToChartOfAccountsCode" />
        <bean parent="FieldDefinition"
              p:attributeName="reportsToOrganizationCode" />
        <bean parent="FieldDefinition"
              p:attributeName="responsibilityCenterCode" />
      </list>
    </property>
    <property name="resultFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="chartOfAccountsCode" />
        <bean parent="FieldDefinition"
              p:attributeName="organizationCode" />
        <bean parent="FieldDefinition"
              p:attributeName="reportsToChartOfAccountsCode" />
        <bean parent="FieldDefinition"
              p:attributeName="reportsToOrganizationCode" />
        <bean parent="FieldDefinition"
              p:attributeName="responsibilityCenterCode" />
      </list>
    </property>
  </bean>
</beans>
