<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="BudgetConstructionDocument" parent="BudgetConstructionDocument-parentBean" />

  <bean id="BudgetConstructionDocument-parentBean" abstract="true" parent="TransactionalDocumentEntry">
    <property name="label" value="Budget Construction Document" />
    <property name="shortLabel" value="BCDoc" />
    <property name="summary" value="&amp;nbsp;" />
    <property name="description" value="Budget Construction Document" />
    <property name="documentTypeName" value="BudgetConstructionDocument" />
    <property name="documentClass" value="org.kuali.module.budget.document.BudgetConstructionDocument" />
    <property name="businessRulesClass" value="org.kuali.module.budget.rules.BudgetConstructionRules" />
    <property name="documentAuthorizerClass" value="org.kuali.module.budget.document.authorization.BudgetConstructionDocumentAuthorizer" />
    <property name="documentTypeCode" value="BC" />
    <property name="authorizations" >
      <list>
        <bean parent="AuthorizationDefinition">
          <property name="action" value="initiate" />
          <property name="authorizedGroups" >
            <set>
              <value>kualiUniversalGroup</value>
            </set>
          </property>
        </bean>
      </list>
    </property>
    <property name="attributes" >
      <list>
        <ref bean="BudgetConstructionDocument-accountNumber" />
        <ref bean="BudgetConstructionDocument-budgetLockUserIdentifier" />
        <ref bean="BudgetConstructionDocument-budgetTransactionLockUserIdentifier" />
        <ref bean="BudgetConstructionDocument-chartOfAccountsCode" />
        <ref bean="BudgetConstructionDocument-documentNumber" />
        <ref bean="BudgetConstructionDocument-organizationLevelChartOfAccountsCode" />
        <ref bean="BudgetConstructionDocument-organizationLevelCode" />
        <ref bean="BudgetConstructionDocument-organizationLevelOrganizationCode" />
        <ref bean="BudgetConstructionDocument-subAccountNumber" />
        <ref bean="BudgetConstructionDocument-universityFiscalYear" />
        <ref bean="BudgetConstructionDocument-versionNumber" />
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->


  <bean id="BudgetConstructionDocument-accountNumber" parent="BudgetConstructionDocument-accountNumber-parentBean" />

  <bean id="BudgetConstructionDocument-accountNumber-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="accountNumber" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Account Number" />
    <property name="shortLabel" value="Chart/Account" />
    <property name="maxLength" value="7" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="9" />
    </property>
    <property name="summary" value="accountNumber" />
    <property name="description" value="accountNumber description..." />
  </bean>
  <bean id="BudgetConstructionDocument-budgetLockUserIdentifier" parent="BudgetConstructionDocument-budgetLockUserIdentifier-parentBean" />

  <bean id="BudgetConstructionDocument-budgetLockUserIdentifier-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="budgetLockUserIdentifier" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Budget Lock User Identifier" />
    <property name="shortLabel" value="Identifier" />
    <property name="maxLength" value="10" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="12" />
    </property>
    <property name="summary" value="budgetLockUserIdentifier" />
    <property name="description" value="budgetLockUserIdentifier description..." />
  </bean>
  <bean id="BudgetConstructionDocument-budgetTransactionLockUserIdentifier" parent="BudgetConstructionDocument-budgetTransactionLockUserIdentifier-parentBean" />

  <bean id="BudgetConstructionDocument-budgetTransactionLockUserIdentifier-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="budgetTransactionLockUserIdentifier" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Budget Transaction Lock User Identifier" />
    <property name="shortLabel" value="Identifier" />
    <property name="maxLength" value="10" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="12" />
    </property>
    <property name="summary" value="budgetTransactionLockUserIdentifier" />
    <property name="description" value="budgetTransactionLockUserIdentifier description..." />
  </bean>
  <bean id="BudgetConstructionDocument-chartOfAccountsCode" parent="BudgetConstructionDocument-chartOfAccountsCode-parentBean" />

  <bean id="BudgetConstructionDocument-chartOfAccountsCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="chartOfAccountsCode" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Chart Of Accounts Code" />
    <property name="shortLabel" value="Code" />
    <property name="maxLength" value="2" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="4" />
    </property>
    <property name="summary" value="chartOfAccountsCode" />
    <property name="description" value="chartOfAccountsCode description..." />
  </bean>
  <bean id="BudgetConstructionDocument-documentNumber" parent="BudgetConstructionDocument-documentNumber-parentBean" />

  <bean id="BudgetConstructionDocument-documentNumber-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="documentNumber" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Document Number" />
    <property name="shortLabel" value="Number" />
    <property name="maxLength" value="14" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="16" />
    </property>
    <property name="summary" value="documentNumber" />
    <property name="description" value="documentNumber description..." />
  </bean>
  <bean id="BudgetConstructionDocument-organizationLevelChartOfAccountsCode" parent="BudgetConstructionDocument-organizationLevelChartOfAccountsCode-parentBean" />

  <bean id="BudgetConstructionDocument-organizationLevelChartOfAccountsCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="organizationLevelChartOfAccountsCode" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Organization Level Chart Of Accounts Code" />
    <property name="shortLabel" value="Code" />
    <property name="maxLength" value="2" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="4" />
    </property>
    <property name="summary" value="organizationLevelChartOfAccountsCode" />
    <property name="description" value="organizationLevelChartOfAccountsCode description..." />
  </bean>
  <bean id="BudgetConstructionDocument-organizationLevelCode" parent="BudgetConstructionDocument-organizationLevelCode-parentBean" />

  <bean id="BudgetConstructionDocument-organizationLevelCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="organizationLevelCode" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Organization Level Code" />
    <property name="shortLabel" value="Current Level" />
    <property name="maxLength" value="7" />
    <property name="validationPattern" >
      <bean parent="NumericValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="9" />
    </property>
    <property name="summary" value="organizationLevelCode" />
    <property name="description" value="organizationLevelCode description..." />
  </bean>
  <bean id="BudgetConstructionDocument-organizationLevelOrganizationCode" parent="BudgetConstructionDocument-organizationLevelOrganizationCode-parentBean" />

  <bean id="BudgetConstructionDocument-organizationLevelOrganizationCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="organizationLevelOrganizationCode" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Organization Level Organization Code" />
    <property name="shortLabel" value="Level Chart/Org" />
    <property name="maxLength" value="4" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="6" />
    </property>
    <property name="summary" value="organizationLevelOrganizationCode" />
    <property name="description" value="organizationLevelOrganizationCode description..." />
  </bean>
  <bean id="BudgetConstructionDocument-subAccountNumber" parent="BudgetConstructionDocument-subAccountNumber-parentBean" />

  <bean id="BudgetConstructionDocument-subAccountNumber-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="subAccountNumber" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Sub Account Number" />
    <property name="shortLabel" value="Sub-Account" />
    <property name="maxLength" value="5" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="7" />
    </property>
    <property name="summary" value="subAccountNumber" />
    <property name="description" value="subAccountNumber description..." />
  </bean>
  <bean id="BudgetConstructionDocument-universityFiscalYear" parent="BudgetConstructionDocument-universityFiscalYear-parentBean" />

  <bean id="BudgetConstructionDocument-universityFiscalYear-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="universityFiscalYear" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="University Fiscal Year" />
    <property name="shortLabel" value="Fiscal Year" />
    <property name="maxLength" value="4" />
    <property name="validationPattern" >
      <bean parent="NumericValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="6" />
    </property>
    <property name="summary" value="universityFiscalYear" />
    <property name="description" value="universityFiscalYear description..." />
  </bean>
  <bean id="BudgetConstructionDocument-versionNumber" parent="BudgetConstructionDocument-versionNumber-parentBean" />

  <bean id="BudgetConstructionDocument-versionNumber-parentBean" abstract="true" parent="GenericAttributes-versionNumber">
  </bean>
</beans>
