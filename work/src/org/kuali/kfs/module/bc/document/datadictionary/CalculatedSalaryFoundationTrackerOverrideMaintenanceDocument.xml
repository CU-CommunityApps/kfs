<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "../../../../core/datadictionary/dataDictionary-1_0.dtd" >

<dictionaryEntry>
    <maintenanceDocument>
        <businessObjectClass>org.kuali.module.budget.bo.CalculatedSalaryFoundationTrackerOverride</businessObjectClass>
        <maintainableClass>org.kuali.core.maintenance.KualiMaintainableImpl</maintainableClass>
        <businessRulesClass>org.kuali.module.budget.rules.CalculatedSalaryFoundationTrackerOverrideRule</businessRulesClass>

        <documentAuthorizerClass>org.kuali.core.document.authorization.MaintenanceDocumentAuthorizerBase</documentAuthorizerClass>
        <authorizations>
            <authorization action="initiate">
                <workgroups>
                    <workgroup>KUALI_ROLE_MAINTENANCE</workgroup>
                </workgroups>
            </authorization>
        </authorizations>

        <documentTypeName>CalculatedSalaryFoundationTrackerOverrideMaintenanceDocument</documentTypeName>
        <documentTypeCode>CSFO</documentTypeCode>

        <label>Calculated Salary Foundation Tracker Override</label>
        <shortLabel>CSF Tracker Maint Override Doc</shortLabel>

        <summary>Calculated Salary Foundation Tracker Override Maintenance Document</summary>
        <description>Document used to create or update Calculated Salary Foundation Tracker Override Maintenance Document objects</description>

        <lockingKeys>
            <field attributeName="universityFiscalYear" />
            <field attributeName="chartOfAccountsCode" />
            <field attributeName="accountNumber" />
            <field attributeName="subAccountNumber" />
            <field attributeName="financialObjectCode" />
            <field attributeName="financialSubObjectCode" />
            <field attributeName="positionNumber" />
            <field attributeName="emplid" />  
        </lockingKeys>

        <defaultExistenceChecks>
            <reference attributeName="chartOfAccounts" activeIndicatorAttributeName="finChartOfAccountActiveIndicator" activeIndicatorReversed="false" attributeToHighlightOnFail="chartOfAccountsCode" displayFieldName="Chart Of Accounts Code" />
            <reference attributeName="account" activeIndicatorAttributeName="accountClosedIndicator" activeIndicatorReversed="true" attributeToHighlightOnFail="accountNumber" displayFieldName="Account Number" />
            <reference attributeName="financialObject" activeIndicatorAttributeName="financialObjectActiveCode" attributeToHighlightOnFail="financialObjectCode" />
            <reference attributeName="universityFiscal" attributeToHighlightOnFail="universityFiscalYear" />
        </defaultExistenceChecks>

        <maintainableSections>
            <maintainableSection title="Edit Calculated Salary Foundation Tracker Override">
                <maintainableItems>
                    <maintainableField  name="universityFiscalYear" required="true" />
                    <maintainableField  name="chartOfAccountsCode" required="true" />
                     <maintainableField name="accountNumber" required="true" />
                     <maintainableField name="subAccountNumber" required="true" />
                     <maintainableField name="financialObjectCode" required="true" />
                     <maintainableField name="financialSubObjectCode" required="true" />
                     <maintainableField name="positionNumber" required="true" />
                     <maintainableField name="emplid" required="true" />
                     <maintainableField name="csfAmount" required="true" />
                     <maintainableField name="csfFullTimeEmploymentQuantity" required="true" />
                     <maintainableField name="csfFundingStatusCode" required="true" />
                     <maintainableField name="csfTimePercent" required="true" /> 
                     <maintainableField name="csfDeleteCode" required="false" />                                                   
                    <maintainableField name="versionNumber" />
                </maintainableItems>
            </maintainableSection>
        </maintainableSections>
    </maintenanceDocument>
</dictionaryEntry>
