<dictionaryEntry>
    <maintenanceDocument>
        <businessObjectClass>org.kuali.module.budget.bo.BudgetConstructionIntendedIncumbent</businessObjectClass>
        <maintainableClass>org.kuali.core.maintenance.KualiMaintainableImpl</maintainableClass>

        <documentAuthorizerClass>org.kuali.core.document.authorization.MaintenanceDocumentAuthorizerBase</documentAuthorizerClass>
        <authorizations>
            <authorization action="initiate">
                <workgroups>
                    <workgroup>SY_MAINTAINENANCE_USERS</workgroup>
                </workgroups>
            </authorization>
        </authorizations>

        <documentTypeName>IntendedIncumbentMaintenanceDocument</documentTypeName>
        <documentTypeCode>IINC</documentTypeCode>

        <label>Intended Incumbent</label>
        <shortLabel>BC - BC Intended Incumbent Code Maint Doc</shortLabel>

        <summary>Budget Construction Intended Incumbent Maintenance Document</summary>
        <description>Document used to create or update Budget Construction Intended Incumbent Maintenance Document objects</description>

        <lockingKeys>
            <field attributeName="emplid" />
        </lockingKeys>

        <maintainableSections>
            <maintainableSection title="Edit BC Intended Incumbent">
                <maintainableItems>
                    <maintainableField name="emplid" required="true" />
                    <maintainableField name="personName" required="true" />
                    <maintainableField name="setidSalary" required="false" />
                    <maintainableField name="salaryAdministrationPlan" required="false" />
                    <maintainableField name="grade" required="false" />
                    <maintainableField name="iuClassificationLevel" required="false" />                   
                    <maintainableField name="versionNumber" />
                </maintainableItems>
            </maintainableSection>
        </maintainableSections>
    </maintenanceDocument>
</dictionaryEntry>
