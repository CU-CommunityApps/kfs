<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="AppointmentFundingDurationMaintenanceDocument" parent="AppointmentFundingDurationMaintenanceDocument-parentBean" />

  <bean id="AppointmentFundingDurationMaintenanceDocument-parentBean" abstract="true" parent="MaintenanceDocumentEntry">
    <property name="businessObjectClass" value="org.kuali.kfs.module.bc.businessobject.BudgetConstructionDuration" />
    <property name="maintainableClass" value="org.kuali.rice.kns.maintenance.KualiMaintainableImpl" />
    <property name="maintainableSections" >
      <list>
        <ref bean="AppointmentFundingDurationMaintenanceDocument-EditBCDuration" />
      </list>
    </property>
    <property name="lockingKeys" >
      <list>
        <value>appointmentDurationCode</value>
      </list>
    </property>
    <property name="label" value="Appointment Funding Duration" />
    <property name="shortLabel" value="BC - BC Duration Code Maint Doc" />
    <property name="summary" value="Budget Construction Duration Maintenance Document" />
    <property name="description" value="Document used to create or update Budget Construction Duration Maintenance Document objects" />
    <property name="documentTypeName" value="AppointmentFundingDurationMaintenanceDocument" />
    <property name="documentAuthorizerClass" value="org.kuali.kfs.sys.document.authorization.FinancialSystemMaintenanceDocumentAuthorizerBase" />
    <property name="documentTypeCode" value="DURA" />
  </bean>

<!-- Maintenance Section Definitions -->


  <bean id="AppointmentFundingDurationMaintenanceDocument-EditBCDuration" parent="AppointmentFundingDurationMaintenanceDocument-EditBCDuration-parentBean" />

  <bean id="AppointmentFundingDurationMaintenanceDocument-EditBCDuration-parentBean" abstract="true" parent="MaintainableSectionDefinition">
    <property name="id" value="Edit BC Duration" />
    <property name="title" value="Edit BC Duration" />
    <property name="maintainableItems" >
      <list>
        <bean parent="MaintainableFieldDefinition"
              p:name="appointmentDurationCode"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="appointmentDurationDescription"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="active"
              p:required="false" />
      </list>
    </property>
  </bean>
</beans>
