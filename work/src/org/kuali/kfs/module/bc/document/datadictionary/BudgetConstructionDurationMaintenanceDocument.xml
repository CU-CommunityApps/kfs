<?xml version="1.0" encoding="UTF-8"?><beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p" xsi:schemaLocation="http://www.springframework.org/schema/beans         http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="AppointmentFundingDurationMaintenanceDocument" parent="AppointmentFundingDurationMaintenanceDocument-parentBean"/>

  <bean id="AppointmentFundingDurationMaintenanceDocument-parentBean" abstract="true" parent="MaintenanceDocumentEntry">
    <property name="businessObjectClass" value="org.kuali.kfs.module.bc.businessobject.BudgetConstructionDuration"/>
    <property name="maintainableClass" value="org.kuali.kfs.sys.document.FinancialSystemMaintainable"/>
    <property name="maintainableSections">
      <list>
        <ref bean="AppointmentFundingDurationMaintenanceDocument-EditBCDuration"/>
      </list>
    </property>
    <property name="lockingKeys">
      <list>
        <value>appointmentDurationCode</value>
      </list>
    </property>
    
    
    
    
    <property name="documentTypeName" value="DURA"/>
    <property name="documentAuthorizerClass" value="org.kuali.kfs.sys.document.authorization.FinancialSystemMaintenanceDocumentAuthorizerBase"/>
    </bean>

<!-- Maintenance Section Definitions -->


  <bean id="AppointmentFundingDurationMaintenanceDocument-EditBCDuration" parent="AppointmentFundingDurationMaintenanceDocument-EditBCDuration-parentBean"/>

  <bean id="AppointmentFundingDurationMaintenanceDocument-EditBCDuration-parentBean" abstract="true" parent="MaintainableSectionDefinition">
    <property name="id" value="Edit BC Duration"/>
    <property name="title" value="Edit BC Duration"/>
    <property name="maintainableItems">
      <list>
        <bean parent="MaintainableFieldDefinition" p:name="appointmentDurationCode" p:required="true"/>
        <bean parent="MaintainableFieldDefinition" p:name="appointmentDurationDescription" p:required="true"/>
        <bean parent="MaintainableFieldDefinition" p:name="active" p:required="false"/>
      </list>
    </property>
  </bean>
</beans>