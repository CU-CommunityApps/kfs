<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "../../../core/datadictionary/dataDictionary-1_0.dtd">
	
<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.module.gl.bo.AccountBalanceByConsolidation">
        <customAttributeEnabled>false</customAttributeEnabled>
        <inquiry>
            <title>Available Account Balance</title>
            <inquiryFields>
				<field attributeName="universityFiscalYear"/>
				<field attributeName="chartOfAccountsCode"/>
				<field attributeName="accountNumber"/>
				<field attributeName="objectCode"/>
				<field attributeName="subAccountNumber"/>
				<field attributeName="subObjectCode"/>             
				<field attributeName="currentBudgetLineBalanceAmount"/>
				<field attributeName="accountLineActualsBalanceAmount"/>
				<field attributeName="accountLineEncumbranceBalanceAmount"/>
				<field attributeName="dummyBusinessObject.genericAmount"/>
            </inquiryFields>
        </inquiry>
		
        <lookup>
        	<lookupableID>glAccountBalanceByConsolidationLookupable</lookupableID>
			<title>Available Account Balance By Consolidation</title>
			
			<instructions>Lookup Available Account Balance for the specific account.</instructions>			
			<defaultSort attributeName="dummyBusinessObject.consolidationObjectCode" />
			
			<lookupFields>
				<lookupField attributeName="universityFiscalYear" required="true" noLookup="false">
                	<defaultValueFinderClass>
						org.kuali.core.lookup.valueFinder.FiscalYearFinder
					</defaultValueFinderClass>
                </lookupField>
                				
				<lookupField attributeName="chartOfAccountsCode" required="true" noLookup="false"/>
				<lookupField attributeName="accountNumber" required="true" noLookup="false"/>
				<lookupField attributeName="subAccountNumber" noLookup="false"/>
				<lookupField attributeName="dummyBusinessObject.consolidationOption">
					<defaultValueFinderClass>
						org.kuali.module.gl.web.optionfinder.GLConsolidationOptionFinder
					</defaultValueFinderClass>
				</lookupField>
				
				<lookupField attributeName="dummyBusinessObject.costShareOption">
					<defaultValueFinderClass>
						org.kuali.module.gl.web.optionfinder.GLCostShareOptionFinder
					</defaultValueFinderClass>
				</lookupField>				
			</lookupFields>
			
			<resultFields>
				<field attributeName="universityFiscalYear"/>
				<field attributeName="chartOfAccountsCode"/>
				<field attributeName="accountNumber"/>
				<field attributeName="subAccountNumber"/>
				
	            <field attributeName="dummyBusinessObject.consolidationObjectCode"/>
	            <field attributeName="dummyBusinessObject.consolidationReportingSortCode"/>
	            <field attributeName="dummyBusinessObject.reportingSortCode"/> 
	                       
				<field attributeName="currentBudgetLineBalanceAmount"/>
				<field attributeName="accountLineActualsBalanceAmount"/>
				<field attributeName="accountLineEncumbranceBalanceAmount"/>
				<field attributeName="dummyBusinessObject.genericAmount"/>
			</resultFields>
		</lookup>
		
        <attributes>
            <attributeReference name="universityFiscalYear" 
            					sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" 
            					sourceAttributeName="genericFiscalYear" />
										
            <attributeReference name="chartOfAccountsCode" 
            					sourceClassName="org.kuali.module.chart.bo.Chart" 
            					sourceAttributeName="chartOfAccountsCode">
				<control>
					<select valuesFinderClass="org.kuali.core.lookup.keyvalues.ChartValuesFinder"/>					
				</control>
			</attributeReference>
			
            <attributeReference name="accountNumber" 
            					sourceClassName="org.kuali.module.chart.bo.Account" 
            					sourceAttributeName="accountNumber" />
								
            <attributeReference name="subAccountNumber" 
            					sourceClassName="org.kuali.module.chart.bo.SubAccount" 
            					sourceAttributeName="subAccountNumber"/>
								
            <attributeReference name="objectCode" 
            					sourceClassName="org.kuali.module.chart.bo.ObjectCode" 
            					sourceAttributeName="financialObjectCode"/>
								
            <attributeReference name="subObjectCode" 
            					sourceClassName="org.kuali.module.chart.bo.SubObjCd" 
            					sourceAttributeName="financialSubObjectCode" />            													

			<attributeReference name="currentBudgetLineBalanceAmount" 
            					sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" 
            					sourceAttributeName="genericAmount">
                <label>Budget Amount</label>
                <shortLabel>Amount</shortLabel>
                <summary>currentBudgetLineBalanceAmount</summary>
                <description>currentBudgetLineBalanceAmount description...</description>
            </attributeReference>
			
			<attributeReference name="accountLineActualsBalanceAmount" 
            					sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" 
            					sourceAttributeName="genericAmount">
                <label>Actuals Amount</label>
                <shortLabel>Actuals Balance Amount</shortLabel>
                <summary>accountLineActualsBalanceAmount</summary>
                <description>accountLineActualsBalanceAmount description...</description>
            </attributeReference>

			<attributeReference name="accountLineEncumbranceBalanceAmount" 
            					sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" 
            					sourceAttributeName="genericAmount">
                <label>Encumbrance Amount</label>
                <shortLabel>Encumbrance Balance</shortLabel>
                <summary>accountLineEncumbranceBalanceAmount</summary>
                <description>accountLineEncumbranceBalanceAmount description...</description>
            </attributeReference>

            <attributeReference name="timestamp" 
            					sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" 
					            sourceAttributeName="genericTimestamp" >
                <label>Transaction Entry Processed</label>
                <shortLabel>Entry Processed</shortLabel>
                <summary>transactionEntryProcessed</summary>
                <description>transactionEntryProcessed description...</description>
            </attributeReference>
                        					
			<attributeReference name="dummyBusinessObject.consolidationOption"
            					sourceClassName="org.kuali.module.gl.bo.DummyBusinessObject" 
            					sourceAttributeName="consolidationOption"/> 

			<attributeReference name="dummyBusinessObject.costShareOption"
            					sourceClassName="org.kuali.module.gl.bo.DummyBusinessObject" 
            					sourceAttributeName="costShareOption"/> 
            						        
			<attributeReference name="dummyBusinessObject.genericAmount"
            					sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" 
            					sourceAttributeName="genericAmount"> 
			    <label>Variance</label>
                <shortLabel>Variance</shortLabel>
                <summary>Variance</summary>
                <description>Variance...</description>      					
            </attributeReference>
            
            <attributeReference name="dummyBusinessObject.consolidationReportingSortCode"
            					sourceClassName="org.kuali.module.chart.bo.ObjectCons" 
            					sourceAttributeName="financialReportingSortCode"/> 
            					
	        <attributeReference name="dummyBusinessObject.reportingSortCode"
	                    		sourceClassName="org.kuali.module.chart.bo.ObjectType" 
            					sourceAttributeName="financialReportingSortCode"/> 
            					
	        <attributeReference name="dummyBusinessObject.consolidationObjectCode"
            					sourceClassName="org.kuali.module.chart.bo.ObjLevel" 
            					sourceAttributeName="financialConsolidationObjectCode"/> 	         
            					 
        </attributes>
		
    </businessObject>
</dictionaryEntry>