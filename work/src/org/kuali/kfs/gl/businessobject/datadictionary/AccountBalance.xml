<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "../../../core/datadictionary/dataDictionary-1_0.dtd">

<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.module.gl.bo.AccountBalance">

        <inquiry>
            <title>Available Balances</title>
            <inquirySections><inquirySection title="Header Text"><inquiryFields>
                <field attributeName="universityFiscalYear" />
                <field attributeName="chartOfAccountsCode" />
                <field attributeName="accountNumber" />
                <field attributeName="subAccountNumber" />
                <field attributeName="objectCode" />
                <field attributeName="subObjectCode" />
                <field attributeName="currentBudgetLineBalanceAmount" />
                <field attributeName="accountLineActualsBalanceAmount" />
                <field attributeName="accountLineEncumbranceBalanceAmount" />
                <field attributeName="dummyBusinessObject.genericAmount" />
            </inquiryFields></inquirySection></inquirySections>
        </inquiry>

        <lookup>
            <lookupableID>glAccountBalanceLookupable</lookupableID>
            <title>Available Balances Lookup</title>

            <instructions>Lookup Available Account Balance for the specific account.</instructions>
            <defaultSort attributeName="objectCode" />

            <lookupFields>
                <lookupField attributeName="universityFiscalYear" required="true">
                    <defaultValueFinderClass>org.kuali.kfs.lookup.valuefinder.FiscalYearFinder</defaultValueFinderClass>
                </lookupField>

                <lookupField attributeName="chartOfAccountsCode" required="true" />
                <lookupField attributeName="accountNumber" required="true" />
                <lookupField attributeName="subAccountNumber" forceLookup="true" />
                <lookupField attributeName="objectCode" />
                <lookupField attributeName="subObjectCode" />
                <lookupField attributeName="dummyBusinessObject.consolidationOption">
                    <defaultValueFinderClass>org.kuali.module.gl.web.optionfinder.GLConsolidationOptionFinder</defaultValueFinderClass>
                </lookupField>

                <lookupField attributeName="dummyBusinessObject.pendingEntryOption">
                    <defaultValueFinderClass>org.kuali.module.gl.web.optionfinder.GLPendingEntryOptionFinder</defaultValueFinderClass>
                </lookupField>
            </lookupFields>

            <resultFields>
                <field attributeName="universityFiscalYear" />
                <field attributeName="chartOfAccountsCode" />
                <field attributeName="accountNumber" />
                <field attributeName="subAccountNumber" />
                <field attributeName="objectCode" />
                <field attributeName="subObjectCode" />

                <field attributeName="currentBudgetLineBalanceAmount" />
                <field attributeName="accountLineActualsBalanceAmount" />
                <field attributeName="accountLineEncumbranceBalanceAmount" />
                <field attributeName="dummyBusinessObject.genericAmount" />
            </resultFields>
        </lookup>

        <attributes>
            <attributeReference name="universityFiscalYear" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="genericFiscalYear">
                <label>Fiscal Year</label>
                <shortLabel>Fiscal Year</shortLabel>
            </attributeReference>

            <attributeReference name="chartOfAccountsCode" sourceClassName="org.kuali.module.chart.bo.Chart" sourceAttributeName="chartOfAccountsCode">
                <control>
                    <text size="2" />
                </control>
            </attributeReference>

            <attributeReference name="accountNumber" sourceClassName="org.kuali.module.chart.bo.Account" sourceAttributeName="accountNumber">
            </attributeReference>

            <attributeReference name="subAccountNumber" sourceClassName="org.kuali.module.chart.bo.SubAccount" sourceAttributeName="subAccountNumber">
            </attributeReference>

            <attributeReference name="objectCode" sourceClassName="org.kuali.module.chart.bo.ObjectCode" sourceAttributeName="financialObjectCode">
            </attributeReference>

            <attributeReference name="subObjectCode" sourceClassName="org.kuali.module.chart.bo.SubObjCd" sourceAttributeName="financialSubObjectCode">
            </attributeReference>

            <attributeReference name="currentBudgetLineBalanceAmount" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="genericAmount">
                <label>Budget Amount</label>
                <shortLabel>Amount</shortLabel>
                <formatterClass>org.kuali.core.web.format.CurrencyFormatter</formatterClass>
            </attributeReference>

            <attributeReference name="accountLineActualsBalanceAmount" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="genericAmount">
                <label>Actuals Amount</label>
                <shortLabel>Actuals Balance Amount</shortLabel>
                <formatterClass>org.kuali.core.web.format.CurrencyFormatter</formatterClass>
            </attributeReference>

            <attributeReference name="accountLineEncumbranceBalanceAmount" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="genericAmount">
                <label>Encumbrance Amount</label>
                <shortLabel>Encumbrance Balance</shortLabel>
                <formatterClass>org.kuali.core.web.format.CurrencyFormatter</formatterClass>
            </attributeReference>

            <attributeReference name="timestamp" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="genericTimestamp">
                <label>Transaction Entry Processed</label>
                <shortLabel>Entry Processed</shortLabel>
            </attributeReference>

            <attributeReference name="dummyBusinessObject.pendingEntryOption" sourceClassName="org.kuali.module.gl.bo.TransientBalanceInquiryAttributes" sourceAttributeName="pendingEntryOption" />

            <attributeReference name="dummyBusinessObject.consolidationOption" sourceClassName="org.kuali.module.gl.bo.TransientBalanceInquiryAttributes" sourceAttributeName="consolidationOption">
            </attributeReference>

            <attributeReference name="dummyBusinessObject.genericAmount" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="genericAmount">
                <label>Variance</label>
                <shortLabel>Variance</shortLabel>
                <formatterClass>org.kuali.core.web.format.CurrencyFormatter</formatterClass>
            </attributeReference>

            <attributeReference name="dummyBusinessObject.linkButtonOption" sourceClassName="org.kuali.module.gl.bo.TransientBalanceInquiryAttributes" sourceAttributeName="linkButtonOption">
                <label>Drill Down</label>
                <shortLabel>Drill Down</shortLabel>
            </attributeReference>
        </attributes>
    </businessObject>
</dictionaryEntry>