                <shortLabel>linkButtonOption</shortLabel>
                <maxLength>2</maxLength>
                <control>
                    <text size="2" />
                </control>
                <summary>linkButtonOption</summary>
                <description>linkButtonOption</description>
            </attribute>

            <attribute name="genericText">
                <label>genericText</label>
                <shortLabel>genericText</shortLabel>
                <maxLength>10</maxLength>
                <control>
                    <text size="10" />
                </control>
                <summary>genericText</summary>
                <description>genericText</description>
