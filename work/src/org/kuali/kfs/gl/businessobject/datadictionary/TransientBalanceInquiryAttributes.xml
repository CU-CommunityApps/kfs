<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "../dataDictionary-1_0.dtd">
<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.module.gl.bo.TransientBalanceInquiryAttributes">


        <attributes>
            <attribute name="pendingEntryOption">
                <label>Include Pending Ledger Entry</label>
                <shortLabel>Include Pending Ledger Entry</shortLabel>
                <!-- making this a small number will cause gl lookups to fail -->
                <maxLength>20</maxLength>
                <control>
                    <radio valuesFinderClass="org.kuali.module.gl.web.optionfinder.GLPendingEntryOptionFinder" />
                </control>
            </attribute>

            <attribute name="consolidationOption">
                <label>Consolidation Option</label>
                <shortLabel>Consolidation Option</shortLabel>
                <!-- making this a small number will cause gl lookups to fail -->
                <maxLength>20</maxLength>
                <control>
                    <radio valuesFinderClass="org.kuali.module.gl.web.optionfinder.GLConsolidationOptionFinder" />
                </control>
            </attribute>

            <attribute name="amountViewOption">
                <label>Amount View Option</label>
                <shortLabel>Amount View</shortLabel>
                <!-- making this a small number will cause gl lookups to fail -->
                <maxLength>20</maxLength>
                <control>
                    <radio valuesFinderClass="org.kuali.module.gl.web.optionfinder.GLAmountViewOptionFinder" />
                </control>
            </attribute>

            <attribute name="costShareOption">
                <label>Include Cost Share Sub-Accounts</label>
                <shortLabel>Cost Share</shortLabel>
                <!-- making this a small number will cause gl lookups to fail -->
                <maxLength>20</maxLength>
                <control>
                    <radio valuesFinderClass="org.kuali.module.gl.web.optionfinder.GLCostShareOptionFinder" />
                </control>
            </attribute>

            <attribute name="linkButtonOption">
                <label>linkButtonOption</label>
                <shortLabel>linkButtonOption</shortLabel>
                <!-- making this a small number will cause gl lookups to fail -->
                <maxLength>20</maxLength>
                <control>
                    <text size="2" />
                </control>
            </attribute>

            <attribute name="genericText">
                <label>genericText</label>
                <shortLabel>genericText</shortLabel>
                <maxLength>10</maxLength>
                <control>
                    <text size="10" />
                </control>
            </attribute>
        </attributes>
    </businessObject>
</dictionaryEntry>
