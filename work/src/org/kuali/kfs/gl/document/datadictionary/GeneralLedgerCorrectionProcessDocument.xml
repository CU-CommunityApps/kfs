<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="GeneralLedgerCorrectionProcessDocument" parent="GeneralLedgerCorrectionProcessDocument-parentBean" />

  <bean id="GeneralLedgerCorrectionProcessDocument-parentBean" abstract="true" parent="TransactionalDocumentEntry">
    <property name="label" value="General Ledger Correction Process" />
    <property name="shortLabel" value="GLCPDoc" />
    <property name="summary" value="General Ledger Correction Process Document" />
    <property name="description" value="General Ledger Correction Process Document" />
    <property name="helpDefinition" >
      <bean parent="HelpDefinition" p:parameterClass="org.kuali.kfs.gl.document.GeneralLedgerCorrectionProcessDocument" p:parameterName="GENERAL_LEDGER_CORRECTION_PROCESS" />
    </property>
    <property name="documentTypeName" value="GeneralLedgerCorrectionProcessDocument" />
    <property name="documentClass" value="org.kuali.kfs.gl.document.GeneralLedgerCorrectionProcessDocument" />
    <property name="documentAuthorizerClass" value="org.kuali.kfs.gl.document.authorization.CorrectionDocumentAuthorizer" />
    <property name="documentTypeCode" value="GLCP" />
   
    <property name="authorizations" >
      <list>
        <bean parent="AuthorizationDefinition">
          <property name="action" value="initiate" />
          <property name="authorizedGroups" >
            <set>
              <value>FP_GENERAL_LEDGER_CORRECTION_PROCESS_USERS</value>
            </set>
          </property>
        </bean>
      </list>
    </property>
  </bean>


</beans>
