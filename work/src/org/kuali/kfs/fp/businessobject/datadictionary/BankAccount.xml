<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="BankAccount" parent="BankAccount-parentBean" />

  <bean id="BankAccount-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.module.financial.bo.BankAccount" />
    <property name="inquiryDefinition" >
      <ref bean="BankAccount-inquiryDefinition" />
    </property>
    <property name="lookupDefinition" >
      <ref bean="BankAccount-lookupDefinition" />
    </property>
    <property name="titleAttribute" value="finDocumentBankAccountNumber" />
    <property name="objectLabel" value="Bank Account" />
    <property name="attributes" >
      <list>
        <ref bean="BankAccount-financialDocumentBankCode" />
        <ref bean="BankAccount-finDocumentBankAccountNumber" />
        <ref bean="BankAccount-finDocumentBankAccountDesc" />
        <ref bean="BankAccount-cashOffsetFinancialChartOfAccountCode" />
        <ref bean="BankAccount-cashOffsetAccountNumber" />
        <ref bean="BankAccount-cashOffsetSubAccountNumber" />
        <ref bean="BankAccount-cashOffsetObjectCode" />
        <ref bean="BankAccount-cashOffsetSubObjectCode" />
        <ref bean="BankAccount-versionNumber" />
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->


  <bean id="BankAccount-financialDocumentBankCode" parent="BankAccount-financialDocumentBankCode-parentBean" />

  <bean id="BankAccount-financialDocumentBankCode-parentBean" abstract="true" parent="Bank-financialDocumentBankCode">
  </bean>
  <bean id="BankAccount-finDocumentBankAccountNumber" parent="BankAccount-finDocumentBankAccountNumber-parentBean" />

  <bean id="BankAccount-finDocumentBankAccountNumber-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="finDocumentBankAccountNumber" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Bank Account Number" />
    <property name="shortLabel" value="Bank Acct" />
    <property name="maxLength" value="15" />
    <property name="required" value="true" />
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="15" />
    </property>
    <property name="displayMask" >
      <bean parent="Mask">
        <property name="maskFormatter" >
          <bean parent="MaskFormatterLiteral"
                p:literal="***************" />
        </property>
      </bean>
    </property>
    <property name="displayWorkgroup" value="kualiUniversalGroup" />
  </bean>
  <bean id="BankAccount-finDocumentBankAccountDesc" parent="BankAccount-finDocumentBankAccountDesc-parentBean" />

  <bean id="BankAccount-finDocumentBankAccountDesc-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="finDocumentBankAccountDesc" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Bank Account Description" />
    <property name="shortLabel" value="Bank Acct Desc" />
    <property name="maxLength" value="40" />
    <property name="required" value="true" />
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="40" />
    </property>
  </bean>
  <bean id="BankAccount-cashOffsetFinancialChartOfAccountCode" parent="BankAccount-cashOffsetFinancialChartOfAccountCode-parentBean" />

  <bean id="BankAccount-cashOffsetFinancialChartOfAccountCode-parentBean" abstract="true" parent="Chart-chartOfAccountsCode">
    <property name="name" value="cashOffsetFinancialChartOfAccountCode" />
    <property name="label" value="Cash Offset Chart Of Accounts" />
    <property name="shortLabel" value="Cash Off CoA" />
    <property name="required" value="false" />
  </bean>
  <bean id="BankAccount-cashOffsetAccountNumber" parent="BankAccount-cashOffsetAccountNumber-parentBean" />

  <bean id="BankAccount-cashOffsetAccountNumber-parentBean" abstract="true" parent="Account-accountNumber">
    <property name="name" value="cashOffsetAccountNumber" />
    <property name="label" value="Cash Offset Account Number" />
    <property name="shortLabel" value="Cash Off Acct" />
    <property name="required" value="false" />
  </bean>
  <bean id="BankAccount-cashOffsetSubAccountNumber" parent="BankAccount-cashOffsetSubAccountNumber-parentBean" />

  <bean id="BankAccount-cashOffsetSubAccountNumber-parentBean" abstract="true" parent="SubAccount-subAccountNumber">
    <property name="name" value="cashOffsetSubAccountNumber" />
    <property name="label" value="Cash Offset Sub-Account Number" />
    <property name="shortLabel" value="Cash Off Sub-Acct Nbr" />
    <property name="required" value="false" />
  </bean>
  <bean id="BankAccount-cashOffsetObjectCode" parent="BankAccount-cashOffsetObjectCode-parentBean" />

  <bean id="BankAccount-cashOffsetObjectCode-parentBean" abstract="true" parent="ObjectCode-financialObjectCode">
    <property name="name" value="cashOffsetObjectCode" />
    <property name="label" value="Cash Offset Object Code" />
    <property name="shortLabel" value="Cash Off Obj Cd" />
    <property name="required" value="false" />
  </bean>
  <bean id="BankAccount-cashOffsetSubObjectCode" parent="BankAccount-cashOffsetSubObjectCode-parentBean" />

  <bean id="BankAccount-cashOffsetSubObjectCode-parentBean" abstract="true" parent="SubObjCd-financialSubObjectCode">
    <property name="name" value="cashOffsetSubObjectCode" />
    <property name="label" value="Cash Offset Sub-Object Code" />
    <property name="shortLabel" value="Cash Off Sub-Obj Cd" />
    <property name="required" value="false" />
  </bean>
  <bean id="BankAccount-versionNumber" parent="BankAccount-versionNumber-parentBean" />

  <bean id="BankAccount-versionNumber-parentBean" abstract="true" parent="GenericAttributes-versionNumber">
  </bean>

<!-- Business Object Inquiry Definition -->


  <bean id="BankAccount-inquiryDefinition" parent="BankAccount-inquiryDefinition-parentBean" />

  <bean id="BankAccount-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
    <property name="title" value="Bank Account Inquiry" />
    <property name="inquirySections" >
      <list>
        <bean parent="InquirySectionDefinition">
          <property name="title" value="Header Text" />
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields" >
            <list>
              <bean parent="FieldDefinition"
                    p:attributeName="financialDocumentBankCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="finDocumentBankAccountNumber" />
              <bean parent="FieldDefinition"
                    p:attributeName="finDocumentBankAccountDesc" />
              <bean parent="FieldDefinition"
                    p:attributeName="cashOffsetFinancialChartOfAccountCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="cashOffsetAccountNumber" />
              <bean parent="FieldDefinition"
                    p:attributeName="cashOffsetSubAccountNumber" />
              <bean parent="FieldDefinition"
                    p:attributeName="cashOffsetObjectCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="cashOffsetSubObjectCode" />
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Business Object Lookup Definition -->


  <bean id="BankAccount-lookupDefinition" parent="BankAccount-lookupDefinition-parentBean" />

  <bean id="BankAccount-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
    <property name="title" value="Bank Account Lookup" />
    <property name="instructions" value="Lookup a Bank Account" />
    <property name="defaultSort" >
      <bean parent="SortDefinition">
        <property name="attributeNames" >
          <list>
            <value>financialDocumentBankCode</value>
          </list>
        </property>
      </bean>
    </property>
    <property name="lookupFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="financialDocumentBankCode" />
        <bean parent="FieldDefinition"
              p:attributeName="finDocumentBankAccountNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="finDocumentBankAccountDesc" />
        <bean parent="FieldDefinition"
              p:attributeName="cashOffsetFinancialChartOfAccountCode" />
        <bean parent="FieldDefinition"
              p:attributeName="cashOffsetAccountNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="cashOffsetSubAccountNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="cashOffsetObjectCode" />
        <bean parent="FieldDefinition"
              p:attributeName="cashOffsetSubObjectCode" />
      </list>
    </property>
    <property name="resultFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="financialDocumentBankCode" />
        <bean parent="FieldDefinition"
              p:attributeName="finDocumentBankAccountNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="finDocumentBankAccountDesc" />
        <bean parent="FieldDefinition"
              p:attributeName="cashOffsetFinancialChartOfAccountCode" />
        <bean parent="FieldDefinition"
              p:attributeName="cashOffsetAccountNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="cashOffsetSubAccountNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="cashOffsetObjectCode" />
        <bean parent="FieldDefinition"
              p:attributeName="cashOffsetSubObjectCode" />
      </list>
    </property>
  </bean>
</beans>
