<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "../dataDictionary-1_0.dtd">
<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.module.financial.bo.BudgetAdjustmentTargetAccountingLine">
        <customAttributeEnabled>false</customAttributeEnabled>

        <attributes>

            <attribute name="budgetAdjustmentPeriodCode" forceUppercase="true">
                <label>Budget Adjustment Period Code</label>
                <shortLabel>Period Code</shortLabel>

                <maxLength>2</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="4" />
                </control>

                <summary>budgetAdjustmentPeriodCode</summary>
                <description>budgetAdjustmentPeriodCode description...</description>
			</attribute>

            <attribute name="currentBudgetAdjustmentAmount" forceUppercase="true">
                <label>Current Budget Adjustment Amount</label>
                <shortLabel>Current Amount</shortLabel>

                <maxLength>19</maxLength>
                <validationPattern><floatingPoint /></validationPattern>

                <control>
                    <text size="21" />
                </control>

                <summary>currentBudgetAdjustmentAmount</summary>
                <description>currentBudgetAdjustmentAmount description...</description>
			</attribute>

            <attribute name="baseBudgetAdjustmentAmount" forceUppercase="true">
                <label>Base Budget Adjustment Amount</label>
                <shortLabel>Base Amount</shortLabel>

                <maxLength>19</maxLength>
                <validationPattern><floatingPoint /></validationPattern>

                <control>
                    <text size="21" />
                </control>

                <summary>baseBudgetAdjustmentAmount</summary>
                <description>baseBudgetAdjustmentAmount description...</description>
			</attribute>

            <attributeReference name="financialDocumentNumber"
            					sourceClassName="org.kuali.core.document.DocumentHeader"
            					sourceAttributeName="financialDocumentNumber" >
            	<required>false</required>
            </attributeReference>
            
            <attributeReference name="sequenceNumber" 
					            sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" 
					            sourceAttributeName="genericAmount">
				<required>true</required>
			</attributeReference>

            <attributeReference name="chartOfAccountsCode"
					            sourceClassName="org.kuali.module.chart.bo.Chart"
					            sourceAttributeName="chartOfAccountsCode">
				<required>true</required>
				<control>
            		<select valuesFinderClass="org.kuali.core.lookup.keyvalues.SimpleChartValuesFinder"/>
        		</control>
			</attributeReference>

            <attributeReference name="accountNumber"
					            sourceClassName="org.kuali.module.chart.bo.Account"
					            sourceAttributeName="accountNumber">
				<required>true</required>
			</attributeReference>

            <attributeReference name="postingYear"
					            sourceClassName="org.kuali.core.bo.AttributeReferenceDummy"
					            sourceAttributeName="genericFiscalYear" >
				<required>true</required>
			</attributeReference>

            <attributeReference name="financialObjectCode"
					            sourceClassName="org.kuali.module.chart.bo.ObjectCode"
					            sourceAttributeName="financialObjectCode">
				<required>true</required>
			</attributeReference>

            <attributeReference name="subAccountNumber"
					            sourceClassName="org.kuali.module.chart.bo.SubAccount"
					            sourceAttributeName="subAccountNumber">
                <required>false</required>
            </attributeReference>
            
            <attributeReference name="financialSubObjectCode"
            					sourceClassName="org.kuali.module.chart.bo.SubObjCd"
            					sourceAttributeName="financialSubObjectCode">
            	<required>false</required>
            </attributeReference>

            <attributeReference name="projectCode"
            					sourceClassName="org.kuali.module.chart.bo.ProjectCode"
            					sourceAttributeName="code">
            	<required>false</required>
            </attributeReference>				

			<attributeReference name="overrideCode" 
					            sourceClassName="org.kuali.core.bo.SourceAccountingLine" 
					            sourceAttributeName="overrideCode" />

			<attributeReference name="organizationReferenceId" 
					            sourceClassName="org.kuali.core.bo.SourceAccountingLine" 
					            sourceAttributeName="organizationReferenceId" />

            <attributeReference name="versionNumber"
            					sourceClassName="org.kuali.core.bo.AttributeReferenceDummy"
            					sourceAttributeName="versionNumber" >
            	<required>false</required>
            </attributeReference>

        </attributes>
    </businessObject>
</dictionaryEntry>
