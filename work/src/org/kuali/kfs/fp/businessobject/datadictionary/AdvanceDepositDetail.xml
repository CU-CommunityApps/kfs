<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "../dataDictionary-1_0.dtd">
<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.module.financial.bo.AdvanceDepositDetail">


        <attributes>
            <attributeReference name="documentNumber" sourceClassName="org.kuali.core.bo.DocumentHeader" sourceAttributeName="documentNumber" />
            <attributeReference name="financialDocumentTypeCode" sourceClassName="org.kuali.core.bo.DocumentType" sourceAttributeName="financialDocumentTypeCode" />
            <attribute name="financialDocumentColumnTypeCode" forceUppercase="true">
                <label>Column Type Code</label>
                <shortLabel>Column Type Code</shortLabel>

                <maxLength>1</maxLength>

                <validationPattern>
                    <alpha />
                </validationPattern>
                <required>true</required>
                <control>
                    <text size="3" />
                </control>

                <summary>
                    <![CDATA[&nbsp;]]>
                </summary>
                <description>
                    <![CDATA[&nbsp;]]>
                </description>
            </attribute>
            <attributeReference name="financialDocumentLineNumber" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="genericAmount">
                <required>true</required>
            </attributeReference>
            <attributeReference name="financialDocumentAdvanceDepositAmount" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="genericAmount">
                <required>true</required>
                <description>The total dollar amount of the advance deposit.</description>
            </attributeReference>
            <attributeReference name="financialDocumentAdvanceDepositDate" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="genericDate">
                <label>Date</label>
                <shortLabel>Date</shortLabel>
                <required>true</required>
            </attributeReference>
            <attribute name="financialDocumentAdvanceDepositDescription" forceUppercase="true">
                <label>Description</label>
                <shortLabel>Description</shortLabel>

                <maxLength>40</maxLength>
                <validationPattern>
                    <anyCharacter allowWhitespace="true" />
                </validationPattern>

                <control>
                    <text size="42" />
                </control>

                <summary>
                    <![CDATA[&nbsp;]]>
                </summary>
                <description>A brief description of the transaction. This field is for departmental use only, and does not appear on financial reports.</description>
            </attribute>
            <attribute name="financialDocumentAdvanceDepositReferenceNumber" forceUppercase="true">
                <label>Reference Number</label>
                <shortLabel>Reference Number</shortLabel>
                <maxLength>10</maxLength>
                <required>true</required>              
                <control>
                    <text size="16" />
                </control>               
                <summary>
                	<![CDATA[&nbsp;]]>
                </summary>
                <description>The Reference Number is used to record the bank's unique transaction number for the deposit.</description>              
            </attribute>
			 <attributeReference name="financialDocumentBankCode" sourceClassName="org.kuali.module.financial.bo.Bank" sourceAttributeName="financialDocumentBankCode">
                <label>Bank</label>
                <shortLabel>Bank</shortLabel>
                <required>true</required>
                <summary>
                	<![CDATA[&nbsp;]]>
                </summary>
                <description>The code that designates the bank at which the deposit was made.</description>
            </attributeReference>
			<attributeReference name="financialDocumentBankAccountNumber" sourceClassName="org.kuali.module.financial.bo.BankAccount" sourceAttributeName="finDocumentBankAccountNumber">
                <label>Bank Account</label>
                <shortLabel>Bank Account</shortLabel>
                <required>true</required>
                <summary>
                	<![CDATA[&nbsp;]]>
                </summary>
                <description>The bank account number into which the funds were deposited.</description>
            </attributeReference>
        </attributes>
    </businessObject>
</dictionaryEntry>