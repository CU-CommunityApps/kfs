<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="FiscalYearFunctionControl" parent="FiscalYearFunctionControl-parentBean" />

  <bean id="FiscalYearFunctionControl-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.module.financial.bo.FiscalYearFunctionControl" />
    <property name="inquiryDefinition" >
      <ref bean="FiscalYearFunctionControl-inquiryDefinition" />
    </property>
    <property name="lookupDefinition" >
      <ref bean="FiscalYearFunctionControl-lookupDefinition" />
    </property>
    <property name="objectLabel" value="Fiscal Year Function Control" />
    <property name="attributes" >
      <list>
        <ref bean="FiscalYearFunctionControl-universityFiscalYear" />
        <ref bean="FiscalYearFunctionControl-financialSystemFunctionControlCode" />
        <ref bean="FiscalYearFunctionControl-financialSystemFunctionActiveIndicator" />
        <ref bean="FiscalYearFunctionControl-versionNumber" />
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->


  <bean id="FiscalYearFunctionControl-universityFiscalYear" parent="FiscalYearFunctionControl-universityFiscalYear-parentBean" />

  <bean id="FiscalYearFunctionControl-universityFiscalYear-parentBean" abstract="true" parent="GenericAttributes-genericFiscalYear">
    <property name="name" value="universityFiscalYear" />
    <property name="required" value="true" />
  </bean>
  <bean id="FiscalYearFunctionControl-financialSystemFunctionControlCode" parent="FiscalYearFunctionControl-financialSystemFunctionControlCode-parentBean" />

  <bean id="FiscalYearFunctionControl-financialSystemFunctionControlCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="financialSystemFunctionControlCode" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Financial System Function Control Code" />
    <property name="shortLabel" value="Code" />
    <property name="maxLength" value="6" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="8" />
    </property>
  </bean>
  <bean id="FiscalYearFunctionControl-financialSystemFunctionActiveIndicator" parent="FiscalYearFunctionControl-financialSystemFunctionActiveIndicator-parentBean" />

  <bean id="FiscalYearFunctionControl-financialSystemFunctionActiveIndicator-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="financialSystemFunctionActiveIndicator" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Financial System Function Active Indicator" />
    <property name="shortLabel" value="Indicator" />
    <property name="maxLength" value="1" />
    <property name="control" >
      <bean parent="CheckboxControlDefinition" />
    </property>
    <property name="formatterClass" value="org.kuali.core.web.format.BooleanFormatter" />
  </bean>
  <bean id="FiscalYearFunctionControl-versionNumber" parent="FiscalYearFunctionControl-versionNumber-parentBean" />

  <bean id="FiscalYearFunctionControl-versionNumber-parentBean" abstract="true" parent="GenericAttributes-versionNumber">
  </bean>

<!-- Business Object Inquiry Definition -->


  <bean id="FiscalYearFunctionControl-inquiryDefinition" parent="FiscalYearFunctionControl-inquiryDefinition-parentBean" />

  <bean id="FiscalYearFunctionControl-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
    <property name="title" value="Fiscal Year Function Control Inquiry" />
    <property name="inquirySections" >
      <list>
        <bean parent="InquirySectionDefinition">
          <property name="title" value="Header Text" />
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields" >
            <list>
              <bean parent="FieldDefinition"
                    p:attributeName="universityFiscalYear" />
              <bean parent="FieldDefinition"
                    p:attributeName="financialSystemFunctionControlCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="financialSystemFunctionActiveIndicator" />
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Business Object Lookup Definition -->


  <bean id="FiscalYearFunctionControl-lookupDefinition" parent="FiscalYearFunctionControl-lookupDefinition-parentBean" />

  <bean id="FiscalYearFunctionControl-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
    <property name="title" value="Fiscal Year Function Control Lookup" />
    <property name="instructions" value="Lookup an Fiscal Year Function Control" />
    <property name="defaultSort" >
      <bean parent="SortDefinition">
      </bean>
    </property>
    <property name="lookupFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="universityFiscalYear" />
        <bean parent="FieldDefinition"
              p:attributeName="financialSystemFunctionControlCode" />
        <bean parent="FieldDefinition"
              p:attributeName="financialSystemFunctionActiveIndicator" />
      </list>
    </property>
    <property name="resultFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="universityFiscalYear" />
        <bean parent="FieldDefinition"
              p:attributeName="financialSystemFunctionControlCode" />
        <bean parent="FieldDefinition"
              p:attributeName="financialSystemFunctionActiveIndicator" />
      </list>
    </property>
  </bean>
</beans>
