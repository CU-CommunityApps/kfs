<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="Bank" parent="Bank-parentBean" />

  <bean id="Bank-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.module.financial.bo.Bank" />
    <property name="inquiryDefinition" >
      <ref bean="Bank-inquiryDefinition" />
    </property>
    <property name="lookupDefinition" >
      <ref bean="Bank-lookupDefinition" />
    </property>
    <property name="titleAttribute" value="financialDocumentBankCode" />
    <property name="objectLabel" value="Bank" />
    <property name="attributes" >
      <list>
        <ref bean="Bank-financialDocumentBankCode" />
        <ref bean="Bank-financialDocumentBankName" />
        <ref bean="Bank-financialDocumentBankShortNm" />
        <ref bean="Bank-bankRoutingNumber" />
        <ref bean="Bank-versionNumber" />
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->


  <bean id="Bank-financialDocumentBankCode" parent="Bank-financialDocumentBankCode-parentBean" />

  <bean id="Bank-financialDocumentBankCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="financialDocumentBankCode" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Bank Code" />
    <property name="shortLabel" value="Bank Code" />
    <property name="maxLength" value="4" />
    <property name="validationPattern" >
      <bean parent="AlphaNumericValidationPattern" />
    </property>
    <property name="required" value="true" />
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="4" />
    </property>
  </bean>
  <bean id="Bank-financialDocumentBankName" parent="Bank-financialDocumentBankName-parentBean" />

  <bean id="Bank-financialDocumentBankName-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="financialDocumentBankName" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Bank Name" />
    <property name="shortLabel" value="Name" />
    <property name="maxLength" value="40" />
    <property name="required" value="true" />
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="40" />
    </property>
  </bean>
  <bean id="Bank-financialDocumentBankShortNm" parent="Bank-financialDocumentBankShortNm-parentBean" />

  <bean id="Bank-financialDocumentBankShortNm-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="financialDocumentBankShortNm" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Bank Short Name" />
    <property name="shortLabel" value="Short Name" />
    <property name="maxLength" value="12" />
    <property name="required" value="true" />
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="12" />
    </property>
  </bean>
  <bean id="Bank-bankRoutingNumber" parent="Bank-bankRoutingNumber-parentBean" />

  <bean id="Bank-bankRoutingNumber-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="bankRoutingNumber" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Bank Routing Number" />
    <property name="shortLabel" value="Routing Nbr" />
    <property name="maxLength" value="9" />
    <property name="validationPattern" >
      <bean parent="AlphaNumericValidationPattern" />
    </property>
    <property name="required" value="false" />
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="12" />
    </property>
  </bean>
  <bean id="Bank-versionNumber" parent="Bank-versionNumber-parentBean" />

  <bean id="Bank-versionNumber-parentBean" abstract="true" parent="GenericAttributes-versionNumber">
  </bean>

<!-- Business Object Inquiry Definition -->


  <bean id="Bank-inquiryDefinition" parent="Bank-inquiryDefinition-parentBean" />

  <bean id="Bank-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
    <property name="title" value="Bank Inquiry" />
    <property name="inquirySections" >
      <list>
        <bean parent="InquirySectionDefinition">
          <property name="title" value="Header Text" />
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields" >
            <list>
              <bean parent="FieldDefinition"
                    p:attributeName="financialDocumentBankCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="financialDocumentBankName" />
              <bean parent="FieldDefinition"
                    p:attributeName="financialDocumentBankShortNm" />
              <bean parent="FieldDefinition"
                    p:attributeName="bankRoutingNumber" />
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Business Object Lookup Definition -->


  <bean id="Bank-lookupDefinition" parent="Bank-lookupDefinition-parentBean" />

  <bean id="Bank-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
    <property name="title" value="Bank Lookup" />
    <property name="instructions" value="Lookup a Bank Account" />
    <property name="defaultSort" >
      <bean parent="SortDefinition">
        <property name="attributeNames" >
          <list>
            <value>financialDocumentBankCode</value>
          </list>
        </property>
      </bean>
    </property>
    <property name="lookupFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="financialDocumentBankCode" />
        <bean parent="FieldDefinition"
              p:attributeName="financialDocumentBankName" />
        <bean parent="FieldDefinition"
              p:attributeName="financialDocumentBankShortNm" />
        <bean parent="FieldDefinition"
              p:attributeName="bankRoutingNumber" />
      </list>
    </property>
    <property name="resultFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="financialDocumentBankCode"
              p:forceInquiry="true" />
        <bean parent="FieldDefinition"
              p:attributeName="financialDocumentBankName" />
        <bean parent="FieldDefinition"
              p:attributeName="financialDocumentBankShortNm" />
        <bean parent="FieldDefinition"
              p:attributeName="bankRoutingNumber" />
      </list>
    </property>
  </bean>
</beans>
