					            sourceClassName="org.kuali.module.chart.bo.Chart"
					            sourceAttributeName="chartOfAccountsCode">
				<required>true</required>
				<control>
            		<select valuesFinderClass="org.kuali.core.lookup.keyvalues.SimpleChartValuesFinder"/>
        		</control>
			</attributeReference>

            <attributeReference name="postingYear"
					            sourceClassName="org.kuali.core.bo.AttributeReferenceDummy"
