<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="OwnershipTypeCode" parent="OwnershipTypeCode-parentBean" />

  <bean id="OwnershipTypeCode-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.kfs.fp.businessobject.OwnershipTypeCode" />
    <property name="inquiryDefinition" >
      <ref bean="OwnershipTypeCode-inquiryDefinition" />
    </property>
    <property name="lookupDefinition" >
      <ref bean="OwnershipTypeCode-lookupDefinition" />
    </property>
    <property name="titleAttribute" value="code" />
    <property name="objectLabel" value="Disbursement Voucher Ownership Type" />
    <property name="attributes" >
      <list>
        <ref bean="OwnershipTypeCode-code" />
        <ref bean="OwnershipTypeCode-name" />
        <ref bean="OwnershipTypeCode-active" />
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->


  <bean id="OwnershipTypeCode-code" parent="OwnershipTypeCode-code-parentBean" />

  <bean id="OwnershipTypeCode-code-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="code" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Ownership Type Code" />
    <property name="shortLabel" value="Type Code" />
    <property name="maxLength" value="1" />
    <property name="validationPattern" >
      <bean parent="AlphaNumericValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="1" />
    </property>
  </bean>
  <bean id="OwnershipTypeCode-name" parent="OwnershipTypeCode-name-parentBean" />

  <bean id="OwnershipTypeCode-name-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="name" />
    <property name="forceUppercase" value="false" />
    <property name="label" value="Ownership Type Name" />
    <property name="shortLabel" value="Name" />
    <property name="maxLength" value="25" />
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="25" />
    </property>
  </bean>
  <bean id="OwnershipTypeCode-active" parent="OwnershipTypeCode-active-parentBean" />

  <bean id="OwnershipTypeCode-active-parentBean" abstract="true" parent="GenericAttributes-activeIndicator">
    <property name="name" value="active" />
  </bean>


<!-- Business Object Inquiry Definition -->


  <bean id="OwnershipTypeCode-inquiryDefinition" parent="OwnershipTypeCode-inquiryDefinition-parentBean" />

  <bean id="OwnershipTypeCode-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
    <property name="title" value="Disbursement Voucher Ownership Type Inquiry" />
    <property name="inquirySections" >
      <list>
        <bean parent="InquirySectionDefinition">
          <property name="title" value="" />
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields" >
            <list>
              <bean parent="FieldDefinition"
                    p:attributeName="code" />
              <bean parent="FieldDefinition"
                    p:attributeName="name" />
              <bean parent="FieldDefinition"
                    p:attributeName="active" />
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Business Object Lookup Definition -->


  <bean id="OwnershipTypeCode-lookupDefinition" parent="OwnershipTypeCode-lookupDefinition-parentBean" />

  <bean id="OwnershipTypeCode-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
    <property name="title" value="Disbursement Voucher Ownership Type Lookup" />
    <property name="instructions" value="Lookup an Ownership Type Code" />
    <property name="defaultSort" >
      <bean parent="SortDefinition">
        <property name="attributeNames" >
          <list>
            <value>code</value>
          </list>
        </property>
      </bean>
    </property>
    <property name="lookupFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="code" />
        <bean parent="FieldDefinition"
              p:attributeName="name" />
        <bean parent="FieldDefinition"
              p:attributeName="active" />
      </list>
    </property>
    <property name="resultFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="code" />
        <bean parent="FieldDefinition"
              p:attributeName="name" />
        <bean parent="FieldDefinition"
              p:attributeName="active" />
      </list>
    </property>
  </bean>
</beans>
