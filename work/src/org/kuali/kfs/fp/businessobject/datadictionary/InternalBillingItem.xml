<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "../../../core/datadictionary/dataDictionary-1_0.dtd">
<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.module.financial.bo.InternalBillingItem">
        <customAttributeEnabled>false</customAttributeEnabled>

        <attributes>
            <attributeReference name="itemUnitAmount" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="genericAmount">
                <label>Item Unit Amount</label>
                <shortLabel>Item Cost</shortLabel>
                <exclusiveMin>0</exclusiveMin>
                <required>true</required>
                <summary><![CDATA[&nbsp;]]></summary>
                <description>itemUnitAmount description...</description>
            </attributeReference>

            <attribute name="itemStockDescription" forceUppercase="true">
                <label>Item Stock Description</label>
                <shortLabel>Description</shortLabel>

                <maxLength>40</maxLength>

                <control>
                    <text size="30" />
                </control>

                <summary><![CDATA[&nbsp;]]></summary>
                <description>The description of the goods or services.</description>
            </attribute>

            <attributeReference name="itemServiceDate" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="genericTimestamp">
                <label>Item Service Date</label>
                <shortLabel>Date</shortLabel>
                <summary><![CDATA[&nbsp;]]></summary>
                <description>itemServiceDate description...</description>
            </attributeReference>

            <attributeReference name="total" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="genericAmount">
                <label>Total</label>
                <shortLabel>Total</shortLabel>
                <summary><![CDATA[&nbsp;]]></summary>
                <description>total description...</description>
            </attributeReference>

            <attribute name="itemStockNumber" forceUppercase="true">
                <label>Item Stock Number</label>
                <shortLabel>Stock#</shortLabel>

                <maxLength>9</maxLength>

                <control>
                    <text size="11" />
                </control>

                <summary><![CDATA[&nbsp;]]></summary>
                <description>The stock number of the goods or services.</description>
            </attribute>

            <attributeReference name="objectId" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="objectId" />

            <attributeReference name="itemSequenceId" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="transactionEntrySequenceId" />

            <attribute name="unitOfMeasureCode" forceUppercase="true">
                <label>Unit Of Measure Code</label>
                <shortLabel>UoM</shortLabel>

                <maxLength>2</maxLength>

                <control>
                    <text size="2" />
                </control>

                <summary><![CDATA[&nbsp;]]></summary>
                <description>The unit of measure of the goods or services.</description>
            </attribute>

            <attribute name="itemQuantity" forceUppercase="true">
                <label>Item Quantity</label>
                <shortLabel>Quantity</shortLabel>

                <maxLength>22</maxLength>
                <exclusiveMin>0</exclusiveMin>
                <validationPattern>
                    <numeric />
                </validationPattern>
                <required>true</required>

                <control>
                    <text size="10" />
                </control>

                <summary><![CDATA[&nbsp;]]></summary>
                <description>The quantity of goods or services.</description>
            </attribute>

            <attributeReference name="versionNumber" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="versionNumber" />

            <attributeReference name="documentNumber" sourceClassName="org.kuali.core.bo.DocumentHeader" sourceAttributeName="documentNumber" />
        </attributes>

    </businessObject>
</dictionaryEntry>
