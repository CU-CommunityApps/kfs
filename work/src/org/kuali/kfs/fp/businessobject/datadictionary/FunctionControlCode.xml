<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "../dataDictionary-1_0.dtd">
<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.module.financial.bo.FunctionControlCode">
        <customAttributeEnabled>false</customAttributeEnabled>

        <inquiry>
            <title>Function Control Code Inquiry</title>

            <inquiryFields>
                <field attributeName="financialSystemFunctionControlCode" />
                <field attributeName="financialSystemFunctionDescription" />
                <field attributeName="financialSystemFunctionDefaultIndicator" />
            </inquiryFields>
        </inquiry>

        <lookup>
            <title>Function Control Code Lookup</title>

            <menubar>
                <![CDATA[<a href="maintenance.do?methodToCall=start&businessObjectClassName=org.kuali.module.financial.bo.FunctionControlCode"><img src="images/tinybutton-createnew.gif" alt="create new" width="70" height="15"/></a>]]>
            </menubar>

            <instructions>Lookup an Function Control Code</instructions>

            <defaultSort sortAscending="true">
                <sortAttributes>
                    <sortAttribute attributeName="financialSystemFunctionControlCode" />
                </sortAttributes>
            </defaultSort>

            <lookupFields>
                <lookupField attributeName="financialSystemFunctionControlCode" required="false" />
                <lookupField attributeName="financialSystemFunctionDescription" required="false" />
                <lookupField attributeName="financialSystemFunctionDefaultIndicator" required="false" />
            </lookupFields>

            <resultFields>
                <field attributeName="financialSystemFunctionControlCode" />
                <field attributeName="financialSystemFunctionDescription" />
                <field attributeName="financialSystemFunctionDefaultIndicator" />
            </resultFields>
        </lookup>
        
       <titleAttribute>financialSystemFunctionControlCode</titleAttribute>
       
        <attributes>

            <attribute name="financialSystemFunctionControlCode" forceUppercase="true">
                <label>Financial System Function Control Code</label>
                <shortLabel>Code</shortLabel>

                <maxLength>6</maxLength>
                <validationPattern>
                    <anyCharacter />
                </validationPattern>

                <control>
                    <text size="8" />
                </control>

                <summary><![CDATA[&nbsp;]]></summary>
                <description>Any of various indicators that control Budget Construction.</description>
            </attribute>

            <attribute name="financialSystemFunctionDescription" forceUppercase="true">
                <label>Financial System Function Description</label>
                <shortLabel>Description</shortLabel>

                <maxLength>40</maxLength>
                <validationPattern>
                    <anyCharacter allowWhitespace="true" />
                </validationPattern>

                <control>
                    <text size="42" />
                </control>

                <summary><![CDATA[&nbsp;]]></summary>
                <description>The functional description for the Yes/No indicator</description>
            </attribute>

            <attribute name="financialSystemFunctionDefaultIndicator" forceUppercase="true">
                <label>Financial System Function Default Indicator</label>
                <shortLabel>Indicator</shortLabel>

                <maxLength>$attribute.maxLength</maxLength>

                <control>
                    <checkbox />
                </control>

                <summary><![CDATA[&nbsp;]]></summary>
                <description>The Yes/No indicator of the default value when value sfor a new Fiscal Year are generated.</description>
            </attribute>

            <attributeReference name="versionNumber" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="versionNumber" />

        </attributes>
    </businessObject>
</dictionaryEntry>
