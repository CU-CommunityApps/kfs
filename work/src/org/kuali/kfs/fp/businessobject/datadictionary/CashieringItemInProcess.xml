<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "../../../core/datadictionary/dataDictionary-1_0.dtd">
<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.module.financial.bo.CashieringItemInProcess">


        <inquiry>
            <title>Cashiering Items In Process</title>

            <inquirySections><inquirySection title="Item In Process"><inquiryFields>
                <field attributeName="workgroupName" />
                <field attributeName="itemIdentifier" />
                <field attributeName="itemAmount" />
                <field attributeName="itemReducedAmount" />
                <field attributeName="itemRemainingAmount" />
                <field attributeName="itemOpenDate" />
                <field attributeName="itemClosedDate" />
                <field attributeName="itemDescription" />
            </inquiryFields></inquirySection></inquirySections>
        </inquiry>

        <lookup>
            <title>Cashiering Items In Process Lookup</title>

            <instructions>Look up Cashiering Items In Process</instructions>

            <defaultSort sortAscending="true">
                <sortAttributes>
                    <sortAttribute attributeName="workgroupName" />
                    <sortAttribute attributeName="itemIdentifier" />
                </sortAttributes>
            </defaultSort>

            <lookupFields>
                <lookupField attributeName="workgroupName" required="false" />
                <lookupField attributeName="itemIdentifier" required="false" />
                <lookupField attributeName="itemAmount" required="false" />
                <lookupField attributeName="itemRemainingAmount" required="false" />
                <lookupField attributeName="itemOpenDate" required="false" />
                <lookupField attributeName="itemClosedDate" required="false" />
                <lookupField attributeName="itemDescription" required="false" />
            </lookupFields>

            <resultFields>
                <field attributeName="workgroupName" />
                <field attributeName="itemIdentifier" />
                <field attributeName="itemAmount" />
                <field attributeName="itemRemainingAmount" />
                <field attributeName="itemOpenDate" />
                <field attributeName="itemClosedDate" />
                <field attributeName="itemDescription" />
            </resultFields>
        </lookup>

        <attributes>
			<attribute name="workgroupName" forceUppercase="true">
                <label>Workgroup Name</label>
                <shortLabel>Name</shortLabel>
                
                <maxLength>70</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="72" />
                </control>
            </attribute>

            <attribute name="itemIdentifier" forceUppercase="true">
                <label>Item Identifier</label>
                <shortLabel>Identifier</shortLabel>

                <maxLength>9</maxLength>
                <validationPattern><numeric /></validationPattern>

                <control>
                    <text size="11" />
                </control>
			</attribute>

            <attribute name="itemAmount" forceUppercase="true">
                <label>Item Amount</label>
                <shortLabel>Amount</shortLabel>

                <maxLength>19</maxLength>
                <validationPattern><floatingPoint /></validationPattern>

                 <control>
                    <text size="21" />
                </control>
            </attribute>

            <attribute name="itemReducedAmount" forceUppercase="true">
                <label>Item Reduced Amount</label>
                <shortLabel>Amount</shortLabel>

                <maxLength>19</maxLength>
                <validationPattern><floatingPoint /></validationPattern>

                 <control>
                    <text size="21" />
                </control>
            </attribute>

            <attribute name="itemRemainingAmount" forceUppercase="true">
                <label>Item Remaining Amount</label>
                <shortLabel>Amount</shortLabel>

                <maxLength>19</maxLength>
                <validationPattern><floatingPoint /></validationPattern>

                 <control>
                    <text size="21" />
                </control>
            </attribute>

            <attributeReference name="itemOpenDate" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="genericTimestamp">
                <label>Item Open Date</label>
                <shortLabel>Date</shortLabel>

                <control>
                    <text size="9" />
                </control>
			</attributeReference>

            <attributeReference name="itemClosedDate" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="genericTimestamp">
                <label>Item Closed Date</label>
                <shortLabel>Date</shortLabel>

                <control>
                    <text size="9" />
                </control>
			</attributeReference>

			<attribute name="itemDescription" forceUppercase="true">
                <label>Item Description</label>
                <shortLabel>Description</shortLabel>
                
                <maxLength>200</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="80" />
                </control>
            </attribute>
            
            <attribute name="currentPayment">
            	<label>Current Payment</label>
                <shortLabel>Amount</shortLabel>

                <maxLength>19</maxLength>
                <validationPattern><floatingPoint /></validationPattern>

                 <control>
                    <text size="6" />
                </control>
            </attribute>

            <attributeReference name="versionNumber" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="versionNumber" />

        </attributes>
    </businessObject>
</dictionaryEntry>
