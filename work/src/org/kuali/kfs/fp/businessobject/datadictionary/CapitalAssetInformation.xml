<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xmlns:dd="http://rice.kuali.org/dd"
    xsi:schemaLocation="
http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd
http://rice.kuali.org/dd http://rice.kuali.org/dd/dd.xsd">

  <bean id="CapitalAssetInformation" parent="CapitalAssetInformation-parentBean" />

  <bean id="CapitalAssetInformation-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.kfs.fp.businessobject.CapitalAssetInformation" />
    <property name="objectLabel" value="Capital Asset Information" />
    <property name="attributes" >
      <list>
        <ref bean="CapitalAssetInformation-documentNumber" />
        <ref bean="CapitalAssetInformation-campusCode" />
 	    <ref bean="CapitalAssetInformation-buildingCode" />
    	<ref bean="CapitalAssetInformation-buildingRoomNumber" />
    	<ref bean="CapitalAssetInformation-buildingSubRoomNumber" />
    	<ref bean="CapitalAssetInformation-vendorHeaderGeneratedIdentifier" />
    	<ref bean="CapitalAssetInformation-vendorDetailAssignedIdentifier" />
    	<ref bean="CapitalAssetInformation-vendorNumber" />
    	<ref bean="CapitalAssetInformation-capitalAssetNumber" />
    	<ref bean="CapitalAssetInformation-capitalAssetTagNumber" />
    	<ref bean="CapitalAssetInformation-capitalAssetQuantity" />
    	<ref bean="CapitalAssetInformation-capitalAssetTypeCode" />
    	<ref bean="CapitalAssetInformation-capitalAssetManufacturerName" />
    	<ref bean="CapitalAssetInformation-capitalAssetDescription" />
    	<ref bean="CapitalAssetInformation-capitalAssetManufacturerModelNumber" />
    	<ref bean="CapitalAssetInformation-capitalAssetSerialNumber" />
      </list>
    </property>
    <property name="relationships" >
      <list>
        <dd:relationship objectAttribute="capitalAssetManagementAsset">
            <dd:primitiveAttribute source="capitalAssetNumber" target="capitalAssetNumber" />
        </dd:relationship>
        <dd:relationship objectAttribute="capitalAssetManagementAssetType">
            <dd:primitiveAttribute source="capitalAssetTypeCode" target="capitalAssetTypeCode" />
        </dd:relationship>        
      </list>
    </property>
  </bean>
  
  <!-- Attributes -->
  
  <bean id="CapitalAssetInformation-documentNumber" parent="CapitalAssetInformation-documentNumber-parentBean" />
  
  <bean id="CapitalAssetInformation-documentNumber-parentBean" abstract="true" parent="DocumentHeader-documentNumber" />
  
  <bean id="CapitalAssetInformation-campusCode" parent="CapitalAssetInformation-campusCode-parentBean" />
  
  <bean id="CapitalAssetInformation-campusCode-parentBean" abstract="true" parent="Room-campusCode">
  	<property name="name" value="campusCode"/>
  	<property name="required" value="false" />
  </bean>
  
  <bean id="CapitalAssetInformation-buildingCode" parent="CapitalAssetInformation-buildingCode-parentBean" />
  
  <bean id="CapitalAssetInformation-buildingCode-parentBean" abstract="true" parent="Room-buildingCode">
   	<property name="name" value="buildingCode"/>
  	<property name="required" value="false" />
  </bean>
  
  <bean id="CapitalAssetInformation-buildingRoomNumber" parent="CapitalAssetInformation-buildingRoomNumber-parentBean" />
  
  <bean id="CapitalAssetInformation-buildingRoomNumber-parentBean" abstract="true" parent="Room-buildingRoomNumber">
	<property name="name" value="buildingRoomNumber"/>
	<property name="required" value="false" />
  </bean>
  
  <bean id="CapitalAssetInformation-buildingSubRoomNumber" parent="CapitalAssetInformation-buildingSubRoomNumber-parentBean" />
  
  <bean id="CapitalAssetInformation-buildingSubRoomNumber-parentBean" abstract="true" parent="ExternalizableAttributeDefinitionProxy"
  	p:sourceExternalizableBusinessObjectInterface="org.kuali.kfs.integration.cam.CapitalAssetManagementAsset"
	p:sourceAttributeName="buildingSubRoomNumber">
	<property name="name" value="buildingSubRoomNumber"/>
  </bean> 
  
  <bean id="CapitalAssetInformation-vendorHeaderGeneratedIdentifier" parent="CapitalAssetInformation-vendorHeaderGeneratedIdentifier-parentBean" />
  
  <bean id="CapitalAssetInformation-vendorHeaderGeneratedIdentifier-parentBean" abstract="true" parent="VendorDetail-vendorHeaderGeneratedIdentifier"/>
  
  <bean id="CapitalAssetInformation-vendorDetailAssignedIdentifier" parent="CapitalAssetInformation-vendorDetailAssignedIdentifier-parentBean"/>
  
  <bean id="CapitalAssetInformation-vendorDetailAssignedIdentifier-parentBean" abstract="true" parent="VendorDetail-vendorDetailAssignedIdentifier"/>
  
  <bean id="CapitalAssetInformation-vendorNumber" parent="CapitalAssetInformation-vendorNumber-parentBean"/>
  
  <bean id="CapitalAssetInformation-vendorNumber-parentBean" abstract="true" parent="VendorDetail-vendorNumber"/>  
  
  <bean id="CapitalAssetInformation-capitalAssetNumber" parent="CapitalAssetInformation-capitalAssetNumber-parentBean">
     <property name="shortLabel" value="Vendor Number" />
    <property name="label" value="Vendor Number" />
    <property name="summary" value="Vendor Number" />
    <property name="description" value="Vendor Number" />
  </bean>
  
  <bean id="CapitalAssetInformation-capitalAssetNumber-parentBean" abstract="true" parent="ExternalizableAttributeDefinitionProxy"
  	p:sourceExternalizableBusinessObjectInterface="org.kuali.kfs.integration.cam.CapitalAssetManagementAsset"
	p:sourceAttributeName="capitalAssetNumber">
	<property name="name" value="capitalAssetNumber"/>
  </bean> 
  
  <bean id="CapitalAssetInformation-capitalAssetTagNumber" parent="CapitalAssetInformation-capitalAssetTagNumber-parentBean" />
  
  <bean id="CapitalAssetInformation-capitalAssetTagNumber-parentBean" abstract="true" parent="ExternalizableAttributeDefinitionProxy"
  	p:sourceExternalizableBusinessObjectInterface="org.kuali.kfs.integration.cam.CapitalAssetManagementAsset"
	p:sourceAttributeName="campusTagNumber">
  	<property name="name" value="capitalAssetTagNumber" />
  </bean>
  
  <bean id="CapitalAssetInformation-capitalAssetQuantity" parent="CapitalAssetInformation-capitalAssetQuantity-parentBean" />
  
  <bean id="CapitalAssetInformation-capitalAssetQuantity-parentBean" abstract="true" parent="ExternalizableAttributeDefinitionProxy"
  	p:sourceExternalizableBusinessObjectInterface="org.kuali.kfs.integration.cam.CapitalAssetManagementAsset"
	p:sourceAttributeName="quantity">
      <property name="name" value="capitalAssetQuantity" />
  </bean>
  
  <bean id="CapitalAssetInformation-capitalAssetTypeCode" parent="CapitalAssetInformation-capitalAssetTypeCode-parentBean" />
  
  <bean id="CapitalAssetInformation-capitalAssetTypeCode-parentBean" abstract="true" parent="ExternalizableAttributeDefinitionProxy"
  	p:sourceExternalizableBusinessObjectInterface="org.kuali.kfs.integration.cam.CapitalAssetManagementAsset"
	p:sourceAttributeName="capitalAssetTypeCode">
      <property name="name" value="capitalAssetTypeCode" />
  </bean>
  
  <bean id="CapitalAssetInformation-capitalAssetManufacturerName" parent="CapitalAssetInformation-capitalAssetManufacturerName-parentBean" />
  
  <bean id="CapitalAssetInformation-capitalAssetManufacturerName-parentBean" abstract="true" parent="ExternalizableAttributeDefinitionProxy"
  	p:sourceExternalizableBusinessObjectInterface="org.kuali.kfs.integration.cam.CapitalAssetManagementAsset"
	p:sourceAttributeName="manufacturerName">
     <property name="name" value="capitalAssetManufacturerName" />
  </bean>
  
  <bean id="CapitalAssetInformation-capitalAssetDescription" parent="CapitalAssetInformation-capitalAssetDescription-parentBean" />
  
  <bean id="CapitalAssetInformation-capitalAssetDescription-parentBean" abstract="true" parent="ExternalizableAttributeDefinitionProxy"
  	p:sourceExternalizableBusinessObjectInterface="org.kuali.kfs.integration.cam.CapitalAssetManagementAsset"
	p:sourceAttributeName="capitalAssetDescription">
     <property name="name" value="capitalAssetDescription" />
  </bean> 
  
  <bean id="CapitalAssetInformation-capitalAssetManufacturerModelNumber" parent="CapitalAssetInformation-capitalAssetManufacturerModelNumber-parentBean" />
  
  <bean id="CapitalAssetInformation-capitalAssetManufacturerModelNumber-parentBean" abstract="true" parent="ExternalizableAttributeDefinitionProxy"
  	p:sourceExternalizableBusinessObjectInterface="org.kuali.kfs.integration.cam.CapitalAssetManagementAsset"
	p:sourceAttributeName="manufacturerModelNumber">
     <property name="name" value="capitalAssetManufacturerModelNumber" />
  </bean>  
  
  <bean id="CapitalAssetInformation-capitalAssetSerialNumber" parent="CapitalAssetInformation-capitalAssetSerialNumber-parentBean" />
  
  <bean id="CapitalAssetInformation-capitalAssetSerialNumber-parentBean" abstract="true" parent="ExternalizableAttributeDefinitionProxy"
  	p:sourceExternalizableBusinessObjectInterface="org.kuali.kfs.integration.cam.CapitalAssetManagementAsset"
	p:sourceAttributeName="serialNumber">
     <property name="name" value="capitalAssetSerialNumber" />
  </bean> 
</beans>