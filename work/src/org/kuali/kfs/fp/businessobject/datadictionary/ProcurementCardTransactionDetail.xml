<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "../dataDictionary-1_0.dtd">
<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.module.financial.bo.ProcurementCardTransactionDetail">
        <customAttributeEnabled>false</customAttributeEnabled>

        <inquiry>
            <title>Procurement Card Transaction Detail Inquiry</title>

            <inquirySections><inquirySection title="Header Text"><inquiryFields>
                <field attributeName="documentNumber" />
                <field attributeName="financialDocumentTransactionLineNumber" />
                <field attributeName="transactionReferenceNumber" />
                <field attributeName="transactionBillingCurrencyCode" />
                <field attributeName="transactionCurrencyExchangeRate" />
                <field attributeName="transactionDate" />
                <field attributeName="transactionOriginalCurrencyAmount" />
                <field attributeName="transactionOriginalCurrencyCode" />
                <field attributeName="transactionPointOfSaleCode" />
                <field attributeName="transactionPostingDate" />
                <field attributeName="transactionPurchaseIdentifierDescription" />
                <field attributeName="transactionPurchaseIdentifierIndicator" />
                <field attributeName="transactionSalesTaxAmount" />
                <field attributeName="transactionSettlementAmount" />
                <field attributeName="transactionTaxExemptIndicator" />
                <field attributeName="transactionTravelAuthorizationCode" />
                <field attributeName="transactionUnitContactName" />
                <field attributeName="transactionCycleEndDate" />
                <field attributeName="transactionCycleStartDate" />
                <field attributeName="transactionTotalAmount" />
            </inquiryFields></inquirySection></inquirySections>
        </inquiry>

        <attributes>

            <attributeReference name="documentNumber" sourceClassName="org.kuali.core.bo.DocumentHeader" sourceAttributeName="documentNumber" />

            <attribute name="financialDocumentTransactionLineNumber" forceUppercase="true">
                <label>Financial Document Transaction Line Number</label>
                <shortLabel>Number</shortLabel>

                <maxLength>7</maxLength>
                <validationPattern>
                    <numeric />
                </validationPattern>

                <control>
                    <text size="9" />
                </control>

                <summary><![CDATA[&nbsp;]]></summary>
                <description>The line number for the transaction in grouping lines for the same transaction.</description>
            </attribute>

            <attribute name="transactionBillingCurrencyCode" forceUppercase="true">
                <label>Transaction Billing Currency Code</label>
                <shortLabel>Code</shortLabel>

                <maxLength>3</maxLength>

                <control>
                    <text size="5" />
                </control>

                <summary><![CDATA[&nbsp;]]></summary>
                <description>The system code for the billing currency in the transaction.</description>
            </attribute>

            <attribute name="transactionCurrencyExchangeRate" forceUppercase="true">
                <label>Transaction Currency Exchange Rate</label>
                <shortLabel>Rate</shortLabel>

                <maxLength>10</maxLength>
                <validationPattern>
                    <floatingPoint />
                </validationPattern>

                <control>
                    <text size="12" />
                </control>

                <summary><![CDATA[&nbsp;]]></summary>
                <description>The exchange rate for the billing currency in the transaction.</description>
            </attribute>

            <attribute name="transactionDate" forceUppercase="true">
                <label>Transaction Date</label>
                <shortLabel>Date</shortLabel>

                <maxLength>7</maxLength>
                <validationPattern>
                    <date />
                </validationPattern>

                <control>
                    <text size="9" />
                </control>

                <summary><![CDATA[&nbsp;]]></summary>
                <description>The date of the transaction, entered directly or selected in the calendar.</description>
            </attribute>

            <attribute name="transactionOriginalCurrencyAmount" forceUppercase="true">
                <label>Transaction Original Currency Amount</label>
                <shortLabel>Amount</shortLabel>

                <maxLength>19</maxLength>
                <validationPattern>
                    <floatingPoint />
                </validationPattern>

                <control>
                    <text size="21" />
                </control>

                <summary><![CDATA[&nbsp;]]></summary>
                <description>The amount of the transaction in the original currency.</description>
            </attribute>

            <attribute name="transactionOriginalCurrencyCode" forceUppercase="true">
                <label>Transaction Original Currency Code</label>
                <shortLabel>Code</shortLabel>

                <maxLength>3</maxLength>

                <control>
                    <text size="5" />
                </control>

                <summary><![CDATA[&nbsp;]]></summary>
                <description>The system code for the original currency in the transaction.</description>
            </attribute>

            <attribute name="transactionPointOfSaleCode" forceUppercase="true">
                <label>Transaction Point Of Sale Code</label>
                <shortLabel>Code</shortLabel>

                <maxLength>25</maxLength>

                <control>
                    <text size="27" />
                </control>

                <summary><![CDATA[&nbsp;]]></summary>
                <description>The system code for the point of sale in the transaction.</description>
            </attribute>

            <attribute name="transactionPostingDate" forceUppercase="true">
                <label>Transaction Posting Date</label>
                <shortLabel>Date</shortLabel>

                <maxLength>7</maxLength>
                <validationPattern>
                    <date />
                </validationPattern>

                <control>
                    <text size="9" />
                </control>

                <summary><![CDATA[&nbsp;]]></summary>
                <description>The posting date for the transaction, entered directly or selected in the calendar.</description>
            </attribute>

            <attribute name="transactionPurchaseIdentifierDescription" forceUppercase="true">
                <label>Transaction Purchase Identifier Description</label>
                <shortLabel>Description</shortLabel>

                <maxLength>25</maxLength>

                <control>
                    <text size="27" />
                </control>

                <summary><![CDATA[&nbsp;]]></summary>
                <description>The Yes/No indicator of an entry for the Purchase Identifier Description</description>
            </attribute>

            <attribute name="transactionPurchaseIdentifierIndicator" forceUppercase="true">
                <label>Transaction Purchase Identifier Indicator</label>
                <shortLabel>Indicator</shortLabel>

                <maxLength>1</maxLength>

                <control>
                    <checkbox />
                </control>

                <summary><![CDATA[&nbsp;]]></summary>
                <description>The Yes/No indicator of an entry for the Purchase Identifier Description</description>
            </attribute>

            <attribute name="transactionReferenceNumber" forceUppercase="true">
                <label>Transaction Reference Number</label>
                <shortLabel>Number</shortLabel>

                <maxLength>23</maxLength>

                <control>
                    <text size="25" />
                </control>

                <summary><![CDATA[&nbsp;]]></summary>
                <description>The reference number for the transaction assigned by the bank.</description>
            </attribute>

            <attribute name="transactionSalesTaxAmount" forceUppercase="true">
                <label>Transaction Sales Tax Amount</label>
                <shortLabel>Amount</shortLabel>

                <maxLength>19</maxLength>
                <validationPattern>
                    <floatingPoint />
                </validationPattern>

                <control>
                    <text size="21" />
                </control>

                <summary><![CDATA[&nbsp;]]></summary>
                <description>The amount of sales tax assessed on the transaction.</description>
            </attribute>

            <attribute name="transactionSettlementAmount" forceUppercase="true">
                <label>Transaction Settlement Amount</label>
                <shortLabel>Amount</shortLabel>

                <maxLength>19</maxLength>
                <validationPattern>
                    <floatingPoint />
                </validationPattern>

                <control>
                    <text size="21" />
                </control>

                <summary><![CDATA[&nbsp;]]></summary>
                <description>The final amount payable for a disputed transaction.</description>
            </attribute>

            <attribute name="transactionTaxExemptIndicator" forceUppercase="true">
                <label>Transaction Tax Exempt Indicator</label>
                <shortLabel>Indicator</shortLabel>

                <maxLength>1</maxLength>

                <control>
                    <checkbox />
                </control>

                <summary><![CDATA[&nbsp;]]></summary>
                <description>The Yes/No indicator of tax exemption.</description>
            </attribute>

            <attribute name="transactionTravelAuthorizationCode" forceUppercase="true">
                <label>Transaction Travel Authorization Code</label>
                <shortLabel>Code</shortLabel>

                <maxLength>50</maxLength>

                <control>
                    <text size="52" />
                </control>

                <summary><![CDATA[&nbsp;]]></summary>
                <description>The system code for authorization of travel for the transaction.</description>
            </attribute>

            <attribute name="transactionUnitContactName" forceUppercase="true">
                <label>Transaction Unit Contact Name</label>
                <shortLabel>Name</shortLabel>

                <maxLength>35</maxLength>

                <control>
                    <text size="37" />
                </control>

                <summary><![CDATA[&nbsp;]]></summary>
                <description>The name of the contact unit for the transaction.</description>
            </attribute>

            <attribute name="transactionCycleEndDate" forceUppercase="true">
                <label>Transaction Cycle End Date</label>
                <shortLabel>Date</shortLabel>

                <maxLength>7</maxLength>
                <validationPattern>
                    <date />
                </validationPattern>

                <control>
                    <text size="9" />
                </control>

                <summary><![CDATA[&nbsp;]]></summary>
                <description>The end date of the transaction cycle, entered directly or selected in the calendar.</description>
            </attribute>

            <attribute name="transactionCycleStartDate" forceUppercase="true">
                <label>Transaction Cycle Start Date</label>
                <shortLabel>Date</shortLabel>

                <maxLength>7</maxLength>
                <validationPattern>
                    <date />
                </validationPattern>

                <control>
                    <text size="9" />
                </control>

                <summary><![CDATA[&nbsp;]]></summary>
                <description>The start date of the transaction cycle, entered directly or selected in the calendar.</description>
            </attribute>

            <attribute name="transactionTotalAmount" forceUppercase="true">
                <label>Transaction Total Amount</label>
                <shortLabel>Amount</shortLabel>

                <maxLength>19</maxLength>
                <validationPattern>
                    <floatingPoint />
                </validationPattern>

                <control>
                    <text size="21" />
                </control>

                <summary><![CDATA[&nbsp;]]></summary>
                <description>The total amount of the transaction.</description>
            </attribute>

            <attributeReference name="versionNumber" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="versionNumber" />

        </attributes>
    </businessObject>
</dictionaryEntry>
