<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "../../../core/datadictionary/dataDictionary-1_0.dtd">
<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.module.financial.bo.TravelExpenseTypeCode">
        <customAttributeEnabled>false</customAttributeEnabled>

        <inquiry>
            <title>Travel Expense Type Code Inquiry</title>

            <inquiryFields>
                <field attributeName="code" />
                <field attributeName="name" />
            </inquiryFields>
        </inquiry>

        <lookup>
            <title>Travel Expense Type Code Lookup</title>

            <menubar>
                <![CDATA[<a href="maintenance.do?methodToCall=start&businessObjectClassName=org.kuali.module.financial.bo.TravelExpenseTypeCode"><img src="images/tinybutton-createnew.gif" alt="create new" width="70" height="15"/></a>]]>
            </menubar>

            <instructions>Lookup a Travel Expense Type Code</instructions>

            <defaultSort attributeName="code" sortAscending="true" />

            <lookupFields>
                <lookupField attributeName="code" required="false" />
                <lookupField attributeName="name" required="false" />
            </lookupFields>

            <resultFields>
                <field attributeName="code" />
                <field attributeName="name" />
                <field attributeName="prepaidExpense" />
            </resultFields>
        </lookup>

        <titleAttribute>code</titleAttribute>

        <attributes>

            <attribute name="code" forceUppercase="true">
                <label>Travel Expense Type Code</label>
                <shortLabel>Type Code</shortLabel>

                <maxLength>2</maxLength>
                <validationPattern>
                    <alphaNumeric />
                </validationPattern>

                <control>
                    <text size="2" />
                </control>

                <summary>Travel Expense Type Code</summary>
                <description>The code uniquely identify a travel expense type.</description>
            </attribute>

            <attribute name="prepaidExpense" forceUppercase="false">
                <label>Prepaid Expense Indicator</label>
                <shortLabel>Prepaid</shortLabel>

                <maxLength>3</maxLength>

                <control>
                    <checkbox />
                </control>

                <summary><![CDATA[&nbsp;]]></summary>
                <description>The Yes/No indicator of a prepaid expense</description>
            </attribute>

            <attribute name="name" forceUppercase="true">
                <label>Travel Expense Type Name</label>
                <shortLabel>Name</shortLabel>

                <maxLength>25</maxLength>

                <control>
                    <text size="25" />
                </control>

                <summary><![CDATA[&nbsp;]]></summary>
                <description>Travel Expense Type Name</description>
            </attribute>

            <attributeReference name="versionNumber" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="versionNumber" />

        </attributes>
    </businessObject>
</dictionaryEntry>

