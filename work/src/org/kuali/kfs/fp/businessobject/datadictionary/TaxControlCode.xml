<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="TaxControlCode" parent="TaxControlCode-parentBean" />

  <bean id="TaxControlCode-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.module.financial.bo.TaxControlCode" />
    <property name="inquiryDefinition" >
      <ref bean="TaxControlCode-inquiryDefinition" />
    </property>
    <property name="lookupDefinition" >
      <ref bean="TaxControlCode-lookupDefinition" />
    </property>
    <property name="titleAttribute" value="code" />
    <property name="objectLabel" value="Disbursement Voucher Tax Control" />
    <property name="attributes" >
      <list>
        <ref bean="TaxControlCode-code" />
        <ref bean="TaxControlCode-name" />
        <ref bean="TaxControlCode-versionNumber" />
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->


  <bean id="TaxControlCode-code" parent="TaxControlCode-code-parentBean" />

  <bean id="TaxControlCode-code-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="code" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Tax Control Code" />
    <property name="shortLabel" value="Tax Code" />
    <property name="maxLength" value="1" />
    <property name="validationPattern" >
      <bean parent="AlphaNumericValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="1" />
    </property>
  </bean>
  <bean id="TaxControlCode-name" parent="TaxControlCode-name-parentBean" />

  <bean id="TaxControlCode-name-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="name" />
    <property name="forceUppercase" value="false" />
    <property name="label" value="Tax Control Name" />
    <property name="shortLabel" value="Name" />
    <property name="maxLength" value="25" />
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="25" />
    </property>
  </bean>
  <bean id="TaxControlCode-versionNumber" parent="TaxControlCode-versionNumber-parentBean" />

  <bean id="TaxControlCode-versionNumber-parentBean" abstract="true" parent="AttributeReferenceDummy-versionNumber">
  </bean>

<!-- Business Object Inquiry Definition -->


  <bean id="TaxControlCode-inquiryDefinition" parent="TaxControlCode-inquiryDefinition-parentBean" />

  <bean id="TaxControlCode-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
    <property name="title" value="Disbursement Voucher Tax Control Inquiry" />
    <property name="inquirySections" >
      <list>
        <bean parent="InquirySectionDefinition">
          <property name="title" value="Header Text" />
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields" >
            <list>
              <bean parent="FieldDefinition"
                    p:attributeName="code" />
              <bean parent="FieldDefinition"
                    p:attributeName="name" />
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Business Object Lookup Definition -->


  <bean id="TaxControlCode-lookupDefinition" parent="TaxControlCode-lookupDefinition-parentBean" />

  <bean id="TaxControlCode-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
    <property name="title" value="Disbursement Voucher Tax Control Lookup" />
    <property name="instructions" value="Lookup a Tax Control Code" />
    <property name="defaultSort" >
      <bean parent="SortDefinition">
        <property name="attributeNames" >
          <list>
            <value>code</value>
          </list>
        </property>
      </bean>
    </property>
    <property name="lookupFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="code" />
        <bean parent="FieldDefinition"
              p:attributeName="name" />
      </list>
    </property>
    <property name="resultFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="code" />
        <bean parent="FieldDefinition"
              p:attributeName="name" />
      </list>
    </property>
  </bean>
</beans>
