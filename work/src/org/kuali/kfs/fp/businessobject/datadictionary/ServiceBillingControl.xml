<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="ServiceBillingControl" parent="ServiceBillingControl-parentBean" />

  <bean id="ServiceBillingControl-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.module.financial.bo.ServiceBillingControl" />
    <property name="inquiryDefinition" >
      <ref bean="ServiceBillingControl-inquiryDefinition" />
    </property>
    <property name="lookupDefinition" >
      <ref bean="ServiceBillingControl-lookupDefinition" />
    </property>
    <property name="objectLabel" value="Service Billing Control" />
    <property name="attributes" >
      <list>
        <ref bean="ServiceBillingControl-chartOfAccountsCode" />
        <ref bean="ServiceBillingControl-accountNumber" />
        <ref bean="ServiceBillingControl-workgroupName" />
        <ref bean="ServiceBillingControl-versionNumber" />
        <ref bean="ServiceBillingControl-objectId" />
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->


  <bean id="ServiceBillingControl-chartOfAccountsCode" parent="ServiceBillingControl-chartOfAccountsCode-parentBean" />

  <bean id="ServiceBillingControl-chartOfAccountsCode-parentBean" abstract="true" parent="Chart-chartOfAccountsCode">
  </bean>
  <bean id="ServiceBillingControl-accountNumber" parent="ServiceBillingControl-accountNumber-parentBean" />

  <bean id="ServiceBillingControl-accountNumber-parentBean" abstract="true" parent="Account-accountNumber">
  </bean>
  <bean id="ServiceBillingControl-workgroupName" parent="ServiceBillingControl-workgroupName-parentBean" />

  <bean id="ServiceBillingControl-workgroupName-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="workgroupName" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Workgroup Name" />
    <property name="shortLabel" value="Workgroup Name" />
    <property name="maxLength" value="70" />
    <property name="required" value="true" />
    <property name="control" >
      <bean parent="WorkflowWorkgroupControlDefinition" />
    </property>
  </bean>
  <bean id="ServiceBillingControl-versionNumber" parent="ServiceBillingControl-versionNumber-parentBean" />

  <bean id="ServiceBillingControl-versionNumber-parentBean" abstract="true" parent="GenericAttributes-versionNumber">
  </bean>
  <bean id="ServiceBillingControl-objectId" parent="ServiceBillingControl-objectId-parentBean" />

  <bean id="ServiceBillingControl-objectId-parentBean" abstract="true" parent="GenericAttributes-objectId">
  </bean>

<!-- Business Object Inquiry Definition -->


  <bean id="ServiceBillingControl-inquiryDefinition" parent="ServiceBillingControl-inquiryDefinition-parentBean" />

  <bean id="ServiceBillingControl-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
    <property name="title" value="Service Billing Control Inquiry" />
    <property name="inquirySections" >
      <list>
        <bean parent="InquirySectionDefinition">
          <property name="title" value="Header Text" />
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields" >
            <list>
              <bean parent="FieldDefinition"
                    p:attributeName="chartOfAccountsCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="accountNumber" />
              <bean parent="FieldDefinition"
                    p:attributeName="workgroupName" />
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Business Object Lookup Definition -->


  <bean id="ServiceBillingControl-lookupDefinition" parent="ServiceBillingControl-lookupDefinition-parentBean" />

  <bean id="ServiceBillingControl-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
    <property name="title" value="Service Billing Control Lookup" />
    <property name="instructions" value="Lookup a Service Billing Control" />
    <property name="defaultSort" >
      <bean parent="SortDefinition">
        <property name="attributeNames" >
          <list>
            <value>chartOfAccountsCode</value>
          </list>
        </property>
      </bean>
    </property>
    <property name="lookupFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="chartOfAccountsCode" />
        <bean parent="FieldDefinition"
              p:attributeName="accountNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="workgroupName" />
      </list>
    </property>
    <property name="resultFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="chartOfAccountsCode" />
        <bean parent="FieldDefinition"
              p:attributeName="accountNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="workgroupName" />
      </list>
    </property>
  </bean>
</beans>
