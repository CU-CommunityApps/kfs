        <ref bean="DisbursementVoucherPayeeDetail-disbVchrRemitCountryCode" />
        <ref bean="DisbursementVoucherPayeeDetail-disbVchrPayeeEmployeeCode" />
        <ref bean="DisbursementVoucherPayeeDetail-disbVchrAlienPaymentCode" />
      </list>
    </property>
  </bean>

