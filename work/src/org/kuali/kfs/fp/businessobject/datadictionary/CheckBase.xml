<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "../../../core/datadictionary/dataDictionary-1_0.dtd">
<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.module.financial.bo.CheckBase">


        <attributes>
            <attribute name="checkNumber" forceUppercase="false">
                <label>Check/Batch #</label>
                <shortLabel>Check/Batch #</shortLabel>

                <maxLength>10</maxLength>
                <required>true</required>

                <control>
                    <text size="10" />
                </control>
            </attribute>

            <attributeReference name="checkDate" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="genericTimestamp">
                <label>Check Date</label>
                <shortLabel>Date</shortLabel>
                <required>true</required>
                <control>
                    <text size="10" />
                </control>
            </attributeReference>

            <attribute name="description" forceUppercase="false">
                <label>Description</label>
                <shortLabel>Description</shortLabel>

                <maxLength>40</maxLength>

                <control>
                    <text size="40" />
                </control>
            </attribute>
            
            <attribute name="financialDocumentTypeCode" forceUppercase="true">
    			<label>Financial Document Type Code</label>
                <shortLabel>FinDocTypCd</shortLabel>

                <maxLength>4</maxLength>
                <required>false</required>

                <control>
                    <hidden />
                </control>
    		</attribute>
    		
    		<attribute name="cashieringRecordSource" forceUppercase="true">
    			<label>Cashiering Record Source</label>
                <shortLabel>CshrgRcdSrc</shortLabel>

                <maxLength>1</maxLength>
                <required>false</required>

                <control>
                    <hidden />
                </control>
    		</attribute>
            

            <attributeReference name="financialDocumentDepositLineNumber" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="genericAmount">
                <required>false</required>
            </attributeReference>

            <attributeReference name="amount" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="genericAmount">
                <required>true</required>
            </attributeReference>

            <attributeReference name="sequenceId" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="genericAmount">
                <required>true</required>
            </attributeReference>

            <attributeReference name="documentNumber" sourceClassName="org.kuali.core.bo.DocumentHeader" sourceAttributeName="documentNumber" />
        </attributes>
    </businessObject>
</dictionaryEntry>

