<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "../../../core/datadictionary/dataDictionary-1_0.dtd">
<dictionaryEntry>
	<businessObject businessObjectClass="org.kuali.module.financial.bo.CheckBase">
		<customAttributeEnabled>false</customAttributeEnabled>

		<attributes>
			<attribute name="checkNumber" forceUppercase="false">
                <label>Check/Batch #</label>
                <shortLabel>Check/Batch #</shortLabel>

                <maxLength>15</maxLength>
				<required>true</required>

                <control>
                    <text size="15" />
                </control>

                <summary>Check Number</summary>
                <description>Check Number...</description>
            </attribute>

            <attributeReference name="checkDate" 
					            sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" 
					            sourceAttributeName="genericTimestamp">
                <label>Check Date</label>
                <shortLabel>Date</shortLabel>
				<required>true</required>
                <control>
                    <text size="10" />
                </control>
			</attributeReference>		            

			<attribute name="description" forceUppercase="false">
                <label>Description</label>
                <shortLabel>Description</shortLabel>

                <maxLength>40</maxLength>

                <control>
                    <text size="40" />
                </control>

                <summary>Description</summary>
                <description>Description...</description>
            </attribute>            
            
            <attributeReference name="interimDepositAmount" 
					            sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" 
					            sourceAttributeName="genericBoolean">
				<required>true</required>
			</attributeReference>		            

            <attributeReference name="amount" 
					            sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" 
					            sourceAttributeName="genericAmount">
				<required>true</required>
			</attributeReference>		            

            <attributeReference name="sequenceId" 
					            sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" 
					            sourceAttributeName="genericAmount">
				<required>true</required>
			</attributeReference>
			
            <attributeReference name="financialDocumentNumber" 
            					sourceClassName="org.kuali.core.document.DocumentHeader" 
            					sourceAttributeName="financialDocumentNumber" />
		</attributes>
	</businessObject>
</dictionaryEntry>

