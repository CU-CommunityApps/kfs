<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="CreditCardType" parent="CreditCardType-parentBean" />

  <bean id="CreditCardType-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.kfs.fp.businessobject.CreditCardType" />
    <property name="inquiryDefinition" >
      <ref bean="CreditCardType-inquiryDefinition" />
    </property>
    <property name="lookupDefinition" >
      <ref bean="CreditCardType-lookupDefinition" />
    </property>
    <property name="titleAttribute" value="financialDocumentCreditCardTypeCode" />
    <property name="objectLabel" value="Credit Card Type" />
    <property name="attributes" >
      <list>
        <ref bean="CreditCardType-financialDocumentCreditCardTypeCode" />
        <ref bean="CreditCardType-financialDocumentCreditCardCompanyName" />
        <ref bean="CreditCardType-versionNumber" />
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->


  <bean id="CreditCardType-financialDocumentCreditCardTypeCode" parent="CreditCardType-financialDocumentCreditCardTypeCode-parentBean" />

  <bean id="CreditCardType-financialDocumentCreditCardTypeCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="financialDocumentCreditCardTypeCode" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Credit Card Type Code" />
    <property name="shortLabel" value="Code" />
    <property name="maxLength" value="2" />
    <property name="validationPattern" >
      <bean parent="AlphaNumericValidationPattern" />
    </property>
    <property name="required" value="true" />
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="2" />
    </property>
  </bean>
  <bean id="CreditCardType-financialDocumentCreditCardCompanyName" parent="CreditCardType-financialDocumentCreditCardCompanyName-parentBean" />

  <bean id="CreditCardType-financialDocumentCreditCardCompanyName-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="financialDocumentCreditCardCompanyName" />
    <property name="forceUppercase" value="false" />
    <property name="label" value="Company Name" />
    <property name="shortLabel" value="Name" />
    <property name="maxLength" value="40" />
    <property name="required" value="true" />
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="40" />
    </property>
  </bean>
  <bean id="CreditCardType-versionNumber" parent="CreditCardType-versionNumber-parentBean" />

  <bean id="CreditCardType-versionNumber-parentBean" abstract="true" parent="GenericAttributes-versionNumber">
  </bean>

<!-- Business Object Inquiry Definition -->


  <bean id="CreditCardType-inquiryDefinition" parent="CreditCardType-inquiryDefinition-parentBean" />

  <bean id="CreditCardType-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
    <property name="title" value="Credit Card Type Inquiry" />
    <property name="inquirySections" >
      <list>
        <bean parent="InquirySectionDefinition">
          <property name="title" value="Header Text" />
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields" >
            <list>
              <bean parent="FieldDefinition"
                    p:attributeName="financialDocumentCreditCardTypeCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="financialDocumentCreditCardCompanyName" />
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Business Object Lookup Definition -->


  <bean id="CreditCardType-lookupDefinition" parent="CreditCardType-lookupDefinition-parentBean" />

  <bean id="CreditCardType-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
    <property name="title" value="Credit Card Type Lookup" />
    <property name="instructions" value="Lookup a Credit Card Type" />
    <property name="defaultSort" >
      <bean parent="SortDefinition">
        <property name="attributeNames" >
          <list>
            <value>financialDocumentCreditCardTypeCode</value>
          </list>
        </property>
      </bean>
    </property>
    <property name="lookupFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="financialDocumentCreditCardTypeCode" />
        <bean parent="FieldDefinition"
              p:attributeName="financialDocumentCreditCardCompanyName" />
      </list>
    </property>
    <property name="resultFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="financialDocumentCreditCardTypeCode" />
        <bean parent="FieldDefinition"
              p:attributeName="financialDocumentCreditCardCompanyName" />
      </list>
    </property>
  </bean>
</beans>
