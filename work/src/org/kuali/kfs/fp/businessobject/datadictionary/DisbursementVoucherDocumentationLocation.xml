<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="DisbursementVoucherDocumentationLocation" parent="DisbursementVoucherDocumentationLocation-parentBean" />

  <bean id="DisbursementVoucherDocumentationLocation-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.module.financial.bo.DisbursementVoucherDocumentationLocation" />
    <property name="inquiryDefinition" >
      <ref bean="DisbursementVoucherDocumentationLocation-inquiryDefinition" />
    </property>
    <property name="lookupDefinition" >
      <ref bean="DisbursementVoucherDocumentationLocation-lookupDefinition" />
    </property>
    <property name="objectLabel" value="Disbursement Voucher Documentation Location" />
    <property name="attributes" >
      <list>
        <ref bean="DisbursementVoucherDocumentationLocation-disbursementVoucherDocumentationLocationCode" />
        <ref bean="DisbursementVoucherDocumentationLocation-disbursementVoucherDocumentationLocationName" />
        <ref bean="DisbursementVoucherDocumentationLocation-disbursementVoucherDocumentationLocationAddress" />
        <ref bean="DisbursementVoucherDocumentationLocation-versionNumber" />
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->


  <bean id="DisbursementVoucherDocumentationLocation-disbursementVoucherDocumentationLocationCode" parent="DisbursementVoucherDocumentationLocation-disbursementVoucherDocumentationLocationCode-parentBean" />

  <bean id="DisbursementVoucherDocumentationLocation-disbursementVoucherDocumentationLocationCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="disbursementVoucherDocumentationLocationCode" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Documentation Location Code" />
    <property name="shortLabel" value="Location Code" />
    <property name="maxLength" value="2" />
    <property name="validationPattern" >
      <bean parent="AlphaNumericValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="2" />
    </property>
  </bean>
  <bean id="DisbursementVoucherDocumentationLocation-disbursementVoucherDocumentationLocationName" parent="DisbursementVoucherDocumentationLocation-disbursementVoucherDocumentationLocationName-parentBean" />

  <bean id="DisbursementVoucherDocumentationLocation-disbursementVoucherDocumentationLocationName-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="disbursementVoucherDocumentationLocationName" />
    <property name="forceUppercase" value="false" />
    <property name="label" value="Documentation Location Name" />
    <property name="shortLabel" value="Name" />
    <property name="maxLength" value="25" />
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="25" />
    </property>
  </bean>
  <bean id="DisbursementVoucherDocumentationLocation-disbursementVoucherDocumentationLocationAddress" parent="DisbursementVoucherDocumentationLocation-disbursementVoucherDocumentationLocationAddress-parentBean" />

  <bean id="DisbursementVoucherDocumentationLocation-disbursementVoucherDocumentationLocationAddress-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="disbursementVoucherDocumentationLocationAddress" />
    <property name="forceUppercase" value="false" />
    <property name="label" value="Documentation Location Address" />
    <property name="shortLabel" value="Address" />
    <property name="maxLength" value="255" />
    <property name="control" >
      <bean parent="TextareaControlDefinition"
            p:rows="4"
            p:cols="60" />
    </property>
  </bean>
  <bean id="DisbursementVoucherDocumentationLocation-versionNumber" parent="DisbursementVoucherDocumentationLocation-versionNumber-parentBean" />

  <bean id="DisbursementVoucherDocumentationLocation-versionNumber-parentBean" abstract="true" parent="GenericAttributes-versionNumber">
  </bean>

<!-- Business Object Inquiry Definition -->


  <bean id="DisbursementVoucherDocumentationLocation-inquiryDefinition" parent="DisbursementVoucherDocumentationLocation-inquiryDefinition-parentBean" />

  <bean id="DisbursementVoucherDocumentationLocation-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
    <property name="title" value="Disbursement Voucher Documentation Location Inquiry" />
    <property name="inquirySections" >
      <list>
        <bean parent="InquirySectionDefinition">
          <property name="title" value="Header Text" />
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields" >
            <list>
              <bean parent="FieldDefinition"
                    p:attributeName="disbursementVoucherDocumentationLocationCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="disbursementVoucherDocumentationLocationName" />
              <bean parent="FieldDefinition"
                    p:attributeName="disbursementVoucherDocumentationLocationAddress" />
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Business Object Lookup Definition -->


  <bean id="DisbursementVoucherDocumentationLocation-lookupDefinition" parent="DisbursementVoucherDocumentationLocation-lookupDefinition-parentBean" />

  <bean id="DisbursementVoucherDocumentationLocation-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
    <property name="title" value="Disbursement Voucher Documentation Location Lookup" />
    <property name="instructions" value="Lookup a Disbursement Voucher Documentation Location" />
    <property name="defaultSort" >
      <bean parent="SortDefinition">
        <property name="attributeNames" >
          <list>
            <value>disbursementVoucherDocumentationLocationCode</value>
          </list>
        </property>
      </bean>
    </property>
    <property name="lookupFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="disbursementVoucherDocumentationLocationCode" />
        <bean parent="FieldDefinition"
              p:attributeName="disbursementVoucherDocumentationLocationName" />
        <bean parent="FieldDefinition"
              p:attributeName="disbursementVoucherDocumentationLocationAddress" />
      </list>
    </property>
    <property name="resultFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="disbursementVoucherDocumentationLocationCode" />
        <bean parent="FieldDefinition"
              p:attributeName="disbursementVoucherDocumentationLocationName" />
        <bean parent="FieldDefinition"
              p:attributeName="disbursementVoucherDocumentationLocationAddress" />
      </list>
    </property>
  </bean>
</beans>
