<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">
<beans>
	<bean id="travelMileageRateDao" class="org.kuali.module.financial.dao.ojb.TravelMileageRateDaoOjb">
		<property name="jcdAlias">
			<value>dataSource</value>
		</property>
	</bean>
	
    <bean id="disbursementVoucherTaxService" class="org.kuali.module.financial.service.impl.DisbursementVoucherTaxServiceImpl">
        <property name="kualiConfigurationService">
            <ref bean="kualiConfigurationService" />
        </property>
        <property name="businessObjectService">
            <ref bean="businessObjectService" />
        </property>
        <property name="maintenanceDocumentService">
            <ref bean="maintenanceDocumentService" />
        </property>
    </bean>

    <bean id="disbursementVoucherCoverSheetService" class="org.kuali.module.financial.service.impl.DisbursementVoucherCoverSheetServiceImpl">
        <property name="kualiConfigurationService">
            <ref bean="kualiConfigurationService" />
        </property>
        <property name="businessObjectService">
            <ref bean="businessObjectService" />
        </property>
        <property name="persistenceService">
            <ref bean="persistenceService" />
        </property>
    </bean>
    
    <bean id="disbursementVoucherTravelService" class="org.kuali.module.financial.service.impl.DisbursementVoucherTravelServiceImpl">
        <property name="travelMileageRateDao">
           <ref bean="travelMileageRateDao"/>
        </property>
        <property name="dateTimeService">
           <ref bean="dateTimeService"/>
        </property>
    </bean>
        
    <bean id="checkService" class="org.kuali.module.financial.service.impl.CheckServiceImpl">
       <property name="checkDao">
          <ref bean="checkDao"/>
       </property>
    </bean>

    <bean id="cashDrawerService" class="org.kuali.module.financial.service.impl.CashDrawerServiceImpl">
        <property name="cashDrawerDao">
            <ref bean="cashDrawerDao" />
        </property>
        <property name="cashManagementService">
            <ref bean="cashManagementService" />
        </property>
    </bean>
     
     <bean id="accountPresenceService" class="org.kuali.module.financial.service.impl.AccountPresenceServiceImpl">
        <property name="balanceDao">
            <ref bean="glBalanceDao" />
        </property>
     </bean>
     
     <bean id="cashManagementService" class="org.kuali.module.financial.service.impl.CashManagementServiceImpl">
        <property name="businessObjectService">
            <ref bean="businessObjectService" />
        </property>
        <property name="workflowDocumentService">
            <ref bean="workflowDocumentService" />
        </property>
        <property name="documentService">
            <ref bean="documentService" />
        </property>
        <property name="cashDrawerService">
            <ref bean="cashDrawerService" />
        </property>
        <property name="dateTimeService">
            <ref bean="dateTimeService" />
        </property>
     </bean>

    <bean id="flexibleOffsetAccountService" class="org.kuali.module.financial.service.impl.FlexibleOffsetAccountServiceImpl">
        <property name="kualiConfigurationService">
            <ref bean="kualiConfigurationService" />
        </property>
        <property name="businessObjectService">
            <ref bean="businessObjectService" />
        </property>
    </bean>

</beans>