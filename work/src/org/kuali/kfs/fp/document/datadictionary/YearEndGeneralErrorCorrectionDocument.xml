<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="YearEndGeneralErrorCorrectionDocument" parent="YearEndGeneralErrorCorrectionDocument-parentBean" />

  <bean id="YearEndGeneralErrorCorrectionDocument-parentBean" abstract="true" parent="TransactionalDocumentEntry">
    <property name="allowsErrorCorrection" value="true" />
    <property name="label" value="Year End General Error Correction" />
    <property name="shortLabel" value="Year End General Error Correction" />
    <property name="summary" value="&amp;nbsp;" />
    <property name="description" value="The Year End version of the General Error Correction Document (GEC) document. &lt;br&gt; &lt;br&gt; See FIS documentation here: &lt;a 
            target=&quot;_blank&quot; href=&quot;http://www.fms.indiana.edu/fis/documentation/gec.pdf&quot;&gt; 
            http://www.fms.indiana.edu/fis/documentation/gec.pdf &lt;/a&gt;" />
    <property name="helpDefinition" >
      <bean parent="HelpDefinition" p:parameterClass="org.kuali.kfs.fp.document.YearEndGeneralErrorCorrectionDocument" p:parameterName="YEAR_END_GENERAL_ERROR_CORRECTION" />
    </property>
    <property name="documentTypeName" value="YearEndGeneralErrorCorrectionDocument" />
    <property name="documentClass" value="org.kuali.kfs.fp.document.YearEndGeneralErrorCorrectionDocument" />
    <property name="businessRulesClass" value="org.kuali.kfs.sys.document.validation.impl.AccountingRuleEngineRuleBase" />
    <property name="documentAuthorizerClass" value="org.kuali.kfs.sys.document.authorization.AccountingDocumentAuthorizerBase" />
    <property name="documentPresentationControllerClass" value="org.kuali.kfs.sys.document.authorization.LedgerPostingDocumentPresentationControllerBase" />
    <property name="documentTypeCode" value="YEGE" />
    <property name="sessionDocument" value="true" />
    <property name="allowsCopy" value="true" />
    <property name="workflowProperties" >
      <ref bean="YearEndGeneralErrorCorrectionDocument-workflowProperties" />
    </property>
    <property name="authorizations" >
      <list>
        <bean parent="AuthorizationDefinition">
          <property name="action" value="initiate" />
          <property name="authorizedGroups" >
            <set>
              <value>kualiUniversalGroup</value>
            </set>
          </property>
        </bean>
      </list>
    </property>
    <property name="validationMap" ref="YearEndGeneralErrorCorrectionDocument-validations" />
    <property name="accountingLineGroups">
    	<map>
    		<entry>
    			<key><value>source</value></key>
	    		<ref bean="YearEndGeneralErrorCorrectionDocument-sourceAccountingLineGroup" parent="AccountingLineGroup" />
	    	</entry>
	    	<entry>
	    		<key><value>target</value></key>
	    		<ref bean="YearEndGeneralErrorCorrectionDocument-targetAccountingLineGroup" parent="AccountingLineGroup" />
	    	</entry>
    	</map>
    </property> 
    <property name="importedLineParserClass" value="org.kuali.kfs.sys.businessobject.AccountingLineParserBase" />    
  </bean>

<!-- Exported Workflow Properties -->


  <bean id="YearEndGeneralErrorCorrectionDocument-workflowProperties" parent="YearEndGeneralErrorCorrectionDocument-workflowProperties-parentBean" />

  <bean id="YearEndGeneralErrorCorrectionDocument-workflowProperties-parentBean" abstract="true" parent="WorkflowProperties">
    <property name="workflowPropertyGroups" >
      <list>
        <bean parent="WorkflowPropertyGroup">
          <property name="workflowProperties" >
            <list>
              <bean parent="WorkflowProperty"
                    p:path="documentHeader" />
              <bean parent="WorkflowProperty"
                    p:path="targetAccountingLines" />
              <bean parent="WorkflowProperty"
                    p:path="sourceAccountingLines" />
              <bean parent="WorkflowProperty"
                    p:path="sourceAccountingLines.account" />
              <bean parent="WorkflowProperty"
                    p:path="targetAccountingLines.account" />
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>
  
   <!-- Validations -->  
  <bean id="YearEndGeneralErrorCorrectionDocument-validations" parent="YearEndGeneralErrorCorrectionDocument-validations-parentBean" />
  
  <bean id="YearEndGeneralErrorCorrectionDocument-validations-parentBean" class="org.springframework.beans.factory.config.MapFactoryBean">
  	<property name="sourceMap">
		<map>
			<entry>
				<key><value>org.kuali.kfs.sys.document.validation.event.AttributedRouteDocumentEvent</value></key>
				<value>YearEndGeneralErrorCorrection-routeDocumentValidation</value>
			</entry>
			<entry>
				<key><value>org.kuali.kfs.sys.document.validation.event.AttributedApproveDocumentEvent</value></key>
				<value>YearEndGeneralErrorCorrection-approveDocumentValidation</value>
			</entry>
			<entry>
				<key><value>org.kuali.kfs.sys.document.validation.event.AttributedBlanketApproveDocumentEvent</value></key>
				<value>YearEndGeneralErrorCorrection-blanketApproveDocumentValidation</value>
			</entry>
			<entry>
				<key><value>org.kuali.kfs.sys.document.validation.event.AttributedAddAccountingLineEvent</value></key>
				<value>YearEndGeneralErrorCorrection-addAccountingLineValidation</value>
			</entry>
			<entry>
				<key><value>org.kuali.kfs.sys.document.validation.event.AttributedDeleteAccountingLineEvent</value></key>
				<value>YearEndGeneralErrorCorrection-deleteAccountingLineValidation</value>
			</entry>
			<entry>
				<key><value>org.kuali.kfs.sys.document.validation.event.AttributedUpdateAccountingLineEvent</value></key>
				<value>YearEndGeneralErrorCorrection-updateAccountingLineValidation</value>
			</entry>
			<entry>
				<key><value>org.kuali.kfs.sys.document.validation.event.AttributedReviewAccountingLineEvent</value></key>
				<value>YearEndGeneralErrorCorrection-reviewAccountingLineValidation</value>
			</entry>
    	</map>
    </property>
  </bean>

  <!-- accounting line groups -->
  <bean id="YearEndGeneralErrorCorrectionDocument-sourceAccountingLineGroup" parent="GeneralErrorCorrectionDocument-sourceAccountingLineGroup-parentBean" /> 
  <bean id="YearEndGeneralErrorCorrectionDocument-targetAccountingLineGroup" parent="GeneralErrorCorrectionDocument-targetAccountingLineGroup-parentBean" />	
</beans>
