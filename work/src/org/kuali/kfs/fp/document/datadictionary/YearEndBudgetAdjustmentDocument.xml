<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="YearEndBudgetAdjustmentDocument" parent="YearEndBudgetAdjustmentDocument-parentBean" />

  <bean id="YearEndBudgetAdjustmentDocument-parentBean" abstract="true" parent="TransactionalDocumentEntry">
    <property name="allowsErrorCorrection" value="true" />
    <property name="label" value="Year End Budget Adjustment" />
    <property name="shortLabel" value="Year End Budget Adjustment" />
    <property name="summary" value="&amp;nbsp;" />
    <property name="description" value="The Year End version of the Budget Adjustment document." />
    <property name="helpDefinition" >
      <bean parent="HelpDefinition"
            p:parameterNamespace="KFS-FP"
            p:parameterName="YEAR_END_BUDGET_ADJUSTMENT"
            p:parameterDetailType="YearEndBudgetAdjustment" />
    </property>
    <property name="documentTypeName" value="YearEndBudgetAdjustmentDocument" />
    <property name="documentClass" value="org.kuali.module.financial.document.YearEndBudgetAdjustmentDocument" />
    <property name="businessRulesClass" value="org.kuali.module.financial.rules.YearEndBudgetAdjustmentDocumentRule" />
    <property name="documentAuthorizerClass" value="org.kuali.module.financial.document.authorization.YearEndBudgetAdjustmentDocumentAuthorizer" />
    <property name="preRulesCheckClass" value="org.kuali.module.financial.rules.BudgetAdjustmentDocumentPreRules" />
    <property name="documentTypeCode" value="YEBA" />
    <property name="allowsCopy" value="true" />
    <property name="workflowProperties" >
      <ref bean="YearEndBudgetAdjustmentDocument-workflowProperties" />
    </property>
    <property name="authorizations" >
      <list>
        <bean parent="AuthorizationDefinition">
          <property name="action" value="initiate" />
          <property name="authorizedGroups" >
            <set>
              <value>kualiUniversalGroup</value>
            </set>
          </property>
        </bean>
      </list>
    </property>
    <property name="attributes" >
      <list>
        <ref bean="YearEndBudgetAdjustmentDocument-nextPositionSourceLineNumber" />
        <ref bean="YearEndBudgetAdjustmentDocument-nextPositionTargetLineNumber" />
        <ref bean="YearEndBudgetAdjustmentDocument-postingYear" />
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->


  <bean id="YearEndBudgetAdjustmentDocument-nextPositionSourceLineNumber" parent="YearEndBudgetAdjustmentDocument-nextPositionSourceLineNumber-parentBean" />

  <bean id="YearEndBudgetAdjustmentDocument-nextPositionSourceLineNumber-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="nextPositionSourceLineNumber" />
    <property name="forceUppercase" value="false" />
    <property name="label" value="Next PS Source Line Number" />
    <property name="shortLabel" value="Next PS Source" />
    <property name="maxLength" value="7" />
    <property name="validationPattern" >
      <bean parent="NumericValidationPattern" />
    </property>
    <property name="required" value="false" />
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="10" />
    </property>
    <property name="summary" value="&amp;nbsp;" />
    <property name="description" value="&amp;nbsp;" />
  </bean>
  <bean id="YearEndBudgetAdjustmentDocument-nextPositionTargetLineNumber" parent="YearEndBudgetAdjustmentDocument-nextPositionTargetLineNumber-parentBean" />

  <bean id="YearEndBudgetAdjustmentDocument-nextPositionTargetLineNumber-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="nextPositionTargetLineNumber" />
    <property name="forceUppercase" value="false" />
    <property name="label" value="Next PS Target Line Number" />
    <property name="shortLabel" value="Next PS Target" />
    <property name="maxLength" value="7" />
    <property name="validationPattern" >
      <bean parent="NumericValidationPattern" />
    </property>
    <property name="required" value="false" />
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="10" />
    </property>
    <property name="summary" value="&amp;nbsp;" />
    <property name="description" value="&amp;nbsp;" />
  </bean>
  <bean id="YearEndBudgetAdjustmentDocument-postingYear" parent="YearEndBudgetAdjustmentDocument-postingYear-parentBean" />

  <bean id="YearEndBudgetAdjustmentDocument-postingYear-parentBean" abstract="true" parent="AttributeReferenceDummy-genericFiscalYear">
    <property name="name" value="postingYear" />
    <property name="required" value="true" />
    <property name="control" >
      <bean parent="SelectControlDefinition"
            p:valuesFinderClass="org.kuali.module.financial.lookup.keyvalues.BudgetAdjustmentFiscalYearValuesFinder"
            p:includeKeyInLabel="false" />
    </property>
  </bean>

<!-- Exported Workflow Properties -->


  <bean id="YearEndBudgetAdjustmentDocument-workflowProperties" parent="YearEndBudgetAdjustmentDocument-workflowProperties-parentBean" />

  <bean id="YearEndBudgetAdjustmentDocument-workflowProperties-parentBean" abstract="true" parent="WorkflowProperties">
    <property name="workflowPropertyGroups" >
      <list>
        <bean parent="WorkflowPropertyGroup">
          <property name="workflowProperties" >
            <list>
              <bean parent="WorkflowProperty"
                    p:path="documentHeader" />
              <bean parent="WorkflowProperty"
                    p:path="sourceAccountingLines" />
              <bean parent="WorkflowProperty"
                    p:path="sourceAccountingLines.account" />
              <bean parent="WorkflowProperty"
                    p:path="targetAccountingLines" />
              <bean parent="WorkflowProperty"
                    p:path="targetAccountingLines.account" />
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>
</beans>
