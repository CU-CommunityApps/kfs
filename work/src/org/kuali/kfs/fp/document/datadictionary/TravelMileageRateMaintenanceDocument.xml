<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="TravelMileageRateMaintenanceDocument" parent="TravelMileageRateMaintenanceDocument-parentBean" />

  <bean id="TravelMileageRateMaintenanceDocument-parentBean" abstract="true" parent="MaintenanceDocumentEntry">
    <property name="businessObjectClass" value="org.kuali.kfs.fp.businessobject.TravelMileageRate" />
    <property name="maintainableClass" value="org.kuali.core.maintenance.KualiMaintainableImpl" />
    <property name="maintainableSections" >
      <list>
        <ref bean="TravelMileageRateMaintenanceDocument-EditTravelMileageRate" />
      </list>
    </property>
    <property name="lockingKeys" >
      <list>
        <value>disbursementVoucherMileageEffectiveDate</value>
        <value>mileageLimitAmount</value>
      </list>
    </property>
    <property name="label" value="Disbursement Voucher Travel Mileage Rate" />
    <property name="shortLabel" value="Disbursement Voucher Travel Mileage Rate" />
    <property name="summary" value="Travel Mileage Rate Maintenance Document" />
    <property name="description" value="Document used to create or update Travel Mileage Rate objects" />
    <property name="helpDefinition" >
      <bean parent="HelpDefinition"
            p:parameterNamespace="KFS-FP"
            p:parameterName="TRAVEL_MILEAGE_RATE"
            p:parameterDetailType="TravelMileageRate" />
    </property>
    <property name="documentTypeName" value="TravelMileageRateMaintenanceDocument" />
    <property name="documentAuthorizerClass" value="org.kuali.kfs.sys.document.authorization.FinancialSystemMaintenanceDocumentAuthorizerBase" />
    <property name="documentTypeCode" value="DVML" />
    <property name="workflowProperties" >
      <ref bean="TravelMileageRateMaintenanceDocument-workflowProperties" />
    </property>
    <property name="authorizations" >
      <list>
        <bean parent="AuthorizationDefinition">
          <property name="action" value="initiate" />
          <property name="authorizedGroups" >
            <set>
              <value>SY_MAINTENANCE_USERS</value>
            </set>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Maintenance Section Definitions -->


  <bean id="TravelMileageRateMaintenanceDocument-EditTravelMileageRate" parent="TravelMileageRateMaintenanceDocument-EditTravelMileageRate-parentBean" />

  <bean id="TravelMileageRateMaintenanceDocument-EditTravelMileageRate-parentBean" abstract="true" parent="MaintainableSectionDefinition">
    <property name="id" value="Edit Travel Mileage Rate" />
    <property name="title" value="Edit Travel Mileage Rate" />
    <property name="maintainableItems" >
      <list>
        <bean parent="MaintainableFieldDefinition"
              p:name="disbursementVoucherMileageEffectiveDate"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="mileageLimitAmount"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="mileageRate"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="versionNumber" />
      </list>
    </property>
  </bean>

<!-- Exported Workflow Properties -->


  <bean id="TravelMileageRateMaintenanceDocument-workflowProperties" parent="TravelMileageRateMaintenanceDocument-workflowProperties-parentBean" />

  <bean id="TravelMileageRateMaintenanceDocument-workflowProperties-parentBean" abstract="true" parent="WorkflowProperties">
  </bean>
</beans>
