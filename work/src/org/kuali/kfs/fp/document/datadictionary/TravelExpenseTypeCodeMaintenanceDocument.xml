<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="TravelExpenseTypeCodeMaintenanceDocument" parent="TravelExpenseTypeCodeMaintenanceDocument-parentBean" />

  <bean id="TravelExpenseTypeCodeMaintenanceDocument-parentBean" abstract="true" parent="MaintenanceDocumentEntry">
    <property name="businessObjectClass" value="org.kuali.module.financial.bo.TravelExpenseTypeCode" />
    <property name="maintainableClass" value="org.kuali.core.maintenance.KualiMaintainableImpl" />
    <property name="maintainableSections" >
      <list>
        <ref bean="TravelExpenseTypeCodeMaintenanceDocument-EditTravelExpenseType" />
      </list>
    </property>
    <property name="lockingKeys" >
      <list>
        <value>code</value>
      </list>
    </property>
    <property name="label" value="Disbursement Voucher Travel Expense Type" />
    <property name="shortLabel" value="Disbursement Voucher Travel Expense Type" />
    <property name="summary" value="Travel Expense Type Code Maintenance Document" />
    <property name="description" value="Document used to create or update Travel Expense Type objects" />
    <property name="helpDefinition" >
      <bean parent="HelpDefinition"
            p:parameterNamespace="KFS-FP"
            p:parameterName="TRAVEL_EXPENSE_TYPE_CODE"
            p:parameterDetailType="TravelExpenseTypeCode" />
    </property>
    <property name="documentTypeName" value="TravelExpenseTypeCodeMaintenanceDocument" />
    <property name="documentAuthorizerClass" value="org.kuali.core.document.authorization.MaintenanceDocumentAuthorizerBase" />
    <property name="documentTypeCode" value="DVET" />
    <property name="workflowProperties" >
      <ref bean="TravelExpenseTypeCodeMaintenanceDocument-workflowProperties" />
    </property>
    <property name="authorizations" >
      <list>
        <bean parent="AuthorizationDefinition">
          <property name="action" value="initiate" />
          <property name="authorizedGroups" >
            <set>
              <value>SY_MAINTENANCE_USERS</value>
            </set>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Maintenance Section Definitions -->


  <bean id="TravelExpenseTypeCodeMaintenanceDocument-EditTravelExpenseType" parent="TravelExpenseTypeCodeMaintenanceDocument-EditTravelExpenseType-parentBean" />

  <bean id="TravelExpenseTypeCodeMaintenanceDocument-EditTravelExpenseType-parentBean" abstract="true" parent="MaintainableSectionDefinition">
    <property name="id" value="Edit Travel Expense Type" />
    <property name="title" value="Edit Travel Expense Type" />
    <property name="maintainableItems" >
      <list>
        <bean parent="MaintainableFieldDefinition"
              p:name="code"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="name"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="prepaidExpense"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="versionNumber" />
      </list>
    </property>
  </bean>

<!-- Exported Workflow Properties -->


  <bean id="TravelExpenseTypeCodeMaintenanceDocument-workflowProperties" parent="TravelExpenseTypeCodeMaintenanceDocument-workflowProperties-parentBean" />

  <bean id="TravelExpenseTypeCodeMaintenanceDocument-workflowProperties-parentBean" abstract="true" parent="WorkflowProperties">
  </bean>
</beans>
