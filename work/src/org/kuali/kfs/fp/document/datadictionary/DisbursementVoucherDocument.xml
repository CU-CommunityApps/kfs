    <property name="name" value="disbVchrPaymentMethodCode" />
    <property name="forceUppercase" value="false" />
    <property name="label" value="Payment Method" />
    <property name="shortLabel" value="Pay Mth" />
    <property name="maxLength" value="1" />
    <property name="required" value="true" />
    <property name="control" >
      <bean parent="SelectControlDefinition"
            p:valuesFinderClass="org.kuali.kfs.fp.businessobject.options.PaymentMethodValuesFinder"
            p:includeKeyInLabel="false" />
    </property>
    <property name="summary" value="&amp;nbsp;" />
    <property name="description" value="The description of the method of the document Payment. Select one from the pulldown list." />
  </bean>
  <bean id="DisbursementVoucherDocument-campusCode" parent="DisbursementVoucherDocument-campusCode-parentBean" />

