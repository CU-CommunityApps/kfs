<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="YearEndDistributionOfIncomeAndExpenseDocument" parent="YearEndDistributionOfIncomeAndExpenseDocument-parentBean" />

  <bean id="YearEndDistributionOfIncomeAndExpenseDocument-parentBean" abstract="true" parent="TransactionalDocumentEntry">
    <property name="allowsErrorCorrection" value="true" />
    <property name="label" value="Year End Distribution Of Income And Expense" />
    <property name="shortLabel" value="Year End Distribution Of Income And Expense" />
    <property name="summary" value="&amp;nbsp;" />
    <property name="description" value="The Year End version of the Distribution of Income and Expense (DI) document. &lt;br&gt; &lt;br&gt; See FIS documentation here: &lt;a target=&quot;_blank&quot; href=&quot;http://www.fms.indiana.edu/fis/documentation/di.pdf&quot;&gt; http://www.fms.indiana.edu/fis/documentation/di.pdf &lt;/a&gt;" />
    <property name="helpDefinition" >
      <bean parent="HelpDefinition"
            p:parameterNamespace="KFS-FP"
            p:parameterName="YEAR_END_DISTRIBUTION_OF_INCOME_AND_EXPENSE"
            p:parameterDetailType="YearEndDistributionOfIncomeAndExpense" />
    </property>
    <property name="documentTypeName" value="YearEndDistributionOfIncomeAndExpenseDocument" />
    <property name="documentClass" value="org.kuali.kfs.fp.document.YearEndDistributionOfIncomeAndExpenseDocument" />
    <property name="businessRulesClass" value="org.kuali.kfs.fp.document.validation.impl.YearEndDistributionOfIncomeAndExpenseDocumentRule" />
    <property name="documentAuthorizerClass" value="org.kuali.kfs.sys.document.authorization.AccountingDocumentAuthorizerBase" />
    <property name="documentTypeCode" value="YEDI" />
    <property name="allowsCopy" value="true" />
    <property name="workflowProperties" >
      <ref bean="YearEndDistributionOfIncomeAndExpenseDocument-workflowProperties" />
    </property>
    <property name="authorizations" >
      <list>
        <bean parent="AuthorizationDefinition">
          <property name="action" value="initiate" />
          <property name="authorizedGroups" >
            <set>
              <value>kualiUniversalGroup</value>
            </set>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Exported Workflow Properties -->


  <bean id="YearEndDistributionOfIncomeAndExpenseDocument-workflowProperties" parent="YearEndDistributionOfIncomeAndExpenseDocument-workflowProperties-parentBean" />

  <bean id="YearEndDistributionOfIncomeAndExpenseDocument-workflowProperties-parentBean" abstract="true" parent="WorkflowProperties">
    <property name="workflowPropertyGroups" >
      <list>
        <bean parent="WorkflowPropertyGroup">
          <property name="workflowProperties" >
            <list>
              <bean parent="WorkflowProperty"
                    p:path="documentHeader" />
              <bean parent="WorkflowProperty"
                    p:path="targetAccountingLines" />
              <bean parent="WorkflowProperty"
                    p:path="sourceAccountingLines" />
              <bean parent="WorkflowProperty"
                    p:path="sourceAccountingLines.account" />
              <bean parent="WorkflowProperty"
                    p:path="targetAccountingLines.account" />
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>
</beans>
