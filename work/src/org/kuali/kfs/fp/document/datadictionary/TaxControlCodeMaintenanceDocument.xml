<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="TaxControlCodeMaintenanceDocument" parent="TaxControlCodeMaintenanceDocument-parentBean" />

  <bean id="TaxControlCodeMaintenanceDocument-parentBean" abstract="true" parent="MaintenanceDocumentEntry">
    <property name="businessObjectClass" value="org.kuali.kfs.fp.businessobject.TaxControlCode" />
    <property name="maintainableClass" value="org.kuali.core.maintenance.KualiMaintainableImpl" />
    <property name="maintainableSections" >
      <list>
        <ref bean="TaxControlCodeMaintenanceDocument-EditTaxControl" />
      </list>
    </property>
    <property name="lockingKeys" >
      <list>
        <value>code</value>
      </list>
    </property>
    <property name="label" value="Disbursement Voucher Tax Control" />
    <property name="shortLabel" value="Disbursement Voucher Tax Control" />
    <property name="summary" value="Tax Control Code Maintenance Document" />
    <property name="description" value="Document used to create or update Tax Control objects" />
    <property name="helpDefinition" >
      <bean parent="HelpDefinition" p:parameterClass="org.kuali.kfs.fp.businessobject.TaxControlCode" p:parameterName="TAX_CONTROL" />
    </property>
    <property name="documentTypeName" value="TaxControlCodeMaintenanceDocument" />
    <property name="documentAuthorizerClass" value="org.kuali.kfs.sys.document.authorization.FinancialSystemMaintenanceDocumentAuthorizerBase" />
    <property name="documentTypeCode" value="DVCC" />
    <property name="workflowProperties" >
      <ref bean="TaxControlCodeMaintenanceDocument-workflowProperties" />
    </property>
    <property name="authorizations" >
      <list>
        <bean parent="AuthorizationDefinition">
          <property name="action" value="initiate" />
          <property name="authorizedGroups" >
            <set>
              <value>SY_MAINTENANCE_USERS</value>
            </set>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Maintenance Section Definitions -->


  <bean id="TaxControlCodeMaintenanceDocument-EditTaxControl" parent="TaxControlCodeMaintenanceDocument-EditTaxControl-parentBean" />

  <bean id="TaxControlCodeMaintenanceDocument-EditTaxControl-parentBean" abstract="true" parent="MaintainableSectionDefinition">
    <property name="id" value="Edit Tax Control" />
    <property name="title" value="Edit Tax Control" />
    <property name="maintainableItems" >
      <list>
        <bean parent="MaintainableFieldDefinition"
              p:name="code"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="name"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="versionNumber" />
      </list>
    </property>
  </bean>

<!-- Exported Workflow Properties -->


  <bean id="TaxControlCodeMaintenanceDocument-workflowProperties" parent="TaxControlCodeMaintenanceDocument-workflowProperties-parentBean" />

  <bean id="TaxControlCodeMaintenanceDocument-workflowProperties-parentBean" abstract="true" parent="WorkflowProperties">
  </bean>
</beans>
