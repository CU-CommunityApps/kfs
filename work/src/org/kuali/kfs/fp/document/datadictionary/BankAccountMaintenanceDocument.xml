<dictionaryEntry>
	<maintenanceDocument>
        <businessObjectClass>org.kuali.module.financial.bo.BankAccount</businessObjectClass>
        <maintainableClass>org.kuali.core.maintenance.KualiMaintainableImpl</maintainableClass>
		<businessRulesClass>org.kuali.module.financial.rules.BankAccountRule</businessRulesClass>
        <documentAuthorizerClass>org.kuali.core.document.DocumentAuthorizerBase</documentAuthorizerClass>
        <authorizations>
            <authorization action="initiate">
                <workgroups>
                    <workgroup>kualiUniversalGroup</workgroup>
                </workgroups>
            </authorization>
        </authorizations>

		<documentTypeName>KualiBankAccountMaintenanceDocument</documentTypeName>
		<documentTypeCode>BACC</documentTypeCode>

		<label>Bank Account Maintenance Document</label>
		<shortLabel>BankAcctMaintDoc</shortLabel>
		
		<summary>Bank Account maintenance document</summary>
		<description>Document used to create or update Bank Account objects</description>

        <lockingKeys>
			<field attributeName="financialDocumentBankCode" />
			<field attributeName="finDocumentBankAccountNumber" />
		</lockingKeys>        

		<defaultExistenceChecks>
			<reference  attributeName="bank" 
						attributeToHighlightOnFail="financialDocumentBankCode" />
		</defaultExistenceChecks>
		

	    <maintainableSections>
	        <maintainableSection title="Edit Bank Account">
	           	 <maintainableItems>
	            	<maintainableField name="financialDocumentBankCode" required="true" />
	           		<maintainableField name="finDocumentBankAccountNumber" required="true" />
	           		<maintainableField name="finDocumentBankAccountDesc" required="true" />	           		
	           		<maintainableField name="chartOfAccountsCode" />
	            	<maintainableField name="organizationCode" />	     
	            	<maintainableField name="universityAcctChartOfAcctCd" />
	            	<maintainableField name="universityAccountNumber" />	     	                   
				    <maintainableField name="cashOffsetFinancialChartOfAccountCode" />
				    <maintainableField name="cashOffsetAccountNumber" />				
				    <maintainableField name="cashOffsetObjectCode" />				
				    <maintainableField name="cashOffsetSubObjectCode" />
	         	</maintainableItems>
	        </maintainableSection>
	    </maintainableSections>
	</maintenanceDocument>
</dictionaryEntry>
