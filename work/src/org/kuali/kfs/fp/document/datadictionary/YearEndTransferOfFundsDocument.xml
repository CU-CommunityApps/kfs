<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="YearEndTransferOfFundsDocument" parent="YearEndTransferOfFundsDocument-parentBean" />

  <bean id="YearEndTransferOfFundsDocument-parentBean" abstract="true" parent="TransactionalDocumentEntry">
    <property name="allowsErrorCorrection" value="true" />
    <property name="label" value="Year End Transfer Of Funds" />
    <property name="shortLabel" value="Year End Transfer Of Funds" />
    <property name="summary" value="&amp;nbsp;" />
    <property name="description" value="The year end version of the Transfer of Funds (TOF) document. See FIS documentation here: &lt;a target=&quot;_blank&quot; href=&quot;http://www.fms.indiana.edu/fis/documentation/tf6.pdf&quot;&gt; http://www.fms.indiana.edu/fis/documentation/tf6.pdf &lt;/a&gt;" />
    <property name="helpDefinition" >
      <bean parent="HelpDefinition" p:parameterClass="org.kuali.kfs.fp.document.YearEndTransferOfFundsDocument" p:parameterName="YEAR_END_TRANSFER_OF_FUNDS" />
    </property>
    <property name="documentTypeName" value="YearEndTransferOfFundsDocument" />
    <property name="documentClass" value="org.kuali.kfs.fp.document.YearEndTransferOfFundsDocument" />
    <property name="businessRulesClass" value="org.kuali.kfs.sys.document.validation.impl.AccountingRuleEngineRuleBase" />
    <property name="documentAuthorizerClass" value="org.kuali.kfs.sys.document.authorization.AccountingDocumentAuthorizerBase" />
    <property name="documentTypeCode" value="YETF" />
    <property name="allowsCopy" value="true" />
    <property name="workflowProperties" >
      <ref bean="YearEndTransferOfFundsDocument-workflowProperties" />
    </property>
    <property name="authorizations" >
      <list>
        <bean parent="AuthorizationDefinition">
          <property name="action" value="initiate" />
          <property name="authorizedGroups" >
            <set>
              <value>kualiUniversalGroup</value>
            </set>
          </property>
        </bean>
      </list>
    </property>
    <property name="routingDataGenerators">
    	<list>
    		<value>subFundGroupRoutingDataGenerator</value>
    	</list>
    </property>
  </bean>

<!-- Exported Workflow Properties -->


  <bean id="YearEndTransferOfFundsDocument-workflowProperties" parent="YearEndTransferOfFundsDocument-workflowProperties-parentBean" />

  <bean id="YearEndTransferOfFundsDocument-workflowProperties-parentBean" abstract="true" parent="WorkflowProperties">
    <property name="workflowPropertyGroups" >
      <list>
        <bean parent="WorkflowPropertyGroup">
          <property name="workflowProperties" >
            <list>
              <bean parent="WorkflowProperty"
                    p:path="documentHeader" />
              <bean parent="WorkflowProperty"
                    p:path="targetAccountingLines" />
              <bean parent="WorkflowProperty"
                    p:path="sourceAccountingLines" />
              <bean parent="WorkflowProperty"
                    p:path="sourceAccountingLines.account" />
              <bean parent="WorkflowProperty"
                    p:path="targetAccountingLines.account" />
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>
  
  <!-- Validations -->
  
  <bean id="YearEndTransferOfFundsDocument-validations" parent="YearEndTransferOfFundsDocument-validations-parentBean" />
  
  <bean id="YearEndTransferOfFundsDocument-validations-parentBean" class="org.springframework.beans.factory.config.MapFactoryBean">
  	<property name="sourceMap">
		<map>
			<entry>
				<key><value>org.kuali.kfs.sys.document.validation.event.AttributedRouteDocumentEvent</value></key>
				<value>YearEndTransferOfFunds-routeDocumentValidation</value>
			</entry>
			<entry>
				<key><value>org.kuali.kfs.sys.document.validation.event.AttributedApproveDocumentEvent</value></key>
				<value>YearEndTransferOfFunds-approveDocumentValidation</value>
			</entry>
			<entry>
				<key><value>org.kuali.kfs.sys.document.validation.event.AttributedBlanketApproveDocumentEvent</value></key>
				<value>YearEndTransferOfFunds-blanketApproveDocumentValidation</value>
			</entry>
			<entry>
				<key><value>org.kuali.kfs.sys.document.validation.event.AttributedAddAccountingLineEvent</value></key>
				<value>YearEndTransferOfFunds-addAccountingLineValidation</value>
			</entry>
			<entry>
				<key><value>org.kuali.kfs.sys.document.validation.event.AttributedDeleteAccountingLineEvent</value></key>
				<value>YearEndTransferOfFunds-deleteAccountingLineValidation</value>
			</entry>
			<entry>
				<key><value>org.kuali.kfs.sys.document.validation.event.AttributedUpdateAccountingLineEvent</value></key>
				<value>YearEndTransferOfFunds-updateAccountingLineValidation</value>
			</entry>
			<entry>
				<key><value>org.kuali.kfs.sys.document.validation.event.AttributedReviewAccountingLineEvent</value></key>
				<value>YearEndTransferOfFunds-reviewAccountingLineValidation</value>
			</entry>
    	</map>
    </property>
  </bean>
</beans>
