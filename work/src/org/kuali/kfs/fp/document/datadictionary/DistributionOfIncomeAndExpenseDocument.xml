<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="DistributionOfIncomeAndExpenseDocument" parent="DistributionOfIncomeAndExpenseDocument-parentBean" />

  <bean id="DistributionOfIncomeAndExpenseDocument-parentBean" abstract="true" parent="TransactionalDocumentEntry">
    <property name="allowsErrorCorrection" value="true" />
    <property name="label" value="Distribution Of Income And Expense" />
    <property name="shortLabel" value="Distribution Of Income And Expense" />
    <property name="summary" value="&amp;nbsp;" />
    <property name="description" value="The Distribution of Income and Expense (DI) document is used to distribute income or expense, or assets and liabilities. &lt;br&gt; Amounts being distributed are usually the result of an accumulation of transactions that need to be divided up between various accounts. &lt;br&gt; &lt;br&gt; See FIS documentation here: &lt;a target=&quot;_blank&quot; href=&quot;http://www.fms.indiana.edu/fis/documentation/di.pdf&quot;&gt; http://www.fms.indiana.edu/fis/documentation/di.pdf &lt;/a&gt;" />
    <property name="helpDefinition" >
      <bean parent="HelpDefinition" p:parameterClass="org.kuali.kfs.fp.document.DistributionOfIncomeAndExpenseDocument" p:parameterName="DISTRIBUTION_OF_INCOME_AND_EXPENSE" />
    </property>
    <property name="documentTypeName" value="DistributionOfIncomeAndExpenseDocument" />
    <property name="documentClass" value="org.kuali.kfs.fp.document.DistributionOfIncomeAndExpenseDocument" />
    <property name="businessRulesClass" value="org.kuali.kfs.fp.document.validation.impl.DistributionOfIncomeAndExpenseDocumentRule" />
    <property name="documentAuthorizerClass" value="org.kuali.kfs.fp.document.authorization.DistributionOfIncomeAndExpenseDocumentAuthorizer" />
    <property name="documentTypeCode" value="DI" />
    <property name="allowsCopy" value="true" />
    <property name="workflowProperties" >
      <ref bean="DistributionOfIncomeAndExpenseDocument-workflowProperties" />
    </property>
    <property name="authorizations" >
      <list>
        <bean parent="AuthorizationDefinition">
          <property name="action" value="initiate" />
          <property name="authorizedGroups" >
            <set>
              <value>kualiUniversalGroup</value>
            </set>
          </property>
        </bean>
      </list>
    </property>
        <property name="accountingLineGroups">
    	<map>
    		<entry>
    			<key><value>source</value></key>
	    		<ref bean="DistributionOfIncomeAndExpenseDocument-sourceAccountingLineGroup" parent="AccountingLineGroup" />
	    	</entry>
	    	<entry>
	    		<key><value>target</value></key>
	    		<ref bean="DistributionOfIncomeAndExpenseDocument-targetAccountingLineGroup" parent="AccountingLineGroup" />
	    	</entry>
    	</map>
    </property>
      </bean>

<!-- Exported Workflow Properties -->


  <bean id="DistributionOfIncomeAndExpenseDocument-workflowProperties" parent="DistributionOfIncomeAndExpenseDocument-workflowProperties-parentBean" />

  <bean id="DistributionOfIncomeAndExpenseDocument-workflowProperties-parentBean" abstract="true" parent="WorkflowProperties">
    <property name="workflowPropertyGroups" >
      <list>
        <bean parent="WorkflowPropertyGroup">
          <property name="workflowProperties" >
            <list>
              <bean parent="WorkflowProperty"
                    p:path="documentHeader" />
              <bean parent="WorkflowProperty"
                    p:path="targetAccountingLines" />
              <bean parent="WorkflowProperty"
                    p:path="sourceAccountingLines" />
              <bean parent="WorkflowProperty"
                    p:path="sourceAccountingLines.account" />
              <bean parent="WorkflowProperty"
                    p:path="targetAccountingLines.account" />
<!-- The next two entries are to support a routing attribute that is not in
the base data set. -->                    
              <bean parent="WorkflowProperty"
                    p:path="sourceAccountingLines.objectCode" />
              <bean parent="WorkflowProperty"
                    p:path="targetAccountingLines.objectCode" />
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>
    <!-- accounting line groups -->
  <bean id="DistributionOfIncomeAndExpenseDocument-sourceAccountingLineGroup" parent="DistributionOfIncomeAndExpenseDocument-sourceAccountingLineGroup-parentBean" />
  
  <bean id="DistributionOfIncomeAndExpenseDocument-sourceAccountingLineGroup-parentBean" parent="AccountingLineGroup" abstract="true">
	  <property name="accountingLineView" ref="FinancialProcessing-SalesTax-accountingLineView" />
	  <property name="accountingLineClass" value="org.kuali.kfs.sys.businessobject.SourceAccountingLine" />
	  <property name="groupLabel" value="From" />
	  <property name="accountingLineAuthorizerClass" value="org.kuali.kfs.fp.document.authorization.DistributionOfIncomeAndExpenseAccountingLineAuthorizer" />
	  <property name="importedLinePropertyPrefix" value="source" />
	  <property name="totals" ref="AccountingDocument-sourceGroupTotals" />
	  <property name="errorKey" value="document.sourceAccounting*,sourceAccountingLines,newSourceLine*" /> <!-- note people copying this: AV and JV have extra things that belong here -->
	</bean>
	
	<bean id="DistributionOfIncomeAndExpenseDocument-targetAccountingLineGroup" parent="DistributionOfIncomeAndExpenseDocument-targetAccountingLineGroup-parentBean" />
	
	<bean id="DistributionOfIncomeAndExpenseDocument-targetAccountingLineGroup-parentBean" parent="AccountingLineGroup" abstract="true">
	  <property name="accountingLineView" ref="FinancialProcessing-SalesTax-accountingLineView" />
	  <property name="accountingLineClass" value="org.kuali.kfs.sys.businessobject.TargetAccountingLine" />
	  <property name="groupLabel" value="To" />
	  <property name="accountingLineAuthorizerClass" value="org.kuali.kfs.fp.document.authorization.DistributionOfIncomeAndExpenseAccountingLineAuthorizer" />
	  <property name="importedLinePropertyPrefix" value="target" />
	  <property name="totals" ref="AccountingDocument-targetGroupTotals" />
	  <property name="errorKey" value="document.targetAccounting*,targetAccountingLines,newTargetLine*" />
	</bean>
</beans>
