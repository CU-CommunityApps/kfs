<dictionaryEntry>
    <maintenanceDocument>
        <businessObjectClass>org.kuali.module.financial.bo.TravelCompanyCode</businessObjectClass>
        <maintainableClass>org.kuali.core.maintenance.KualiMaintainableImpl</maintainableClass>

        <documentAuthorizerClass>org.kuali.core.document.MaintenanceDocumentAuthorizerBase</documentAuthorizerClass>
        <authorizations>
            <authorization action="initiate">
                <workgroups>
                    <workgroup>KUALI_TABLE_MAINTAINERS</workgroup>
                </workgroups>
            </authorization>
        </authorizations>

        <documentTypeName>KualiTravelCompanyCodeMaintenanceDocument</documentTypeName>
        <documentTypeCode>DVIC</documentTypeCode>

        <label>Travel Company Code Maintenance Document</label>
        <shortLabel>Travel Company Maint Doc</shortLabel>

        <summary>Travel Company Code Maintenance Document</summary>
        <description>Document used to create or update Travel company code objects</description>

        <lockingKeys>
            <field attributeName="code" />
        </lockingKeys>
		
		<defaultExistenceChecks>
            <reference attributeName="travelExpenseTypeCode" attributeToHighlightOnFail="code" />
        </defaultExistenceChecks>
		
        <maintainableSections>
            <maintainableSection title="Edit Travel Company Code">
                <maintainableItems>
                    <maintainableField name="code" required="true" />
                    <maintainableField name="name" required="true" />
                    <maintainableField name="versionNumber" />
                </maintainableItems>
            </maintainableSection>
        </maintainableSections>
    </maintenanceDocument>
</dictionaryEntry>
