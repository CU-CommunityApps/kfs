<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "../../../../core/datadictionary/dataDictionary-1_0.dtd">

<dictionaryEntry>
    <maintenanceDocument>
        <businessObjectClass>org.kuali.module.financial.bo.DisbursementVoucherDocumentationLocation</businessObjectClass>
        <maintainableClass>org.kuali.core.maintenance.KualiMaintainableImpl</maintainableClass>

        <documentAuthorizerClass>org.kuali.core.document.MaintenanceDocumentAuthorizerBase</documentAuthorizerClass>
        <authorizations>
            <authorization action="initiate">
                <workgroups>
                    <workgroup>KUALI_ROLE_MAINTENANCE</workgroup>
                </workgroups>
            </authorization>
        </authorizations>

        <documentTypeName>KualiDocumentationLocationMaintenanceDocument</documentTypeName>
        <documentTypeCode>DVDL</documentTypeCode>

        <label>Documentation Location Maintenance Document</label>
        <shortLabel>Doc Loc Maint Doc</shortLabel>

        <summary>Documentation Location Maintenance Document</summary>
        <description>Document used to create or update Documentation Location objects</description>
        <help securityGroupName="Kuali.DisbursementVoucher.Help" parameterName="DISBURSEMENT_VOUCHER_HELP_DOCUMENTATION_LOCATION" />

        <lockingKeys>
            <field attributeName="disbursementVoucherDocumentationLocationCode" />
        </lockingKeys>

        <maintainableSections>
            <maintainableSection title="Edit Documentation Location">
                <maintainableItems>
                    <maintainableField name="disbursementVoucherDocumentationLocationCode" required="true" />
                    <maintainableField name="disbursementVoucherDocumentationLocationName" required="true" />
                    <maintainableField name="disbursementVoucherDocumentationLocationAddress" required="true" />
                    <maintainableField name="versionNumber" />
                </maintainableItems>
            </maintainableSection>
        </maintainableSections>
    </maintenanceDocument>
</dictionaryEntry>
