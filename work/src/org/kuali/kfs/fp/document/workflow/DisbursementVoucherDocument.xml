<?xml version='1.0' encoding='UTF-8'?> 
<data xmlns="ns:workflow" xmlns:fo="http://www.w3.org/1999/XSL/Format" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="ns:workflow resource:WorkflowData">
  <documentTypes xmlns="ns:workflow/DocumentType" xsi:schemaLocation="ns:workflow/DocumentType resource:DocumentType">
    <documentType>
      <name>DV</name>
      <parent>FP</parent>
      <docHandler>${application.url}/financialDisbursementVoucher.do?methodToCall=docHandler</docHandler>
      <routePaths>
        <routePath>
          <start name="AdHoc" nextNode="PayeeIsPurchaseOrderVendor"/>
          <split name="PayeeIsPurchaseOrderVendor" nextNode="Account">
            <branch name="True">
              <requests name="Purchasing" nextNode="JoinPayeeIsPurchaseOrderVendor"/>
            </branch>
            <branch name="False">
              <simple name="NoOpPayee" nextNode="JoinPayeeIsPurchaseOrderVendor"/>
            </branch>
            <join name="JoinPayeeIsPurchaseOrderVendor"/>
          </split>
          <requests name="Account" nextNode="AccountingOrganizationHierarchy"/>
          <requests name="AccountingOrganizationHierarchy" nextNode="RequiresTaxReview"/>
          <split name="RequiresTaxReview" nextNode="Award">
            <branch name="True">
              <requests name="Tax" nextNode="JoinRequiresTaxReview"/>
            </branch>
            <branch name="False">
              <simple name="NoOpTaxReview" nextNode="JoinRequiresTaxReview"/>
            </branch>
            <join name="JoinRequiresTaxReview"/>
          </split>
          <requests name="Award" nextNode="RequiresTravelReview"/>
          <split name="RequiresTravelReview" nextNode="Campus">
            <branch name="True">
              <requests name="Travel" nextNode="JoinRequiresTravelReview"/>
            </branch>
            <branch name="False">
              <simple name="NoOpTravelReview" nextNode="JoinRequiresTravelReview"/>
            </branch>
            <join name="JoinRequiresTravelReview"/>
          </split>
          <requests name="Campus" nextNode="PaymentMethod"/>
          <requests name="PaymentMethod"/>
        </routePath>
      </routePaths>
      <routeNodes>
        <start name="AdHoc"/>
        <split name="PayeeIsPurchaseOrderVendor">
          <type>org.kuali.kfs.sys.document.workflow.SimpleBooleanSplitNode</type>
        </split>
        <split name="RequiresTaxReview">
          <type>org.kuali.kfs.sys.document.workflow.SimpleBooleanSplitNode</type>
        </split>
        <split name="RequiresTravelReview">
          <type>org.kuali.kfs.sys.document.workflow.SimpleBooleanSplitNode</type>
        </split>
        <simple name="NoOpPayee">
          <type>org.kuali.rice.kew.engine.node.NoOpNode</type>
        </simple>
        <simple name="NoOpTaxReview">
          <type>org.kuali.rice.kew.engine.node.NoOpNode</type>
        </simple>
        <simple name="NoOpTravelReview">
          <type>org.kuali.rice.kew.engine.node.NoOpNode</type>
        </simple>
        <join name="JoinPayeeIsPurchaseOrderVendor"/>
        <join name="JoinRequiresTaxReview"/>
        <join name="JoinRequiresTravelReview"/>
        <role name="Purchasing">
          <qualifierResolverClass>org.kuali.rice.kns.workflow.attribute.DataDictionaryQualifierResolver</qualifierResolverClass>
          <activationType>P</activationType>
        </role>
        <role name="Account">
          <qualifierResolverClass>org.kuali.rice.kns.workflow.attribute.DataDictionaryQualifierResolver</qualifierResolverClass>
          <activationType>P</activationType>
        </role>
        <role name="AccountingOrganizationHierarchy">
          <qualifierResolverClass>org.kuali.rice.kns.workflow.attribute.DataDictionaryQualifierResolver</qualifierResolverClass>
          <activationType>P</activationType>
        </role>
        <role name="Tax">
          <qualifierResolverClass>org.kuali.rice.kns.workflow.attribute.DataDictionaryQualifierResolver</qualifierResolverClass>
          <activationType>P</activationType>
        </role>
        <role name="Campus">
          <qualifierResolverClass>org.kuali.rice.kns.workflow.attribute.DataDictionaryQualifierResolver</qualifierResolverClass>
          <activationType>P</activationType>
        </role>
        <role name="Travel">
          <qualifierResolverClass>org.kuali.rice.kns.workflow.attribute.DataDictionaryQualifierResolver</qualifierResolverClass>
          <activationType>P</activationType>
        </role>
        <role name="PaymentMethod">
          <qualifierResolverClass>org.kuali.rice.kns.workflow.attribute.DataDictionaryQualifierResolver</qualifierResolverClass>
          <activationType>P</activationType>
        </role>
        <role name="Award">
          <qualifierResolverClass>org.kuali.rice.kns.workflow.attribute.DataDictionaryQualifierResolver</qualifierResolverClass>
          <activationType>P</activationType>
        </role>
      </routeNodes>
    </documentType>
  </documentTypes>
</data>