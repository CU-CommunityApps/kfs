<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="ParameterDetailType" parent="ParameterDetailType-parentBean" />

  <bean id="ParameterDetailType-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.rice.kns.bo.ParameterDetailType" />
    <property name="inquiryDefinition" >
      <ref bean="ParameterDetailType-inquiryDefinition" />
    </property>
    <property name="lookupDefinition" >
      <ref bean="ParameterDetailType-lookupDefinition" />
    </property>
    <property name="titleAttribute" value="parameterDetailTypeName" />
    <property name="objectLabel" value="Parameter Component" />
    <property name="attributes" >
      <list>
        <ref bean="ParameterDetailType-parameterNamespaceCode" />
        <ref bean="ParameterDetailType-parameterDetailTypeCode" />
        <ref bean="ParameterDetailType-parameterDetailTypeName" />
        <ref bean="ParameterDetailType-parameterNamespace.codeAndDescription" />
        <ref bean="ParameterDetailType-active" />
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->


  <bean id="ParameterDetailType-parameterNamespaceCode" parent="ParameterDetailType-parameterNamespaceCode-parentBean" />

  <bean id="ParameterDetailType-parameterNamespaceCode-parentBean" abstract="true" parent="Namespace-code">
    <property name="name" value="parameterNamespaceCode" />
    <property name="control" >
      <bean parent="SelectControlDefinition"
            p:valuesFinderClass="org.kuali.rice.kns.lookup.keyvalues.NamespaceValuesFinder"
            p:includeKeyInLabel="false" />
    </property>
  </bean>
  <bean id="ParameterDetailType-parameterDetailTypeCode" parent="ParameterDetailType-parameterDetailTypeCode-parentBean" />

  <bean id="ParameterDetailType-parameterDetailTypeCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="parameterDetailTypeCode" />
    <property name="forceUppercase" value="false" />
    <property name="label" value="Parameter Component" />
    <property name="shortLabel" value="Component" />
    <property name="maxLength" value="100" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern"
            p:allowWhitespace="true" />
    </property>
    <property name="required" value="true" />
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="40" />
    </property>
    <property name="summary" value="Code identifying the parameter Component." />
    <property name="description" value="Code identifying the parameter Component." />
  </bean>
  <bean id="ParameterDetailType-parameterDetailTypeName" parent="ParameterDetailType-parameterDetailTypeName-parentBean" />

  <bean id="ParameterDetailType-parameterDetailTypeName-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="parameterDetailTypeName" />
    <property name="forceUppercase" value="false" />
    <property name="label" value="Parameter Component Name" />
    <property name="shortLabel" value="Comp. Nm" />
    <property name="maxLength" value="255" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern"
            p:allowWhitespace="true" />
    </property>
    <property name="required" value="true" />
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="50" />
    </property>
    <property name="summary" value="The name of the parameter Component." />
    <property name="description" value="The name of the parameter Component." />
  </bean>
  <bean id="ParameterDetailType-parameterNamespace.codeAndDescription" parent="ParameterDetailType-parameterNamespace.codeAndDescription-parentBean" />

  <bean id="ParameterDetailType-parameterNamespace.codeAndDescription-parentBean" abstract="true" parent="Namespace-codeAndDescription">
    <property name="name" value="parameterNamespace.codeAndDescription" />
  </bean>
  <bean id="ParameterDetailType-active" parent="ParameterDetailType-active-parentBean" />

  <bean id="ParameterDetailType-active-parentBean" abstract="true" parent="GenericAttributes-activeIndicator">
    <property name="name" value="active" />
  </bean>

<!-- Business Object Inquiry Definition -->


  <bean id="ParameterDetailType-inquiryDefinition" parent="ParameterDetailType-inquiryDefinition-parentBean" />

  <bean id="ParameterDetailType-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
    <property name="title" value="Parameter Component Inquiry" />
    <property name="inquirySections" >
      <list>
        <bean parent="InquirySectionDefinition">
          <property name="title" value="Parameter Component Information" />
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields" >
            <list>
              <bean parent="FieldDefinition"
                    p:attributeName="parameterDetailTypeCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="parameterDetailTypeName" />
              <bean parent="FieldDefinition"
                    p:attributeName="parameterNamespace.codeAndDescription"
                    p:noInquiry="true" />
              <bean parent="FieldDefinition"
                    p:attributeName="active" />
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Business Object Lookup Definition -->


  <bean id="ParameterDetailType-lookupDefinition" parent="ParameterDetailType-lookupDefinition-parentBean" />

  <bean id="ParameterDetailType-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
    <property name="title" value="Parameter Component Lookup" />
    <property name="lookupableID" value="parameterDetailTypeLookupable" />
    <property name="instructions" value="Look up a Parameter Component." />
    <property name="defaultSort" >
      <bean parent="SortDefinition">
        <property name="attributeNames" >
          <list>
            <value>parameterDetailTypeName</value>
          </list>
        </property>
      </bean>
    </property>
    <property name="lookupFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="parameterNamespaceCode" />
        <bean parent="FieldDefinition"
              p:attributeName="parameterDetailTypeName"
              p:noLookup="true" />
        <bean parent="FieldDefinition"
              p:defaultValue="Y"
              p:attributeName="active" />
      </list>
    </property>
    <property name="resultFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="parameterDetailTypeName"
              p:noInquiry="true" />
        <bean parent="FieldDefinition"
              p:attributeName="parameterNamespace.codeAndDescription"
              p:noInquiry="true" />
        <bean parent="FieldDefinition"
              p:attributeName="active" />
      </list>
    </property>
  </bean>
</beans>
