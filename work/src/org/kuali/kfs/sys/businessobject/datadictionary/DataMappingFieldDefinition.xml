<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

	<bean id="DataMappingFieldDefinition"
		parent="DataMappingFieldDefinition-parentBean" />

	<bean id="DataMappingFieldDefinition-parentBean" abstract="true"
		parent="BusinessObjectEntry">
		<property name="businessObjectClass"
			value="org.kuali.kfs.sys.businessobject.DataMappingFieldDefinition" />

		<property name="lookupDefinition">
			<ref bean="DataMappingFieldDefinition-lookupDefinition" />
		</property>
		<property name="titleAttribute" value="fieldName" />
		<property name="objectLabel"
			value="Data Mapping Field Definition" />
		<property name="attributes">
			<list>
				<ref bean="DataMappingFieldDefinition-namespaceCode" />
				<ref bean="DataMappingFieldDefinition-componentClass" />
				<ref
					bean="DataMappingFieldDefinition-functionalFieldDescription.businessObjectProperty.businessObjectComponent.componentLabel" />
				<ref bean="DataMappingFieldDefinition-propertyName" />
				<ref
					bean="DataMappingFieldDefinition-functionalFieldDescription.businessObjectProperty.propertyLabel" />
				<ref bean="DataMappingFieldDefinition-tableName" />
				<ref bean="DataMappingFieldDefinition-fieldName" />
				<ref
					bean="DataMappingFieldDefinition-functionalFieldDescription.functionalFieldDescription" />
			</list>
		</property>
		<property name="relationships">
			<list>
				<bean parent="RelationshipDefinition">
					<property name="objectAttributeName"
						value="functionalFieldDescription.businessObjectProperty.businessObjectComponent" />
					<property name="primitiveAttributes">
						<list>
							<bean parent="PrimitiveAttributeDefinition"
								p:sourceName="componentClass" p:targetName="componentClass" />
						</list>
					</property>
					<property name="supportAttributes">
						<list>
							<bean parent="SupportAttributeDefinition"
								p:sourceName="namespaceCode" p:targetName="namespaceCode" />
							<bean parent="SupportAttributeDefinition"
								p:sourceName="functionalFieldDescription.businessObjectProperty.businessObjectComponent.componentLabel"
								p:targetName="componentLabel" />
						</list>
					</property>
				</bean>
				<bean parent="RelationshipDefinition">
					<property name="objectAttributeName"
						value="functionalFieldDescription.businessObjectProperty" />
					<property name="primitiveAttributes">
						<list>
							<bean parent="PrimitiveAttributeDefinition"
								p:sourceName="componentClass" p:targetName="componentClass" />
							<bean parent="PrimitiveAttributeDefinition"
								p:sourceName="propertyName" p:targetName="propertyName" />
						</list>
					</property>
					<property name="supportAttributes">
						<list>
							<bean parent="SupportAttributeDefinition"
								p:sourceName="namespaceCode" p:targetName="namespaceCode" />
							<bean parent="SupportAttributeDefinition"
								p:sourceName="functionalFieldDescription.businessObjectProperty.businessObjectComponent.componentLabel"
								p:targetName="businessObjectComponent.componentLabel" />
							<bean parent="SupportAttributeDefinition"
								p:sourceName="functionalFieldDescription.businessObjectProperty.propertyLabel"
								p:targetName="propertyLabel" />
						</list>
					</property>
				</bean>
				<bean parent="RelationshipDefinition">
					<property name="objectAttributeName"
						value="functionalFieldDescription" />
					<property name="primitiveAttributes">
						<list>
							<bean parent="PrimitiveAttributeDefinition"
								p:sourceName="componentClass" p:targetName="componentClass" />
							<bean parent="PrimitiveAttributeDefinition"
								p:sourceName="propertyName" p:targetName="propertyName" />
						</list>
					</property>
					<property name="supportAttributes">
						<list>
							<bean parent="SupportAttributeDefinition"
								p:sourceName="namespaceCode" p:targetName="namespaceCode" />
							<bean parent="SupportAttributeDefinition"
								p:sourceName="functionalFieldDescription.businessObjectProperty.businessObjectComponent.componentLabel"
								p:targetName="businessObjectProperty.businessObjectComponent.componentLabel" />
							<bean parent="SupportAttributeDefinition"
								p:sourceName="functionalFieldDescription.businessObjectProperty.propertyLabel"
								p:targetName="businessObjectProperty.propertyLabel" />
						</list>
					</property>
				</bean>
			</list>
		</property>
	</bean>

	<!-- Attribute Definitions -->
	<bean id="DataMappingFieldDefinition-namespaceCode"
		parent="DataMappingFieldDefinition-namespaceCode-parentBean" />

	<bean id="DataMappingFieldDefinition-namespaceCode-parentBean"
		abstract="true" parent="BusinessObjectComponent-namespaceCode" />

	<bean id="DataMappingFieldDefinition-componentClass"
		parent="DataMappingFieldDefinition-componentClass-parentBean" />

	<bean id="DataMappingFieldDefinition-componentClass-parentBean"
		abstract="true" parent="BusinessObjectComponent-componentClass">
		<property name="control">
			<bean parent="HiddenControlDefinition" />
		</property>
	</bean>

	<bean
		id="DataMappingFieldDefinition-functionalFieldDescription.businessObjectProperty.businessObjectComponent.componentLabel"
		parent="DataMappingFieldDefinition-functionalFieldDescription.businessObjectProperty.businessObjectComponent.componentLabel-parentBean" />

	<bean
		id="DataMappingFieldDefinition-functionalFieldDescription.businessObjectProperty.businessObjectComponent.componentLabel-parentBean"
		abstract="true" parent="FunctionalFieldDescription-businessObjectProperty.businessObjectComponent.componentLabel">
		<property name="name"
			value="functionalFieldDescription.businessObjectProperty.businessObjectComponent.componentLabel" />
	</bean>

	<bean id="DataMappingFieldDefinition-propertyName"
		parent="DataMappingFieldDefinition-propertyName-parentBean" />

	<bean id="DataMappingFieldDefinition-propertyName-parentBean"
		abstract="true" parent="BusinessObjectProperty-propertyName" />

	<bean
		id="DataMappingFieldDefinition-functionalFieldDescription.businessObjectProperty.propertyLabel"
		parent="DataMappingFieldDefinition-functionalFieldDescription.businessObjectProperty.propertyLabel-parentBean" />

	<bean
		id="DataMappingFieldDefinition-functionalFieldDescription.businessObjectProperty.propertyLabel-parentBean"
		abstract="true" parent="FunctionalFieldDescription-businessObjectProperty.propertyLabel">
		<property name="name"
			value="functionalFieldDescription.businessObjectProperty.propertyLabel" />
	</bean>

	<bean id="DataMappingFieldDefinition-tableName"
		parent="DataMappingFieldDefinition-tableName-parentBean" />

	<bean id="DataMappingFieldDefinition-tableName-parentBean"
		abstract="true" parent="AttributeDefinition">
		<property name="name" value="tableName" />
		<property name="label" value="Table Name" />
		<property name="maxLength" value="50" />
		<property name="control">
			<bean parent="TextControlDefinition" p:size="30" />
		</property>
	</bean>

	<bean id="DataMappingFieldDefinition-fieldName"
		parent="DataMappingFieldDefinition-fieldName-parentBean" />

	<bean id="DataMappingFieldDefinition-fieldName-parentBean"
		abstract="true" parent="AttributeDefinition">
		<property name="name" value="fieldName" />
		<property name="label" value="Field Name" />
		<property name="maxLength" value="50" />
		<property name="control">
			<bean parent="TextControlDefinition" p:size="30" />
		</property>
	</bean>

	<bean id="DataMappingFieldDefinition-functionalFieldDescription.functionalFieldDescription"
		parent="DataMappingFieldDefinition-functionalFieldDescription.functionalFieldDescription-parentBean" />

	<bean id="DataMappingFieldDefinition-functionalFieldDescription.functionalFieldDescription-parentBean"
		abstract="true" parent="FunctionalFieldDescription-functionalFieldDescription">
		<property name="name" value="functionalFieldDescription.functionalFieldDescription" />
	</bean>		

	<!-- Business Object Lookup Definition -->

	<bean id="DataMappingFieldDefinition-lookupDefinition"
		parent="DataMappingFieldDefinition-lookupDefinition-parentBean" />

	<bean id="DataMappingFieldDefinition-lookupDefinition-parentBean"
		abstract="true" parent="LookupDefinition">
		<property name="lookupableID"
			value="dataMappingFieldDefinitionLookupable" />
		<property name="title"
			value="Data Mapping Field Definition Lookup" />
		<property name="instructions"
			value="Lookup a Data Mapping Field Definition" />
		<property name="lookupFields">
			<list>
				<bean parent="FieldDefinition"
					p:attributeName="namespaceCode" p:noLookup="true"
					p:required="true" />
				<bean parent="FieldDefinition"
					p:attributeName="functionalFieldDescription.businessObjectProperty.businessObjectComponent.componentLabel"
					p:required="true" />
				<bean parent="FieldDefinition"
					p:attributeName="functionalFieldDescription.businessObjectProperty.propertyLabel" />
				<bean parent="FieldDefinition"
					p:attributeName="functionalFieldDescription.functionalFieldDescription" />
				<bean parent="FieldDefinition"
					p:attributeName="tableName" />
				<bean parent="FieldDefinition"
					p:attributeName="fieldName" />
			</list>
		</property>
		<property name="resultFields">
			<list>
				<bean parent="FieldDefinition"
					p:attributeName="namespaceCode" />
				<bean parent="FieldDefinition"
					p:attributeName="functionalFieldDescription.businessObjectProperty.businessObjectComponent.componentLabel" />
				<bean parent="FieldDefinition"
					p:attributeName="functionalFieldDescription.businessObjectProperty.propertyLabel" />
				<bean parent="FieldDefinition"
					p:attributeName="functionalFieldDescription.functionalFieldDescription" />
				<bean parent="FieldDefinition"
					p:attributeName="tableName" />
				<bean parent="FieldDefinition"
					p:attributeName="fieldName" />
			</list>
		</property>
	</bean>
</beans>
