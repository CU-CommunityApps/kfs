<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="TaxState" parent="TaxState-parentBean" />

  <bean id="TaxState-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.kfs.sys.businessobject.TaxState" />
    <property name="inquiryDefinition" >
      <ref bean="TaxState-inquiryDefinition" />
    </property>
    <property name="lookupDefinition" >
      <ref bean="TaxState-lookupDefinition" />
    </property>
    <property name="titleAttribute" value="taxStateCode" />
    <property name="objectLabel" value="taxState" />
    <property name="attributes" >
      <list>
        <ref bean="TaxState-stateCode" />
        <ref bean="TaxState-taxRegionCode" />
        <ref bean="TaxState-active" />
        <ref bean="TaxState-versionNumber" />
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->


  <bean id="TaxState-stateCode" parent="TaxState-stateCode-parentBean" />

  <bean id="TaxState-stateCode-parentBean" abstract="true" parent="State-postalStateCode">
    <property name="name" value="stateCode" />
  </bean>
  
  <bean id="TaxState-taxRegionCode" parent="TaxState-taxRegionCode-parentBean" />

  <bean id="TaxState-taxRegionCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="taxRegionCode" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Tax Region Code" />
    <property name="shortLabel" value="Tax Region" />
    <property name="maxLength" value="10" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern"
            p:allowWhitespace="true" />
    </property>
    <property name="required" value="true" />
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="10" />
    </property>
    <property name="summary" value="Tax Region Code" />
    <property name="description" value="Tax Region Code." />
  </bean>
  
  <bean id="TaxState-active" parent="TaxState-active-parentBean" />

  <bean id="TaxState-active-parentBean" abstract="true" parent="GenericAttributes-activeIndicator">
    <property name="name" value="active" />
  </bean>
  
  <bean id="TaxState-versionNumber" parent="TaxState-versionNumber-parentBean" />

  <bean id="TaxState-versionNumber-parentBean" abstract="true" parent="GenericAttributes-versionNumber">
  </bean>

<!-- Business Object Inquiry Definition -->


  <bean id="TaxState-inquiryDefinition" parent="TaxState-inquiryDefinition-parentBean" />

  <bean id="TaxState-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
    <property name="title" value="TaxState Inquiry" />
    <property name="inquirySections" >
      <list>
        <bean parent="InquirySectionDefinition">
          <property name="title" value="" />
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields" >
            <list>
              <bean parent="FieldDefinition"
                    p:attributeName="stateCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="taxRegionCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="active" />
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Business Object Lookup Definition -->


  <bean id="TaxState-lookupDefinition" parent="TaxState-lookupDefinition-parentBean" />

  <bean id="TaxState-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
    <property name="title" value="TaxState Code Lookup" />
    <property name="instructions" value="Lookup a TaxState" />
    <property name="defaultSort" >
      <bean parent="SortDefinition">
        <property name="attributeNames" >
          <list>
            <value>stateCode</value>
            <value>taxRegionCode</value>
          </list>
        </property>
      </bean>
    </property>
    <property name="lookupFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="stateCode" />
        <bean parent="FieldDefinition"
              p:attributeName="taxRegionCode" />
        <bean parent="FieldDefinition"
              p:attributeName="active" />
      </list>
    </property>
    <property name="resultFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="stateCode" />
        <bean parent="FieldDefinition"
              p:attributeName="taxRegionCode" />
        <bean parent="FieldDefinition"
              p:attributeName="active" />
      </list>
    </property>
  </bean>
</beans>
