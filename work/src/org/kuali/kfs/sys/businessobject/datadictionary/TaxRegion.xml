<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="TaxRegion" parent="TaxRegion-parentBean" />

  <bean id="TaxRegion-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.kfs.sys.businessobject.TaxRegion" />
	<property name="inquiryDefinition" >
      <ref bean="TaxRegion-inquiryDefinition" />
    </property>
    <property name="lookupDefinition" >
      <ref bean="TaxRegion-lookupDefinition" />
    </property>
    <property name="titleAttribute" value="TaxRegionCode" />
    <property name="objectLabel" value="Tax Region" />
    <property name="attributes" >
      <list>
        <ref bean="TaxRegion-taxRegionCode" />
        <ref bean="TaxRegion-taxRegionName" />
        <ref bean="TaxRegion-taxRegionTypeCode" />
        <ref bean="TaxRegion-taxRegionUseTaxIndicator" />
        <ref bean="TaxRegion-chartOfAccountsCode" />
        <ref bean="TaxRegion-accountNumber" />
        <ref bean="TaxRegion-financialObjectCode" />
        <ref bean="TaxRegion-active" />
        <ref bean="TaxRegion-versionNumber" />
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->


  <bean id="TaxRegion-taxRegionCode" parent="TaxRegion-taxRegionCode-parentBean" />

  <bean id="TaxRegion-taxRegionCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="taxRegionCode" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Tax District Code" />
    <property name="shortLabel" value="Tax District Code" />
    <property name="maxLength" value="10" />
    <property name="validationPattern" >
      <bean parent="AlphaNumericValidationPattern" />
    </property>
    <property name="required" value="true" />
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="10" />
    </property>
    <property name="summary" value="Tax District Code" />
    <property name="description" value="The code uniquely identifies a tax district." />
  </bean>
  
  <bean id="TaxRegion-taxRegionName" parent="TaxRegion-taxRegionName-parentBean" />

  <bean id="TaxRegion-taxRegionName-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="taxRegionName" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Tax District Name" />
    <property name="shortLabel" value="Tax District Name" />
    <property name="maxLength" value="100" />
    <property name="validationPattern" >
      <bean parent="AlphaNumericValidationPattern" />
    </property>
    <property name="required" value="true" />
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="40" />
    </property>
    <property name="summary" value="Tax District Name" />
    <property name="description" value="Name of the tax district." />
  </bean>
  
  <bean id="TaxRegion-taxRegionTypeCode" parent="TaxRegion-taxRegionTypeCode-parentBean" />
  
  <bean id="TaxRegion-taxRegionTypeCode-parentBean" abstract="true" parent="TaxRegionType-taxRegionTypeCode" />
  
  <bean id="TaxRegion-taxRegionUseTaxIndicator" parent="TaxRegion-taxRegionUseTaxIndicator-parentBean" />
  
  <bean id="TaxRegion-taxRegionUseTaxIndicator-parentBean" abstract="true" parent="AttributeDefinition">
  	<property name="name" value="taxRegionUseTaxIndicator" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Tax Region Use Tax Indicator" />
    <property name="shortLabel" value="Use Tax Indicator" />
    <property name="maxLength" value="1" />
    <property name="validationPattern" >
      <bean parent="AlphaNumericValidationPattern" />
    </property>
    <property name="required" value="false" />
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="1" />
    </property>
    <property name="summary" value="Tax Region Use Tax Indicator" />
    <property name="description" value="Indicator for Use Tax" />
  </bean>

  <bean id="TaxRegion-chartOfAccountsCode" parent="TaxRegion-chartOfAccountsCode-parentBean" />

  <bean id="TaxRegion-chartOfAccountsCode-parentBean" abstract="true" parent="Chart-chartOfAccountsCode" >
	<property name="label" value="Liability Chart Code" />
  </bean>

  <bean id="TaxRegion-accountNumber" parent="TaxRegion-accountNumber-parentBean" />
  
  <bean id="TaxRegion-accountNumber-parentBean" abstract="true" parent="Account-accountNumber" >
	<property name="label" value="Liability Account Number" />
  </bean>
  
  <bean id="TaxRegion-financialObjectCode" parent="TaxRegion-financialObjectCode-parentBean" />

  <bean id="TaxRegion-financialObjectCode-parentBean" abstract="true" parent="ObjectCode-financialObjectCode" >
  	<property name="label" value="Liability Object Code" />
  </bean>

  <bean id="TaxRegion-active" parent="TaxRegion-active-parentBean" />

  <bean id="TaxRegion-active-parentBean" abstract="true" parent="GenericAttributes-activeIndicator" >
      <property name="name" value="active" />
  </bean>

  <bean id="TaxRegion-versionNumber" parent="TaxRegion-versionNumber-parentBean" />

  <bean id="TaxRegion-versionNumber-parentBean" abstract="true" parent="GenericAttributes-versionNumber" />

<!-- Business Object Lookup Definition -->


  <bean id="TaxRegion-lookupDefinition" parent="TaxRegion-lookupDefinition-parentBean" />

  <bean id="TaxRegion-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
    <property name="title" value="Tax District Lookup" />
    <property name="instructions" value="Lookup a Tax District" />
    <property name="defaultSort" >
      <bean parent="SortDefinition">
        <property name="attributeNames" >
          <list>
            <value>taxRegionCode</value>
            <value>taxRegionName</value>
          </list>
        </property>
      </bean>
    </property>
    <property name="lookupFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="taxRegionCode" />
        <bean parent="FieldDefinition"
              p:attributeName="taxRegionName" />
        <bean parent="FieldDefinition"
              p:attributeName="active" />
      </list>
    </property>
    <property name="resultFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="taxRegionCode"
              p:forceInquiry="true" />
        <bean parent="FieldDefinition"
              p:attributeName="taxRegionName" />
        <bean parent="FieldDefinition"
              p:attributeName="chartOfAccountsCode" />
        <bean parent="FieldDefinition"
              p:attributeName="accountNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="financialObjectCode" />
        <bean parent="FieldDefinition"
              p:attributeName="active" />
           
      </list>
    </property>
  </bean>
  
  <bean id="TaxRegionRate" parent="TaxRegionRate-parentBean" />

  <bean id="TaxRegionRate-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.kfs.sys.businessobject.TaxRegionRate" />
    <property name="objectLabel" value="TaxRegionRate" />
    <property name="attributes" >
      <list>
        <ref bean="TaxRegionRate-taxRegionCode" />
        <ref bean="TaxRegionRate-effectiveDate" />
        <ref bean="TaxRegionRate-taxRate" />
        <ref bean="TaxRegionRate-versionNumber" />
      </list>
    </property>
  </bean>
  
  <bean id="TaxRegionRate-taxRegionCode" parent="TaxRegionRate-taxRegionCode-parentBean" />
  <bean id="TaxRegionRate-taxRegionCode-parentBean" abstract="true" parent="TaxRegion-taxRegionCode">
	<property name="name" value="taxRegionCode" />
	<property name="control" >
      <bean parent="HiddenControlDefinition" />
    </property>
  </bean>

  <bean id="TaxRegionRate-effectiveDate" parent="TaxRegionRate-effectiveDate-parentBean" />
  <bean id="TaxRegionRate-effectiveDate-parentBean" abstract="true" parent="GenericAttributes-genericDate" >
    <property name="name" value="effectiveDate" />
    <property name="label" value="Effective Date" />
    <property name="shortLabel" value="Date" />
  </bean>

  <bean id="TaxRegionRate-taxRate" parent="TaxRegionRate-taxRate-parentBean" />

  <bean id="TaxRegionRate-taxRate-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="taxRate" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Tax Rate" />
    <property name="shortLabel" value="Tax Rate" />
    <property name="maxLength" value="10" />
    <property name="validationPattern" >
      <bean parent="FloatingPointValidationPattern" />
    </property>
    <property name="required" value="true" />
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="10" />
    </property>
    <property name="summary" value="Tax Rate" />
    <property name="description" value="Tax rate." />
  </bean>
  
  <bean id="TaxRegionRate-versionNumber" parent="TaxRegionRate-versionNumber-parentBean" />
  <bean id="TaxRegionRate-versionNumber-parentBean" abstract="true" parent="GenericAttributes-versionNumber" />

  <bean id="TaxRegion-inquiryDefinition" parent="TaxRegion-inquiryDefinition-parentBean" />

  <bean id="TaxRegion-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition" 
  	p:title="Tax District Inquiry">
    <property name="inquirySections" >
      <list>
        <bean parent="InquirySectionDefinition">
          <property name="title" value="Tax District Attributes" />
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields" >
            <list>
              <bean parent="FieldDefinition" p:attributeName="taxRegionCode" p:noInquiry="true" />
              <bean parent="FieldDefinition" p:attributeName="taxRegionName" />
              <bean parent="FieldDefinition" p:attributeName="chartOfAccountsCode" />
              <bean parent="FieldDefinition" p:attributeName="accountNumber" />
              <bean parent="FieldDefinition" p:attributeName="financialObjectCode" />
              <bean parent="FieldDefinition" p:attributeName="active" />
            </list>
          </property>
        </bean>
        <bean parent="InquirySectionDefinition">
          <property name="title" value="Tax District Rates" />
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields" >
            <list>
              <bean parent="InquiryCollectionDefinition">
                <property name="attributeName" value="taxRegionRates" />
                <property name="businessObjectClass" value="org.kuali.kfs.sys.businessobject.TaxRegionRate" />
                <property name="numberOfColumns" value="1" />
                <property name="inquiryFields" >
                  <list>
                    <bean parent="FieldDefinition" p:attributeName="effectiveDate" />
                    <bean parent="FieldDefinition" p:attributeName="taxRate" />
                  </list>
                </property>
              </bean>
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

</beans>
