<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="FinancialSystemDocumentTypeCode" parent="FinancialSystemDocumentTypeCode-parentBean" />

  <bean id="FinancialSystemDocumentTypeCode-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.kfs.sys.businessobject.FinancialSystemDocumentTypeCode" />
    <property name="inquiryDefinition" >
      <ref bean="FinancialSystemDocumentTypeCode-inquiryDefinition" />
    </property>
    <property name="lookupDefinition" >
      <ref bean="FinancialSystemDocumentTypeCode-lookupDefinition" />
    </property>
    <property name="titleAttribute" value="financialSystemDocumentTypeCode" />
    <property name="objectLabel" value="Financial System Document Type Code" />
    <property name="attributes" >
      <list>
        <ref bean="FinancialSystemDocumentTypeCode-financialSystemDocumentTypeCode" />
        <ref bean="FinancialSystemDocumentTypeCode-documentTypeName" />
        <ref bean="FinancialSystemDocumentTypeCode-active" />
        <ref bean="FinancialSystemDocumentTypeCode-transactionScrubberOffsetGenerationIndicator" />
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->

  <bean id="FinancialSystemDocumentTypeCode-financialSystemDocumentTypeCode" parent="FinancialSystemDocumentTypeCode-financialSystemDocumentTypeCode-parentBean" />

  <bean id="FinancialSystemDocumentTypeCode-financialSystemDocumentTypeCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="forceUppercase" value="true" />
    <property name="shortLabel" value="Type Code" />
    <property name="maxLength" value="4" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="required" value="true" />
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="6" />
    </property>
    <property name="name" value="financialSystemDocumentTypeCode" />
    <property name="label" value="Financial System Document Type Code" />
  </bean>
  <bean id="FinancialSystemDocumentTypeCode-documentTypeName" parent="FinancialSystemDocumentTypeCode-documentTypeName-parentBean" />

  <bean id="FinancialSystemDocumentTypeCode-documentTypeName-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="shortLabel" value="Doc Type Name" />
    <property name="maxLength" value="255" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern"
            p:allowWhitespace="true" />
    </property>
    <property name="required" value="true" />
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="40" />
    </property>
    <property name="name" value="documentTypeName" />
    <property name="label" value="Document Type Name" />
  </bean>
  
  <bean id="FinancialSystemDocumentTypeCode-active" parent="FinancialSystemDocumentTypeCode-active-parentBean" />

  <bean id="FinancialSystemDocumentTypeCode-active-parentBean" abstract="true" parent="GenericAttributes-activeIndicator">
    <property name="name" value="active" />
  </bean>

  <bean id="FinancialSystemDocumentTypeCode-transactionScrubberOffsetGenerationIndicator" parent="FinancialSystemDocumentTypeCode-transactionScrubberOffsetGenerationIndicator-parentBean" />

  <bean id="FinancialSystemDocumentTypeCode-transactionScrubberOffsetGenerationIndicator-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="forceUppercase" value="false" />
    <property name="shortLabel" value="Scrubber Offset Generated" />
    <property name="maxLength" value="1" />
    <property name="required" value="true" />
    <property name="control" >
      <bean parent="CheckboxControlDefinition" />
    </property>
    <property name="name" value="transactionScrubberOffsetGenerationIndicator" />
    <property name="label" value="Transaction Scrubber Offset Generation Indicator" />
  </bean>

<!-- Business Object Inquiry Definition -->


  <bean id="FinancialSystemDocumentTypeCode-inquiryDefinition" parent="FinancialSystemDocumentTypeCode-inquiryDefinition-parentBean" />

  <bean id="FinancialSystemDocumentTypeCode-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
    <property name="inquirySections" >
      <list>
        <bean parent="InquirySectionDefinition">
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields" >
            <list>
              <bean parent="FieldDefinition"
                    p:attributeName="financialSystemDocumentTypeCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="documentTypeName" />
              <bean parent="FieldDefinition"
                    p:attributeName="active" />
              <bean parent="FieldDefinition"
                    p:attributeName="transactionScrubberOffsetGenerationIndicator" />
            </list>
          </property>
          <property name="title" value="Financial System Document Type Code" />
        </bean>
      </list>
    </property>
    <property name="title" value="Financial System Document Type Code Inquiry" />
  </bean>

<!-- Business Object Lookup Definition -->

  <bean id="FinancialSystemDocumentTypeCode-lookupDefinition" parent="FinancialSystemDocumentTypeCode-lookupDefinition-parentBean" />

  <bean id="FinancialSystemDocumentTypeCode-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
    <property name="instructions" value="Lookup a Financial System Document Type Code" />
    <property name="defaultSort" >
      <bean parent="SortDefinition">
        <property name="attributeNames" >
          <list>
            <value>financialSystemDocumentTypeCode</value>
          </list>
        </property>
      </bean>
    </property>
    <property name="lookupFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="financialSystemDocumentTypeCode" />
        <bean parent="FieldDefinition"
              p:attributeName="documentTypeName" />
        <bean parent="FieldDefinition"
              p:attributeName="active"
              p:defaultValue="true" />
        <bean parent="FieldDefinition"
              p:attributeName="transactionScrubberOffsetGenerationIndicator" />
      </list>
    </property>
    <property name="resultFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="financialSystemDocumentTypeCode" />
        <bean parent="FieldDefinition"
              p:attributeName="documentTypeName" />
        <bean parent="FieldDefinition"
              p:attributeName="active" />
        <bean parent="FieldDefinition"
              p:attributeName="transactionScrubberOffsetGenerationIndicator" />
      </list>
    </property>
    <property name="title" value="Financial System Document Type Code Lookup" />
  </bean>

</beans>
