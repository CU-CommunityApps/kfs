<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="KimAttributes" parent="KfsKimAttributes-parentBean" />

  <bean id="KfsKimAttributes-parentBean" abstract="true" parent="KimAttributes-parentBean"
        p:businessObjectClass="org.kuali.kfs.sys.identity.KimAttributes"
        p:objectLabel="KFS Core KIM Attributes">
    <property name="attributes">
      <list merge="true">
        <ref bean="KimAttributes-chartOfAccountsCode" />
        <ref bean="KimAttributes-accountNumber" />
        <ref bean="KimAttributes-organizationCode" />
        <ref bean="KimAttributes-descendHierarchy" />
        <ref bean="KimAttributes-fromAmount" />
        <ref bean="KimAttributes-toAmount" />
        <ref bean="KimAttributes-accountingLineOverrideCode" />
        <ref bean="KimAttributes-subFundGroupCode" />
        <ref bean="KimAttributes-purchasingCommodityCode" />
        <ref bean="KimAttributes-contractManagerCode" />
        <ref bean="KimAttributes-customerProfileId" />
        <ref bean="KimAttributes-vendorTypeCode" />
        <ref bean="KimAttributes-contractsAndGrantsAccountResponsibilityId" />
        <ref bean="KimAttributes-disbursementVoucherPaymentMethodCode" />
        <ref bean="KimAttributes-subAccountNumber" />
        <ref bean="KimAttributes-researchRiskTypeCode" />
        <ref bean="KimAttributes-QuestionType-questionTypeCode" />
        <ref bean="KimAttributes-QuestionType-active" />
        <ref bean="KimAttributes-QuestionType-questionTypeDescription" />
        <ref bean="KimAttributes-QuestionType-questionTypeSortNumber" />
        <ref bean="KimAttributes-QuestionType-questionTypeWorkgroupName" />
        <ref bean="KimAttributes-QuestionType-questionTypeNotificationValue" />
        <ref bean="KimAttributes-sensitiveDataCode" />
      </list>
    </property>
  </bean>

  <bean id="KimAttributes-chartOfAccountsCode" parent="Chart-chartOfAccountsCode" />
  <bean id="KimAttributes-accountNumber" parent="Account-accountNumber" />
  <bean id="KimAttributes-organizationCode" parent="Organization-organizationCode" />

  <bean id="KimAttributes-accountingLineOverrideCode" parent="KimAttributes-accountingLineOverrideCode-parentBean" />
  <bean id="KimAttributes-accountingLineOverrideCode-parentBean" abstract="true" parent="SourceAccountingLine-overrideCode" 
  	p:name="accountingLineOverrideCode" 
	p:label="Accounting Line Override Code"
    p:shortLabel="Override Code" />

  <bean id="KimAttributes-subFundGroupCode" parent="SubFundGroup-subFundGroupCode" />
  <bean id="KimAttributes-purchasingCommodityCode" parent="CommodityCode-purchasingCommodityCode" />
  <bean id="KimAttributes-contractManagerCode" parent="ContractManager-contractManagerCode" />

  <bean id="KimAttributes-customerProfileId" parent="KimAttributes-customerProfileId-parentBean" />
  <bean id="KimAttributes-customerProfileId-parentBean" abstract="true" parent="CustomerProfile-id"
  	p:name="customerProfileId" 
	p:label="Customer Profile Id"
    p:shortLabel="Id" />


  <bean id="KimAttributes-vendorTypeCode" parent="VendorType-vendorTypeCode" />
  <bean id="KimAttributes-contractsAndGrantsAccountResponsibilityId" parent="Account-contractsAndGrantsAccountResponsibilityId" />

  <bean id="KimAttributes-disbursementVoucherPaymentMethodCode" parent="KimAttributes-disbursementVoucherPaymentMethodCode-parentBean" />
  <bean id="KimAttributes-disbursementVoucherPaymentMethodCode-parentBean" abstract="true" parent="DisbursementVoucherDocument-disbVchrPaymentMethodCode"
  	p:name="disbursementVoucherPaymentMethodCode" />

  <bean id="KimAttributes-subAccountNumber" parent="SubAccount-subAccountNumber" />

  <bean id="KimAttributes-fromAmount" parent="KimAttributes-fromAmount-parentBean" />
  <bean id="KimAttributes-fromAmount-parentBean" abstract="true" parent="FinancialSystemDocumentHeader-financialDocumentTotalAmount"
  	p:name="fromAmount" 
	p:label="From Amount"
    p:shortLabel="From Amount" />

  <bean id="KimAttributes-toAmount" parent="KimAttributes-toAmount-parentBean" />
  <bean id="KimAttributes-toAmount-parentBean" abstract="true" parent="FinancialSystemDocumentHeader-financialDocumentTotalAmount" 
  	p:name="toAmount" 
	p:label="To Amount"
    p:shortLabel="To Amount" />

  <bean id="KimAttributes-descendHierarchy" parent="KimAttributes-descendHierarchy-parentBean" />
  <bean id="KimAttributes-descendHierarchy-parentBean" abstract="true" parent="GenericAttributes-genericBoolean"
    p:name="descendHierarchy"
    p:label="Descends Hierarchy"
    p:shortLabel="Descends Hierarchy" />

  <bean id="KimAttributes-researchRiskTypeCode" parent="ResearchRiskType-researchRiskTypeCode" />
  <bean id="KimAttributes-QuestionType-questionTypeCode" parent="QuestionType-questionTypeCode" />
  <bean id="KimAttributes-QuestionType-active" parent="QuestionType-active" />
  <bean id="KimAttributes-QuestionType-questionTypeDescription" parent="QuestionType-questionTypeDescription" />
  <bean id="KimAttributes-QuestionType-questionTypeSortNumber" parent="QuestionType-questionTypeSortNumber" />
  <bean id="KimAttributes-QuestionType-questionTypeWorkgroupName" parent="QuestionType-questionTypeWorkgroupName" />
  <bean id="KimAttributes-QuestionType-questionTypeNotificationValue" parent="QuestionType-questionTypeNotificationValue" />
  <bean id="KimAttributes-sensitiveDataCode" parent="SensitiveData-sensitiveDataCode" />

</beans>