<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="TaxPostalCode" parent="TaxPostalCode-parentBean" />

  <bean id="TaxPostalCode-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.kfs.sys.businessobject.TaxPostalCode" />
    <property name="inquiryDefinition" >
      <ref bean="TaxPostalCode-inquiryDefinition" />
    </property>
    <property name="lookupDefinition" >
      <ref bean="TaxPostalCode-lookupDefinition" />
    </property>
    <property name="titleAttribute" value="taxPostalCode" />
    <property name="objectLabel" value="TaxPostalCode" />
    <property name="attributes" >
      <list>
        <ref bean="TaxPostalCode-postalCode" />
        <ref bean="TaxPostalCode-taxRegionCode" />
        <ref bean="TaxPostalCode-active" />
        <ref bean="TaxPostalCode-versionNumber" />
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->


 <bean id="TaxPostalCode-postalCode" parent="TaxPostalCode-postalCode-parentBean" />

  <bean id="TaxPostalCode-postalCode-parentBean" abstract="true" parent="PostalZipCode-postalZipCode">
    <property name="name" value="postalCode" />
  </bean>
  
  <bean id="TaxPostalCode-taxRegionCode" parent="TaxPostalCode-taxRegionCode-parentBean" />

  <bean id="TaxPostalCode-taxRegionCode-parentBean" abstract="true" parent="TaxState-taxRegionCode">
    
  </bean>
  
  <bean id="TaxPostalCode-active" parent="TaxPostalCode-active-parentBean" />

  <bean id="TaxPostalCode-active-parentBean" abstract="true" parent="GenericAttributes-activeIndicator">
    <property name="name" value="active" />
  </bean>
  
  <bean id="TaxPostalCode-versionNumber" parent="TaxPostalCode-versionNumber-parentBean" />

  <bean id="TaxPostalCode-versionNumber-parentBean" abstract="true" parent="GenericAttributes-versionNumber">
  </bean>

<!-- Business Object Inquiry Definition -->


  <bean id="TaxPostalCode-inquiryDefinition" parent="TaxPostalCode-inquiryDefinition-parentBean" />

  <bean id="TaxPostalCode-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
    <property name="title" value="TaxPostalCode Inquiry" />
    <property name="inquirySections" >
      <list>
        <bean parent="InquirySectionDefinition">
          <property name="title" value="" />
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields" >
            <list>
              <bean parent="FieldDefinition"
                    p:attributeName="postalCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="taxRegionCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="active" />
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Business Object Lookup Definition -->


  <bean id="TaxPostalCode-lookupDefinition" parent="TaxPostalCode-lookupDefinition-parentBean" />

  <bean id="TaxPostalCode-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
    <property name="title" value="TaxPostalCode Code Lookup" />
    <property name="instructions" value="Lookup a TaxPostalCode" />
    <property name="defaultSort" >
      <bean parent="SortDefinition">
        <property name="attributeNames" >
          <list>
            <value>postalCode</value>
            <value>taxRegionCode</value>
          </list>
        </property>
      </bean>
    </property>
    <property name="lookupFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="postalCode" />
        <bean parent="FieldDefinition"
              p:attributeName="taxRegionCode" />
        <bean parent="FieldDefinition"
              p:attributeName="active" />
      </list>
    </property>
    <property name="resultFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="postalCode" />
        <bean parent="FieldDefinition"
              p:attributeName="taxRegionCode" />
        <bean parent="FieldDefinition"
              p:attributeName="active" />
      </list>
    </property>
  </bean>
</beans>
