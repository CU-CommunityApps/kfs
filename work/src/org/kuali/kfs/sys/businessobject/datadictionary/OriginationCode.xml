    "../../../core/datadictionary/dataDictionary-1_0.dtd">
<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.kfs.bo.OriginationCode">


        <inquiry>
            <title>Origination Code Inquiry</title>

            <inquirySections><inquirySection title="Header Text"><inquiryFields>
                <field attributeName="financialSystemOriginationCode" />
                <field attributeName="nextFinancialDocumentNumber" />
                <field attributeName="financialSystemServerName" />
                <field attributeName="financialSystemDatabaseName" />
                <field attributeName="financialSystemDatabaseDesc" />
                <field attributeName="nextCapitalAssetNumber" />
                <field attributeName="nextNonCapitalAssetNumber" />
                <field attributeName="nextCgProposalNumber" />
                <field attributeName="nextContractsAndGrantsAgencyNumber" />
                <field attributeName="nextContractsAndGrantsSubcontractorNumber" />
                <field attributeName="nextDisbVchrPayeeIdNbr" />
                <field attributeName="nextFdocAttachmentIdentifier" />
                <field attributeName="acctRcvbNextCustomerNumber" />
                <field attributeName="financialDocumentNextItemIdentifier" />
            </inquiryFields></inquirySection></inquirySections>
        </inquiry>

        <lookup>
            <title>Origination Code Lookup</title>

            <instructions>Lookup an Origination Code</instructions>

            <defaultSort attributeName="financialSystemOriginationCode" sortAscending="true" />

            <lookupFields>
                <lookupField attributeName="financialSystemOriginationCode" required="false" />
                <lookupField attributeName="nextFinancialDocumentNumber" required="false" />
                <lookupField attributeName="financialSystemServerName" required="false" />
                <lookupField attributeName="financialSystemDatabaseName" required="false" />
                <lookupField attributeName="financialSystemDatabaseDesc" required="false" />
                <lookupField attributeName="nextCapitalAssetNumber" required="false" />
                <lookupField attributeName="nextNonCapitalAssetNumber" required="false" />
                <lookupField attributeName="nextCgProposalNumber" required="false" />
                <lookupField attributeName="nextContractsAndGrantsAgencyNumber" required="false" />
                <lookupField attributeName="nextContractsAndGrantsSubcontractorNumber" required="false" />
                <lookupField attributeName="nextDisbVchrPayeeIdNbr" required="false" />
                <lookupField attributeName="nextFdocAttachmentIdentifier" required="false" />
                <lookupField attributeName="acctRcvbNextCustomerNumber" required="false" />
                <lookupField attributeName="financialDocumentNextItemIdentifier" required="false" />
            </lookupFields>

            <resultFields>
                <field attributeName="financialSystemOriginationCode" />
                <field attributeName="nextFinancialDocumentNumber" />
                <field attributeName="financialSystemServerName" />
                <field attributeName="financialSystemDatabaseName" />
                <field attributeName="financialSystemDatabaseDesc" />
                <field attributeName="nextCapitalAssetNumber" />
                <field attributeName="nextNonCapitalAssetNumber" />
                <field attributeName="nextCgProposalNumber" />
                <field attributeName="nextContractsAndGrantsAgencyNumber" />
                <field attributeName="nextContractsAndGrantsSubcontractorNumber" />
                <field attributeName="nextDisbVchrPayeeIdNbr" />
                <field attributeName="nextFdocAttachmentIdentifier" />
                <field attributeName="acctRcvbNextCustomerNumber" />
                <field attributeName="financialDocumentNextItemIdentifier" />
            </resultFields>
        </lookup>

        <titleAttribute>financialSystemOriginationCode</titleAttribute>

        <attributes>

            <attribute name="financialSystemOriginationCode" forceUppercase="true">
                <label>Origination Code</label>
                <shortLabel>Origin Code</shortLabel>

                <maxLength>2</maxLength>
                <validationPattern>
                    <alphaNumeric />
                </validationPattern>
                <required>true</required>
                <control>
                    <text size="2" />
                </control>
            </attribute>

            <attribute name="nextFinancialDocumentNumber" forceUppercase="false">
                <label>Next Document Number</label>
                <shortLabel>Next Doc No</shortLabel>

                <maxLength>9</maxLength>
                <validationPattern>
                    <alphaNumeric />
                </validationPattern>
                <required>true</required>
                <control>
                    <text size="9" />
                </control>
            </attribute>

            <attribute name="financialSystemServerName" forceUppercase="false">
                <label>Server Name</label>
                <shortLabel>Server Name</shortLabel>

                <maxLength>20</maxLength>
                <validationPattern>
                    <alphaNumeric />
                </validationPattern>
                <required>true</required>
                <control>
                    <text size="20" />
                </control>
            </attribute>

            <attribute name="financialSystemDatabaseName" forceUppercase="false">
                <label>Database Name</label>
                <shortLabel>DB Name</shortLabel>

                <maxLength>15</maxLength>
                <validationPattern>
                    <alphaNumeric />
                </validationPattern>
                <required>true</required>
                <control>
                    <text size="15" />
                </control>
            </attribute>

            <attribute name="financialSystemDatabaseDesc" forceUppercase="false">
                <label>Database Description</label>
                <shortLabel>DB Desc</shortLabel>

                <maxLength>40</maxLength>
                <validationPattern>
                    <alphaNumeric allowWhitespace="true"/>
                </validationPattern>

                <control>
                    <text size="40" />
                </control>
            </attribute>

            <attribute name="nextCapitalAssetNumber" forceUppercase="false">
                <label>Next Capital Asset Number</label>
                <shortLabel>Next Cap Asset No</shortLabel>

                <maxLength>12</maxLength>
                <validationPattern>
                    <numeric />
                </validationPattern>

                <control>
                    <text size="12" />
                </control>
            </attribute>

            <attribute name="nextNonCapitalAssetNumber" forceUppercase="false">
                <label>Next Non-Capital Asset Number</label>
                <shortLabel>Next Non-Cap Asset No</shortLabel>

                <maxLength>12</maxLength>
                <validationPattern>
                    <numeric />
                </validationPattern>

                <control>
                    <text size="12" />
                </control>
            </attribute>

            <attribute name="nextCgProposalNumber" forceUppercase="false">
                <label>Next CG Proposal Number</label>
                <shortLabel>Next CG Proposal No</shortLabel>

                <maxLength>12</maxLength>
                <validationPattern>
                    <numeric />
                </validationPattern>

                <control>
                    <text size="12" />
                </control>
            </attribute>

            <attribute name="nextContractsAndGrantsAgencyNumber" forceUppercase="false">
                <label>Next Contract and Grants Agency Number</label>
                <shortLabel>Next CG Agency No</shortLabel>

                <maxLength>5</maxLength>
                <validationPattern>
                    <alphaNumeric />
                </validationPattern>

                <control>
                    <text size="5" />
                </control>
            </attribute>

            <attribute name="nextContractsAndGrantsSubcontractorNumber" forceUppercase="false">
                <label>Next Contract and Grants Subcontractor Number</label>
                <shortLabel>Next CG Agency Subcontractor No</shortLabel>

                <maxLength>5</maxLength>
                <validationPattern>
                    <alphaNumeric />
                </validationPattern>

                <control>
                    <text size="5" />
                </control>
            </attribute>

            <attribute name="nextDisbVchrPayeeIdNbr" forceUppercase="false">
                <label>Next Disbursement Voucher Payee Id Number</label>
                <shortLabel>Next DV Payee Id No</shortLabel>

                <maxLength>12</maxLength>
                <validationPattern>
                    <numeric />
                </validationPattern>

                <control>
                    <text size="12" />
                </control>
            </attribute>

            <attribute name="nextFdocAttachmentIdentifier" forceUppercase="false">
                <label>Next Attachment Identifier</label>
                <shortLabel>Next Atchmt Id</shortLabel>

                <maxLength>12</maxLength>
                <validationPattern>
                    <numeric />
                </validationPattern>

                <control>
                    <text size="12" />
                </control>
            </attribute>

            <attribute name="acctRcvbNextCustomerNumber" forceUppercase="false">
                <label>Next Accounts Receivable Customer Number</label>
                <shortLabel>Next AR Cust No</shortLabel>

                <maxLength>12</maxLength>
                <validationPattern>
                    <numeric />
                </validationPattern>

                <control>
                    <text size="12" />
                </control>
            </attribute>

            <attribute name="financialDocumentNextItemIdentifier" forceUppercase="false">
                <label>Next Item Identifier</label>
                <shortLabel>Next Item Id</shortLabel>

                <maxLength>12</maxLength>
                <validationPattern>
                    <numeric />
                </validationPattern>

                <control>
                    <text size="12" />
                </control>
            </attribute>

            <attributeReference name="versionNumber" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="versionNumber" />

        </attributes>
