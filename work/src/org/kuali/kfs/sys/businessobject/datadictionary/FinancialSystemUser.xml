<!-- KFS User Maintenance Document -->

  <bean id="FinancialSystemUserMaintenanceDocument" parent="FinancialSystemUserMaintenanceDocument-parentBean" />

  <bean id="FinancialSystemUserMaintenanceDocument-parentBean" abstract="true" parent="MaintenanceDocumentEntry">
    <property name="businessObjectClass" value="org.kuali.kfs.bo.FinancialSystemUser" />
    <property name="maintainableClass" value="org.kuali.kfs.maintenance.FinancialSystemUserMaintainable" />
    <property name="maintainableSections" >
      <list>
        <ref bean="FinancialSystemUserMaintenanceDocument-UniversalUserSection" />
        <ref bean="FinancialSystemUserMaintenanceDocument-FinancialSystemUserSection" />
        <ref bean="FinancialSystemUserMaintenanceDocument-PrimaryOrganizationsSection" />
        <ref bean="FinancialSystemUserMaintenanceDocument-OrganizationSecuritySection" />
      </list>
    </property>
    <property name="defaultExistenceChecks" >
      <list>
        <bean parent="ReferenceDefinition" p:attributeName="chartOfAccounts" p:attributeToHighlightOnFail="chartOfAccountsCode" />
        <bean parent="ReferenceDefinition" p:attributeName="organization" p:attributeToHighlightOnFail="organizationCode" />
      </list>
    </property>
    <property name="lockingKeys">
      <list>
        <value>personUniversalIdentifier</value>
      </list>
    </property>
    <property name="label" value="Financial System User" />
    <property name="shortLabel" value="Financial System User" />
