<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="FinancialSystemUser" parent="FinancialSystemUser-parentBean" />

  <bean id="FinancialSystemUser-parentBean" abstract="true" parent="UniversalUser">
    <property name="businessObjectClass" value="org.kuali.kfs.bo.FinancialSystemUser" />
    <property name="inquiryDefinition" ref="FinancialSystemUser-inquiryDefinition" />
    <property name="lookupDefinition" ref="FinancialSystemUser-lookupDefinition" />
    <property name="attributes" >
      <list merge="false">
        <ref bean="FinancialSystemUser-personUniversalIdentifier" />
        <ref bean="FinancialSystemUser-activeFinancialSystemUser" />
        <ref bean="FinancialSystemUser-chartOfAccountsCode" />
        <ref bean="FinancialSystemUser-organizationCode" />
        <ref bean="FinancialSystemUser-versionNumber" />
        <ref bean="FinancialSystemUser-embeddedUniversalUser.personUserIdentifier" />
        <ref bean="FinancialSystemUser-embeddedUniversalUser.personPayrollIdentifier" />
        <ref bean="FinancialSystemUser-embeddedUniversalUser.personName" />
        <ref bean="FinancialSystemUser-embeddedUniversalUser.employeeStatusCode" />
        <ref bean="FinancialSystemUser-embeddedUniversalUser.employeeTypeCode" />
        <ref bean="FinancialSystemUser-embeddedUniversalUser.student" />
        <ref bean="FinancialSystemUser-embeddedUniversalUser.staff" />
        <ref bean="FinancialSystemUser-embeddedUniversalUser.faculty" />
        <ref bean="FinancialSystemUser-embeddedUniversalUser.affiliate" />
        <ref bean="FinancialSystemUser-embeddedUniversalUser.primaryDepartmentCode" />
        <ref bean="FinancialSystemUser-embeddedUniversalUser.personEmailAddress" />
        <ref bean="FinancialSystemUser-embeddedUniversalUser.personFirstName" />
        <ref bean="FinancialSystemUser-embeddedUniversalUser.personLastName" />
        <ref bean="FinancialSystemUser-embeddedUniversalUser.personMiddleName" />
        <ref bean="FinancialSystemUser-embeddedUniversalUser.personCampusAddress" />
        <ref bean="FinancialSystemUser-embeddedUniversalUser.personLocalPhoneNumber" />
        <ref bean="FinancialSystemUser-embeddedUniversalUser.personBaseSalaryAmount" />
        <ref bean="FinancialSystemUser-embeddedUniversalUser.personTaxIdentifier" />
        <ref bean="FinancialSystemUser-embeddedUniversalUser.personTaxIdentifierTypeCode" />
        <ref bean="FinancialSystemUser-embeddedUniversalUser.financialSystemsEncryptedPasswordText" />
        <ref bean="FinancialSystemUser-embeddedUniversalUser.campusCode" />
        <ref bean="FinancialSystemUser-embeddedUniversalUser.versionNumber" />
      </list>
    </property>
    <property name="relationships" >
      <list>
        <bean parent="RelationshipDefinition">
          <property name="objectAttributeName" value="embeddedUniversalUser" />
          <property name="primitiveAttributes" >
            <list>
              <bean parent="PrimitiveAttributeDefinition"
                    p:sourceName="personUniversalIdentifier"
                    p:targetName="personUniversalIdentifier" />
            </list>
          </property>
          <property name="supportAttributes" >
            <list>
              <bean parent="SupportAttributeDefinition"
                    p:identifier="true"
                    p:sourceName="embeddedUniversalUser.personUserIdentifier"
                    p:targetName="personUserIdentifier" />
              <bean parent="SupportAttributeDefinition"
                    p:sourceName="embeddedUniversalUser.personName"
                    p:targetName="personName" />
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

