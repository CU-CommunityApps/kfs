<!-- Business Object Inquiry Definition -->


  <bean id="FinancialSystemUser-inquiryDefinition" parent="FinancialSystemUser-inquiryDefinition-parentBean" />

  <!-- Override the UU inquiry to force it to use the KFS User inquirable class -->
  <bean id="UniversalUser-inquiryDefinition" parent="UniversalUser-inquiryDefinition-parentBean"
    p:title="Financial System User Inquiry"
    p:inquirableClass="org.kuali.kfs.inquiry.FinancialSystemUserInquirableImpl">  
  </bean>

  <bean id="FinancialSystemUser-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition"
  	p:title="Financial System User Inquiry"
    p:inquirableClass="org.kuali.kfs.inquiry.FinancialSystemUserInquirableImpl">
    <!-- merge in additional sections to UU sections -->
    <property name="inquirySections" >
      <list>
        <bean parent="InquirySectionDefinition">
          <property name="title" value="Universal User Attributes" />
          <property name="numberOfColumns" value="2" />
          <property name="inquiryFields" >
            <list>
              <bean parent="FieldDefinition" p:attributeName="embeddedUniversalUser.personUserIdentifier" p:noInquiry="true" />
              <bean parent="FieldDefinition" p:attributeName="personUniversalIdentifier" p:noInquiry="true" />
              <bean parent="FieldDefinition" p:attributeName="embeddedUniversalUser.personName" p:noInquiry="true" />
              <bean parent="FieldDefinition" p:attributeName="embeddedUniversalUser.personPayrollIdentifier" p:noInquiry="true" />
              <bean parent="FieldDefinition" p:attributeName="embeddedUniversalUser.staff" p:noInquiry="true" />
              <bean parent="FieldDefinition" p:attributeName="embeddedUniversalUser.faculty" p:noInquiry="true" />
              <bean parent="FieldDefinition" p:attributeName="embeddedUniversalUser.student" p:noInquiry="true" />
              <bean parent="FieldDefinition" p:attributeName="embeddedUniversalUser.affiliate" p:noInquiry="true" />
              <bean parent="FieldDefinition" p:attributeName="embeddedUniversalUser.employeeStatusCode" p:noInquiry="true" />
              <bean parent="FieldDefinition" p:attributeName="embeddedUniversalUser.employeeTypeCode" p:noInquiry="true" />
              <bean parent="FieldDefinition" p:attributeName="embeddedUniversalUser.campusCode" />
              <bean parent="FieldDefinition" p:attributeName="embeddedUniversalUser.personEmailAddress" p:noInquiry="true" />
            </list>
          </property>
        </bean>
        <bean parent="InquirySectionDefinition">
          <property name="title" value="Financial System User Attributes" />
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields" >
            <list>
              <bean parent="FieldDefinition" p:attributeName="activeFinancialSystemUser" />
              <bean parent="FieldDefinition" p:attributeName="chartOfAccountsCode" />
              <bean parent="FieldDefinition" p:attributeName="organizationCode" />
            </list>
          </property>
        </bean>
        <bean parent="InquirySectionDefinition">
          <property name="title" value="Primary Organizations" />
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields" >
            <list>
              <bean parent="InquiryCollectionDefinition">
                <property name="attributeName" value="primaryOrganizations" />
                <property name="businessObjectClass" value="org.kuali.kfs.bo.FinancialSystemUserPrimaryOrganization" />
                <property name="numberOfColumns" value="1" />
                <property name="inquiryFields" >
                  <list>
                    <bean parent="FieldDefinition" p:attributeName="moduleId" />
                    <bean parent="FieldDefinition" p:attributeName="chartOfAccountsCode" />
                    <bean parent="FieldDefinition" p:attributeName="organizationCode" />
                    <bean parent="FieldDefinition" p:attributeName="active" />
                  </list>
                </property>
              </bean>
            </list>
          </property>
        </bean>
        <bean parent="InquirySectionDefinition">
          <property name="title" value="Organization Security" />
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields" >
            <list>
              <bean parent="InquiryCollectionDefinition">
                <property name="attributeName" value="organizationSecurity" />
                <property name="businessObjectClass" value="org.kuali.kfs.bo.FinancialSystemUserOrganizationSecurity" />
                <property name="numberOfColumns" value="1" />
                <property name="inquiryFields" >
                  <list>
                    <bean parent="FieldDefinition" p:attributeName="moduleId" />
                    <bean parent="FieldDefinition" p:attributeName="chartOfAccountsCode" />
                    <bean parent="FieldDefinition" p:attributeName="organizationCode" />
                    <bean parent="FieldDefinition" p:attributeName="descendOrgHierarchy" />
                    <bean parent="FieldDefinition" p:attributeName="active" />
                  </list>
                </property>
              </bean>
            </list>
          </property>
        </bean>
    
      </list>
    </property>
  </bean>

<!-- Business Object Lookup Definition -->

  <bean id="FinancialSystemUser-lookupDefinition" parent="FinancialSystemUser-lookupDefinition-parentBean" />

  <bean id="FinancialSystemUser-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
    <property name="title" value="Financial System User Lookup" />
    <property name="lookupableID" value="financialSystemUserLookupable" />
    <property name="instructions" value="Lookup a User" />
    <property name="defaultSort" >
      <bean parent="SortDefinition">
        <property name="attributeName" value="embeddedUniversalUser.personUserIdentifier" />
      </bean>
    </property>
    <property name="lookupFields" >
      <list>
        <bean parent="FieldDefinition" p:attributeName="embeddedUniversalUser.personUserIdentifier" p:noLookup="true"/>
        <bean parent="FieldDefinition" p:attributeName="embeddedUniversalUser.personLastName" p:noLookup="true" />
        <bean parent="FieldDefinition" p:attributeName="embeddedUniversalUser.personFirstName" p:noLookup="true" />
        <bean parent="FieldDefinition" p:attributeName="personUniversalIdentifier" p:noLookup="true" />
        <bean parent="FieldDefinition" p:attributeName="embeddedUniversalUser.personPayrollIdentifier" p:noLookup="true" />
        <bean parent="FieldDefinition" p:attributeName="embeddedUniversalUser.personEmailAddress" p:noLookup="true" />
        <bean parent="FieldDefinition" p:attributeName="embeddedUniversalUser.campusCode" />
        <bean parent="FieldDefinition" p:attributeName="embeddedUniversalUser.staff" p:noLookup="true" />
        <bean parent="FieldDefinition" p:attributeName="embeddedUniversalUser.faculty" p:noLookup="true" />
        <bean parent="FieldDefinition" p:attributeName="embeddedUniversalUser.student" p:noLookup="true" />
        <bean parent="FieldDefinition" p:attributeName="embeddedUniversalUser.affiliate" p:noLookup="true" />
        <bean parent="FieldDefinition" p:attributeName="embeddedUniversalUser.employeeStatusCode" />
        <bean parent="FieldDefinition" p:attributeName="embeddedUniversalUser.employeeTypeCode" />
        <bean parent="FieldDefinition" p:attributeName="chartOfAccountsCode" />
        <bean parent="FieldDefinition" p:attributeName="organizationCode" />
        <bean parent="FieldDefinition" p:attributeName="activeFinancialSystemUser" />
      </list>
    </property>
    <property name="resultFields" >
      <list>
        <bean parent="FieldDefinition" p:attributeName="embeddedUniversalUser.personUserIdentifier" p:noInquiry="true" />
        <bean parent="FieldDefinition" p:attributeName="personUniversalIdentifier" />
        <bean parent="FieldDefinition" p:attributeName="embeddedUniversalUser.personPayrollIdentifier" p:noInquiry="true" />
        <bean parent="FieldDefinition" p:attributeName="embeddedUniversalUser.personName" p:noInquiry="true" />
        <bean parent="FieldDefinition" p:attributeName="embeddedUniversalUser.campusCode" p:noInquiry="true" />
        <bean parent="FieldDefinition" p:attributeName="chartOfAccountsCode" />
        <bean parent="FieldDefinition" p:attributeName="organizationCode" />
        <bean parent="FieldDefinition" p:attributeName="embeddedUniversalUser.employeeStatusCode" />
        <bean parent="FieldDefinition" p:attributeName="embeddedUniversalUser.employeeTypeCode" />
        <bean parent="FieldDefinition" p:attributeName="activeFinancialSystemUser" />
      </list>
    </property>
  </bean>

