<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="TaxDistrict" parent="TaxDistrict-parentBean" />

  <bean id="TaxDistrict-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.kfs.sys.businessobject.TaxDistrict" />
<!--<property name="inquiryDefinition" >
      <ref bean="TaxDistrict-inquiryDefinition" />
    </property> -->
    <property name="lookupDefinition" >
      <ref bean="TaxDistrict-lookupDefinition" />
    </property>
    <property name="objectLabel" value="TaxDistrict" />
    <property name="attributes" >
      <list>
        <ref bean="TaxDistrict-taxDistrictCode" />
        <ref bean="TaxDistrict-taxDistrictName" />
        <ref bean="TaxDistrict-chartOfAccountsCode" />
        <ref bean="TaxDistrict-accountNumber" />
        <ref bean="TaxDistrict-financialObjectCode" />
        <ref bean="TaxDistrict-active" />
        <ref bean="TaxDistrict-versionNumber" />
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->


  <bean id="TaxDistrict-taxDistrictCode" parent="TaxDistrict-taxDistrictCode-parentBean" />

  <bean id="TaxDistrict-taxDistrictCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="taxDistrictCode" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Tax District Code" />
    <property name="shortLabel" value="Tax District Code" />
    <property name="maxLength" value="10" />
    <property name="validationPattern" >
      <bean parent="AlphaNumericValidationPattern" />
    </property>
    <property name="required" value="true" />
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="10" />
    </property>
    <property name="summary" value="Tax District Code" />
    <property name="description" value="The code uniquely identifies a tax district." />
  </bean>
  
  <bean id="TaxDistrict-taxDistrictName" parent="TaxDistrict-taxDistrictName-parentBean" />

  <bean id="TaxDistrict-taxDistrictName-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="taxDistrictName" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Tax District Name" />
    <property name="shortLabel" value="Tax District Name" />
    <property name="maxLength" value="100" />
    <property name="validationPattern" >
      <bean parent="AlphaNumericValidationPattern" />
    </property>
    <property name="required" value="true" />
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="40" />
    </property>
    <property name="summary" value="Tax District Name" />
    <property name="description" value="Name of the tax district." />
  </bean>

  <bean id="TaxDistrict-chartOfAccountsCode" parent="TaxDistrict-chartOfAccountsCode-parentBean" />

  <bean id="TaxDistrict-chartOfAccountsCode-parentBean" abstract="true" parent="Chart-chartOfAccountsCode" />

  <bean id="TaxDistrict-accountNumber" parent="TaxDistrict-accountNumber-parentBean" />
  
  <bean id="TaxDistrict-accountNumber-parentBean" abstract="true" parent="Account-accountNumber" />
  
  <bean id="TaxDistrict-financialObjectCode" parent="TaxDistrict-financialObjectCode-parentBean" />

  <bean id="TaxDistrict-financialObjectCode-parentBean" abstract="true" parent="ObjectCode-financialObjectCode" />

  <bean id="TaxDistrict-active" parent="TaxDistrict-active-parentBean" />

  <bean id="TaxDistrict-active-parentBean" abstract="true" parent="GenericAttributes-activeIndicator" >
      <property name="name" value="active" />
  </bean>

  <bean id="TaxDistrict-versionNumber" parent="TaxDistrict-versionNumber-parentBean" />

  <bean id="TaxDistrict-versionNumber-parentBean" abstract="true" parent="GenericAttributes-versionNumber" />

<!-- Business Object Lookup Definition -->


  <bean id="TaxDistrict-lookupDefinition" parent="TaxDistrict-lookupDefinition-parentBean" />

  <bean id="TaxDistrict-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
    <property name="title" value="Tax District Lookup" />
    <property name="instructions" value="Lookup a Tax District" />
    <property name="defaultSort" >
      <bean parent="SortDefinition">
        <property name="attributeNames" >
          <list>
            <value>taxDistrictCode</value>
            <value>taxDistrictName</value>
          </list>
        </property>
      </bean>
    </property>
    <property name="lookupFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="taxDistrictCode" />
        <bean parent="FieldDefinition"
              p:attributeName="taxDistrictName" />
        <bean parent="FieldDefinition"
              p:attributeName="active" />
      </list>
    </property>
    <property name="resultFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="taxDistrictCode"
              p:forceInquiry="true" />
        <bean parent="FieldDefinition"
              p:attributeName="taxDistrictName" />
        <bean parent="FieldDefinition"
              p:attributeName="chartOfAccountsCode" />
        <bean parent="FieldDefinition"
              p:attributeName="accountNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="financialObjectCode" />
        <bean parent="FieldDefinition"
              p:attributeName="active" />
           
      </list>
    </property>
  </bean>
  
  <bean id="TaxDistrictRate" parent="TaxDistrictRate-parentBean" />

  <bean id="TaxDistrictRate-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.kfs.sys.businessobject.TaxDistrictRate" />
    <property name="objectLabel" value="TaxDistrictRate" />
    <property name="attributes" >
      <list>
        <ref bean="TaxDistrictRate-taxDistrictCode" />
        <ref bean="TaxDistrictRate-effectiveDate" />
        <ref bean="TaxDistrictRate-taxRate" />
        <ref bean="TaxDistrictRate-versionNumber" />
      </list>
    </property>
  </bean>
  
  <bean id="TaxDistrictRate-taxDistrictCode" parent="TaxDistrictRate-taxDistrictCode-parentBean" />
  <bean id="TaxDistrictRate-taxDistrictCode-parentBean" abstract="true" parent="TaxDistrict-taxDistrictCode">
	<property name="control" >
      <bean parent="HiddenControlDefinition" />
    </property>
  </bean>

  <bean id="TaxDistrictRate-effectiveDate" parent="TaxDistrictRate-effectiveDate-parentBean" />
  <bean id="TaxDistrictRate-effectiveDate-parentBean" abstract="true" parent="GenericAttributes-genericDate" >
    <property name="name" value="effectiveDate" />
    <property name="label" value="Effective Date" />
    <property name="shortLabel" value="Date" />
  </bean>

  <bean id="TaxDistrictRate-taxRate" parent="TaxDistrictRate-taxRate-parentBean" />

  <bean id="TaxDistrictRate-taxRate-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="taxRate" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Tax Rate" />
    <property name="shortLabel" value="Tax Rate" />
    <property name="maxLength" value="10" />
    <property name="validationPattern" >
      <bean parent="FloatingPointValidationPattern" />
    </property>
    <property name="required" value="true" />
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="10" />
    </property>
    <property name="summary" value="Tax Rate" />
    <property name="description" value="Tax rate." />
  </bean>
  
  <bean id="TaxDistrictRate-versionNumber" parent="TaxDistrictRate-versionNumber-parentBean" />
  <bean id="TaxDistrictRate-versionNumber-parentBean" abstract="true" parent="GenericAttributes-versionNumber" />

</beans>
