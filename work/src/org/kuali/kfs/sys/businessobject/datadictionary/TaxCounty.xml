<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="TaxCounty" parent="TaxCounty-parentBean" />

  <bean id="TaxCounty-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.kfs.sys.businessobject.TaxCounty" />
    <property name="inquiryDefinition" >
      <ref bean="TaxCounty-inquiryDefinition" />
    </property>
    <property name="lookupDefinition" >
      <ref bean="TaxCounty-lookupDefinition" />
    </property>
    <property name="titleAttribute" value="taxCountyCode" />
    <property name="objectLabel" value="taxCounty" />
    <property name="attributes" >
      <list>
        <ref bean="TaxCounty-countyCode" />
        <ref bean="TaxCounty-stateCode" />
        <ref bean="TaxCounty-taxRegionCode" />
        <ref bean="TaxCounty-active" />
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->


  <bean id="TaxCounty-countyCode" parent="TaxCounty-countyCode-parentBean" />

  <bean id="TaxCounty-countyCode-parentBean" abstract="true" parent="County-countyCode">
    <property name="name" value="countyCode" />
  </bean>
  
  <bean id="TaxCounty-stateCode" parent="TaxCounty-stateCode-parentBean" />

  <bean id="TaxCounty-stateCode-parentBean" abstract="true" parent="TaxState-stateCode">
      </bean>
  
  <bean id="TaxCounty-taxRegionCode" parent="TaxCounty-taxRegionCode-parentBean" />

  <bean id="TaxCounty-taxRegionCode-parentBean" abstract="true" parent="TaxState-taxRegionCode">
    
  </bean>
  
  <bean id="TaxCounty-active" parent="TaxCounty-active-parentBean" />

  <bean id="TaxCounty-active-parentBean" abstract="true" parent="GenericAttributes-activeIndicator">
    <property name="name" value="active" />
  </bean>
  

<!-- Business Object Inquiry Definition -->


  <bean id="TaxCounty-inquiryDefinition" parent="TaxCounty-inquiryDefinition-parentBean" />

  <bean id="TaxCounty-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
    <property name="title" value="TaxCounty Inquiry" />
    <property name="inquirySections" >
      <list>
        <bean parent="InquirySectionDefinition">
          <property name="title" value="" />
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields" >
            <list>
              <bean parent="FieldDefinition"
                    p:attributeName="countyCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="stateCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="taxRegionCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="active" />
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Business Object Lookup Definition -->


  <bean id="TaxCounty-lookupDefinition" parent="TaxCounty-lookupDefinition-parentBean" />

  <bean id="TaxCounty-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
    <property name="title" value="TaxCounty Code Lookup" />
    <property name="instructions" value="Lookup a TaxCounty" />
    <property name="defaultSort" >
      <bean parent="SortDefinition">
        <property name="attributeNames" >
          <list>
            <value>countyCode</value>
            <value>stateCode</value>
            <value>taxRegionCode</value>
          </list>
        </property>
      </bean>
    </property>
    <property name="lookupFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="countyCode" />
        <bean parent="FieldDefinition"
              p:attributeName="stateCode" />
        <bean parent="FieldDefinition"
              p:attributeName="taxRegionCode" />
        <bean parent="FieldDefinition"
              p:attributeName="active" />
      </list>
    </property>
    <property name="resultFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="countyCode" />
        <bean parent="FieldDefinition"
              p:attributeName="stateCode" />
        <bean parent="FieldDefinition"
              p:attributeName="taxRegionCode" />
        <bean parent="FieldDefinition"
              p:attributeName="active" />
      </list>
    </property>
  </bean>
</beans>
