<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="ParameterNamespace" parent="ParameterNamespace-parentBean" />

  <bean id="ParameterNamespace-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.rice.kns.bo.ParameterNamespace" />
    <property name="inquiryDefinition" >
      <ref bean="ParameterNamespace-inquiryDefinition" />
    </property>
    <property name="lookupDefinition" >
      <ref bean="ParameterNamespace-lookupDefinition" />
    </property>
    <property name="titleAttribute" value="name" />
    <property name="objectLabel" value="Parameter Module" />
    <property name="attributes" >
      <list>
        <ref bean="ParameterNamespace-code" />
        <ref bean="ParameterNamespace-name" />
        <ref bean="ParameterNamespace-codeAndDescription" />
        <ref bean="ParameterNamespace-applicationNamespaceCode" />
        <ref bean="ParameterNamespace-active" />
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->


  <bean id="ParameterNamespace-code" parent="ParameterNamespace-code-parentBean" />

  <bean id="ParameterNamespace-code-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="code" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Module Code" />
    <property name="shortLabel" value="Module Cd" />
    <property name="maxLength" value="20" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="required" value="true" />
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="10" />
    </property>
    <property name="summary" value="Code identifying the module." />
    <property name="description" value="Code identifying the module." />
  </bean>
  <bean id="ParameterNamespace-name" parent="ParameterNamespace-name-parentBean" />

  <bean id="ParameterNamespace-name-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="name" />
    <property name="forceUppercase" value="false" />
    <property name="label" value="Module Name" />
    <property name="shortLabel" value="Module Nm" />
    <property name="maxLength" value="40" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern"
            p:allowWhitespace="true" />
    </property>
    <property name="required" value="true" />
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="40" />
    </property>
    <property name="summary" value="The name of the module." />
    <property name="description" value="The name of the module." />
  </bean>
  <bean id="ParameterNamespace-codeAndDescription" parent="ParameterNamespace-codeAndDescription-parentBean" />

  <bean id="ParameterNamespace-codeAndDescription-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="codeAndDescription" />
    <property name="forceUppercase" value="false" />
    <property name="label" value="Module" />
    <property name="shortLabel" value="Module" />
    <property name="maxLength" value="100" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern"
            p:allowWhitespace="true" />
    </property>
    <property name="required" value="false" />
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="40" />
    </property>
    <property name="summary" value="The name of the module." />
    <property name="description" value="The name of the module." />
  </bean>
  <bean id="ParameterNamespace-active" parent="ParameterNamespace-active-parentBean" />

  <bean id="ParameterNamespace-active-parentBean" abstract="true" parent="GenericAttributes-activeIndicator">
    <property name="name" value="active" />
  </bean>
  <bean id="ParameterNamespace-applicationNamespaceCode" parent="ParameterNamespace-applicationNamespaceCode-parentBean" />

  <bean id="ParameterNamespace-applicationNamespaceCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="forceUppercase" value="false" />
    <property name="shortLabel" value="Appl Nmspc" />
    <property name="maxLength" value="20" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="required" value="false" />
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="10" />
    </property>
    <property name="summary" value="The code of the application namespace." />
    <property name="name" value="applicationNamespaceCode" />
    <property name="label" value="Application Namespace Code" />
    <property name="description" value="The code of the application namespace." />
  </bean>
  

<!-- Business Object Inquiry Definition -->


  <bean id="ParameterNamespace-inquiryDefinition" parent="ParameterNamespace-inquiryDefinition-parentBean" />

  <bean id="ParameterNamespace-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
    <property name="title" value="Parameter Module Inquiry" />
    <property name="inquirySections" >
      <list>
        <bean parent="InquirySectionDefinition">
          <property name="title" value="Module Information" />
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields" >
            <list>
              <bean parent="FieldDefinition"
                    p:attributeName="code" />
              <bean parent="FieldDefinition"
                    p:attributeName="name"
                    p:noInquiry="true" />
              <bean parent="FieldDefinition"
                    p:attributeName="applicationNamespaceCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="active" />
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Business Object Lookup Definition -->


  <bean id="ParameterNamespace-lookupDefinition" parent="ParameterNamespace-lookupDefinition-parentBean" />

  <bean id="ParameterNamespace-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
    <property name="title" value="Parameter Module Lookup" />
    <property name="instructions" value="Look up a Module." />
    <property name="defaultSort" >
      <bean parent="SortDefinition">
        <property name="attributeNames" >
          <list>
            <value>name</value>
          </list>
        </property>
      </bean>
    </property>
    <property name="lookupFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="code"
              p:noLookup="true" />
        <bean parent="FieldDefinition"
              p:attributeName="name" />
        <bean parent="FieldDefinition"
              p:attributeName="applicationNamespaceCode" />
        <bean parent="FieldDefinition"
              p:defaultValue="Y"
              p:attributeName="active" />
      </list>
    </property>
    <property name="resultFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="code"
              p:forceInquiry="true" />
        <bean parent="FieldDefinition"
              p:attributeName="name" />
        <bean parent="FieldDefinition"
              p:attributeName="applicationNamespaceCode" />
        <bean parent="FieldDefinition"
              p:attributeName="active" />
      </list>
    </property>
  </bean>
</beans>
