<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="FunctionalFieldDescriptionMaintenanceDocument" parent="FunctionalFieldDescriptionMaintenanceDocument-parentBean" />

  <bean id="FunctionalFieldDescriptionMaintenanceDocument-parentBean" abstract="true" parent="MaintenanceDocumentEntry">
    <property name="businessObjectClass" value="org.kuali.kfs.sys.businessobject.FunctionalFieldDescription" />
    <property name="maintainableClass" value="org.kuali.core.maintenance.KualiMaintainableImpl" />
    <property name="maintainableSections" >
      <list>
        <ref bean="FunctionalFieldDescriptionMaintenanceDocument-CreateFunctionalFieldDescriptionFields" />
      </list>
    </property>
    <property name="label" value="Functional Field Description" />
    <property name="shortLabel" value="Functional Field Description" />
    <property name="summary" value="Functional Field Description maintenance document" />
    <property name="description" value="Document used to create Functional Field Description objects" />
    
    <property name="defaultExistenceChecks" >
      <list>
        <bean parent="ReferenceDefinition"
              p:attributeName="parameterNamespace"
              p:attributeToHighlightOnFail="namespaceCode" />
      </list>
    </property>
    
    <property name="documentAuthorizerClass" value="org.kuali.core.document.authorization.MaintenanceDocumentAuthorizerBase" />
    <property name="workflowProperties" >
      <ref bean="BuildingMaintenanceDocument-workflowProperties" />
    </property>
    <property name="authorizations" >
      <list>
        <bean parent="AuthorizationDefinition">
          <property name="action" value="initiate" />
          <property name="authorizedGroups" >
            <set>
              <value>SY_MAINTENANCE_USERS</value>
            </set>
          </property>
        </bean>
      </list>
    </property>
    <property name="lockingKeys" >
      <list>
        <value>namespaceCode</value>
      </list>
    </property>
    <property name="helpDefinition" >
      <bean parent="HelpDefinition" p:parameterClass="org.kuali.kfs.sys.businessobject.FunctionalFieldDescription" p:parameterName="FUNCTIONAL_FIELD_DESCRIPTION" />
    </property>
    <property name="documentTypeName" value="FunctionalFieldDescriptionMaintenanceDocument" />
    <property name="documentTypeCode" value="FUNCTIONALFD" />
     
  </bean>

<!-- Maintenance Section Definitions -->


  <bean id="FunctionalFieldDescriptionMaintenanceDocument-CreateFunctionalFieldDescriptionFields" parent="FunctionalFieldDescriptionMaintenanceDocument-CreateFunctionalFieldDescriptionFields-parentBean" />

  <bean id="FunctionalFieldDescriptionMaintenanceDocument-CreateFunctionalFieldDescriptionFields-parentBean" abstract="true" parent="MaintainableSectionDefinition">
    <property name="id" value="CreateFunctionalFieldDescription" />
    <property name="title" value="Create Functional Field Description" />
    <property name="maintainableItems" >
      <list>
        <bean parent="MaintainableFieldDefinition" p:name="componentClass" p:required="true" />
        <bean parent="MaintainableFieldDefinition" p:name="propertyName" p:required="true" />
        <bean parent="MaintainableFieldDefinition" p:name="functionalFieldDescription" />
        <bean parent="MaintainableFieldDefinition" p:name="active" p:required="true" p:defaultValue="true" />
        <bean parent="MaintainableFieldDefinition" p:name="versionNumber" />
      </list>
    </property>
  </bean>
 
</beans>