    <bean id="SearchingAttribute-financialDocumentTotalAmount" parent="WorkflowAttributeMetaData-financialDocumentTotalAmount" class="org.kuali.rice.kns.datadictionary.SearchingAttribute" />
    <bean id="RoutingAttribute-financialDocumentTotalAmount" parent="WorkflowAttributeMetaData-financialDocumentTotalAmount" class="org.kuali.rice.kns.datadictionary.RoutingAttribute" />
    <bean id="WorkflowAttributeMetaData-organizationCode" abstract="true">
    	<property name="businessObjectClassName" value="org.kuali.kfs.coa.businessobject.Organization" />
    	<property name="attributeName" value="organizationCode" />
    </bean>
    <bean id="SearchingAttribute-organizationCode" parent="WorkflowAttributeMetaData-organizationCode" class="org.kuali.rice.kns.datadictionary.SearchingAttribute" />
    <bean id="RoutingAttribute-organizationCode" parent="WorkflowAttributeMetaData-organizationCode" class="org.kuali.rice.kns.datadictionary.RoutingAttribute" />
    <bean id="WorkflowAttributeMetaData-overrideCode" abstract="true">
    	<property name="businessObjectClassName" value="org.kuali.kfs.sys.businessobject.AccountingLine" />
    	<property name="attributeName" value="overrideCode" />
    </bean>
    <bean id="SearchingAttribute-overrideCode" parent="WorkflowAttributeMetaData-overrideCode" class="org.kuali.rice.kns.datadictionary.SearchingAttribute" />
    <bean id="RoutingAttribute-overrideCode" parent="WorkflowAttributeMetaData-overrideCode" class="org.kuali.rice.kns.datadictionary.RoutingAttribute" />
    <bean id="WorkflowAttributeMetaData-subFundGroupCode" abstract="true">
    	<property name="businessObjectClassName" value="org.kuali.kfs.coa.businessobject.SubFundGroup" />
    	<property name="attributeName" value="subFundGroupCode" />
    </bean>
    <bean id="SearchingAttribute-subFundGroupCode" parent="WorkflowAttributeMetaData-subFundGroupCode" class="org.kuali.rice.kns.datadictionary.SearchingAttribute" />
    <bean id="RoutingAttribute-subFundGroupCode" parent="WorkflowAttributeMetaData-subFundGroupCode" class="org.kuali.rice.kns.datadictionary.RoutingAttribute" />
    <bean id="WorkflowAttributeMetaData-contractsAndGrantsAccountResponsibilityId" abstract="true">
    	<property name="businessObjectClassName" value="org.kuali.kfs.coa.businessobject.Account" />
    	<property name="attributeName" value="contractsAndGrantsAccountResponsibilityId" />
    </bean>
    <bean id="SearchingAttribute-contractsAndGrantsAccountResponsibilityId" parent="WorkflowAttributeMetaData-contractsAndGrantsAccountResponsibilityId" class="org.kuali.rice.kns.datadictionary.SearchingAttribute" />
    <bean id="RoutingAttribute-contractsAndGrantsAccountResponsibilityId" parent="WorkflowAttributeMetaData-contractsAndGrantsAccountResponsibilityId" class="org.kuali.rice.kns.datadictionary.RoutingAttribute" />
    <bean id="WorkflowAttributeMetaData-campusCode" abstract="true">
    	<property name="businessObjectClassName" value="org.kuali.rice.kns.bo.Campus" />
    	<property name="attributeName" value="campusCode" />
    </bean>
    <bean id="SearchingAttribute-campusCode" parent="WorkflowAttributeMetaData-campusCode" class="org.kuali.rice.kns.datadictionary.SearchingAttribute" />
    <bean id="RoutingAttribute-campusCode" parent="WorkflowAttributeMetaData-campusCode" class="org.kuali.rice.kns.datadictionary.RoutingAttribute" />
	
	<bean id="SearchingType-AccountingDocument-chartOfAccountsCode" class="org.kuali.rice.kns.datadictionary.SearchingTypeDefinition">
        <property name="searchingAttribute" ref="SearchingAttribute-chartOfAccountsCode" />
        <property name="documentValues">
        	<list>
        		<value>sourceAccountingLines.chartOfAccountsCode</value>
        		<value>targetAccountingLines.chartOfAccountsCode</value>
        	</list>
        </property>
    </bean>

	<bean id="SearchingType-AccountingDocument-accountNumber" class="org.kuali.rice.kns.datadictionary.SearchingTypeDefinition">
        <property name="searchingAttribute" ref="SearchingAttribute-accountNumber" />
        <property name="documentValues">
        	<list>
        		<value>sourceAccountingLines.accountNumber</value>
        		<value>targetAccountingLines.accountNumber</value>
        	</list>
        </property>
    </bean>
    
