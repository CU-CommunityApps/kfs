    </bean>
    <bean id="SearchingAttribute-accountNumber" parent="WorkflowAttributeMetaData-accountNumber" class="org.kuali.rice.kns.datadictionary.SearchingAttribute" />
    <bean id="RoutingAttribute-accountNumber" parent="WorkflowAttributeMetaData-accountNumber" class="org.kuali.rice.kns.datadictionary.RoutingAttribute" />
    <bean id="WorkflowAttributeMetaData-financialDocumentTotalAmount" abstract="true">
