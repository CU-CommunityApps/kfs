    	<property name="attributeName" value="chartOfAccountsCode" />
    </bean>
    <bean id="SearchingAttribute-chartOfAccountsCode" parent="WorkflowAttributeMetaData-chartOfAccountsCode" class="org.kuali.rice.kns.datadictionary.SearchingAttribute" />
