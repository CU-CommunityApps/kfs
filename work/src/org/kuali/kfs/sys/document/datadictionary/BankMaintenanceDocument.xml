<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="BankMaintenanceDocument" parent="BankMaintenanceDocument-parentBean" />

  <bean id="BankMaintenanceDocument-parentBean" abstract="true" parent="MaintenanceDocumentEntry">
    <property name="businessObjectClass" value="org.kuali.kfs.sys.businessobject.Bank" />
    <property name="maintainableClass" value="org.kuali.rice.kns.maintenance.KualiMaintainableImpl" />
    <property name="maintainableSections" >
      <list>
        <ref bean="BankMaintenanceDocument-EditBank" />
      </list>
    </property>
    <property name="defaultExistenceChecks" >
      <list>
        <bean parent="ReferenceDefinition"
              p:attributeName="cashOffsetFinancialChartOfAccount"
              p:activeIndicatorAttributeName="active"
              p:attributeToHighlightOnFail="cashOffsetFinancialChartOfAccountCode" />
        <bean parent="ReferenceDefinition"
              p:attributeName="cashOffsetAccount"
              p:activeIndicatorAttributeName="active"
              p:attributeToHighlightOnFail="cashOffsetAccountNumber" />
        <bean parent="ReferenceDefinition"
              p:attributeName="cashOffsetObject"
              p:activeIndicatorAttributeName="active"
              p:attributeToHighlightOnFail="cashOffsetObjectCode" />
        <bean parent="ReferenceDefinition"
              p:attributeName="cashOffsetSubAccount"
              p:activeIndicatorAttributeName="active"
              p:attributeToHighlightOnFail="cashOffsetSubAccountNumber" />
        <bean parent="ReferenceDefinition"
              p:attributeName="cashOffsetSubObject"
              p:activeIndicatorAttributeName="active"
              p:attributeToHighlightOnFail="cashOffsetSubObjectCode" />
        <bean parent="ReferenceDefinition"
              p:attributeName="continuationBank"
              p:activeIndicatorAttributeName="active"
              p:attributeToHighlightOnFail="continuationBankCode" />
      </list>
    </property>
    <property name="lockingKeys" >
      <list>
        <value>bankCode</value>
      </list>
    </property>
    <property name="label" value="Bank" />
    <property name="shortLabel" value="Bank" />
    <property name="summary" value="Bank maintenance document" />
    <property name="description" value="Document used to create or update Bank objects" />
    <property name="helpDefinition" >
      <bean parent="HelpDefinition" p:parameterClass="org.kuali.kfs.sys.businessobject.Bank" p:parameterName="BANK" />
    </property>
    <property name="documentTypeName" value="BankMaintenanceDocument" />
    <property name="businessRulesClass" value="org.kuali.kfs.sys.document.validation.impl.BankRule" />
    <property name="documentAuthorizerClass" value="org.kuali.kfs.sys.document.authorization.FinancialSystemMaintenanceDocumentAuthorizerBase" />
    <property name="documentTypeCode" value="BANK" />
    <property name="workflowProperties" >
      <ref bean="BankMaintenanceDocument-workflowProperties" />
    </property>
    <property name="authorizations" >
      <list>
        <bean parent="AuthorizationDefinition">
          <property name="action" value="initiate" />
          <property name="authorizedGroups" >
            <set>
              <value>SY_MAINTENANCE_USERS</value>
            </set>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Maintenance Section Definitions -->


  <bean id="BankMaintenanceDocument-EditBank" parent="BankMaintenanceDocument-EditBank-parentBean" />

  <bean id="BankMaintenanceDocument-EditBank-parentBean" abstract="true" parent="MaintainableSectionDefinition">
    <property name="id" value="Edit Bank" />
    <property name="title" value="Edit Bank" />
    <property name="maintainableItems" >
      <list>
        <bean parent="MaintainableFieldDefinition"
              p:name="bankCode"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="bankName"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="bankShortName"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="bankRoutingNumber"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="bankAccountNumber"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="bankAccountDescription" />
        <bean parent="MaintainableFieldDefinition"
              p:name="cashOffsetFinancialChartOfAccountCode" />
        <bean parent="MaintainableFieldDefinition"
              p:name="cashOffsetAccountNumber" />
        <bean parent="MaintainableFieldDefinition"
              p:name="cashOffsetSubAccountNumber" />
        <bean parent="MaintainableFieldDefinition"
              p:name="cashOffsetObjectCode" />
        <bean parent="MaintainableFieldDefinition"
              p:name="cashOffsetSubObjectCode" />
        <bean parent="MaintainableFieldDefinition"
              p:name="continuationBankCode" />
        <bean parent="MaintainableFieldDefinition"
              p:name="bankDepositIndicator" />                           
        <bean parent="MaintainableFieldDefinition"
              p:name="bankDisbursementIndicator" />
        <bean parent="MaintainableFieldDefinition"
              p:name="bankAchIndicator" />
        <bean parent="MaintainableFieldDefinition"
              p:name="bankCheckIndicator" />
        <bean parent="MaintainableFieldDefinition"
              p:name="active" />               
        <bean parent="MaintainableFieldDefinition"
              p:name="versionNumber" />
      </list>
    </property>
  </bean>

<!-- Exported Workflow Properties -->


  <bean id="BankMaintenanceDocument-workflowProperties" parent="BankMaintenanceDocument-workflowProperties-parentBean" />

  <bean id="BankMaintenanceDocument-workflowProperties-parentBean" abstract="true" parent="WorkflowProperties">
  </bean>
</beans>
