<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xmlns:dd="http://rice.kuali.org/dd"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd
        http://rice.kuali.org/dd
        http://rice.kuali.org/dd/dd.xsd">

  <bean id="ACHBankMaintenanceDocument" parent="ACHBankMaintenanceDocument-parentBean" />

  <bean id="ACHBankMaintenanceDocument-parentBean" abstract="true" parent="MaintenanceDocumentEntry">
    <property name="businessObjectClass" value="org.kuali.kfs.pdp.businessobject.ACHBankX" />
    <property name="maintainableClass" value="org.kuali.rice.kns.maintenance.KualiMaintainableImpl" />
    <property name="maintainableSections" >
      <list>
        <ref bean="ACHBankMaintenanceDocument-EditACHBank" />
      </list>
    </property>
    <property name="defaultExistenceChecks" >
      <list>
        <bean parent="ReferenceDefinition"
              p:attributeName="bankState"
              p:attributeToHighlightOnFail="bankStateCode" />
        <bean parent="ReferenceDefinition"
              p:attributeName="postalCode"
              p:attributeToHighlightOnFail="bankZipCode" />
      </list>
    </property>
    <property name="lockingKeys" >
      <list>
        <value>bankRoutingNumber</value>
      </list>
    </property>
    <property name="label" value="ACH Bank" />
    <property name="shortLabel" value="AchBankMaintDoc" />
    <property name="summary" value="Ach bank maintenance document" />
    <property name="description" value="Document used to create or update Ach Bank objects" />
    <property name="helpDefinition" >
      <bean parent="HelpDefinition" p:parameterClass="org.kuali.kfs.pdp.businessobject.ACHBankX" p:parameterName="BANK" />
    </property>
    <property name="documentTypeName" value="ACHBankMaintenanceDocument" />
    <property name="businessRulesClass" value="org.kuali.kfs.pdp.document.validation.impl.AchBankRule" />
    <property name="documentAuthorizerClass" value="org.kuali.kfs.pdp.document.authorization.AchBankDocumentAuthorizer" />
    <property name="documentTypeCode" value="ABNK" />
    <property name="authorizations" >
      <list>
        <bean parent="AuthorizationDefinition">
          <property name="action" value="initiate" />
          <property name="authorizedGroups" >
            <set>
              <value>SY_MAINTENANCE_USERS</value>
            </set>
          </property>
        </bean>
      </list>
    </property>
    <property name="workflowAttributes">
    	<ref bean="ACHBankMaintenanceDocument-workflowAttributes" />
    </property>
  </bean>

<!-- Maintenance Section Definitions -->

  <bean id="ACHBankMaintenanceDocument-EditACHBank" parent="ACHBankMaintenanceDocument-EditACHBank-parentBean" />

  <bean id="ACHBankMaintenanceDocument-EditACHBank-parentBean" abstract="true" parent="MaintainableSectionDefinition">
    <property name="id" value="Edit ACH Bank" />
    <property name="title" value="Edit ACH Bank" />
    <property name="maintainableItems" >
      <list>
        <bean parent="MaintainableFieldDefinition">
          <property name="name" value="bankRoutingNumber" />
          <property name="required" value="true" />
          <property name="displayMask" >
            <bean parent="Mask">
              <property name="maskFormatter" >
                <bean parent="MaskFormatterLiteral"
                      p:literal="*********" />
              </property>
            </bean>
          </property>
          <property name="displayEditMode" value="entry" />
        </bean> 
        <bean parent="MaintainableFieldDefinition"
              p:name="bankOfficeCode"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="bankServiceNumber"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="bankTypeCode"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="bankNewRoutingNumber"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="bankName"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="bankStreetAddress"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="bankCityName"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="bankStateCode"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="bankZipCode"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="bankPhoneAreaCode"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="bankPhonePrefixNumber"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="bankPhoneSuffixNumber"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="bankInstitutionStatusCode" />
        <bean parent="MaintainableFieldDefinition"
              p:name="bankDataViewCode" />
        <bean parent="MaintainableFieldDefinition"
              p:name="active" />
      </list>
    </property>
  </bean>
  
  <!-- workflow attributes -->
  <bean id="ACHBankMaintenanceDocument-workflowAttributes" parent="ACHBankMaintenanceDocument-workflowAttributes-parentBean" />
  
  <bean id="ACHBankMaintenanceDocument-workflowAttributes-parentBean" class="org.kuali.rice.kns.datadictionary.WorkflowAttributes" abstract="true" />
</beans>
