<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="AchTransactionTypeMaintenanceDocument" parent="AchTransactionTypeMaintenanceDocument-parentBean" />

  <bean id="AchTransactionTypeMaintenanceDocument-parentBean" abstract="true" parent="MaintenanceDocumentEntry">
    <property name="businessObjectClass" value="org.kuali.kfs.pdp.businessobject.AchTransactionType" />
    <property name="maintainableClass" value="org.kuali.rice.kns.maintenance.KualiMaintainableImpl" />
    <property name="maintainableSections" >
      <list>
        <ref bean="AchTransactionTypeMaintenanceDocument-EditTransactionType" />
      </list>
    </property>
    
    <property name="lockingKeys" >
      <list>
        <value>transactionType</value>
      </list>
    </property>
    <property name="label" value="ACH Transaction Type" />
    <property name="shortLabel" value="Trans Type" />
    <property name="summary" value="ACH Transaction Type Maintenance Document" />
    <property name="description" value="Document used to create or update ACH Transaction Type Descriptions" />
    <property name="documentTypeName" value="ACHTransactionTypeMaintenanceDocument" />
    <property name="documentAuthorizerClass" value="org.kuali.kfs.sys.document.authorization.FinancialSystemMaintenanceDocumentAuthorizerBase" />
    <property name="documentTypeCode" value="ACHT" />
    <property name="authorizations" >
      <list>
        <bean parent="AuthorizationDefinition">
          <property name="action" value="initiate" />
          <property name="authorizedGroups" >
            <set>
              <value>SY_MAINTENANCE_USERS</value>
            </set>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Maintenance Section Definitions -->


  <bean id="AchTransactionTypeMaintenanceDocument-EditTransactionType" parent="AchTransactionTypeMaintenanceDocument-EditTransactionType-parentBean" />

  <bean id="AchTransactionTypeMaintenanceDocument-EditTransactionType-parentBean" abstract="true" parent="MaintainableSectionDefinition">
    <property name="id" value="Edit Transaction Type" />
    <property name="title" value="Edit Transaction Type" />
    <property name="maintainableItems" >
      <list>
        <bean parent="MaintainableFieldDefinition"
              p:name="transactionType"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="description"
              p:required="true" />
      </list>
    </property>
  </bean>
  
  
</beans>
