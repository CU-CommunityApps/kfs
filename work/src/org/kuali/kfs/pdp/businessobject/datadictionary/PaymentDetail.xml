<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="PaymentDetail" parent="PaymentDetail-parentBean" />

  <bean id="PaymentDetail-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.kfs.pdp.businessobject.PaymentDetail" />
    <property name="inquiryDefinition" >
      <ref bean="PaymentDetail-inquiryDefinition" />
    </property>
    <property name="lookupDefinition" >
      <ref bean="PaymentDetail-lookupDefinition" />
    </property>
    <property name="objectLabel" value="Payment Detail" />
    <property name="attributes" >
      <list>
        <ref bean="PaymentDetail-paymentGroup.payeeName" />
        <ref bean="PaymentDetail-paymentGroup.batch.customerProfile.chartCode" />
        <ref bean="PaymentDetail-paymentGroup.batch.customerProfile.orgCode" />
        <ref bean="PaymentDetail-custPaymentDocNbr" />
        <ref bean="PaymentDetail-paymentGroup.payeeIdTypeCd" />
        <ref bean="PaymentDetail-purchaseOrderNbr" />
        <ref bean="PaymentDetail-paymentGroup.payeeId" />
        <ref bean="PaymentDetail-paymentGroup.batch.customerProfile.subUnitCode" />
        <ref bean="PaymentDetail-invoiceNbr" />
        <ref bean="PaymentDetail-paymentGroup.disbursementTypeCode" />
        <ref bean="PaymentDetail-paymentGroup.processImmediate" />
        <ref bean="PaymentDetail-requisitionNbr" />
        <ref bean="PaymentDetail-paymentGroup.disbursementNbr" />
        <ref bean="PaymentDetail-paymentGroup.pymtSpecialHandling" />
        <ref bean="PaymentDetail-paymentGroup.customerInstitutionNumber" />
        <ref bean="PaymentDetail-paymentGroup.disbursementDate" />
        <ref bean="PaymentDetail-paymentGroup.pymtAttachment" />
        <ref bean="PaymentDetail-paymentGroup.processId" />
        <ref bean="PaymentDetail-paymentGroup.paymentDate" />
        <ref bean="PaymentDetail-id" />
        <ref bean="PaymentDetail-netPaymentAmount" />
        <ref bean="PaymentDetail-paymentGroup.paymentStatusCode" />
        <ref bean="PaymentDetail-paymentGroup.batch.id" />
        <ref bean="PaymentDetail-paymentGroup.disbursementType.name" />
        
        
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->

  <bean id="PaymentDetail-paymentGroup.payeeName" parent="PaymentDetail-paymentGroup.payeeName-parentBean" />

  <bean id="PaymentDetail-paymentGroup.payeeName-parentBean" abstract="true" parent="PaymentGroup-payeeName">
    <property name="name" value="paymentGroup.payeeName" />
  </bean>
  <bean id="PaymentDetail-paymentGroup.batch.customerProfile.chartCode" parent="PaymentDetail-paymentGroup.batch.customerProfile.chartCode-parentBean" />

  <bean id="PaymentDetail-paymentGroup.batch.customerProfile.chartCode-parentBean" abstract="true" parent="PaymentGroup-batch.customerProfile.chartCode">
    <property name="name" value="paymentGroup.batch.customerProfile.chartCode" />
  </bean>

  <bean id="PaymentDetail-custPaymentDocNbr" parent="PaymentDetail-custPaymentDocNbr-parentBean" />

  <bean id="PaymentDetail-custPaymentDocNbr-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="custPaymentDocNbr" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Source Document Number" />
    <property name="shortLabel" value="Doc Nbr" />
    <property name="maxLength" value="32" />
    <property name="validationPattern" >
       <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="32" />
    </property>
    <property name="summary" value="id" />
    <property name="description" value="Source Document Number." />
  </bean>
  <bean id="PaymentDetail-paymentGroup.payeeIdTypeCd" parent="PaymentDetail-paymentGroup.payeeIdTypeCd-parentBean" />

  <bean id="PaymentDetail-paymentGroup.payeeIdTypeCd-parentBean" abstract="true" parent="PaymentGroup-payeeIdTypeCd">
    <property name="name" value="paymentGroup.payeeIdTypeCd" />
  </bean>
  <bean id="PaymentDetail-paymentGroup.batch.customerProfile.orgCode" parent="PaymentDetail-paymentGroup.batch.customerProfile.orgCode-parentBean" />

  <bean id="PaymentDetail-paymentGroup.batch.customerProfile.orgCode-parentBean" abstract="true" parent="PaymentGroup-batch.customerProfile.orgCode">
    <property name="name" value="paymentGroup.batch.customerProfile.orgCode" />
  </bean>
  <bean id="PaymentDetail-purchaseOrderNbr" parent="PaymentDetail-purchaseOrderNbr-parentBean" />

  <bean id="PaymentDetail-purchaseOrderNbr-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="purchaseOrderNbr" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Purchase Order Number" />
    <property name="shortLabel" value="Order Nbr" />
    <property name="maxLength" value="32" />
    <property name="validationPattern" >
       <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="32" />
    </property>
    <property name="summary" value="purchaseOrderNbr" />
    <property name="description" value="Purchase Order Number." />
  </bean>
  <bean id="PaymentDetail-paymentGroup.payeeId" parent="PaymentDetail-paymentGroup.payeeId-parentBean" />

  <bean id="PaymentDetail-paymentGroup.payeeId-parentBean" abstract="true" parent="PaymentGroup-payeeId">
    <property name="name" value="paymentGroup.payeeId" />
  </bean>
  <bean id="PaymentDetail-paymentGroup.batch.customerProfile.subUnitCode" parent="PaymentDetail-paymentGroup.batch.customerProfile.subUnitCode-parentBean" />

  <bean id="PaymentDetail-paymentGroup.batch.customerProfile.subUnitCode-parentBean" abstract="true" parent="PaymentGroup-batch.customerProfile.subUnitCode">
    <property name="name" value="paymentGroup.batch.customerProfile.subUnitCode" />
  </bean>
  <bean id="PaymentDetail-invoiceNbr" parent="PaymentDetail-invoiceNbr-parentBean" />

  <bean id="PaymentDetail-invoiceNbr-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="invoiceNbr" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Invoice Number" />
    <property name="shortLabel" value="Inv Nbr" />
    <property name="maxLength" value="32" />
    <property name="validationPattern" >
       <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="32" />
    </property>
    <property name="summary" value="invoiceNbr" />
    <property name="description" value="Invoice Number." />
  </bean>
  <bean id="PaymentDetail-paymentGroup.disbursementTypeCode" parent="PaymentDetail-paymentGroup.disbursementTypeCode-parentBean" />

  <bean id="PaymentDetail-paymentGroup.disbursementTypeCode-parentBean" abstract="true" parent="PaymentGroup-disbursementTypeCode">
    <property name="name" value="paymentGroup.disbursementTypeCode" />
  </bean>
  <bean id="PaymentDetail-paymentGroup.processImmediate" parent="PaymentDetail-paymentGroup.processImmediate-parentBean" />

  <bean id="PaymentDetail-paymentGroup.processImmediate-parentBean" abstract="true" parent="PaymentGroup-processImmediate">
    <property name="name" value="paymentGroup.processImmediate" />
  </bean>
  <bean id="PaymentDetail-requisitionNbr" parent="PaymentDetail-requisitionNbr-parentBean" />

  <bean id="PaymentDetail-requisitionNbr-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="requisitionNbr" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Requisition Number" />
    <property name="shortLabel" value="Requisition Nbr" />
    <property name="maxLength" value="32" />
    <property name="validationPattern" >
       <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="32" />
    </property>
    <property name="summary" value="requisitionNbr" />
    <property name="description" value="Requisition Number." />
  </bean>
  <bean id="PaymentDetail-paymentGroup.disbursementNbr" parent="PaymentDetail-paymentGroup.disbursementNbr-parentBean" />

  <bean id="PaymentDetail-paymentGroup.disbursementNbr-parentBean" abstract="true" parent="PaymentGroup-disbursementNbr">
    <property name="name" value="paymentGroup.disbursementNbr" />
  </bean>
  <bean id="PaymentDetail-paymentGroup.pymtSpecialHandling" parent="PaymentDetail-paymentGroup.pymtSpecialHandling-parentBean" />

  <bean id="PaymentDetail-paymentGroup.pymtSpecialHandling-parentBean" abstract="true" parent="PaymentGroup-pymtSpecialHandling">
    <property name="name" value="paymentGroup.pymtSpecialHandling" />
  </bean>
  <bean id="PaymentDetail-paymentGroup.customerInstitutionNumber" parent="PaymentDetail-paymentGroup.customerInstitutionNumber-parentBean" />

  <bean id="PaymentDetail-paymentGroup.customerInstitutionNumber-parentBean" abstract="true" parent="PaymentGroup-customerInstitutionNumber">
    <property name="name" value="paymentGroup.customerInstitutionNumber" />
  </bean>
  <bean id="PaymentDetail-paymentGroup.disbursementDate" parent="PaymentDetail-paymentGroup.disbursementDate-parentBean" />

  <bean id="PaymentDetail-paymentGroup.disbursementDate-parentBean" abstract="true" parent="PaymentGroup-disbursementDate">
    <property name="name" value="paymentGroup.disbursementDate" />
  </bean>
  <bean id="PaymentDetail-paymentGroup.pymtAttachment" parent="PaymentDetail-paymentGroup.pymtAttachment-parentBean" />

  <bean id="PaymentDetail-paymentGroup.pymtAttachment-parentBean" abstract="true" parent="PaymentGroup-pymtAttachment">
    <property name="name" value="paymentGroup.pymtAttachment" />
  </bean>
  <bean id="PaymentDetail-paymentGroup.processId" parent="PaymentDetail-paymentGroup.processId-parentBean" />

  <bean id="PaymentDetail-paymentGroup.processId-parentBean" abstract="true" parent="PaymentGroup-processId">
    <property name="name" value="paymentGroup.processId" />
  </bean>
  <bean id="PaymentDetail-paymentGroup.paymentDate" parent="PaymentDetail-paymentGroup.paymentDate-parentBean" />

  <bean id="PaymentDetail-paymentGroup.paymentDate-parentBean" abstract="true" parent="PaymentGroup-paymentDate">
    <property name="name" value="paymentGroup.paymentDate" />
  </bean>
  <bean id="PaymentDetail-id" parent="PaymentDetail-id-parentBean" />

  <bean id="PaymentDetail-id-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="id" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Payment Detail ID" />
    <property name="shortLabel" value="Payment ID" />
    <property name="maxLength" value="7" />
    <property name="validationPattern" >
       <bean parent="NumericValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="32" />
    </property>
    <property name="summary" value="id" />
    <property name="description" value="Payment Detail ID." />
  </bean>
  <bean id="PaymentDetail-netPaymentAmount" parent="PaymentDetail-netPaymentAmount-parentBean" />

  <bean id="PaymentDetail-netPaymentAmount-parentBean" abstract="true" parent="GenericAttributes-genericAmount">
    <property name="name" value="netPaymentAmount" />
    <property name="label" value="Net Payment Amount" />
  </bean>
  <bean id="PaymentDetail-paymentGroup.paymentStatusCode" parent="PaymentDetail-paymentGroup.paymentStatusCode-parentBean" />

  <bean id="PaymentDetail-paymentGroup.paymentStatusCode-parentBean" abstract="true" parent="PaymentGroup-paymentStatusCode">
    <property name="name" value="paymentGroup.paymentStatusCode" />
  </bean>
  <bean id="PaymentDetail-paymentGroup.batch.id" parent="PaymentDetail-paymentGroup.batch.id-parentBean" />

  <bean id="PaymentDetail-paymentGroup.batch.id-parentBean" abstract="true" parent="PaymentGroup-batch.id">
    <property name="name" value="paymentGroup.batch.id" />
  </bean>
   <bean id="PaymentDetail-paymentGroup.disbursementType.name" parent="PaymentDetail-paymentGroup.disbursementType.name-parentBean" />

  <bean id="PaymentDetail-paymentGroup.disbursementType.name-parentBean" abstract="true" parent="PaymentGroup-disbursementType.name">
    <property name="name" value="paymentGroup.disbursementType.name" />
  </bean>

<!-- Business Object Inquiry Definition -->


  <bean id="PaymentDetail-inquiryDefinition" parent="PaymentDetail-inquiryDefinition-parentBean" />

  <bean id="PaymentDetail-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
    <property name="title" value="PaymentDetail Inquiry" />
    <property name="inquirySections" >
      <list>
        <bean parent="InquirySectionDefinition">
          <property name="title" value="" />
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields" >
            <list>
              <bean parent="FieldDefinition"
                    p:attributeName="paymentGroup.payeeName" />
              <bean parent="FieldDefinition"
                    p:attributeName="paymentGroup.batch.customerProfile.chartCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="custPaymentDocNbr" />
              <bean parent="FieldDefinition"
                    p:attributeName="paymentGroup.payeeIdTypeCd" />
              <bean parent="FieldDefinition"
                    p:attributeName="purchaseOrderNbr" />
              <bean parent="FieldDefinition"
                    p:attributeName="paymentGroup.payeeId" />
              <bean parent="FieldDefinition"
                    p:attributeName="paymentGroup.batch.customerProfile.subUnitCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="invoiceNbr" />      
              <bean parent="FieldDefinition"
                    p:attributeName="paymentGroup.disbursementTypeCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="paymentGroup.processImmediate" />
              <bean parent="FieldDefinition"
                    p:attributeName="paymentGroup.processId" />
              <bean parent="FieldDefinition"
                    p:attributeName="id" />
              <bean parent="FieldDefinition"
                    p:attributeName="netPaymentAmount" />
              <bean parent="FieldDefinition"
                    p:attributeName="paymentGroup.paymentStatusCode" />  
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Business Object Lookup Definition -->


  <bean id="PaymentDetail-lookupDefinition" parent="PaymentDetail-lookupDefinition-parentBean" />

  <bean id="PaymentDetail-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
    <property name="title" value="PaymentDetail Lookup" />
    <property name="lookupableID" value="paymentDetailLookupable" />
    <property name="instructions" value="Look up a PaymentDetail" />
    <property name="defaultSort" >
      <bean parent="SortDefinition">
        <property name="attributeNames" >
          <list>
            <value>paymentGroup.payeeName</value>
          </list>
        </property>
        <property name="sortAscending" value="true" />
      </bean>
    </property>
    <property name="lookupFields" >
      <list>
              <bean parent="FieldDefinition"
                    p:attributeName="paymentGroup.payeeName" />
              <bean parent="FieldDefinition"
                    p:attributeName="paymentGroup.payeeIdTypeCd" />
              <bean parent="FieldDefinition"
                    p:attributeName="paymentGroup.payeeId" />
              <bean parent="FieldDefinition"
                    p:attributeName="paymentGroup.disbursementTypeCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="paymentGroup.disbursementNbr" />
              <bean parent="FieldDefinition"
                    p:attributeName="paymentGroup.disbursementDate" />              
              <bean parent="FieldDefinition"
                    p:attributeName="netPaymentAmount" /> 
              <bean parent="FieldDefinition"
                    p:attributeName="paymentGroup.batch.customerProfile.chartCode" />     
              <bean parent="FieldDefinition"
                    p:attributeName="paymentGroup.batch.customerProfile.orgCode" />                                                                                           
              <bean parent="FieldDefinition"
                    p:attributeName="paymentGroup.batch.customerProfile.subUnitCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="paymentGroup.processImmediate" />
              <bean parent="FieldDefinition"
                    p:attributeName="paymentGroup.pymtSpecialHandling" />
              <bean parent="FieldDefinition"
                    p:attributeName="paymentGroup.pymtAttachment" />
              <bean parent="FieldDefinition"
                    p:attributeName="paymentGroup.paymentDate" />                                                                                 
              <bean parent="FieldDefinition"
                    p:attributeName="custPaymentDocNbr" />
              <bean parent="FieldDefinition"
                    p:attributeName="invoiceNbr" />                    
              <bean parent="FieldDefinition"
                    p:attributeName="requisitionNbr" />
              <bean parent="FieldDefinition"
                    p:attributeName="paymentGroup.customerInstitutionNumber" />                                   
              <bean parent="FieldDefinition"
                    p:attributeName="purchaseOrderNbr" />
              <bean parent="FieldDefinition"
                    p:attributeName="paymentGroup.processId" />
              <bean parent="FieldDefinition"
                    p:attributeName="id" /> 
              <bean parent="FieldDefinition"
                    p:attributeName="paymentGroup.paymentStatusCode" />  
              <bean parent="FieldDefinition" p:attributeName="paymentGroup.batch.id"/>
      </list>
    </property>
    <property name="resultFields" >
      <list>     
              <bean parent="FieldDefinition"
                    p:attributeName="paymentGroup.batch.customerProfile.chartCode" />     
              <bean parent="FieldDefinition"
                    p:attributeName="paymentGroup.batch.customerProfile.orgCode" />                                                                                           
              <bean parent="FieldDefinition"
                    p:attributeName="paymentGroup.batch.customerProfile.subUnitCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="custPaymentDocNbr" />                    
              <bean parent="FieldDefinition"
                    p:attributeName="purchaseOrderNbr" />
              <bean parent="FieldDefinition"
                    p:attributeName="invoiceNbr" />   
              <bean parent="FieldDefinition"
                    p:attributeName="paymentGroup.payeeName" />
              <bean parent="FieldDefinition"
                    p:attributeName="paymentGroup.paymentDate" />   
              <bean parent="FieldDefinition"
                    p:attributeName="paymentGroup.disbursementDate" />  
              <bean parent="FieldDefinition"
                    p:attributeName="paymentGroup.paymentStatusCode" />  
              <bean parent="FieldDefinition"
                    p:attributeName="paymentGroup.disbursementType.name" />                                                       
              <bean parent="FieldDefinition"
                    p:attributeName="paymentGroup.disbursementNbr" />  
              <bean parent="FieldDefinition"
                    p:attributeName="netPaymentAmount" />                  
      </list>
    </property>
  </bean>
</beans>
