<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

	<bean id="PaymentGroup" parent="PaymentGroup-parentBean" />

	<bean id="PaymentGroup-parentBean" abstract="true"
		parent="BusinessObjectEntry">
		<property name="businessObjectClass"
			value="org.kuali.kfs.pdp.businessobject.PaymentGroup" />
		<property name="objectLabel" value="Payment Group" />
		<property name="attributes">
			<list>
				<ref bean="PaymentGroup-payeeName" />
				<ref
					bean="PaymentGroup-batch.customerProfile.chartCode" />
				<ref bean="PaymentGroup-payeeIdTypeCd" />
				<ref
					bean="PaymentGroup-batch.customerProfile.orgCode" />
				<ref bean="PaymentGroup-payeeId" />
				<ref
					bean="PaymentGroup-batch.customerProfile.subUnitCode" />
				<ref bean="PaymentGroup-disbursementTypeCode" />
				<ref bean="PaymentGroup-processImmediate" />
				<ref bean="PaymentGroup-disbursementNbr" />
				<ref bean="PaymentGroup-pymtSpecialHandling" />
				<ref bean="PaymentGroup-customerInstitutionNumber" />
				<ref bean="PaymentGroup-disbursementDate" />
				<ref bean="PaymentGroup-pymtAttachment" />
				<ref bean="PaymentGroup-processId" />
				<ref bean="PaymentGroup-paymentDate" />
				<ref bean="PaymentGroup-paymentStatusCode" />
				<ref bean="PaymentGroup-batch.id" />
				<ref bean="PaymentGroup-disbursementType.name" />
				<ref bean="PaymentGroup-paymentDetails.netPaymentAmount" />

				<ref bean="PaymentGroup-versionNumber" />
			</list>
		</property>
		<property name="collections">
			<list>
				<bean parent="CollectionDefinition"
					p:name="paymentDetails" p:label="Payment Details"
					p:shortLabel="Details"
					p:elementLabel="PaymentDetail" />
			</list>
		</property>
	</bean>

	<!-- Attribute Definitions -->


	<bean id="PaymentGroup-batch.customerProfile.chartCode"
		parent="PaymentGroup-batch.customerProfile.chartCode-parentBean" />

	<bean id="PaymentGroup-batch.customerProfile.chartCode-parentBean"
		abstract="true" parent="Batch-customerProfile.chartCode">
		<property name="name" value="batch.customerProfile.chartCode" />
	</bean>
	<bean id="PaymentGroup-payeeName"
		parent="PaymentGroup-payeeName-parentBean" />

	<bean id="PaymentGroup-payeeName-parentBean" abstract="true"
		parent="AttributeDefinition">
		<property name="name" value="payeeName" />
		<property name="forceUppercase" value="true" />
		<property name="label" value="Payee Name" />
		<property name="shortLabel" value="Payee Name" />
		<property name="maxLength" value="32" />
		<property name="validationPattern">
			<bean parent="AnyCharacterValidationPattern" />
		</property>
		<property name="control">
			<bean parent="TextControlDefinition" p:size="32" />
		</property>
		<property name="summary" value="payeeName" />
		<property name="description" value="Payee Name." />
	</bean>
	<bean id="PaymentGroup-payeeIdTypeCd"
		parent="PaymentGroup-payeeIdTypeCd-parentBean" />

	<bean id="PaymentGroup-payeeIdTypeCd-parentBean" abstract="true"
		parent="AttributeDefinition">
		<property name="name" value="payeeIdTypeCd" />
		<property name="forceUppercase" value="true" />
		<property name="label" value="Payee ID Type" />
		<property name="shortLabel" value="Payee ID Type" />
		<property name="maxLength" value="32" />
		<property name="validationPattern">
			<bean parent="AnyCharacterValidationPattern" />
		</property>
		<property name="control">
			<bean parent="TextControlDefinition" p:size="32" />
		</property>
		<property name="summary" value="payeeIdTypeCd" />
		<property name="description" value="Payee ID Type." />
	</bean>
	<bean id="PaymentGroup-batch.customerProfile.orgCode"
		parent="PaymentGroup-batch.customerProfile.orgCode-parentBean" />

	<bean id="PaymentGroup-batch.customerProfile.orgCode-parentBean"
		abstract="true" parent="Batch-customerProfile.orgCode">
		<property name="name" value="batch.customerProfile.orgCode" />
	</bean>
	<bean id="PaymentGroup-payeeId"
		parent="PaymentGroup-payeeId-parentBean" />

	<bean id="PaymentGroup-payeeId-parentBean" abstract="true"
		parent="AttributeDefinition">
		<property name="name" value="payeeId" />
		<property name="forceUppercase" value="true" />
		<property name="label" value="Payee ID" />
		<property name="shortLabel" value="Payee ID" />
		<property name="maxLength" value="7" />
		<property name="validationPattern">
			<bean parent="NumericValidationPattern" />
		</property>
		<property name="control">
			<bean parent="TextControlDefinition" p:size="32" />
		</property>
		<property name="summary" value="payeeId" />
		<property name="description" value="Payee ID." />
	</bean>
	<bean id="PaymentGroup-batch.customerProfile.subUnitCode"
		parent="PaymentGroup-batch.customerProfile.subUnitCode-parentBean" />

	<bean id="PaymentGroup-batch.customerProfile.subUnitCode-parentBean"
		abstract="true" parent="Batch-customerProfile.subUnitCode">
		<property name="name" value="batch.customerProfile.subUnitCode" />
	</bean>
	<bean id="PaymentGroup-disbursementTypeCode"
		parent="PaymentGroup-disbursementTypeCode-parentBean" />

	<bean id="PaymentGroup-disbursementTypeCode-parentBean"
		abstract="true" parent="DisbursementType-code">
		<property name="name" value="disbursementTypeCode" />
	</bean>
	<bean id="PaymentGroup-processImmediate"
		parent="PaymentGroup-processImmediate-parentBean" />

	<bean id="PaymentGroup-processImmediate-parentBean"
		abstract="true" parent="AttributeDefinition">
		<property name="name" value="processImmediate" />
		<property name="forceUppercase" value="true" />
		<property name="label" value="Immediate" />
		<property name="shortLabel" value="Immediate" />
		<property name="maxLength" value="1" />
		<property name="control">
			<bean parent="CheckboxControlDefinition" />
		</property>
		<property name="summary" value="processImmediate" />
		<property name="description" value="Immediate." />
	</bean>
	<bean id="PaymentGroup-disbursementNbr"
		parent="PaymentGroup-disbursementNbr-parentBean" />

	<bean id="PaymentGroup-disbursementNbr-parentBean" abstract="true"
		parent="AttributeDefinition">
		<property name="name" value="disbursementNbr" />
		<property name="forceUppercase" value="true" />
		<property name="label" value="Disbursement Number" />
		<property name="shortLabel" value="Disbursement Nbr" />
		<property name="maxLength" value="7" />
		<property name="validationPattern">
			<bean parent="NumericValidationPattern" />
		</property>
		<property name="control">
			<bean parent="TextControlDefinition" p:size="32" />
		</property>
		<property name="summary" value="disbursementNbr" />
		<property name="description" value="Disbursement Number." />
	</bean>
	<bean id="PaymentGroup-pymtSpecialHandling"
		parent="PaymentGroup-pymtSpecialHandling-parentBean" />

	<bean id="PaymentGroup-pymtSpecialHandling-parentBean"
		abstract="true" parent="AttributeDefinition">
		<property name="name" value="pymtSpecialHandling" />
		<property name="forceUppercase" value="true" />
		<property name="label" value="Special Handling" />
		<property name="shortLabel" value="Special Handling" />
		<property name="maxLength" value="1" />
		<property name="control">
			<bean parent="CheckboxControlDefinition" />
		</property>
		<property name="summary" value="pymtSpecialHandling" />
		<property name="description" value="Special Handling." />
	</bean>
	<bean id="PaymentGroup-customerInstitutionNumber"
		parent="PaymentGroup-customerInstitutionNumber-parentBean" />

	<bean id="PaymentGroup-customerInstitutionNumber-parentBean" abstract="true"
		parent="AttributeDefinition">
		<property name="name" value="customerInstitutionNumber" />
		<property name="forceUppercase" value="true" />
		<property name="label" value="Customer Number for Institution" />
		<property name="shortLabel" value="Customer Nbr for Institution" />
		<property name="maxLength" value="32" />
		<property name="validationPattern">
			<bean parent="AnyCharacterValidationPattern" />
		</property>
		<property name="control">
			<bean parent="TextControlDefinition" p:size="32" />
		</property>
		<property name="summary" value="customerInstitutionNumber" />
		<property name="description" value="Customer Number for Institution." />
	</bean>
	<bean id="PaymentGroup-disbursementDate"
		parent="PaymentGroup-disbursementDate-parentBean" />

	<bean id="PaymentGroup-disbursementDate-parentBean"
		abstract="true" parent="GenericAttributes-genericDate">
		<property name="name" value="disbursementDate" />
		<property name="label" value="Disbursement Date" />
		<property name="shortLabel" value="Date" />
		<property name="summary" value="disbursementDate" />
		<property name="description"
			value="disbursementDate description..." />
	</bean>
    <bean id="PaymentGroup-pymtAttachment"
		parent="PaymentGroup-pymtAttachment-parentBean" />

	<bean id="PaymentGroup-pymtAttachment-parentBean"
		abstract="true" parent="AttributeDefinition">
		<property name="name" value="pymtAttachment" />
		<property name="forceUppercase" value="true" />
		<property name="label" value="Attachment" />
		<property name="shortLabel" value="Attachment" />
		<property name="maxLength" value="1" />
		<property name="control">
			<bean parent="CheckboxControlDefinition" />
		</property>
		<property name="summary" value="pymtAttachment" />
		<property name="description" value="Attachment." />
	</bean>
    <bean id="PaymentGroup-processId"
		parent="PaymentGroup-processId-parentBean" />

	<bean id="PaymentGroup-processId-parentBean"
		abstract="true" parent="PaymentProcess-id">
		<property name="name" value="processId" />
	</bean>
	<bean id="PaymentGroup-paymentDate"
		parent="PaymentGroup-paymentDate-parentBean" />

	<bean id="PaymentGroup-paymentDate-parentBean"
		abstract="true" parent="GenericAttributes-genericDate">
		<property name="name" value="paymentDate" />
		<property name="label" value="Pay Date" />
		<property name="shortLabel" value="Date" />
		<property name="summary" value="paymentDate" />
		<property name="description"
			value="paymentDate description..." />
	</bean>
	<bean id="PaymentGroup-paymentStatusCode"
		parent="PaymentGroup-paymentStatusCode-parentBean" />

	<bean id="PaymentGroup-paymentStatusCode-parentBean"
		abstract="true" parent="PaymentStatus-code">
		<property name="name" value="paymentStatusCode" />
	</bean>
	
	<bean id="PaymentGroup-batch.id"
		parent="PaymentGroup-batch.id-parentBean" />

	<bean id="PaymentGroup-batch.id-parentBean"
		abstract="true" parent="Batch-id">
		<property name="name" value="batch.id" />
	</bean>
    <bean id="PaymentGroup-disbursementType.name"
		parent="PaymentGroup-disbursementType.name-parentBean" />

	<bean id="PaymentGroup-disbursementType.name-parentBean"
		abstract="true" parent="DisbursementType-name">
		<property name="name" value="disbursementType.name" />
	</bean>
	<bean id="PaymentGroup-paymentDetails.netPaymentAmount"
		parent="PaymentGroup-paymentDetails.netPaymentAmount-parentBean" />

	<bean id="PaymentGroup-paymentDetails.netPaymentAmount-parentBean"
		abstract="true" parent="PaymentDetail-netPaymentAmount">
		<property name="name" value="paymentDetails.netPaymentAmount" />
	</bean>
	
	

	<bean id="PaymentGroup-versionNumber"
		parent="PaymentGroup-versionNumber-parentBean" />

	<bean id="PaymentGroup-versionNumber-parentBean" abstract="true"
		parent="GenericAttributes-versionNumber">
	</bean>


</beans>
