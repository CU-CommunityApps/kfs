<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xmlns:dd="http://rice.kuali.org/dd"
    xsi:schemaLocation="
http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd
http://rice.kuali.org/dd http://rice.kuali.org/dd/dd.xsd">

  <bean id="PayeeType" parent="PayeeType-parentBean" />

  <bean id="PayeeType-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.kfs.pdp.businessobject.PayeeType" />
    <property name="inquiryDefinition" >
      <ref bean="PayeeType-inquiryDefinition" />
    </property>
    <property name="lookupDefinition" >
      <ref bean="PayeeType-lookupDefinition" />
    </property>
    <property name="objectLabel" value="Payee Type" />
    <property name="attributes" >
      <list>
        <ref bean="PayeeType-payeeTypeCode" />
        <ref bean="PayeeType-description" />
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->


  <bean id="PayeeType-payeeTypeCode" parent="PayeeType-payeeTypeCode-parentBean" />

  <bean id="PayeeType-payeeTypeCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="payeeTypeCode" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Payee Type Code" />
    <property name="shortLabel" value="Payee Type Code" />
    <property name="maxLength" value="1" />
    <property name="required" value="true" />
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="1" />
    </property>
    <property name="summary" value="Payee Type Code" />
    <property name="description" value="Payee Type Code" />
    <!--property name="allowsNewOrCopy" value="false" /-->
  </bean>
  
  <bean id="PayeeType-description" parent="PayeeType-description-parentBean" />

  <bean id="PayeeType-description-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="description" />
    <property name="forceUppercase" value="false" />
    <property name="label" value="Payee Type Description" />
    <property name="shortLabel" value="Payee Type Description" />
    <property name="maxLength" value="200" />
    <property name="required" value="true" />
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="40" />
    </property>
    <property name="summary" value="Payment Type Description" />
    <property name="description" value="Payment Type Description" />
  </bean>
  
<!-- Business Object Inquiry Definition -->


  <bean id="PayeeType-inquiryDefinition" parent="PayeeType-inquiryDefinition-parentBean" />

  <bean id="PayeeType-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
    <property name="title" value="Payee Type Inquiry" />
    <property name="inquirySections" >
      <list>
        <bean parent="InquirySectionDefinition">
          <property name="title" value="" />
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields" >
            <list>
              <bean parent="FieldDefinition"
                    p:attributeName="payeeTypeCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="description" />
              
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Business Object Lookup Definition -->


  <bean id="PayeeType-lookupDefinition" parent="PayeeType-lookupDefinition-parentBean" />

  <bean id="PayeeType-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
    <property name="title" value="Payee Type Lookup" />
    <property name="instructions" value="Lookup a Payee Type" />
    <property name="defaultSort" >
      <bean parent="SortDefinition">
        <property name="attributeNames" >
          <list>
            <value>payeeTypeCode</value>
          </list>
        </property>
      </bean>
    </property>
    <property name="lookupFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="payeeTypeCode" />
        <bean parent="FieldDefinition"
              p:attributeName="description" />
        
      </list>
    </property>
    <property name="resultFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="payeeTypeCode" />
        <bean parent="FieldDefinition"
              p:attributeName="description" />
      </list>
    </property>
  </bean>
</beans>
