<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "../../../core/datadictionary/dataDictionary-1_0.dtd">
<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.module.pdp.bo.AchBank">

        <inquiry>
            <title>Ach Bank Inquiry</title>

            <inquirySections><inquirySection title="Header Text"><inquiryFields>
                <field attributeName="bankRoutingNumber" />
                <field attributeName="bankOfficeCode" />
                <field attributeName="bankServiceNumber" />
                <field attributeName="bankTypeCode" />
                <field attributeName="bankNewRoutingNumber" />
                <field attributeName="bankName" />
                <field attributeName="bankStreetAddress" />
                <field attributeName="bankCityName" />
                <field attributeName="bankStateCode" />
                <field attributeName="bankZipCode" />
                <field attributeName="bankZipExtensionCode" />
                <field attributeName="bankPhoneAreaCode" />
                <field attributeName="bankPhonePrefixNumber" />
                <field attributeName="bankPhoneSuffixNumber" />
                <field attributeName="bankInstitutionStatusCode" />
                <field attributeName="bankDataViewCode" />
            </inquiryFields></inquirySection></inquirySections>
        </inquiry>
        
        <lookup>
            <title>Ach Bank Lookup</title>

            <instructions>Lookup an Ach Bank</instructions>

            <defaultSort attributeName="bankRoutingNumber" sortAscending="true" />

            <lookupFields>
                <lookupField attributeName="bankRoutingNumber" required="false" />
                <lookupField attributeName="bankName" required="false" />
            </lookupFields>

            <resultFields>
                <field attributeName="bankRoutingNumber" />
                <field attributeName="bankName" />
                <field attributeName="bankCityName" />
                <field attributeName="bankStateCode" />
                <field attributeName="bankZipCode" />
            </resultFields>
        </lookup>
        
       <attributes>

            <attribute name="bankRoutingNumber" forceUppercase="true">
                <label>Bank Routing Number</label>
                <shortLabel>Bank Routing Number</shortLabel>
                <maxLength>9</maxLength>
                <validationPattern>
                    <numeric />
                </validationPattern>
                <required>true</required>
                <control>
                    <text size="9" />
                </control>
                <summary>Bank Routing Number</summary>
                <description>The institution's routing number.</description>
            </attribute>
           
            <attribute name="bankOfficeCode" forceUppercase="true">
                <label>Bank Office Code</label>
                <shortLabel>Office Code</shortLabel>
                <maxLength>1</maxLength>
                <validationPattern>
                    <alphaNumeric />
                </validationPattern>
                <required>true</required>
                <control>
                    <text size="3" />
                </control>
                <summary>Bank Office Code</summary>
                <description>Main office or branch O=main B=branch.</description>
            </attribute>

             <attribute name="bankServiceNumber" forceUppercase="true">
                <label>Bank Service Number</label>
                <shortLabel>Bank Service Number</shortLabel>
                <maxLength>9</maxLength>
                <validationPattern>
                    <numeric />
                </validationPattern>
                <required>true</required>
                <control>
                    <text size="9" />
                </control>
                <summary>Bank Service Number</summary>
                <description>Servicing Fed's main office routing number.</description>
            </attribute>
  
             <attribute name="bankTypeCode" forceUppercase="true">
                <label>Record Type Code</label>
                <shortLabel>Record Type Code</shortLabel>
                <maxLength>1</maxLength>
                <validationPattern>
                    <alphaNumeric />
                </validationPattern>
                <required>true</required>
                <control>
                    <text size="1" />
                </control>
                <summary>Record Type Code</summary>
                <description>The code indicating the ABA number to be used to route or send ACH items to the RFI
                		 0 = Institution is a Federal Reserve Bank
                		 1 = Send items to customer routing number
                		 2 = Send items to customer using new routing number field</description>
            </attribute> 

             <attribute name="bankNewRoutingNumber" forceUppercase="true">
                <label>Bank New Routing Number</label>
                <shortLabel>Bank New Routing Number</shortLabel>
                <maxLength>9</maxLength>
                <validationPattern>
                    <numeric />
                </validationPattern>
                <required>true</required>
                <control>
                    <text size="9" />
                </control>
                <summary>Bank New Routing Number</summary>
                <description>Institution's new routing number resulting from a merger or renumber</description>
            </attribute>

             <attribute name="bankName" forceUppercase="true">
                <label>Bank Name</label>
                <shortLabel>Bank Name</shortLabel>
                <maxLength>36</maxLength>
                <validationPattern>
                    <anyCharacter allowWhitespace="true" />
                </validationPattern>
                <required>true</required>
                <control>
                    <text size="38" />
                </control>
                <summary>Bank Name</summary>
                <description>Commonly used abbreviated name.</description>
            </attribute>

             <attribute name="bankStreetAddress" forceUppercase="true">
                <label>Bank Street Address</label>
                <shortLabel>Bank Street Address</shortLabel>
                <maxLength>36</maxLength>
                <validationPattern>
                    <anyCharacter allowWhitespace="true" />
                </validationPattern>
                <required>true</required>
                <control>
                    <text size="38" />
                </control>
                <summary>Bank Street Address</summary>
                <description>Delivery address</description>
            </attribute>

             <attribute name="bankCityName" forceUppercase="true">
                <label>Bank City Name</label>
                <shortLabel>Bank City Name</shortLabel>
                <maxLength>20</maxLength>
                <validationPattern>
                    <anyCharacter allowWhitespace="true" />
                </validationPattern>
                <required>true</required>
                <control>
                    <text size="22" />
                </control>
                <summary>Bank City Name</summary>
                <description>City name in the delivery address</description>
            </attribute>

			<attributeReference name="bankStateCode" sourceClassName="org.kuali.kfs.bo.State" sourceAttributeName="postalStateCode" />

             <attribute name="bankZipCode" forceUppercase="true">
                <label>Bank Zip Code</label>
                <shortLabel>Bank Zip Code</shortLabel>
                <maxLength>5</maxLength>
                <validationPattern>
                    <numeric />
                </validationPattern>
                <required>true</required>
                <control>
                    <text size="5" />
                </control>
                <summary>Bank Zip Code</summary>
                <description>Zip code in the delivery address</description>
            </attribute>

             <attribute name="bankZipExtensionCode" forceUppercase="true">
                <label>Bank Zip Extension Code</label>
                <shortLabel>Bank Zip Extension Code</shortLabel>
                <maxLength>4</maxLength>
                <validationPattern>
                    <numeric />
                </validationPattern>
                <required>true</required>
                <control>
                    <text size="4" />
                </control>
                <summary>Bank Zip Extension Code</summary>
                <description>Zip code extension in the delivery address</description>
            </attribute>       

             <attribute name="bankPhoneAreaCode" forceUppercase="true">
                <label>Bank Phone Area Code</label>
                <shortLabel>Bank Phone Area Code</shortLabel>
                <maxLength>3</maxLength>
                <validationPattern>
                    <numeric />
                </validationPattern>
                <required>true</required>
                <control>
                    <text size="3" />
                </control>
                <summary>Bank Phone Area Code</summary>
                <description>Area code of the CRF contact telephone number</description>
            </attribute>

             <attribute name="bankPhonePrefixNumber" forceUppercase="true">
                <label>Bank Phone Prefix Number</label>
                <shortLabel>Bank Phone Prefix Number</shortLabel>
                <maxLength>3</maxLength>
                <validationPattern>
                    <numeric />
                </validationPattern>
                <required>true</required>
                <control>
                    <text size="3" />
                </control>
                <summary>Bank Phone Prefix Number</summary>
                <description>Prefix of the CRF contact telephone number</description>
            </attribute>

             <attribute name="bankPhoneSuffixNumber" forceUppercase="true">
                <label>Bank Phone Suffix Number</label>
                <shortLabel>Bank Phone Suffix Number</shortLabel>
                <maxLength>4</maxLength>
                <validationPattern>
                    <numeric />
                </validationPattern>
                <required>true</required>
                <control>
                    <text size="4" />
                </control>
                <summary>Bank Phone Suffix Number</summary>
                <description>Suffix of the CRF contact telephone number</description>
            </attribute>

             <attribute name="bankInstitutionStatusCode" forceUppercase="true">
                <label>Bank Institution Status Code</label>
                <shortLabel>Bank Institution Status Code</shortLabel>
                <maxLength>1</maxLength>
                <validationPattern>
                    <numeric />
                </validationPattern>
                <required>false</required>
                <control>
                    <text size="1" />
                </control>
                <summary>Bank Institution Status Code</summary>
                <description>Code is based on the customers receiver code
                             1=Receives Gov/Comm
                </description>
            </attribute>

             <attribute name="bankDataViewCode" forceUppercase="true">
                <label>Bank Data View Code</label>
                <shortLabel>Bank Data View Code</shortLabel>
                <maxLength>1</maxLength>
                <validationPattern>
                    <numeric />
                </validationPattern>
                <required>false</required>
                <control>
                    <text size="1" />
                </control>
                <summary>Bank Data View Code</summary>
                <description>1=Current view</description>
            </attribute>

            <attributeReference name="versionNumber" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="versionNumber" />
			
        </attributes>
        
    </businessObject>
</dictionaryEntry>