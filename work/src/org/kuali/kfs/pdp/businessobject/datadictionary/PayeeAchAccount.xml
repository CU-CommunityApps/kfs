<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "../../../core/datadictionary/dataDictionary-1_0.dtd">
<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.module.pdp.bo.PayeeAchAccount">

        <inquiry>
            <title>Payee ACH Account</title>
            
            <inquirySections><inquirySection title="Header Text"><inquiryFields>
                <field attributeName="achAccountGeneratedIdentifier" />
                <field attributeName="bankRoutingNumber" />
                <field attributeName="bankAccountNumber" />
                <field attributeName="payeeName" />
                <field attributeName="payeeEmailAddress" />
                <field attributeName="payeeIdentifierTypeCode" />
                <field attributeName="vendorHeaderGeneratedIdentifier" />
                <field attributeName="vendorDetailAssignedIdentifier" />
                <field attributeName="disbVchrPayeeIdNumber" />
                <field attributeName="personUniversalIdentifier" />
                <field attributeName="payeeSocialSecurityNumber" />
                <field attributeName="payeeFederalEmployerIdentificationNumber" />
                <field attributeName="psdTransactionCode" />
                <field attributeName="active" />
                <field attributeName="bankAccountTypeCode" />
            </inquiryFields></inquirySection></inquirySections>
        </inquiry>
        
        <lookup>
            <title>Payee ACH Account Lookup</title>

            <instructions>Lookup a Payee ACH Account</instructions>

            <defaultSort attributeName="achAccountGeneratedIdentifier" sortAscending="true" />

            <lookupFields>
                <lookupField attributeName="bankRoutingNumber" />
                <lookupField attributeName="bankAccountNumber" />
                <lookupField attributeName="bankAccountTypeCode" />
                <lookupField attributeName="payeeName" />
                <lookupField attributeName="payeeIdentifierTypeCode" />
                <lookupField attributeName="vendorHeaderGeneratedIdentifier" />
                <lookupField attributeName="vendorDetailAssignedIdentifier" />
                <lookupField attributeName="disbVchrPayeeIdNumber" />
                <lookupField attributeName="personUniversalIdentifier" />
                <lookupField attributeName="payeeSocialSecurityNumber" />
                <lookupField attributeName="payeeFederalEmployerIdentificationNumber" />
                <lookupField attributeName="psdTransactionCode" />
                <lookupField attributeName="active" />
            </lookupFields>

            <resultFields>
                <field attributeName="payeeName" />
                <field attributeName="payeeEmailAddress" />
                <field attributeName="payeeIdentifierTypeCode" />
                <field attributeName="active" />
            </resultFields>
        </lookup>
        
       <attributes>
            <attribute name="achAccountGeneratedIdentifier" forceUppercase="true">
                <label>ACH Account Generated Identifier</label>
                <shortLabel>ACH Account ID</shortLabel>
                <maxLength>10</maxLength>
                <validationPattern>
                    <numeric />
                </validationPattern>
                <required>true</required>
                <control>
                    <hidden />
                </control>
                <summary>ACH Account Generated Identifier</summary>
                <description>ACH Account Generated ID.</description>
            </attribute>
           
            <attribute name="bankAccountNumber" forceUppercase="true">
                <label>Bank Account Number</label>
                <shortLabel>Bank Account Number</shortLabel>
                <maxLength>17</maxLength>
                <validationPattern>
                    <alphaNumeric />
                </validationPattern>
                <required>true</required>
                <control>
                    <text size="17" />
                </control>
                <attributeAuthorization>
					<displayWorkgroup>KUALI_PDP_ACH</displayWorkgroup>
					<displayMask>
						<maskLiteral literal="*****************" />
					</displayMask>
				</attributeAuthorization>
                <summary>Bank Account Number</summary>
                <description>Bank Account Number.</description>
            </attribute>
            
            <attribute name="bankAccountTypeCode" >
                <label>Account Type</label>
                <shortLabel>Account Type</shortLabel>
                <maxLength>2</maxLength>
                <validationPattern>
                    <charset validChars="23" />
                </validationPattern>
                <required>true</required>
                <control>
                    <select valuesFinderClass="org.kuali.module.chart.lookup.keyvalues.CheckingSavingsValuesFinder" />
                </control>
                <summary>Bank Account Type Code summary...</summary>
                <description>22=Checking 32=Savings.</description>
            </attribute>

             <attribute name="payeeName" forceUppercase="true">
                <label>Name</label>
                <shortLabel>Name</shortLabel>
                <maxLength>50</maxLength>
                <validationPattern>
                    <anyCharacter allowWhitespace="true" />
                </validationPattern>
                <required>true</required>
                <control>
                    <text size="51" />
                </control>
                <summary>Name</summary>
                <description>Payee Name.</description>
            </attribute>
  
             <attribute name="payeeEmailAddress" >
                <label>Email Address</label>
                <shortLabel>Email Address</shortLabel>
                <maxLength>43</maxLength>
                <validationPattern>
                    <emailAddress />
                </validationPattern>
                <required>true</required>
                <control>
                    <text size="43" />
                </control>
                <summary>Payee Email Address</summary>
                <description>Payee Email Address.</description>
             </attribute>
             
             <attribute name="payeeIdentifierTypeCode" forceUppercase="true">
                <label>Payee Type Code</label>
                <shortLabel>Payee Type Code</shortLabel>
                <maxLength>1</maxLength>
                <validationPattern>
                    <charset validChars="EFPSV" />
                </validationPattern>
                <required>true</required>
                <control>
                    <select valuesFinderClass="org.kuali.module.chart.lookup.keyvalues.PayeeTypeCodeValuesFinder" />
                </control>
                <summary>Payee Type Code</summary>
                <description>E=Employee V=Vendor F=FEIN S=SSN P=DV Payee</description>
            </attribute>

             <attribute name="payeeSocialSecurityNumber" >
                <label>Social Security Number</label>
                <shortLabel>SSN</shortLabel>
                <maxLength>9</maxLength>
                <validationPattern>
                    <numeric />
                </validationPattern>
                <control>
                    <text size="9" />
                </control>
                <attributeAuthorization>
					<displayWorkgroup>KAULI_PDP_ACH</displayWorkgroup>
					<displayMask>
						<maskLiteral literal="*********" />
					</displayMask>
				</attributeAuthorization>
                <summary>Payee SSN</summary>
                <description>Payee SSN.</description>
            </attribute>

             <attribute name="payeeFederalEmployerIdentificationNumber" forceUppercase="true">
                <label>FEIN Number</label>
                <shortLabel>FEIN Number</shortLabel>
                <maxLength>9</maxLength>
                <validationPattern>
                    <alphaNumeric />
                </validationPattern>
                <control>
                    <text size="9" />
                </control>
                <summary>Federal Employee Identification Number</summary>
                <description>Federal Employee Identification Number.</description>
            </attribute>

             <attribute name="psdTransactionCode" forceUppercase="true">
                <label>PSD Transaction Code</label>
                <shortLabel>PSD Transaction Code</shortLabel>
                <maxLength>4</maxLength>
                <validationPattern>
                    <anyCharacter />
                </validationPattern>
                <required>true</required>
                <control>
                    <text size="5" />
                </control>
                <summary>PSD Transaction Code</summary>
                <description>PSD Transaction Code.</description>
            </attribute>
            
            <attributeReference name="bankRoutingNumber" sourceClassName="org.kuali.module.pdp.bo.AchBank" sourceAttributeName="bankRoutingNumber" />
            
			<attributeReference name="vendorHeaderGeneratedIdentifier" sourceClassName="org.kuali.module.vendor.bo.VendorDetail" sourceAttributeName="vendorHeaderGeneratedIdentifier">
                <label>Vendor</label>
                <shortLabel>Vendor</shortLabel>
                <control>
                    <lookupHidden/>
                </control>
			</attributeReference>
			
			<attributeReference name="vendorDetailAssignedIdentifier" sourceClassName="org.kuali.module.vendor.bo.VendorDetail" sourceAttributeName="vendorDetailAssignedIdentifier">
                <label>Vendor</label>
                <shortLabel>Vendor</shortLabel>
                <control>
                    <hidden />
                </control>
			</attributeReference>
             
            <attributeReference name="disbVchrPayeeIdNumber" sourceClassName="org.kuali.module.financial.bo.Payee" sourceAttributeName="payeeIdNumber">
            	<label>Disbursement Voucher Payee ID</label>
            </attributeReference> 

             <attributeReference name="personUniversalIdentifier" sourceClassName="org.kuali.core.bo.user.UniversalUser" sourceAttributeName="personUniversalIdentifier">
                <label>Payee User ID</label>
                <shortLabel>Payee User ID</shortLabel>
            </attributeReference>

            <attributeReference name="active" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="activeIndicator" >
            	<required>true</required>
            </attributeReference>      

            <attributeReference name="versionNumber" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="versionNumber" />
			
        </attributes>
        
    </businessObject>
</dictionaryEntry>