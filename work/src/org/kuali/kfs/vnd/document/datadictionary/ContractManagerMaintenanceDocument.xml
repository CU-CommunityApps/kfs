<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="ContractManagerMaintenanceDocument" parent="ContractManagerMaintenanceDocument-parentBean" />

  <bean id="ContractManagerMaintenanceDocument-parentBean" abstract="true" parent="MaintenanceDocumentEntry">
    <property name="businessObjectClass" value="org.kuali.kfs.vnd.businessobject.ContractManager" />
    <property name="maintainableClass" value="org.kuali.core.maintenance.KualiMaintainableImpl" />
    <property name="maintainableSections" >
      <list>
        <ref bean="ContractManagerMaintenanceDocument-EditContractManager" />
      </list>
    </property>
    <property name="lockingKeys" >
      <list>
        <value>contractManagerCode</value>
      </list>
    </property>
    <property name="label" value="Contract Manager" />
    <property name="shortLabel" value="Contract Manager" />
    <property name="helpDefinition" >
      <bean parent="HelpDefinition"
            p:parameterNamespace="KFS-VND"
            p:parameterName="CONTRACT_MANAGER"
            p:parameterDetailType="ContractManager" />
    </property>
    <property name="documentTypeName" value="ContractManagerMaintenanceDocument" />
    <property name="documentAuthorizerClass" value="org.kuali.kfs.sys.document.web.struts.FinancialSystemMaintenanceDocumentAuthorizerBase" />
    <property name="documentTypeCode" value="PMCO" />
    <property name="workflowProperties" >
      <ref bean="ContractManagerMaintenanceDocument-workflowProperties" />
    </property>
    <property name="authorizations" >
      <list>
        <bean parent="AuthorizationDefinition">
          <property name="action" value="initiate" />
          <property name="authorizedGroups" >
            <set>
              <value>SY_MAINTENANCE_USERS</value>
            </set>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Maintenance Section Definitions -->


  <bean id="ContractManagerMaintenanceDocument-EditContractManager" parent="ContractManagerMaintenanceDocument-EditContractManager-parentBean" />

  <bean id="ContractManagerMaintenanceDocument-EditContractManager-parentBean" abstract="true" parent="MaintainableSectionDefinition">
    <property name="id" value="Edit Contract Manager" />
    <property name="title" value="Edit Contract Manager" />
    <property name="maintainableItems" >
      <list>
        <bean parent="MaintainableFieldDefinition"
              p:name="contractManagerCode"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="contractManagerUserIdentifier"
              p:required="true"
              p:overrideFieldConversions="personUserIdentifier:contractManagerUserIdentifier,personName:contractManagerName,personLocalPhoneNumber:contractManagerPhoneNumber"
              p:overrideLookupClass="org.kuali.core.bo.user.UniversalUser" />
        <bean parent="MaintainableFieldDefinition"
              p:name="contractManagerName"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="contractManagerPhoneNumber"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="contractManagerFaxNumber"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="contractManagerDelegationDollarLimit"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="contractManagerSignatureImageLocationDescription"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="versionNumber" />
      </list>
    </property>
  </bean>

<!-- Exported Workflow Properties -->


  <bean id="ContractManagerMaintenanceDocument-workflowProperties" parent="ContractManagerMaintenanceDocument-workflowProperties-parentBean" />

  <bean id="ContractManagerMaintenanceDocument-workflowProperties-parentBean" abstract="true" parent="WorkflowProperties">
  </bean>
</beans>
