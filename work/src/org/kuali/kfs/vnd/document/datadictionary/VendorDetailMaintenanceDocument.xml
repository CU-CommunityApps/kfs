<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "../../../../core/datadictionary/dataDictionary-1_0.dtd">

<dictionaryEntry>
    <maintenanceDocument>
        <businessObjectClass>org.kuali.module.vendor.bo.VendorDetail</businessObjectClass>
        <maintainableClass>org.kuali.module.vendor.maintenance.VendorMaintainableImpl</maintainableClass>
        <businessRulesClass>org.kuali.module.vendor.rules.VendorRule</businessRulesClass>
        <preRulesCheckClass>org.kuali.module.vendor.rules.VendorPreRules</preRulesCheckClass>
        <documentAuthorizerClass>org.kuali.module.vendor.document.authorization.VendorDocumentAuthorizer</documentAuthorizerClass>
        <authorizations>
            <authorization action="initiate">
                <workgroups>
                    <workgroup>kualiUniversalGroup</workgroup>
                </workgroups>
            </authorization>
        </authorizations>

        <documentTypeName>VendorDetailMaintenanceDocument</documentTypeName>
        <documentTypeCode>PVEN</documentTypeCode>

        <label>Vendor</label>
        <help parameterNamespace="KFS-VN" parameterDetailType="VendorDetail" parameterName="VENDOR" />

        <lockingKeys>
            <field attributeName="vendorHeaderGeneratedIdentifier" />
            <field attributeName="vendorDetailAssignedIdentifier" />
        </lockingKeys>

        <maintainableSections>
            <maintainableSection title="Vendor">
                <maintainableItems>
                    <subSectionHeader name="General Information" />
					<maintainableField name="vendorHeaderGeneratedIdentifier" readOnly="true"/>
                    <maintainableField name="vendorDetailAssignedIdentifier" readOnly="true"/>
                    <maintainableField name="vendorNumber" readOnly="true"/>
                    <maintainableField name="vendorName" />
                    <maintainableField name="vendorLastName" />
                    <maintainableField name="vendorFirstName" />
                    <!--  To the next row -->
                    <subSectionHeader name="Corporate Information" />
                    <maintainableField name="vendorHeader.vendorTypeCode" required="true"/>
                    <maintainableField name="vendorHeader.vendorForeignIndicator" required="true">
                        <defaultValue>No</defaultValue>
                    </maintainableField>   
                    <maintainableField name="vendorHeader.vendorTaxNumber" >
                      <fieldAuthorization>
                        <displayEditMode>taxEntry</displayEditMode>
                        <displayMask><maskLiteral literal="*********"/></displayMask>
                      </fieldAuthorization>
                    </maintainableField>
                    
                    <maintainableField name="vendorHeader.vendorTaxTypeCode" />
                    <maintainableField name="vendorHeader.vendorOwnershipCode" required="true"/>
                    <maintainableField name="vendorHeader.vendorOwnershipCategoryCode" />
                    <!--  To the next column -->
                    <maintainableField name="vendorHeader.vendorW9ReceivedIndicator" />
                    <maintainableField name="vendorHeader.vendorW8BenReceivedIndicator" />
                    <maintainableField name="vendorHeader.vendorFederalWithholdingTaxBeginningDate" />
                    <maintainableField name="vendorHeader.vendorFederalWithholdingTaxEndDate" />
                    <maintainableField name="vendorHeader.vendorDebarredIndicator" />
                    <!--  To the next row -->
                    <subSectionHeader name="Detail Information" />
                    <maintainableField name="vendorPaymentTermsCode" />
                    <maintainableField name="vendorPrepaymentIndicator" />
                    <maintainableField name="vendorCreditCardIndicator" />
                    <maintainableField name="vendorMinimumOrderAmount" />
                    <maintainableField name="vendorShippingTitleCode" />
                    <maintainableField name="vendorShippingPaymentTermsCode" />
                    <!--  To the next column -->
                    <maintainableField name="vendorDunsNumber" />
                    <maintainableField name="vendorUrlAddress" />
                    <maintainableField name="vendorConfirmationIndicator" />
                    <maintainableField name="vendorSoldToGeneratedIdentifier" />
                    <maintainableField name="vendorSoldToAssignedIdentifier" />
                    <maintainableField name="vendorSoldToNumber" readOnly="true"/>
                    <maintainableField name="vendorSoldToName" readOnly="true"/>
                    <maintainableField name="vendorRestrictedIndicator" />
                    <maintainableField name="vendorRestrictedDate" readOnly="true"/>
                    <maintainableField name="vendorRestrictedPersonIdentifier" />
                    <maintainableField name="vendorRestrictedPerson.personName" readOnly="true"/>
                    <maintainableField name="vendorRestrictedPerson.personUserIdentifier" readOnly="true"/>                   
                    <maintainableField name="vendorRestrictedReasonText" />
                    <maintainableField name="vendorRemitName" />
                    <maintainableField name="activeIndicator">
                        <defaultValue>true</defaultValue>
                    </maintainableField>
                    <maintainableField name="vendorInactiveReasonCode" />
                    <maintainableField name="versionNumber" />
                    <maintainableField name="vendorHeader.versionNumber" />
                    <maintainableField name="vendorParentIndicator">
	                    <defaultValue>true</defaultValue>
                    </maintainableField>
                    <maintainableField name="vendorFirstLastNameIndicator" />
                </maintainableItems>
            </maintainableSection>
            <maintainableSection title="Address">
                <maintainableItems>
                    <maintainableCollection name="vendorAddresses" businessObjectClass="org.kuali.module.vendor.bo.VendorAddress" summaryTitle="Address">
                        <subSectionHeader name="General Information" />
                    	<maintainableField name="vendorAddressGeneratedIdentifier" readOnly="true"/>
                        <maintainableField name="vendorAddressTypeCode" required="true" />
                        <maintainableField name="vendorLine1Address" required="true"/>
                        <maintainableField name="vendorLine2Address" />
                        <maintainableField name="vendorCityName" required="true"/>
                        <maintainableField name="vendorStateCode" />
                        <maintainableField name="vendorZipCode" />
                        <!--  To the next column -->
                        <maintainableField name="vendorAddressInternationalProvinceName" />
                        <maintainableField name="vendorCountryCode" required="true"/>
                        <maintainableField name="vendorAttentionName" />
                        <maintainableField name="vendorBusinessToBusinessUrlAddress" />
                        <maintainableField name="vendorFaxNumber" />
                        <maintainableField name="vendorAddressEmailAddress" />
                        <maintainableField name="vendorDefaultAddressIndicator" />
                        <!--  maintainableField name="vendorDefaultAddresses" /-->
                        <maintainableField name="newCollectionRecord" />
                        <maintainableField name="versionNumber" />
                        <maintainableField name="vendorHeaderGeneratedIdentifier"  readOnly="true" />
           		        <maintainableField name="vendorDetailAssignedIdentifier"  readOnly="true" />
	                    <maintainableField name="active">
	                        <defaultValue>true</defaultValue>
	                    </maintainableField>
                        
                        <maintainableCollection name="vendorDefaultAddresses" businessObjectClass="org.kuali.module.vendor.bo.VendorDefaultAddress" summaryTitle="Default Address">
                           <maintainableField name="vendorCampusCode" required="true" />
                           <maintainableField name="vendorAddressGeneratedIdentifier"  readOnly="true" />
                           <maintainableField name="vendorDefaultAddressGeneratedIdentifier"  readOnly="true" />
                           <maintainableField name="newCollectionRecord" />
                           <maintainableField name="versionNumber" />
	                       <maintainableField name="active">
	                       	   <defaultValue>true</defaultValue>
	                       </maintainableField>                           
                           <summaryFields>
           		       		   <summaryField name="vendorCampusCode" />
           		       	   </summaryFields>
                        </maintainableCollection>                                      
                        <summaryFields>
           		       		<summaryField name="vendorAddressTypeCode" />
                            <summaryField name="vendorCityName" />
           		       	</summaryFields>
                    </maintainableCollection>
                </maintainableItems>
            </maintainableSection>

            <maintainableSection title="Contact">
                <maintainableItems>
                     <maintainableCollection name="vendorContacts" businessObjectClass="org.kuali.module.vendor.bo.VendorContact" summaryTitle="Contact">
                     	<maintainableField name="vendorContactTypeCode" required="true" />
                        <maintainableField name="vendorContactName" required="true" />
                        <maintainableField name="vendorContactEmailAddress" required="false" />
                        <maintainableField name="vendorLine1Address" required="false" />
                        <maintainableField name="vendorLine2Address" required="false" />                       
                        <maintainableField name="vendorCityName" required="false" />
                        <maintainableField name="vendorStateCode" required="false" />
                        <maintainableField name="vendorZipCode" required="false" />
                        <!--  To the next column -->
                        <maintainableField name="vendorAddressInternationalProvinceName" required="false" />
                        <maintainableField name="vendorCountryCode" required="false" />
                        <maintainableField name="vendorAttentionName" required="false" />                      
                        <maintainableField name="vendorContactCommentText" required="false" />
                        <maintainableField name="newCollectionRecord" />
                        <maintainableField name="versionNumber" />
                        <maintainableField name="vendorContactGeneratedIdentifier"  readOnly="true" />
                        <maintainableField name="vendorHeaderGeneratedIdentifier"  readOnly="true" />
           		        <maintainableField name="vendorDetailAssignedIdentifier"  readOnly="true" />
	                    <maintainableField name="active">
	                        <defaultValue>true</defaultValue>
	                    </maintainableField>
                        
                        <!-- The following collection should display below, in a single column. -->                        
                        <maintainableCollection name="vendorContactPhoneNumbers" businessObjectClass="org.kuali.module.vendor.bo.VendorContactPhoneNumber" summaryTitle="Phone Number">
                           <maintainableField name="vendorPhoneTypeCode" required="true" />
                           <maintainableField name="vendorPhoneNumber" required="true" />
                           <maintainableField name="vendorPhoneExtensionNumber" required="false" />
                           <maintainableField name="newCollectionRecord" />
                           <maintainableField name="versionNumber" />
                           <maintainableField name="vendorContactGeneratedIdentifier"  readOnly="true" />
                           <maintainableField name="vendorContactPhoneGeneratedIdentifier"  readOnly="true" />
	                       <maintainableField name="active">
	                       	   <defaultValue>true</defaultValue>
	                       </maintainableField>
                        <summaryFields>
           		       		<summaryField name="vendorPhoneTypeCode" />
                            <summaryField name="vendorPhoneNumber" />
           		       	</summaryFields>
                        </maintainableCollection>
                        <summaryFields>
           		       		<summaryField name="vendorContactTypeCode" />
                            <summaryField name="vendorContactName" />
           		       	</summaryFields>
                    </maintainableCollection>
                </maintainableItems>
            </maintainableSection>

            <maintainableSection title="Supplier Diversity" >
                <maintainableItems>
                     <maintainableCollection name="vendorHeader.vendorSupplierDiversities" businessObjectClass="org.kuali.module.vendor.bo.VendorSupplierDiversity" summaryTitle="Supplier Diversity">
                        <maintainableField name="vendorSupplierDiversityCode" required="true" />
                        <maintainableField name="vendorHeaderGeneratedIdentifier"  readOnly="true" />
                        <maintainableField name="newCollectionRecord" />
                        <maintainableField name="versionNumber" />
	                    <maintainableField name="active">
	                        <defaultValue>true</defaultValue>
	                    </maintainableField>
                        <summaryFields>
                            <summaryField name="vendorSupplierDiversityCode" />
                        </summaryFields>
                     </maintainableCollection>
                </maintainableItems>
            </maintainableSection>
            
			<maintainableSection title="Shipping Special Conditions">
                <maintainableItems>
                     <maintainableCollection name="vendorShippingSpecialConditions" businessObjectClass="org.kuali.module.vendor.bo.VendorShippingSpecialCondition" summaryTitle="Shipping Special Condition">
                        <maintainableField name="vendorShippingSpecialConditionCode" required="true" />
                        <maintainableField name="vendorHeaderGeneratedIdentifier"  readOnly="true" />
           		        <maintainableField name="vendorDetailAssignedIdentifier"  readOnly="true" />
           		        <maintainableField name="newCollectionRecord" />
                        <maintainableField name="versionNumber" />
	                    <maintainableField name="active">
	                        <defaultValue>true</defaultValue>
	                    </maintainableField>
                        <summaryFields>
           		       		<summaryField name="vendorShippingSpecialConditionCode" />
           		       	</summaryFields>                        
                     </maintainableCollection>
                </maintainableItems>
            </maintainableSection>
					
            <maintainableSection title="Vendor Commodity Codes">
                <maintainableItems>
                     <maintainableCollection name="vendorCommodities" businessObjectClass="org.kuali.module.vendor.bo.VendorCommodityCode" summaryTitle="Vendor Commodity Code">
                        <maintainableField name="purchasingCommodityCode" required="false" />
                        <maintainableField name="vendorHeaderGeneratedIdentifier"  readOnly="true" />
                        <maintainableField name="vendorDetailAssignedIdentifier"  readOnly="true" />
                        <maintainableField name="commodityDefaultIndicator">
                            <defaultValue>true</defaultValue>
                        </maintainableField>
                        <maintainableField name="versionNumber" />
                        <maintainableField name="active">
                            <defaultValue>true</defaultValue>
                        </maintainableField>
                        <summaryFields>
                            <summaryField name="purchasingCommodityCode" />
                        </summaryFields>       
                        <duplicateIdentificationFields>
                            <duplicateIdentificationField name="purchasingCommodityCode"/>
                        </duplicateIdentificationFields>                     
                     </maintainableCollection>
                </maintainableItems>
            </maintainableSection>
            						
            <maintainableSection title="Search Alias">
                <maintainableItems>
                     <maintainableCollection name="vendorAliases" businessObjectClass="org.kuali.module.vendor.bo.VendorAlias">
                        <maintainableField name="vendorAliasName" required="true" />
                        <maintainableField name="newCollectionRecord" />
                        <maintainableField name="versionNumber" />
	                    <maintainableField name="active">
	                        <defaultValue>true</defaultValue>
	                    </maintainableField>
                        <summaryFields>
           		       		<summaryField name="vendorAliasName" />
           		       	</summaryFields>                            
                     </maintainableCollection>
                </maintainableItems>
            </maintainableSection>

            <maintainableSection title="Vendor Phone Number">
                <maintainableItems>
                    <maintainableCollection name="vendorPhoneNumbers" businessObjectClass="org.kuali.module.vendor.bo.VendorPhoneNumber">
                        <maintainableField name="vendorPhoneTypeCode" required="true"/>
                        <maintainableField name="vendorPhoneNumber" required="true"/>
                        <maintainableField name="vendorPhoneExtensionNumber" required="false"/>
                        <maintainableField name="vendorHeaderGeneratedIdentifier"  readOnly="true" />
                        <maintainableField name="vendorDetailAssignedIdentifier"  readOnly="true" />
                        <maintainableField name="vendorPhoneGeneratedIdentifier"  readOnly="true" />    
                        <maintainableField name="newCollectionRecord" />                    
                        <maintainableField name="versionNumber" />
	                    <maintainableField name="active">
	                        <defaultValue>true</defaultValue>
	                    </maintainableField>
                        <summaryFields>
           		       		<summaryField name="vendorPhoneTypeCode" />
           		       		<summaryField name="vendorPhoneNumber" />
           		       	</summaryFields>    
                    </maintainableCollection>
                </maintainableItems>
            </maintainableSection>
            
            <maintainableSection title="Customer Number">
                <maintainableItems>
                     <maintainableCollection name="vendorCustomerNumbers" businessObjectClass="org.kuali.module.vendor.bo.VendorCustomerNumber">
                        <maintainableField name="vendorCustomerNumber" required="true" />
                        <maintainableField name="chartOfAccountsCode" required="true" />
                        <maintainableField name="vendorOrganizationCode" required="false" />
                        <maintainableField name="newCollectionRecord" />
                        <maintainableField name="versionNumber" />
                        <maintainableField name="vendorCustomerNumberGeneratedIdentifier" readOnly="true" />
                        <maintainableField name="vendorDetailAssignedIdentifier" readOnly="true" />
                        <maintainableField name="vendorHeaderGeneratedIdentifier" readOnly="true" />
	                    <maintainableField name="active">
	                        <defaultValue>true</defaultValue>
	                    </maintainableField>
                        <summaryFields>
           		       		<summaryField name="vendorCustomerNumber" />
           		       	</summaryFields>   
                     </maintainableCollection>
                </maintainableItems>
            </maintainableSection>
            
            <maintainableSection title="Contracts">
                <maintainableItems>
                     <maintainableCollection name="vendorContracts" businessObjectClass="org.kuali.module.vendor.bo.VendorContract" summaryTitle="Contract">
                        <maintainableField name="vendorContractName" required="true" />
                        <maintainableField name="vendorContractDescription" required="true" />
                        <maintainableField name="vendorCampusCode" required="true" />
                        <maintainableField name="vendorContractBeginningDate" required="true" />
                        <maintainableField name="vendorContractEndDate" required="true" />
                        <maintainableField name="contractManagerCode" required="true" />
                        <maintainableField name="purchaseOrderCostSourceCode" required="true" />
                        <maintainableField name="vendorPaymentTermsCode" required="true" />
                        <maintainableField name="vendorShippingPaymentTermsCode" required="false" />
                        <maintainableField name="vendorShippingTitleCode" required="false" />
                        <maintainableField name="vendorContractExtensionDate" required="false" />
                        <maintainableField name="organizationAutomaticPurchaseOrderLimit" required="false" />
                        <maintainableField name="newCollectionRecord" />
           		        <maintainableField name="versionNumber" />
           		        <maintainableField name="vendorContractGeneratedIdentifier"  readOnly="true" />
           		        <maintainableField name="vendorHeaderGeneratedIdentifier"  readOnly="true" />
           		        <maintainableField name="vendorDetailAssignedIdentifier"  readOnly="true" />
	                    <maintainableField name="active">
	                        <defaultValue>true</defaultValue>
	                    </maintainableField>

                        <maintainableCollection name="vendorContractOrganizations" businessObjectClass="org.kuali.module.vendor.bo.VendorContractOrganization" summaryTitle="Contract Organization">
                           <maintainableField name="chartOfAccountsCode" required="true" />
                           <maintainableField name="organizationCode" required="true" />
                           <maintainableField name="vendorContractPurchaseOrderLimitAmount" required="false" />
                           <maintainableField name="vendorContractExcludeIndicator" required="false" />
                           <maintainableField name="vendorContractGeneratedIdentifier"  readOnly="true" />
                           <maintainableField name="newCollectionRecord" />
						   <maintainableField name="versionNumber" />
	                       <maintainableField name="active">
	                       	   <defaultValue>true</defaultValue>
	                       </maintainableField>
                           <summaryFields>
                               <summaryField name="chartOfAccountsCode" />
                               <summaryField name="organizationCode" />
                           </summaryFields>
                        </maintainableCollection>
                        <summaryFields>
           		       		<summaryField name="vendorCampusCode" />
                            <summaryField name="vendorContractName" />
           		       	</summaryFields>
                    </maintainableCollection>
                </maintainableItems>
            </maintainableSection>

        </maintainableSections>

		<workflowProperties>
			<workflowPropertyGroup>
				<workflowProperty path="newMaintainableObject.businessObject.vendorHeader" />
			</workflowPropertyGroup>
		</workflowProperties>
    </maintenanceDocument>
</dictionaryEntry>