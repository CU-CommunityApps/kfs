<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="VendorInactiveReasonMaintenanceDocument" parent="VendorInactiveReasonMaintenanceDocument-parentBean" />

  <bean id="VendorInactiveReasonMaintenanceDocument-parentBean" abstract="true" parent="MaintenanceDocumentEntry">
    <property name="businessObjectClass" value="org.kuali.module.vendor.bo.VendorInactiveReason" />
    <property name="maintainableClass" value="org.kuali.core.maintenance.KualiMaintainableImpl" />
    <property name="maintainableSections" >
      <list>
        <ref bean="VendorInactiveReasonMaintenanceDocument-EditVendorInactiveReason" />
      </list>
    </property>
    <property name="lockingKeys" >
      <list>
        <value>vendorInactiveReasonCode</value>
      </list>
    </property>
    <property name="label" value="Vendor Inactive Reason" />
    <property name="shortLabel" value="Vendor Inactive Reason" />
    <property name="helpDefinition" >
      <bean parent="HelpDefinition"
            p:parameterNamespace="KFS-VN"
            p:parameterName="VENDOR_INACTIVE_REASON"
            p:parameterDetailType="VendorInactiveReason" />
    </property>
    <property name="documentTypeName" value="VendorInactiveReasonMaintenanceDocument" />
    <property name="documentAuthorizerClass" value="org.kuali.core.document.authorization.MaintenanceDocumentAuthorizerBase" />
    <property name="documentTypeCode" value="PMIR" />
    <property name="workflowProperties" >
      <ref bean="VendorInactiveReasonMaintenanceDocument-workflowProperties" />
    </property>
    <property name="authorizations" >
      <list>
        <bean parent="AuthorizationDefinition">
          <property name="action" value="initiate" />
          <property name="authorizedGroups" >
            <set>
              <value>PA_MAINTENANCE_USERS</value>
            </set>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Maintenance Section Definitions -->


  <bean id="VendorInactiveReasonMaintenanceDocument-EditVendorInactiveReason" parent="VendorInactiveReasonMaintenanceDocument-EditVendorInactiveReason-parentBean" />

  <bean id="VendorInactiveReasonMaintenanceDocument-EditVendorInactiveReason-parentBean" abstract="true" parent="MaintainableSectionDefinition">
    <property name="id" value="Edit Vendor Inactive Reason" />
    <property name="title" value="Edit Vendor Inactive Reason" />
    <property name="maintainableItems" >
      <list>
        <bean parent="MaintainableFieldDefinition"
              p:name="vendorInactiveReasonCode"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="vendorInactiveReasonDescription"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="active" />
        <bean parent="MaintainableFieldDefinition"
              p:name="versionNumber" />
      </list>
    </property>
  </bean>

<!-- Exported Workflow Properties -->


  <bean id="VendorInactiveReasonMaintenanceDocument-workflowProperties" parent="VendorInactiveReasonMaintenanceDocument-workflowProperties-parentBean" />

  <bean id="VendorInactiveReasonMaintenanceDocument-workflowProperties-parentBean" abstract="true" parent="WorkflowProperties">
  </bean>
</beans>
