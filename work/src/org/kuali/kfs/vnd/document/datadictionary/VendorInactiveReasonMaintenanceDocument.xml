<?xml version="1.0" encoding="UTF-8"?><beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p" xmlns:dd="http://rice.kuali.org/dd" xsi:schemaLocation="http://www.springframework.org/schema/beans         http://www.springframework.org/schema/beans/spring-beans-2.0.xsd         http://rice.kuali.org/dd         http://rice.kuali.org/dd/dd.xsd">

  <bean id="VendorInactiveReasonMaintenanceDocument" parent="VendorInactiveReasonMaintenanceDocument-parentBean"/>

  <bean id="VendorInactiveReasonMaintenanceDocument-parentBean" abstract="true" parent="MaintenanceDocumentEntry">
    <property name="businessObjectClass" value="org.kuali.kfs.vnd.businessobject.VendorInactiveReason"/>
    <property name="maintainableClass" value="org.kuali.kfs.sys.document.FinancialSystemMaintainable"/>
    <property name="maintainableSections">
      <list>
        <ref bean="VendorInactiveReasonMaintenanceDocument-EditVendorInactiveReason"/>
      </list>
    </property>
    <property name="lockingKeys">
      <list>
        <value>vendorInactiveReasonCode</value>
      </list>
    </property>
    
    
    
    <property name="documentTypeName" value="PMIR"/>
    <property name="documentAuthorizerClass" value="org.kuali.kfs.sys.document.authorization.FinancialSystemMaintenanceDocumentAuthorizerBase"/>
    <property name="workflowAttributes">
    	<ref bean="VendorInactiveReasonMaintenanceDocument-workflowAttributes"/>
    </property>
  </bean>

<!-- Maintenance Section Definitions -->

  <bean id="VendorInactiveReasonMaintenanceDocument-EditVendorInactiveReason" parent="VendorInactiveReasonMaintenanceDocument-EditVendorInactiveReason-parentBean"/>

  <bean id="VendorInactiveReasonMaintenanceDocument-EditVendorInactiveReason-parentBean" abstract="true" parent="MaintainableSectionDefinition">
    <property name="id" value="Edit Vendor Inactive Reason"/>
    <property name="title" value="Edit Vendor Inactive Reason"/>
    <property name="maintainableItems">
      <list>
        <bean parent="MaintainableFieldDefinition" p:name="vendorInactiveReasonCode" p:required="true"/>
        <bean parent="MaintainableFieldDefinition" p:name="vendorInactiveReasonDescription" p:required="true"/>
        <bean parent="MaintainableFieldDefinition" p:name="active" p:defaultValue="true"/>
      </list>
    </property>
  </bean>
  
  <!-- workflow attributes -->
  <bean id="VendorInactiveReasonMaintenanceDocument-workflowAttributes" parent="VendorInactiveReasonMaintenanceDocument-workflowAttributes-parentBean"/>
  
  <bean id="VendorInactiveReasonMaintenanceDocument-workflowAttributes-parentBean" class="org.kuali.rice.kns.datadictionary.WorkflowAttributes" abstract="true"/>
</beans>