<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="VendorTypeMaintenanceDocument" parent="VendorTypeMaintenanceDocument-parentBean" />

  <bean id="VendorTypeMaintenanceDocument-parentBean" abstract="true" parent="MaintenanceDocumentEntry">
    <property name="businessObjectClass" value="org.kuali.kfs.vnd.businessobject.VendorType" />
    <property name="maintainableClass" value="org.kuali.rice.kns.maintenance.KualiMaintainableImpl" />
    <property name="maintainableSections" >
      <list>
        <ref bean="VendorTypeMaintenanceDocument-EditVendorType" />
      </list>
    </property>
    <property name="lockingKeys" >
      <list>
        <value>vendorTypeCode</value>
      </list>
    </property>
    <property name="label" value="Vendor Type" />
    <property name="shortLabel" value="Vendor Type" />
    <property name="helpDefinition" >
      <bean parent="HelpDefinition" p:parameterClass="org.kuali.kfs.vnd.businessobject.VendorType" p:parameterName="VENDOR_TYPE" />
    </property>
    <property name="documentTypeName" value="VendorTypeMaintenanceDocument" />
    <property name="documentAuthorizerClass" value="org.kuali.kfs.sys.document.authorization.FinancialSystemMaintenanceDocumentAuthorizerBase" />
    <property name="documentTypeCode" value="PMVT" />
    <property name="workflowProperties" >
      <ref bean="VendorTypeMaintenanceDocument-workflowProperties" />
    </property>
    <property name="authorizations" >
      <list>
        <bean parent="AuthorizationDefinition">
          <property name="action" value="initiate" />
          <property name="authorizedGroups" >
            <set>
              <value>SY_MAINTENANCE_USERS</value>
            </set>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Maintenance Section Definitions -->


  <bean id="VendorTypeMaintenanceDocument-EditVendorType" parent="VendorTypeMaintenanceDocument-EditVendorType-parentBean" />

  <bean id="VendorTypeMaintenanceDocument-EditVendorType-parentBean" abstract="true" parent="MaintainableSectionDefinition">
    <property name="id" value="Edit Vendor Type" />
    <property name="title" value="Edit Vendor Type" />
    <property name="maintainableItems" >
      <list>
        <bean parent="MaintainableFieldDefinition"
              p:name="vendorTypeCode"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="vendorTypeDescription"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="vendorAddressTypeRequiredCode"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="vendorTaxNumberRequiredIndicator" />
        <bean parent="MaintainableFieldDefinition"
              p:name="vendorTypeChangeAllowedIndicator" />
        <bean parent="MaintainableFieldDefinition"
              p:name="vendorContractAllowedIndicator" />
        <bean parent="MaintainableFieldDefinition"
              p:name="vendorShowReviewIndicator" />
        <bean parent="MaintainableFieldDefinition"
              p:name="commodityRequiredIndicator" />
        <bean parent="MaintainableFieldDefinition"
              p:name="vendorReviewText" />
        <bean parent="MaintainableFieldDefinition"
              p:name="versionNumber" />
        <bean parent="MaintainableFieldDefinition"
              p:name="active" />
      </list>
    </property>
  </bean>

<!-- Exported Workflow Properties -->


  <bean id="VendorTypeMaintenanceDocument-workflowProperties" parent="VendorTypeMaintenanceDocument-workflowProperties-parentBean" />

  <bean id="VendorTypeMaintenanceDocument-workflowProperties-parentBean" abstract="true" parent="WorkflowProperties">
  </bean>
</beans>
