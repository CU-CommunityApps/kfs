<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="ShippingTitleMaintenanceDocument" parent="ShippingTitleMaintenanceDocument-parentBean" />

  <bean id="ShippingTitleMaintenanceDocument-parentBean" abstract="true" parent="MaintenanceDocumentEntry">
    <property name="businessObjectClass" value="org.kuali.kfs.vnd.businessobject.ShippingTitle" />
    <property name="maintainableClass" value="org.kuali.rice.kns.maintenance.KualiMaintainableImpl" />
    <property name="maintainableSections" >
      <list>
        <ref bean="ShippingTitleMaintenanceDocument-EditShippingTitle" />
      </list>
    </property>
    <property name="lockingKeys" >
      <list>
        <value>vendorShippingTitleCode</value>
      </list>
    </property>
    <property name="label" value="Shipping Title" />
    <property name="shortLabel" value="Shipping Title" />
    <property name="helpDefinition" >
      <bean parent="HelpDefinition" p:parameterClass="org.kuali.kfs.vnd.businessobject.ShippingTitle" p:parameterName="SHIPPING_TITLE" />
    </property>
    <property name="documentTypeName" value="ShippingTitleMaintenanceDocument" />
    <property name="documentAuthorizerClass" value="org.kuali.kfs.sys.document.authorization.FinancialSystemMaintenanceDocumentAuthorizerBase" />
    <property name="documentTypeCode" value="PMST" />
    <property name="workflowProperties" >
      <ref bean="ShippingTitleMaintenanceDocument-workflowProperties" />
    </property>
    <property name="authorizations" >
      <list>
        <bean parent="AuthorizationDefinition">
          <property name="action" value="initiate" />
          <property name="authorizedGroups" >
            <set>
              <value>SY_MAINTENANCE_USERS</value>
            </set>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Maintenance Section Definitions -->


  <bean id="ShippingTitleMaintenanceDocument-EditShippingTitle" parent="ShippingTitleMaintenanceDocument-EditShippingTitle-parentBean" />

  <bean id="ShippingTitleMaintenanceDocument-EditShippingTitle-parentBean" abstract="true" parent="MaintainableSectionDefinition">
    <property name="id" value="Edit Shipping Title" />
    <property name="title" value="Edit Shipping Title" />
    <property name="maintainableItems" >
      <list>
        <bean parent="MaintainableFieldDefinition"
              p:name="vendorShippingTitleCode"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="vendorShippingTitleDescription"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="active" 
              p:defaultValue="true" />
      </list>
    </property>
  </bean>

<!-- Exported Workflow Properties -->


  <bean id="ShippingTitleMaintenanceDocument-workflowProperties" parent="ShippingTitleMaintenanceDocument-workflowProperties-parentBean" />

  <bean id="ShippingTitleMaintenanceDocument-workflowProperties-parentBean" abstract="true" parent="WorkflowProperties">
  </bean>
</beans>
