<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="ContactTypeMaintenanceDocument" parent="ContactTypeMaintenanceDocument-parentBean" />

  <bean id="ContactTypeMaintenanceDocument-parentBean" abstract="true" parent="MaintenanceDocumentEntry">
    <property name="businessObjectClass" value="org.kuali.module.vendor.bo.ContactType" />
    <property name="maintainableClass" value="org.kuali.core.maintenance.KualiMaintainableImpl" />
    <property name="maintainableSections" >
      <list>
        <ref bean="ContactTypeMaintenanceDocument-EditContactType" />
      </list>
    </property>
    <property name="lockingKeys" >
      <list>
        <value>vendorContactTypeCode</value>
      </list>
    </property>
    <property name="label" value="Contact Type" />
    <property name="shortLabel" value="Contact Type" />
    <property name="helpDefinition" >
      <bean parent="HelpDefinition"
            p:parameterNamespace="KFS-VN"
            p:parameterName="CONTACT_TYPE"
            p:parameterDetailType="ContactType" />
    </property>
    <property name="documentTypeName" value="ContactTypeMaintenanceDocument" />
    <property name="documentAuthorizerClass" value="org.kuali.core.document.authorization.MaintenanceDocumentAuthorizerBase" />
    <property name="documentTypeCode" value="PMCT" />
    <property name="workflowProperties" >
      <ref bean="ContactTypeMaintenanceDocument-workflowProperties" />
    </property>
    <property name="authorizations" >
      <list>
        <bean parent="AuthorizationDefinition">
          <property name="action" value="initiate" />
          <property name="authorizedGroups" >
            <set>
              <value>PA_MAINTENANCE_USERS</value>
            </set>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Maintenance Section Definitions -->


  <bean id="ContactTypeMaintenanceDocument-EditContactType" parent="ContactTypeMaintenanceDocument-EditContactType-parentBean" />

  <bean id="ContactTypeMaintenanceDocument-EditContactType-parentBean" abstract="true" parent="MaintainableSectionDefinition">
    <property name="id" value="Edit Contact Type" />
    <property name="title" value="Edit Contact Type" />
    <property name="maintainableItems" >
      <list>
        <bean parent="MaintainableFieldDefinition"
              p:name="vendorContactTypeCode"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="vendorContactTypeDescription"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="active" />
        <bean parent="MaintainableFieldDefinition"
              p:name="versionNumber" />
      </list>
    </property>
  </bean>

<!-- Exported Workflow Properties -->


  <bean id="ContactTypeMaintenanceDocument-workflowProperties" parent="ContactTypeMaintenanceDocument-workflowProperties-parentBean" />

  <bean id="ContactTypeMaintenanceDocument-workflowProperties-parentBean" abstract="true" parent="WorkflowProperties">
  </bean>
</beans>
