<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="ShippingSpecialConditionMaintenanceDocument" parent="ShippingSpecialConditionMaintenanceDocument-parentBean" />

  <bean id="ShippingSpecialConditionMaintenanceDocument-parentBean" abstract="true" parent="MaintenanceDocumentEntry">
    <property name="businessObjectClass" value="org.kuali.module.vendor.bo.ShippingSpecialCondition" />
    <property name="maintainableClass" value="org.kuali.core.maintenance.KualiMaintainableImpl" />
    <property name="maintainableSections" >
      <list>
        <ref bean="ShippingSpecialConditionMaintenanceDocument-EditShippingSpecialCondition" />
      </list>
    </property>
    <property name="lockingKeys" >
      <list>
        <value>vendorShippingSpecialConditionCode</value>
      </list>
    </property>
    <property name="label" value="Shipping Special Conditions" />
    <property name="shortLabel" value="Shipping Special Conditions" />
    <property name="helpDefinition" >
      <bean parent="HelpDefinition"
            p:parameterNamespace="KFS-VN"
            p:parameterName="SHIPPING_SPECIAL_CONDITION"
            p:parameterDetailType="ShippingSpecialCondition" />
    </property>
    <property name="documentTypeName" value="ShippingSpecialConditionMaintenanceDocument" />
    <property name="documentAuthorizerClass" value="org.kuali.core.document.authorization.MaintenanceDocumentAuthorizerBase" />
    <property name="documentTypeCode" value="PMSS" />
    <property name="workflowProperties" >
      <ref bean="ShippingSpecialConditionMaintenanceDocument-workflowProperties" />
    </property>
    <property name="authorizations" >
      <list>
        <bean parent="AuthorizationDefinition">
          <property name="action" value="initiate" />
          <property name="authorizedGroups" >
            <set>
              <value>PA_MAINTENANCE_USERS</value>
            </set>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Maintenance Section Definitions -->


  <bean id="ShippingSpecialConditionMaintenanceDocument-EditShippingSpecialCondition" parent="ShippingSpecialConditionMaintenanceDocument-EditShippingSpecialCondition-parentBean" />

  <bean id="ShippingSpecialConditionMaintenanceDocument-EditShippingSpecialCondition-parentBean" abstract="true" parent="MaintainableSectionDefinition">
    <property name="id" value="Edit Shipping Special Condition" />
    <property name="title" value="Edit Shipping Special Condition" />
    <property name="maintainableItems" >
      <list>
        <bean parent="MaintainableFieldDefinition"
              p:name="vendorShippingSpecialConditionCode"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="vendorShippingSpecialConditionDescription"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="active" />
        <bean parent="MaintainableFieldDefinition"
              p:name="versionNumber" />
      </list>
    </property>
  </bean>

<!-- Exported Workflow Properties -->


  <bean id="ShippingSpecialConditionMaintenanceDocument-workflowProperties" parent="ShippingSpecialConditionMaintenanceDocument-workflowProperties-parentBean" />

  <bean id="ShippingSpecialConditionMaintenanceDocument-workflowProperties-parentBean" abstract="true" parent="WorkflowProperties">
  </bean>
</beans>
