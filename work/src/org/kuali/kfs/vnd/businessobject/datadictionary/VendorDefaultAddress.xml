<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "../dataDictionary-1_0.dtd">
<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.module.vendor.bo.VendorDefaultAddress">

        <attributes>
            <attributeReference name="vendorAddressGeneratedIdentifier" sourceClassName="org.kuali.module.vendor.bo.VendorAddress" sourceAttributeName="vendorAddressGeneratedIdentifier" >
            	<control>
                    <hidden/>
                </control>
            </attributeReference>
                        
            <attribute name="vendorDefaultAddressGeneratedIdentifier" forceUppercase="true">
                <label>vendor Default Address Generated Identifier</label>
                <shortLabel>Vendor Default AddressGenerated Identifier</shortLabel>

                <maxLength>10</maxLength>
                <validationPattern><numeric /></validationPattern>

                <control>
                    <hidden/>
                </control>

			</attribute>

            <attributeReference name="vendorCampusCode" sourceClassName="org.kuali.module.vendor.bo.VendorAttributeReferenceDummy" sourceAttributeName="campusCode" >
                <label>Set as campus default for</label>
                <shortLabel>Campus</shortLabel>
                <required>true</required>
            </attributeReference>
            
            <attributeReference name="newCollectionRecord" 
                                sourceClassName="org.kuali.core.bo.AttributeReferenceDummy"
                                sourceAttributeName="newCollectionRecord" />

            <attributeReference name="versionNumber" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="versionNumber" />
            <attributeReference name="active" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="activeIndicator"/>
        </attributes>
    </businessObject>
</dictionaryEntry>
