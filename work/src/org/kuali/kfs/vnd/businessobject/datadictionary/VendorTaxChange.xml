<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "../dataDictionary-1_0.dtd">
<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.module.vendor.bo.VendorTaxChange">

        <attributes>

            <attribute name="vendorTaxChangeGeneratedIdentifier" forceUppercase="true">
                <label>Vendor Tax Change Generated Identifier</label>
                <shortLabel>Identifier</shortLabel>

                <maxLength>10</maxLength>
                <validationPattern><numeric /></validationPattern>

                <control>
                    <text size="12" />
                </control>
			</attribute>
            
            <attributeReference name="vendorTaxChangePersonIdentifier" sourceClassName="org.kuali.core.bo.user.UniversalUser" sourceAttributeName="personUniversalIdentifier">
                <label>Vendor Tax Change Person Identifier</label>
                <shortLabel>Vendor Tax Change Person ID</shortLabel>
                <displayLabelAttribute>vendorTaxChangePerson.personUserIdentifier</displayLabelAttribute>
                <required>true</required>
                <control>
                    <hidden />
                </control>
            </attributeReference>
            
            <attributeReference name="vendorTaxChangePerson.personUserIdentifier" sourceClassName="org.kuali.core.bo.user.UniversalUser" sourceAttributeName="personUserIdentifier">
                <label>Vendor Tax Change Person Identifier</label>
                <shortLabel>Vendor Tax Change Person ID</shortLabel>
                <required>false</required>
                <control>
                    <kualiUser universalIdAttributeName="vendorRestrictedPersonIdentifier" userIdAttributeName="vendorRestrictedPerson.personUserIdentifier" personNameAttributeName="vendorRestrictedPerson.personName"/>
                </control>
            </attributeReference>
            
            <attributeReference name="vendorTaxChangePerson.personName" sourceClassName="org.kuali.core.bo.user.UniversalUser" sourceAttributeName="personName">
                <label>Vendor Tax Change Person</label>
                <shortLabel>Name</shortLabel>
                <required>false</required>
                <control>
                    <hidden />
                </control>
            </attributeReference>
                        
            <attributeReference name="vendorTaxChangeDate" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="genericDate">
                <label>Vendor Tax Change Date</label>
                <shortLabel>Vendor Tax Change Date</shortLabel>
			</attributeReference>
            
            <attributeReference name="vendorPreviousTaxTypeCode" sourceClassName="org.kuali.module.vendor.bo.VendorHeader" sourceAttributeName="vendorTaxTypeCode" >
                <label>Vendor Previous Tax Type Code</label>
                <shortLabel>Vendor Previous Tax Type Code</shortLabel>
			</attributeReference>

            <attributeReference name="vendorPreviousTaxNumber" sourceClassName="org.kuali.module.vendor.bo.VendorHeader" sourceAttributeName="vendorTaxNumber">
                <label>Vendor Previous Tax Number</label>
                <shortLabel>Vendor Previous Tax Number</shortLabel>
			</attributeReference>
			
            <attributeReference name="vendorHeaderGeneratedIdentifier" sourceClassName="org.kuali.module.vendor.bo.VendorDetail" sourceAttributeName="vendorHeaderGeneratedIdentifier" />

            <attributeReference name="versionNumber" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="versionNumber" />
        
        </attributes>
    </businessObject>
</dictionaryEntry>
