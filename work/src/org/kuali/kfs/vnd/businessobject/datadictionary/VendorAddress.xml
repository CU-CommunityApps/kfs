<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "../dataDictionary-1_0.dtd">
<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.module.vendor.bo.VendorAddress">

        <inquiry>
            <title>Vendor Address Inquiry</title>

            <inquirySections><inquirySection title="Header Text"><inquiryFields>
                <field attributeName="vendorAddressEmailAddress" />
                <field attributeName="vendorAddressGeneratedIdentifier" />
                <field attributeName="vendorAddressInternationalProvinceName" />
                <field attributeName="vendorAddressTypeCode" />
                <field attributeName="vendorAttentionName" />
                <field attributeName="vendorBusinessToBusinessUrlAddress" />
                <field attributeName="vendorCityName" />
                <field attributeName="vendorCountryCode" />
                <field attributeName="vendorDetailAssignedIdentifier" />
                <field attributeName="vendorHeaderGeneratedIdentifier" />
                <field attributeName="vendorLine1Address" />
                <field attributeName="vendorLine2Address" />
                <field attributeName="vendorStateCode" />
                <field attributeName="vendorZipCode" />
                <field attributeName="vendorFaxNumber" />
                <field attributeName="active"/>               
            </inquiryFields></inquirySection></inquirySections>
        </inquiry>

        <lookup>
            <lookupableID>vendorAddressLookupable</lookupableID>
            
            <title>Vendor Address Lookup</title>

            <instructions>Lookup an Vendor Address</instructions>

            <defaultSort sortAscending="true">
                <sortAttributes>
                    <sortAttribute attributeName="vendorAddressTypeCode" />
                </sortAttributes>
            </defaultSort>

            <lookupFields>
                <lookupField attributeName="vendorHeaderGeneratedIdentifier" required="true" />
                <lookupField attributeName="vendorDetailAssignedIdentifier" required="true" />
                <lookupField attributeName="vendorAddressTypeCode" required="false" />
                <lookupField attributeName="vendorLine1Address" required="false" />
                <lookupField attributeName="vendorLine2Address" required="false" />
                <lookupField attributeName="vendorCityName" required="false" />
                <lookupField attributeName="vendorStateCode" required="false" />
                <lookupField attributeName="vendorZipCode" required="false" />
                <lookupField attributeName="vendorAddressInternationalProvinceName" required="false" />
                <lookupField attributeName="vendorCountryCode" required="false" />
                <lookupField attributeName="vendorAttentionName" required="false" />
                <lookupField attributeName="vendorBusinessToBusinessUrlAddress" required="false" />
                <lookupField attributeName="vendorFaxNumber" required="false" />
                <lookupField attributeName="vendorAddressEmailAddress" required="false" />
                <lookupField attributeName="active" required="false">
                	<defaultValue>Y</defaultValue>
                </lookupField> 
            </lookupFields>

            <resultFields>
                <field attributeName="vendorAddressTypeCode" />
                <field attributeName="vendorLine1Address" />
                <field attributeName="vendorLine2Address" />
                <field attributeName="vendorStateCode" />
                <field attributeName="vendorCityName" />
                <field attributeName="vendorZipCode" />
                <field attributeName="vendorCountryCode" />
                <field attributeName="vendorFaxNumber" />
            	<field attributeName="active" />
            </resultFields>
        </lookup>

        <attributes>

            <attribute name="vendorAddressEmailAddress" forceUppercase="false">
                <label>Email Address</label>
                <shortLabel>Email Address</shortLabel>

                <maxLength>90</maxLength>
                <validationPattern>
                    <emailAddress />
                </validationPattern>

                <control>
                    <text size="45" />
                </control>

			</attribute>

            <attribute name="vendorAddressGeneratedIdentifier" forceUppercase="true">
                <label>Vendor Address Generated Identifier</label>
                <shortLabel>Vendor Address Generated Identifier</shortLabel>

                <maxLength>10</maxLength>
                <validationPattern><numeric /></validationPattern>

                <control>
                    <hidden/>
                </control>

			</attribute>

            <attribute name="vendorBusinessToBusinessUrlAddress" forceUppercase="true">
                <label>URL</label>
                <shortLabel>URL</shortLabel>

                <maxLength>90</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="45" />
                </control>

			</attribute>

            <attribute name="vendorDefaultAddressIndicator" forceUppercase="false">
                <label>Set as Default Address</label>
                <shortLabel>Default Address</shortLabel>

                <maxLength>1</maxLength>

                <control>
                    <select valuesFinderClass="org.kuali.core.lookup.keyvalues.IndicatorYNOnlyValuesFinder" />
                </control>

            </attribute>

			<attributeReference name="vendorFaxNumber" sourceClassName="org.kuali.module.vendor.bo.VendorAttributeReferenceDummy" sourceAttributeName="phoneNumberWithValidation">
                <label>Vendor Fax Number</label>
                <shortLabel>Vendor Fax Number</shortLabel>

                <maxLength>14</maxLength>
 
                <control>
                    <text size="14" />
                </control>

			</attributeReference>
			
			<attributeReference name="vendorAddressTypeCode" sourceClassName="org.kuali.module.vendor.bo.AddressType" sourceAttributeName="vendorAddressTypeCode">
                <label>Address Type</label>
                <required>true</required>
                <control>
                    <select valuesFinderClass="org.kuali.module.vendor.lookup.keyvalues.AddressTypeValuesFinder" />
                </control>
			</attributeReference>
			
			<attributeReference name="newCollectionRecord" 
                                sourceClassName="org.kuali.core.bo.AttributeReferenceDummy"
                                sourceAttributeName="newCollectionRecord" />
			
            <attributeReference name="versionNumber" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="versionNumber" />
			<attributeReference name="vendorLine1Address" sourceClassName="org.kuali.module.vendor.bo.VendorAttributeReferenceDummy" sourceAttributeName="line1Address" >
                <required>true</required>
            </attributeReference>
			<attributeReference name="vendorLine2Address" sourceClassName="org.kuali.module.vendor.bo.VendorAttributeReferenceDummy" sourceAttributeName="line2Address" />
			<attributeReference name="vendorCityName" sourceClassName="org.kuali.module.vendor.bo.VendorAttributeReferenceDummy" sourceAttributeName="cityName" >
                <required>true</required>
            </attributeReference>
            
			<attributeReference name="vendorStateCode" 
				sourceClassName="org.kuali.module.vendor.bo.VendorAttributeReferenceDummy" 
				sourceAttributeName="stateCode">
				
			</attributeReference>
			
			<attributeReference name="vendorZipCode" 
				sourceClassName="org.kuali.module.vendor.bo.VendorAttributeReferenceDummy" 
				sourceAttributeName="zipCode" >
				
			</attributeReference>
			
			<attributeReference name="vendorCountryCode" sourceClassName="org.kuali.module.vendor.bo.VendorAttributeReferenceDummy" sourceAttributeName="countryCode" >
                <required>true</required>
            </attributeReference>
			<attributeReference name="vendorAddressInternationalProvinceName" sourceClassName="org.kuali.module.vendor.bo.VendorAttributeReferenceDummy" sourceAttributeName="internationalProvinceName" />
			<attributeReference name="vendorAttentionName" sourceClassName="org.kuali.module.vendor.bo.VendorAttributeReferenceDummy" sourceAttributeName="attentionName" />
			<attributeReference name="vendorHeaderGeneratedIdentifier" sourceClassName="org.kuali.module.vendor.bo.VendorDetail" sourceAttributeName="vendorHeaderGeneratedIdentifier" />
            <attributeReference name="vendorDetailAssignedIdentifier" sourceClassName="org.kuali.module.vendor.bo.VendorDetail" sourceAttributeName="vendorDetailAssignedIdentifier" />
            <attributeReference name="active" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="activeIndicator"/>
        </attributes>
        <objectLabel>Address</objectLabel>
    </businessObject>
</dictionaryEntry>
