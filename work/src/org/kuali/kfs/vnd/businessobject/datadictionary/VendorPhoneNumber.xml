<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "../dataDictionary-1_0.dtd">
<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.module.vendor.bo.VendorPhoneNumber">


        <attributes>

			<attribute name="vendorPhoneGeneratedIdentifier" forceUppercase="true">
                <label>Vendor Phone Generated Identifier</label>
                <shortLabel>Phone Gen. ID</shortLabel>

                <maxLength>10</maxLength>
                <validationPattern><numeric /></validationPattern>

                <control>
                    <hidden/>
                </control>

                <summary>vendorPhoneGeneratedIdentifier</summary>
                <description>vendorPhoneGeneratedIdentifier description...</description>
			</attribute>

			<attributeReference name="vendorPhoneTypeCode"
            	sourceClassName="org.kuali.module.vendor.bo.PhoneType"
            	sourceAttributeName="vendorPhoneTypeCode">
                <label>Phone Type</label>
                <shortLabel>Phone Type</shortLabel>
                <required>true</required>

                <control>
                    <select valuesFinderClass="org.kuali.module.vendor.lookup.keyvalues.PhoneTypeValuesFinder"/>
                </control>
			</attributeReference>
			
            <attributeReference name="vendorPhoneNumber"
            	sourceClassName="org.kuali.module.vendor.bo.VendorAttributeReferenceDummy"
            	sourceAttributeName="phoneNumberNoValidation">
                <label>Phone Number</label>
                <shortLabel>Phone</shortLabel>
                <required>true</required>

                <summary>vendorPhoneNumber</summary>
                <description>vendorPhoneNumber description...</description>
			</attributeReference>

            <attribute name="vendorPhoneExtensionNumber" forceUppercase="true">
                <label>Extension</label>
                <shortLabel>Extension</shortLabel>

                <maxLength>10</maxLength>
                <validationPattern><anyCharacter allowWhitespace="true" /></validationPattern>

                <control>
                    <text size="12" />
                </control>

                <summary>vendorPhoneExtensionNumber</summary>
                <description>vendorPhoneExtensionNumber description...</description>
			</attribute>
			
			<attributeReference name="newCollectionRecord" 
                                sourceClassName="org.kuali.core.bo.AttributeReferenceDummy"
                                sourceAttributeName="newCollectionRecord" />

            <attributeReference name="vendorHeaderGeneratedIdentifier" sourceClassName="org.kuali.module.vendor.bo.VendorDetail" sourceAttributeName="vendorHeaderGeneratedIdentifier" />
            <attributeReference name="vendorDetailAssignedIdentifier" sourceClassName="org.kuali.module.vendor.bo.VendorDetail" sourceAttributeName="vendorDetailAssignedIdentifier" />
            <attributeReference name="versionNumber" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="versionNumber" />

        </attributes>
    </businessObject>
</dictionaryEntry>
