<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "../dataDictionary-1_0.dtd">
<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.module.vendor.bo.VendorPhoneNumber">
    
        <inquiry>
            <title>Vendor Phone Number Inquiry</title>

            <inquirySections><inquirySection title="Header Text"><inquiryFields>
            	<field attributeName="vendorPhoneTypeCode" />
            	<field attributeName="vendorPhoneNumber" />
                <field attributeName="vendorPhoneExtensionNumber" />
                <field attributeName="vendorPhoneGeneratedIdentifier" />             
                <field attributeName="vendorDetailAssignedIdentifier" />
                <field attributeName="vendorHeaderGeneratedIdentifier" />
                <field attributeName="active"/>               
            </inquiryFields></inquirySection></inquirySections>
        </inquiry>

        <lookup>           
            <lookupableID>vendorPhoneLookupable</lookupableID>
            <title>Vendor Phone Number Lookup</title>

            <instructions>Lookup a Vendor Phone Number</instructions>

            <defaultSort sortAscending="true">
                <sortAttributes>
                    <sortAttribute attributeName="vendorPhoneGeneratedIdentifier" />
                </sortAttributes>
            </defaultSort>

            <lookupFields>
                <lookupField attributeName="vendorPhoneNumber" required="false" />
                <lookupField attributeName="vendorHeaderGeneratedIdentifier" required="false" />      
                <lookupField attributeName="vendorDetailAssignedIdentifier" required="false" /> 
                <lookupField attributeName="vendorPhoneTypeCode" required="false" />         
                <lookupField attributeName="vendorPhoneExtensionNumber" required="false" />                            
                <lookupField attributeName="active" required="false">
                	<defaultValue>Y</defaultValue>
                </lookupField> 
            </lookupFields>

            <resultFields>
                <field attributeName="vendorPhoneNumber" />
            	<field attributeName="vendorHeaderGeneratedIdentifier" />
            	<field attributeName="vendorDetailAssignedIdentifier" />
                <field attributeName="vendorPhoneTypeCode" />
                <field attributeName="vendorPhoneExtensionNumber" />
                <field attributeName="active"/>               
            </resultFields>
        </lookup>
        
        <attributes>
			<attribute name="vendorPhoneGeneratedIdentifier" forceUppercase="true">
                <label>Vendor Phone Generated Identifier</label>
                <shortLabel>Phone Gen. ID</shortLabel>

                <maxLength>10</maxLength>
                <validationPattern><numeric /></validationPattern>

                <control>
                    <hidden/>
                </control>

			</attribute>

			<attributeReference name="vendorPhoneTypeCode"
            	sourceClassName="org.kuali.module.vendor.bo.PhoneType"
            	sourceAttributeName="vendorPhoneTypeCode">
                <label>Phone Type</label>
                <shortLabel>Phone Type</shortLabel>
                <required>true</required>

                <control>
                    <select valuesFinderClass="org.kuali.module.vendor.lookup.keyvalues.PhoneTypeValuesFinder"/>
                </control>
			</attributeReference>
			
            <attributeReference name="vendorPhoneNumber"
            	sourceClassName="org.kuali.module.vendor.bo.VendorAttributeReferenceDummy"
            	sourceAttributeName="phoneNumberNoValidation">
                <label>Phone Number</label>
                <shortLabel>Phone</shortLabel>
                <required>true</required>

			</attributeReference>

            <attribute name="vendorPhoneExtensionNumber" forceUppercase="true">
                <label>Extension</label>
                <shortLabel>Extension</shortLabel>

                <maxLength>10</maxLength>
                <validationPattern><anyCharacter allowWhitespace="true" /></validationPattern>

                <control>
                    <text size="12" />
                </control>

			</attribute>
			
			<attributeReference name="newCollectionRecord" 
                                sourceClassName="org.kuali.core.bo.AttributeReferenceDummy"
                                sourceAttributeName="newCollectionRecord" />

            <attributeReference name="vendorHeaderGeneratedIdentifier" sourceClassName="org.kuali.module.vendor.bo.VendorDetail" sourceAttributeName="vendorHeaderGeneratedIdentifier" />
            <attributeReference name="vendorDetailAssignedIdentifier" sourceClassName="org.kuali.module.vendor.bo.VendorDetail" sourceAttributeName="vendorDetailAssignedIdentifier" />
            <attributeReference name="versionNumber" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="versionNumber" />
            <attributeReference name="active" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="activeIndicator"/>
        </attributes>
    </businessObject>
</dictionaryEntry>
