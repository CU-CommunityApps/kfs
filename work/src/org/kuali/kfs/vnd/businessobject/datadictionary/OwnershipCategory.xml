<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "../dataDictionary-1_0.dtd">
<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.module.vendor.bo.OwnershipCategory">

        <inquiry>
            <title>Ownership Category Inquiry</title>

            <inquirySections><inquirySection title="Header Text"><inquiryFields>
                <field attributeName="vendorOwnershipCategoryDescription" />
                <field attributeName="vendorOwnershipCategoryCode" />
                <field attributeName="active" />
            </inquiryFields></inquirySection></inquirySections>
        </inquiry>

        <lookup>
            <title>Ownership Category Lookup</title>

            <instructions>Lookup an Owner Category</instructions>

            <defaultSort sortAscending="true">
                <sortAttributes>
                    <sortAttribute attributeName="vendorOwnershipCategoryDescription" />
                </sortAttributes>
            </defaultSort>

            <lookupFields>
                <lookupField attributeName="vendorOwnershipCategoryCode" required="false" />
                <lookupField attributeName="vendorOwnershipCategoryDescription" required="false" />
                <lookupField attributeName="active" required="false" />
            </lookupFields>

            <resultFields>
                <field attributeName="vendorOwnershipCategoryCode" />
                <field attributeName="vendorOwnershipCategoryDescription" />
                <field attributeName="active" />
            </resultFields>
        </lookup>

        <titleAttribute>vendorOwnershipCategoryCode</titleAttribute>

        <attributes>

           <attribute name="vendorOwnershipCategoryCode" forceUppercase="true">
                <label>Vendor Ownership Category Code</label>
                <shortLabel>Code</shortLabel>

                <maxLength>4</maxLength>
                <validationPattern><anyCharacter /></validationPattern>
                <required>true</required>

                <control>
                    <text size="6" />
                </control>

			</attribute>

            <attribute name="vendorOwnershipCategoryDescription" forceUppercase="true">
                <label>Vendor Ownership Category Description</label>
                <shortLabel>Description</shortLabel>

                <maxLength>45</maxLength>
                <validationPattern><anyCharacter allowWhitespace="true"/></validationPattern>
                <required>true</required>

                <control>
                    <text size="47" />
                </control>

			</attribute>

            <attributeReference name="active" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="activeIndicator" />

            <attributeReference name="versionNumber" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="versionNumber" />

        </attributes>
        <objectLabel>Ownership Category</objectLabel>
    </businessObject>
</dictionaryEntry>
