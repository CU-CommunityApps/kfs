<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="SupplierDiversity" parent="SupplierDiversity-parentBean" />

  <bean id="SupplierDiversity-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.module.vendor.bo.SupplierDiversity" />
    <property name="inquiryDefinition" >
      <ref bean="SupplierDiversity-inquiryDefinition" />
    </property>
    <property name="lookupDefinition" >
      <ref bean="SupplierDiversity-lookupDefinition" />
    </property>
    <property name="titleAttribute" value="vendorSupplierDiversityCode" />
    <property name="objectLabel" value="Supplier Diversity" />
    <property name="attributes" >
      <list>
        <ref bean="SupplierDiversity-vendorSupplierDiversityCode" />
        <ref bean="SupplierDiversity-vendorSupplierDiversityDescription" />
        <ref bean="SupplierDiversity-active" />
        <ref bean="SupplierDiversity-versionNumber" />
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->


  <bean id="SupplierDiversity-vendorSupplierDiversityCode" parent="SupplierDiversity-vendorSupplierDiversityCode-parentBean" />

  <bean id="SupplierDiversity-vendorSupplierDiversityCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="vendorSupplierDiversityCode" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Supplier Diversity Code" />
    <property name="shortLabel" value="Code" />
    <property name="maxLength" value="4" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="6" />
    </property>
  </bean>
  <bean id="SupplierDiversity-vendorSupplierDiversityDescription" parent="SupplierDiversity-vendorSupplierDiversityDescription-parentBean" />

  <bean id="SupplierDiversity-vendorSupplierDiversityDescription-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="vendorSupplierDiversityDescription" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Supplier Diversity Description" />
    <property name="shortLabel" value="Description" />
    <property name="maxLength" value="45" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern"
            p:allowWhitespace="true" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="47" />
    </property>
  </bean>
  <bean id="SupplierDiversity-active" parent="SupplierDiversity-active-parentBean" />

  <bean id="SupplierDiversity-active-parentBean" abstract="true" parent="GenericAttributes-activeIndicator">
    <property name="name" value="active" />
  </bean>
  <bean id="SupplierDiversity-versionNumber" parent="SupplierDiversity-versionNumber-parentBean" />

  <bean id="SupplierDiversity-versionNumber-parentBean" abstract="true" parent="GenericAttributes-versionNumber">
  </bean>

<!-- Business Object Inquiry Definition -->


  <bean id="SupplierDiversity-inquiryDefinition" parent="SupplierDiversity-inquiryDefinition-parentBean" />

  <bean id="SupplierDiversity-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
    <property name="title" value="Supplier Diversity Inquiry" />
    <property name="inquirySections" >
      <list>
        <bean parent="InquirySectionDefinition">
          <property name="title" value="Header Text" />
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields" >
            <list>
              <bean parent="FieldDefinition"
                    p:attributeName="vendorSupplierDiversityCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="vendorSupplierDiversityDescription" />
              <bean parent="FieldDefinition"
                    p:attributeName="active" />
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Business Object Lookup Definition -->


  <bean id="SupplierDiversity-lookupDefinition" parent="SupplierDiversity-lookupDefinition-parentBean" />

  <bean id="SupplierDiversity-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
    <property name="title" value="Supplier Diversity Lookup" />
    <property name="instructions" value="Lookup a Supplier Diversity" />
    <property name="defaultSort" >
      <bean parent="SortDefinition">
      </bean>
    </property>
    <property name="lookupFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="vendorSupplierDiversityCode" />
        <bean parent="FieldDefinition"
              p:attributeName="vendorSupplierDiversityDescription" />
        <bean parent="FieldDefinition"
              p:attributeName="active" />
      </list>
    </property>
    <property name="resultFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="vendorSupplierDiversityCode" />
        <bean parent="FieldDefinition"
              p:attributeName="vendorSupplierDiversityDescription" />
        <bean parent="FieldDefinition"
              p:attributeName="active" />
      </list>
    </property>
  </bean>
</beans>
