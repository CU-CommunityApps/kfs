<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "../dataDictionary-1_0.dtd">
<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.module.vendor.bo.VendorHeader">

        <attributes>
        
        <!-- Nearly all fields (maybe not tax #) should be changed to be attributeReference, 
             but the current framework will not allow for an attributeReference to refer to 
             another attributeReference.  Since VendorDetail needs to include all these fields 
             as attributeReferences to pull them into the Maint Doc DD, we cannot define any 
             of these fields as attributeReferences. -->
        
            <attributeReference name="vendorFederalWithholdingTaxBeginningDate" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="genericDate">
                <label>Backup Withholding Begin Date</label>
                <shortLabel>Begin Date</shortLabel>

                <formatterClass>org.kuali.core.web.format.DateFormatter</formatterClass>
			</attributeReference>
          
            <attributeReference name="vendorFederalWithholdingTaxEndDate" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="genericDate">
                <label>Backup Withholding End Date</label>
                <shortLabel>End Date</shortLabel>

                <formatterClass>org.kuali.core.web.format.DateFormatter</formatterClass>
			</attributeReference>

            <attribute name="vendorHeaderGeneratedIdentifier" forceUppercase="true">
                <label>Vendor Header Generated Identifier</label>
                <shortLabel>Header Generated ID</shortLabel>

                <maxLength>10</maxLength>
                <validationPattern><numeric /></validationPattern>

                <control>
                    <text size="12" />
                </control>

			</attribute>

            <attribute name="vendorOwnershipCategoryCode" forceUppercase="true">
                <label>Ownership Category</label>
                <shortLabel>Ownership Category</shortLabel>

                <maxLength>2</maxLength>
                <validationPattern><anyCharacter exactLength="2"/></validationPattern>

				<control>
                    <select valuesFinderClass="org.kuali.module.vendor.lookup.keyvalues.VendorOwnershipCategoryValuesFinder" />
                </control>

			</attribute>

            <attribute name="vendorOwnershipCode" forceUppercase="true">
                <label>Ownership Type</label>
                <shortLabel>Ownership</shortLabel>

                <maxLength>2</maxLength>
                <validationPattern><anyCharacter exactLength="2"/></validationPattern>

				<control>
                    <select valuesFinderClass="org.kuali.module.vendor.lookup.keyvalues.VendorOwnershipCodeValuesFinder" />
                </control>

			</attribute>

            <attribute name="vendorTaxNumber" forceUppercase="true">
                <label>Tax Number</label>
                <shortLabel>Tax Num.</shortLabel>

                <maxLength>9</maxLength>
                <validationPattern><numeric exactLength="9"/></validationPattern>

                <control>
                    <text size="11" />
                </control>

                <attributeAuthorization>
                  <displayWorkgroup>KUALI_PURAP_TAXNBR_ACCESSIBLE</displayWorkgroup>
                  <displayMask><maskLiteral literal="*********"/></displayMask>
                </attributeAuthorization>

			</attribute>
			
			<attribute name="vendorTaxTypeCode" forceUppercase="true">
                <label>Tax Number Type</label>
                <shortLabel>Tax Type </shortLabel>

                <maxLength>4</maxLength>
                <validationPattern><anyCharacter exactLength="4"/></validationPattern>

                <control>
                    <radio valuesFinderClass="org.kuali.module.vendor.lookup.keyvalues.TaxPayerTypeWithNoneValuesFinder" />
                </control>

			</attribute>
			
			<attribute name="vendorTypeCode" forceUppercase="true">
                <label>Vendor Type</label>
                <shortLabel>Type</shortLabel>
                <maxLength>4</maxLength>
                <validationPattern><anyCharacter /></validationPattern>
				<control>
                    <select valuesFinderClass="org.kuali.module.vendor.lookup.keyvalues.VendorTypeValuesFinder" />
                </control>                								

            </attribute>

			<attribute name="vendorSupplierDiversities" forceUppercase="true">
                <label>Vendor Supplier Diversity</label>
                <shortLabel>Supplier Diversity</shortLabel>
                <maxLength>45</maxLength>
                <validationPattern><anyCharacter allowWhitespace="true"/></validationPattern>
                <control>
                    <text size="45" />
                </control>             								

            </attribute>
            
            <attribute name="versionNumber" forceUppercase="true">
                <label>Version Number</label>
                <shortLabel>Number</shortLabel>

                <maxLength>4</maxLength>

                <validationPattern>
                    <numeric />
                </validationPattern>

                <control>
                    <hidden />
                </control>

            </attribute>
            
            <attribute name="vendorW8BenReceivedIndicator" forceUppercase="false">
                <label>W-8BEN Received</label>
                <shortLabel>W-8BEN Received</shortLabel>

                <maxLength>1</maxLength>

                <control>
                    <select valuesFinderClass="org.kuali.core.lookup.keyvalues.IndicatorYNNullValuesFinder" />
                </control>

			</attribute>

            <attribute name="vendorW9ReceivedIndicator" forceUppercase="false">
                <label>W9 Received</label>
                <shortLabel>W9 Received</shortLabel>

                <maxLength>1</maxLength>

                <control>
                    <select valuesFinderClass="org.kuali.core.lookup.keyvalues.IndicatorYNNullValuesFinder" />
                </control>

			</attribute>
			
			<attribute name="vendorDebarredIndicator" forceUppercase="false">
                <label>Debarred</label>
                <shortLabel>Debarred</shortLabel>

                <maxLength>1</maxLength>

                <control>
                    <select valuesFinderClass="org.kuali.core.lookup.keyvalues.IndicatorYNNullValuesFinder" />
                </control>

			</attribute>
			
			<attribute name="vendorForeignIndicator" forceUppercase="false">
                <label>Is this a foreign vendor</label>
                <shortLabel>is Foreign</shortLabel>
                <maxLength>1</maxLength>

                <control>
                    <select valuesFinderClass="org.kuali.core.lookup.keyvalues.IndicatorYNOnlyValuesFinder" />
                </control>

			</attribute>
			
        </attributes>
    </businessObject>
</dictionaryEntry>
