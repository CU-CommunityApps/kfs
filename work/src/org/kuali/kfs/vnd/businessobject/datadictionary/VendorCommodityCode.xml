<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "../dataDictionary-1_0.dtd">
<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.module.vendor.bo.VendorCommodityCode">
 
        <inquiry>
            <title>Vendor Commodity Inquiry</title>
             
            <inquirySections>
                <inquirySection title="Header Text">
                    <inquiryFields>
                        <field attributeName="active" />
                        <field attributeName="purchasingCommodityCode" />
                        <field attributeName="commodityDefaultIndicator" />
                        <field attributeName="vendorDetailAssignedIdentifier" />
                        <field attributeName="vendorHeaderGeneratedIdentifier" />
                    </inquiryFields>
                </inquirySection>
            </inquirySections>  
        </inquiry>

        <lookup>
            <title>Vendor Commodity Lookup</title>

            <instructions>Look up a Vendor Commodity</instructions>

            <defaultSort sortAscending="true">
                <sortAttributes>
                    <sortAttribute attributeName="vendorHeaderGeneratedIdentifier" />
                    <sortAttribute attributeName="vendorDetailAssignedIdentifier" />
                    <sortAttribute attributeName="purchasingCommodityCode" />
                </sortAttributes>
            </defaultSort>

            <lookupFields>
                <lookupField attributeName="active" required="false" />
                <lookupField attributeName="purchasingCommodityCode" required="false" />
                <lookupField attributeName="commodityDefaultIndicator" required="false" />
                <lookupField attributeName="vendorDetailAssignedIdentifier" required="false" />
                <lookupField attributeName="vendorHeaderGeneratedIdentifier" required="false" />
            </lookupFields>

            <resultFields>
                <field attributeName="active" />
                <field attributeName="purchasingCommodityCode" />
                <field attributeName="commodityDefaultIndicator" />
                <field attributeName="vendorDetailAssignedIdentifier" />
                <field attributeName="vendorHeaderGeneratedIdentifier" />
            </resultFields>
        </lookup>

        <attributes>

            <attribute name="active" forceUppercase="true">
                <label>Active</label>
                <shortLabel>Active</shortLabel>

                <maxLength>1</maxLength>

                <control>
                    <checkbox />
                </control>

                <summary>active</summary>
                <description>active description...</description>
			</attribute>

            <attribute name="commodityDefaultIndicator" forceUppercase="true">
                <label>Commodity Default Indicator</label>
                <shortLabel>Indicator</shortLabel>

                <maxLength>1</maxLength>

                <control>
                    <checkbox />
                </control>

                <summary>commodityDefaultIndicator</summary>
                <description>commodityDefaultIndicator description...</description>
			</attribute>

			<attributeReference name="purchasingCommodityCode" sourceClassName="org.kuali.module.vendor.bo.CommodityCode" sourceAttributeName="purchasingCommodityCode"/>
            <attributeReference name="vendorHeaderGeneratedIdentifier" sourceClassName="org.kuali.module.vendor.bo.VendorDetail" sourceAttributeName="vendorHeaderGeneratedIdentifier" />
            <attributeReference name="vendorDetailAssignedIdentifier" sourceClassName="org.kuali.module.vendor.bo.VendorDetail" sourceAttributeName="vendorDetailAssignedIdentifier" />
            <attributeReference name="versionNumber" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="versionNumber" />

        </attributes>
    </businessObject>
</dictionaryEntry>
