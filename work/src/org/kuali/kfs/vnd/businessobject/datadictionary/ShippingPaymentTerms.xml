<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="ShippingPaymentTerms" parent="ShippingPaymentTerms-parentBean" />

  <bean id="ShippingPaymentTerms-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.module.vendor.bo.ShippingPaymentTerms" />
    <property name="inquiryDefinition" >
      <ref bean="ShippingPaymentTerms-inquiryDefinition" />
    </property>
    <property name="lookupDefinition" >
      <ref bean="ShippingPaymentTerms-lookupDefinition" />
    </property>
    <property name="titleAttribute" value="vendorShippingPaymentTermsCode" />
    <property name="objectLabel" value="Shipping Payment Terms" />
    <property name="attributes" >
      <list>
        <ref bean="ShippingPaymentTerms-vendorShippingPaymentTermsCode" />
        <ref bean="ShippingPaymentTerms-vendorShippingPaymentTermsDescription" />
        <ref bean="ShippingPaymentTerms-vendorShippingPaymentTermsPayIndicator" />
        <ref bean="ShippingPaymentTerms-active" />
        <ref bean="ShippingPaymentTerms-versionNumber" />
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->


  <bean id="ShippingPaymentTerms-vendorShippingPaymentTermsCode" parent="ShippingPaymentTerms-vendorShippingPaymentTermsCode-parentBean" />

  <bean id="ShippingPaymentTerms-vendorShippingPaymentTermsCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="vendorShippingPaymentTermsCode" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Shipping Payment Terms Code" />
    <property name="shortLabel" value="Code" />
    <property name="maxLength" value="4" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="6" />
    </property>
  </bean>
  <bean id="ShippingPaymentTerms-vendorShippingPaymentTermsDescription" parent="ShippingPaymentTerms-vendorShippingPaymentTermsDescription-parentBean" />

  <bean id="ShippingPaymentTerms-vendorShippingPaymentTermsDescription-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="vendorShippingPaymentTermsDescription" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Shipping Payment Terms Description" />
    <property name="shortLabel" value="Description" />
    <property name="maxLength" value="45" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern"
            p:allowWhitespace="true" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="47" />
    </property>
  </bean>
  <bean id="ShippingPaymentTerms-vendorShippingPaymentTermsPayIndicator" parent="ShippingPaymentTerms-vendorShippingPaymentTermsPayIndicator-parentBean" />

  <bean id="ShippingPaymentTerms-vendorShippingPaymentTermsPayIndicator-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="vendorShippingPaymentTermsPayIndicator" />
    <property name="forceUppercase" value="false" />
    <property name="label" value="Shipping Payment Terms Pay Indicator" />
    <property name="shortLabel" value="Indicator" />
    <property name="maxLength" value="1" />
    <property name="control" >
      <bean parent="CheckboxControlDefinition" />
    </property>
  </bean>
  <bean id="ShippingPaymentTerms-active" parent="ShippingPaymentTerms-active-parentBean" />

  <bean id="ShippingPaymentTerms-active-parentBean" abstract="true" parent="AttributeReferenceDummy-activeIndicator">
    <property name="name" value="active" />
  </bean>
  <bean id="ShippingPaymentTerms-versionNumber" parent="ShippingPaymentTerms-versionNumber-parentBean" />

  <bean id="ShippingPaymentTerms-versionNumber-parentBean" abstract="true" parent="AttributeReferenceDummy-versionNumber">
  </bean>

<!-- Business Object Inquiry Definition -->


  <bean id="ShippingPaymentTerms-inquiryDefinition" parent="ShippingPaymentTerms-inquiryDefinition-parentBean" />

  <bean id="ShippingPaymentTerms-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
    <property name="title" value="Shipping Payment Terms Inquiry" />
    <property name="inquirySections" >
      <list>
        <bean parent="InquirySectionDefinition">
          <property name="title" value="Header Text" />
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields" >
            <list>
              <bean parent="FieldDefinition"
                    p:attributeName="vendorShippingPaymentTermsCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="vendorShippingPaymentTermsDescription" />
              <bean parent="FieldDefinition"
                    p:attributeName="vendorShippingPaymentTermsPayIndicator" />
              <bean parent="FieldDefinition"
                    p:attributeName="active" />
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Business Object Lookup Definition -->


  <bean id="ShippingPaymentTerms-lookupDefinition" parent="ShippingPaymentTerms-lookupDefinition-parentBean" />

  <bean id="ShippingPaymentTerms-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
    <property name="title" value="Shipping Payment Terms Lookup" />
    <property name="instructions" value="Lookup Shipping Payment Terms" />
    <property name="defaultSort" >
      <bean parent="SortDefinition">
      </bean>
    </property>
    <property name="lookupFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="vendorShippingPaymentTermsCode" />
        <bean parent="FieldDefinition"
              p:attributeName="vendorShippingPaymentTermsDescription" />
        <bean parent="FieldDefinition"
              p:attributeName="vendorShippingPaymentTermsPayIndicator" />
        <bean parent="FieldDefinition"
              p:attributeName="active" />
      </list>
    </property>
    <property name="resultFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="vendorShippingPaymentTermsCode" />
        <bean parent="FieldDefinition"
              p:attributeName="vendorShippingPaymentTermsDescription" />
        <bean parent="FieldDefinition"
              p:attributeName="vendorShippingPaymentTermsPayIndicator" />
        <bean parent="FieldDefinition"
              p:attributeName="active" />
      </list>
    </property>
  </bean>
</beans>
