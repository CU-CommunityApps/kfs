				<control>
                    <select valuesFinderClass="org.kuali.module.vendor.lookup.keyvalues.ShippingPaymentTermsValuesFinder" />
                </control>
            </attributeReference>
                        
            <attributeReference name="vendorShippingTitleCode" sourceClassName="org.kuali.module.vendor.bo.ShippingTitle" sourceAttributeName="vendorShippingTitleCode">
                <label>Shipping Title</label>
                <shortLabel>Shipping Title</shortLabel>
                <required>false</required>
				<control>
                    <select valuesFinderClass="org.kuali.module.vendor.lookup.keyvalues.ShippingTitleValuesFinder" />
                </control>
            </attributeReference>
                                    
            <attributeReference name="vendorInactiveReasonCode" sourceClassName="org.kuali.module.vendor.bo.VendorInactiveReason" sourceAttributeName="vendorInactiveReasonCode">
                <label>Inactive Reason</label>
                <shortLabel>Inactive Reason</shortLabel>
				<control>
					<select valuesFinderClass="org.kuali.module.vendor.lookup.keyvalues.InactiveReasonValuesFinder" />
				</control>
            </attributeReference>
            
             <attributeReference name="vendorSoldToGeneratedIdentifier" sourceClassName="org.kuali.module.vendor.bo.VendorDetail" sourceAttributeName="vendorHeaderGeneratedIdentifier">
                <label>Sold To Lookup</label>
                <shortLabel>Sold To Lookup</shortLabel>
                <required>false</required>
                <control>
                    <lookupHidden/>
                </control>
             </attributeReference>           

            <attributeReference name="vendorSoldToAssignedIdentifier" sourceClassName="org.kuali.module.vendor.bo.VendorDetail" sourceAttributeName="vendorDetailAssignedIdentifier">
                <label>Sold To Assigned Identifier</label>
                <shortLabel>Sold To Assigned ID</shortLabel>
                <required>false</required>
            </attributeReference>   
            
            <attributeReference name="vendorMinimumOrderAmount" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="genericAmount">
                <label>Minimum Order Amount</label>
                <shortLabel>Min. Order Amt.</shortLabel>
            </attributeReference>
            
			<attributeReference name="vendorStateForLookup" sourceClassName="org.kuali.module.vendor.bo.VendorAttributeReferenceDummy" sourceAttributeName="stateCode">
				<label>State</label>
				<shortLabel>State</shortLabel>
			</attributeReference>
			
            <attributeReference name="vendorCreditCardIndicator" sourceClassName="org.kuali.module.vendor.bo.VendorAttributeReferenceDummy" sourceAttributeName="yesNoWithBlankIndicator">
                <label>Credit Card</label>
                <shortLabel>Credit Card</shortLabel>
            </attributeReference>
            
            <attributeReference name="vendorConfirmationIndicator" sourceClassName="org.kuali.module.vendor.bo.VendorAttributeReferenceDummy" sourceAttributeName="yesNoWithBlankIndicator">
                <label>Confirmation</label>
                <shortLabel>Confirmation?</shortLabel>
			</attributeReference> 

            <attributeReference name="vendorPrepaymentIndicator" sourceClassName="org.kuali.module.vendor.bo.VendorAttributeReferenceDummy" sourceAttributeName="yesNoWithBlankIndicator">
                <label>Pre-Payment</label>
                <shortLabel>Pre-Payment?</shortLabel>
			</attributeReference>
						      
            <attributeReference name="vendorRestrictedIndicator" sourceClassName="org.kuali.module.vendor.bo.VendorAttributeReferenceDummy" sourceAttributeName="yesNoWithBlankIndicator">
                <label>Restricted</label>
                <shortLabel>Restricted?</shortLabel>
			</attributeReference>
			
            <attributeReference name="vendorParentIndicator" sourceClassName="org.kuali.module.vendor.bo.VendorAttributeReferenceDummy" sourceAttributeName="hiddenIndicator">
                <label>Vendor Parent Indicator</label>
                <shortLabel>Parent?</shortLabel>
			</attributeReference>

            <attributeReference name="vendorFirstLastNameIndicator" sourceClassName="org.kuali.module.vendor.bo.VendorAttributeReferenceDummy" sourceAttributeName="hiddenIndicator">
                <label>Vendor First Last Name Indicator</label>
                <shortLabel>Vendor First Last Name Indicator</shortLabel>
			</attributeReference>											
																           
            <attributeReference name="vendorAddresses.vendorStateCode" sourceClassName="org.kuali.module.vendor.bo.VendorAddress" sourceAttributeName="vendorStateCode"/>

