			</attribute>
			
            <attribute name="vendorSoldToName">
                <label>Sold To Name</label>
                <shortLabel>Sold To Name</shortLabel>

                <maxLength>100</maxLength>
                <validationPattern><anyCharacter allowWhitespace="true"/></validationPattern>

                <control>
                    <text size="45" />
                </control>

                <summary>vendorSoldToName</summary>
                <description>vendorSoldToName description...</description>
			</attribute>
			
