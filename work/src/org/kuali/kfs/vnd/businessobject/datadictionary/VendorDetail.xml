            <attributeReference name="vendorHeader.vendorW8BenReceivedIndicator" sourceClassName="org.kuali.module.vendor.bo.VendorHeader" sourceAttributeName="vendorW8BenReceivedIndicator" />
        
            <attributeReference name="vendorHeader.vendorFederalWithholdingTaxEndDate" sourceClassName="org.kuali.module.vendor.bo.VendorHeader" sourceAttributeName="vendorFederalWithholdingTaxEndDate" />
                        
            <attributeReference name="vendorHeader.vendorDebarredIndicator" sourceClassName="org.kuali.module.vendor.bo.VendorHeader" sourceAttributeName="vendorDebarredIndicator" />
            
            <attributeReference name="vendorHeader.vendorForeignIndicator" sourceClassName="org.kuali.module.vendor.bo.VendorHeader" sourceAttributeName="vendorForeignIndicator" /> 

            <attributeReference name="vendorHeader.vendorTypeCode" sourceClassName="org.kuali.module.vendor.bo.VendorHeader" sourceAttributeName="vendorTypeCode" />
            
