<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.module.vendor.bo.VendorDetail">

		<boNotesEnabled>true</boNotesEnabled>
        <inquiry>
            <title>Vendor Detail Inquiry</title>

            <inquirySections>
                <inquirySection title="Vendor Detail" numberOfColumns="2">
                    <inquiryFields>
                        <inquirySubSectionHeader name="Vendor Header Information" />
                        <field attributeName="vendorNumber" />
                        <field attributeName="vendorName" />
                        <inquirySubSectionHeader name="Corporate Information" />
                        <field attributeName="vendorHeader.vendorTypeCode" />
                        <field attributeName="vendorHeader.vendorW9ReceivedIndicator" />
                        <field attributeName="vendorHeader.vendorForeignIndicator" />
                        <field attributeName="vendorHeader.vendorW8BenReceivedIndicator" />
                        <field attributeName="vendorHeader.vendorTaxNumber" />
                        <field attributeName="vendorHeader.vendorFederalWithholdingTaxBeginningDate" />
                        <field attributeName="vendorHeader.vendorTaxTypeCode" />
                        <field attributeName="vendorHeader.vendorFederalWithholdingTaxEndDate" />
                        <field attributeName="vendorHeader.vendorOwnershipCode" />
                        <field attributeName="vendorHeader.vendorDebarredIndicator" />
                        <field attributeName="vendorHeader.vendorOwnershipCategoryCode" />

                        <inquirySubSectionHeader name="Detail Information" />
                        <field attributeName="vendorPaymentTermsCode" />
                        <field attributeName="vendorSoldToNumber" />
                        <field attributeName="vendorPrepaymentIndicator" />
                        <field attributeName="vendorSoldToName" />
                        <field attributeName="vendorCreditCardIndicator" />
                        <field attributeName="vendorRestrictedIndicator" />
                        <field attributeName="vendorMinimumOrderAmount" />
                        <field attributeName="vendorRestrictedDate" />
                        <field attributeName="vendorShippingTitleCode" />
                        <field attributeName="vendorRestrictedPerson.personName" />
                        <field attributeName="vendorShippingPaymentTermsCode" />
                        <field attributeName="vendorRestrictedReasonText" />
                        <field attributeName="vendorDunsNumber" />
                        <field attributeName="vendorRemitName" />
                        <field attributeName="vendorUrlAddress" />
                        <field attributeName="activeIndicator"/>
                        <field attributeName="vendorConfirmationIndicator" />
                        <field attributeName="vendorInactiveReasonCode" />               
                    </inquiryFields>
                </inquirySection>
                
                <inquirySection title="Address">
                    <inquiryFields>
                        <inquiryCollection name="vendorAddresses" businessObjectClass="org.kuali.module.vendor.bo.VendorAddress" numberOfColumns="2" >
                            <field attributeName="vendorAddressTypeCode" />
                            <field attributeName="vendorCountryCode" />
                            <field attributeName="vendorLine1Address" />
                            <field attributeName="vendorAttentionName" />
                            <field attributeName="vendorLine2Address" />
                            <field attributeName="vendorBusinessToBusinessUrlAddress" />
                            <field attributeName="vendorCityName" />
                            <field attributeName="vendorFaxNumber" />
                            <field attributeName="vendorStateCode" />
                            <field attributeName="vendorAddressEmailAddress" />
                            <field attributeName="vendorZipCode" />
                            <field attributeName="vendorDefaultAddressIndicator" />
                            <field attributeName="vendorAddressInternationalProvinceName" />
                            <field attributeName="active" />
                            <inquiryCollection name="vendorDefaultAddresses" businessObjectClass="org.kuali.module.vendor.bo.VendorDefaultAddress" summaryTitle="Default Address" >
                                <field attributeName="vendorCampusCode" />
                                <field attributeName="active" />
                                <inquirySummaryFields>
           		       		        <inquirySummaryField attributeName="vendorCampusCode" />
           		       	        </inquirySummaryFields>
                            </inquiryCollection>
                            <inquirySummaryFields>
                              <inquirySummaryField attributeName="vendorAddressTypeCode" />
                              <inquirySummaryField attributeName="vendorCityName" />
                            </inquirySummaryFields>
                        </inquiryCollection>
                    </inquiryFields>
                </inquirySection>
                
                <inquirySection title="Contacts">
                    <inquiryFields>
                        <inquiryCollection name="vendorContacts" businessObjectClass="org.kuali.module.vendor.bo.VendorContact" numberOfColumns="2" summaryTitle="Contact" >                       	
                         	<field attributeName="vendorContactTypeCode"/>
                        	<field attributeName="vendorZipCode"/>
                         	<field attributeName="vendorContactName"/>
                        	<field attributeName="vendorAddressInternationalProvinceName"/>
                        	<field attributeName="vendorContactEmailAddress"/>
                        	<field attributeName="vendorCountryCode"/>
                        	<field attributeName="vendorLine1Address"/>
                        	<field attributeName="vendorAttentionName"/>
                        	<field attributeName="vendorLine2Address"/>
                        	<field attributeName="vendorContactCommentText"/>
                         	<field attributeName="vendorCityName"/>
                        	<field attributeName="active" />
 							<field attributeName="vendorStateCode"/>
                        	<inquiryCollection name="vendorContactPhoneNumbers" businessObjectClass="org.kuali.module.vendor.bo.VendorContactPhoneNumber" numberOfColumns="1" summaryTitle="Phone Number" >
                        		<field attributeName="vendorPhoneTypeCode"/>
                        		<field attributeName="vendorPhoneNumber"/>
                        		<field attributeName="vendorPhoneExtensionNumber"/>
                        		<field attributeName="active" />
                        		<inquirySummaryFields>
           		       		        <inquirySummaryField attributeName="vendorPhoneTypeCode" />
                                    <inquirySummaryField attributeName="vendorPhoneNumber" />
           		       	        </inquirySummaryFields>
                        	</inquiryCollection>
                        	<inquirySummaryFields>
           		                <inquirySummaryField attributeName="vendorContactTypeCode" />
                                <inquirySummaryField attributeName="vendorContactName" />
           		       	    </inquirySummaryFields>
                        </inquiryCollection>
                    </inquiryFields>
                </inquirySection>
                
 				<inquirySection title="Supplier Diversities">
                    <inquiryFields>
                        <inquiryCollection name="vendorHeader.vendorSupplierDiversities" businessObjectClass="org.kuali.module.vendor.bo.VendorSupplierDiversity"  summaryTitle="Supplier Diversity" >
                        	<field attributeName="vendorSupplierDiversityCode" />
							<field attributeName="active" />
                        	<inquirySummaryFields>
                                <inquirySummaryField attributeName="vendorSupplierDiversityCode" />
                            </inquirySummaryFields>
                        </inquiryCollection>
                    </inquiryFields>
                </inquirySection>                           
                
    			<inquirySection title="Shipping Special Conditions">
                    <inquiryFields>
                        <inquiryCollection name="vendorShippingSpecialConditions" businessObjectClass="org.kuali.module.vendor.bo.VendorShippingSpecialCondition" summaryTitle="Shipping Special Condition" >
                        	<field attributeName="vendorShippingSpecialConditionCode"/>
							<field attributeName="active" />
                            <inquirySummaryFields>
           		       		    <inquirySummaryField attributeName="vendorShippingSpecialConditionCode" />
           		       	    </inquirySummaryFields>    
                        </inquiryCollection>
                    </inquiryFields>
                </inquirySection>             
         
     			<inquirySection title="Search Alias">
                    <inquiryFields>
                        <inquiryCollection name="vendorAliases" businessObjectClass="org.kuali.module.vendor.bo.VendorAlias">
                        	<field attributeName="vendorAliasName"/>
							<field attributeName="active" />
                        	<inquirySummaryFields>
           		       		    <inquirySummaryField attributeName="vendorAliasName" />
           		       	    </inquirySummaryFields>   
                        </inquiryCollection>
                    </inquiryFields>
                </inquirySection>
                
      			<inquirySection title="Vendor Phone Number">
                    <inquiryFields>
                        <inquiryCollection name="vendorPhoneNumbers" businessObjectClass="org.kuali.module.vendor.bo.VendorPhoneNumber">
                        	<field attributeName="vendorPhoneTypeCode"/>
                        	<field attributeName="vendorPhoneNumber"/>
                        	<field attributeName="vendorPhoneExtensionNumber"/>
							<field attributeName="active" />
                        	<inquirySummaryFields>
           		       		    <inquirySummaryField attributeName="vendorPhoneTypeCode" />
           		       		    <inquirySummaryField attributeName="vendorPhoneNumber" />
           		       	    </inquirySummaryFields> 
                        </inquiryCollection>
                    </inquiryFields>
                </inquirySection>                                                       
                
       			<inquirySection title="Vendor Customer Number">
                    <inquiryFields>
                        <inquiryCollection name="vendorCustomerNumbers" businessObjectClass="org.kuali.module.vendor.bo.VendorCustomerNumber">
                        	<field attributeName="vendorCustomerNumber"/>
                        	<field attributeName="chartOfAccountsCode"/>
                        	<field attributeName="vendorOrganizationCode"/>
							<field attributeName="active" />
                        	<inquirySummaryFields>
           		       		    <inquirySummaryField attributeName="vendorCustomerNumber" />
           		       	    </inquirySummaryFields> 
                        </inquiryCollection>
                    </inquiryFields>
                </inquirySection>
                
                <inquirySection title="Contracts">
                    <inquiryFields>
                        <inquiryCollection name="vendorContracts" businessObjectClass="org.kuali.module.vendor.bo.VendorContract" numberOfColumns="2" summaryTitle="Contract" >
                        	<field attributeName="vendorContractName"/>
                        	<field attributeName="purchaseOrderCostSourceCode"/>
                          	<field attributeName="vendorContractDescription"/>
                        	<field attributeName="vendorPaymentTermsCode"/>
                        	<field attributeName="vendorCampusCode"/>
                        	<field attributeName="vendorShippingPaymentTermsCode"/>
                        	<field attributeName="vendorContractBeginningDate"/>
                        	<field attributeName="vendorShippingTitleCode"/>
                         	<field attributeName="vendorContractEndDate"/>
                        	<field attributeName="vendorContractExtensionDate"/>
                        	<field attributeName="contractManagerCode"/>
                        	<field attributeName="organizationAutomaticPurchaseOrderLimit"/>
							<field attributeName="active" />
                        	<inquiryCollection name="vendorContractOrganizations" businessObjectClass="org.kuali.module.vendor.bo.VendorContractOrganization" summaryTitle="Contract Organization" >
                        		<field attributeName="chartOfAccountsCode"/>
                        		<field attributeName="organizationCode"/>
                        		<field attributeName="vendorContractPurchaseOrderLimitAmount"/>
                        		<field attributeName="vendorContractExcludeIndicator"/>
                        		<field attributeName="active" />
                                <inquirySummaryFields>
                                    <inquirySummaryField attributeName="chartOfAccountsCode" />
                                    <inquirySummaryField attributeName="organizationCode" />
                                </inquirySummaryFields>                        		
                        	</inquiryCollection>
                        	<inquirySummaryFields>
           		       		    <inquirySummaryField attributeName="vendorCampusCode" />
                                <inquirySummaryField attributeName="vendorContractName" />
           		       	    </inquirySummaryFields>
                        </inquiryCollection>
                    </inquiryFields>
                </inquirySection>  
                
                <inquirySection title="Notes">
                    <inquiryFields>
                        <inquiryCollection name="boNotes" businessObjectClass="org.kuali.core.bo.Note">
                        	<field attributeName="noteText"/>
                        	<field attributeName="notePostedTimestamp"/>
                        	<field attributeName="authorUniversal.personName"/>
                        </inquiryCollection>
                    </inquiryFields>
                </inquirySection>         
                
            </inquirySections>
        </inquiry>

        <lookup>
            <lookupableID>vendorLookupable</lookupableID>
           
            <title>Vendor Detail Lookup</title>

            <instructions>Lookup a Vendor Detail</instructions>

            <defaultSort sortAscending="true">
                <sortAttributes>
                    <sortAttribute attributeName="vendorName" />
                </sortAttributes>
            </defaultSort>

            <lookupFields>
                <lookupField attributeName="vendorName" required="false" />
                <lookupField attributeName="vendorHeader.vendorTaxNumber" required="false" />
                <lookupField attributeName="vendorNumber" required="false" />      
                <lookupField attributeName="activeIndicator" required="false" />
                <lookupField attributeName="vendorHeader.vendorTypeCode" required="false" />         
                <lookupField attributeName="vendorAddresses.vendorStateCode" required="false" />                   
                <lookupField attributeName="vendorHeader.vendorSupplierDiversities.vendorSupplierDiversityCode" required="false" />         
            </lookupFields>

            <resultFields>
                <field attributeName="vendorName" />
                <field attributeName="vendorAliases" />
            	<field attributeName="vendorNumber" />
            	<field attributeName="activeIndicator" />
                <field attributeName="vendorHeader.vendorTypeCode" />
                <field attributeName="vendorStateForLookup" />
                <field attributeName="vendorHeader.vendorSupplierDiversities" />
            </resultFields>
        </lookup>

        <titleAttribute>vendorName</titleAttribute>

        <attributes>
			<attribute name="vendorHeaderGeneratedIdentifier" forceUppercase="true">
                <label>Vendor Header Generated Identifier</label>
                <shortLabel>Header Generated ID</shortLabel>

                <maxLength>10</maxLength>
                <validationPattern><numeric /></validationPattern>

                <control>
                    <hidden />
                </control>

                <summary>vendorHeaderGeneratedIdentifier</summary>
                <description>vendorHeaderGeneratedIdentifier description...</description>
			</attribute>
			
			<attribute name="vendorDetailAssignedIdentifier" forceUppercase="true">
                <label>Vendor Detail Assigned Identifier</label>
                <shortLabel>Detail Assigned ID</shortLabel>

                <maxLength>10</maxLength>
                <validationPattern><numeric /></validationPattern>

                <control>
                    <hidden />
                </control>

                <summary>vendorDetailAssignedIdentifier</summary>
                <description>vendorDetailAssignedIdentifier description...</description>
			</attribute>
			
			<attribute name="vendorNumber" forceUppercase="true">
				<label>Vendor Number</label>
				<shortLabel>Vendor #</shortLabel>
				
				<maxLength>20</maxLength>
				<validationPattern><anyCharacter /></validationPattern>
				
				<control>
					<text size="22"/>
				</control>
				
				<summary>vendorNumber</summary>
				<description>A concatenation of the vendorHeaderGeneratedIdentifier, a dash,
				and the vendorDetailAssignedIdentifier</description>
			</attribute>

            <attribute name="vendorDunsNumber" forceUppercase="true">
                <label>DUNS Number</label>
                <shortLabel>DUNS Num.</shortLabel>

                <maxLength>9</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="11" />
                </control>

                <summary>vendorDunsNumber</summary>
                <description>vendorDunsNumber description...</description>
			</attribute>

            <attribute name="vendorName" forceUppercase="true">
                <label>Vendor Name</label>
                <shortLabel>Vendor Name</shortLabel>

                <maxLength>45</maxLength>
                <validationPattern><anyCharacter allowWhitespace="true"/></validationPattern>

                <control>
                    <text size="45" />
                </control>

                <summary>vendorName</summary>
                <description>Only one format may be used for vendor name. Enter either the Vendor Name OR the Vendor First and Last Name.</description>
			</attribute>

            <!-- removed in relation to KULPURAP-800
 
				attribute name="altVendorName" forceUppercase="true">
                <label>Corporate > Division Name</label>
                <shortLabel>Corporate > Division Name</shortLabel>

                <maxLength>45</maxLength>
                <validationPattern><anyCharacter allowWhitespace="true"/></validationPattern>

                <control>
                    <text size="45" />
                </control>

                <summary>Corporate > Division Name</summary>
                <description>Only one format may be used for vendor name. Enter either the Vendor Name OR the Vendor First and Last Name.</description>
			</attribute -->

            <attribute name="vendorLastName" forceUppercase="true">
                <label>Vendor Last Name</label>
                <shortLabel>Vendor Last Name</shortLabel>

                <maxLength>45</maxLength>
                <validationPattern><anyCharacter allowWhitespace="true"/></validationPattern>

                <control>
                    <text size="45" />
                </control>

                <summary>vendorLastName</summary>
                <description>Only one format may be used for vendor name. Enter either the Vendor Name OR the Vendor First and Last Name.</description>
			</attribute>
			
            <attribute name="vendorFirstName" forceUppercase="true">
                <label>Vendor First Name</label>
                <shortLabel>Vendor First Name</shortLabel>

                <maxLength>45</maxLength>
                <validationPattern><anyCharacter allowWhitespace="true"/></validationPattern>

                <control>
                    <text size="45" />
                </control>

                <summary>vendorFirstName</summary>
                <description>Only one format may be used for vendor name. Enter either the Vendor Name OR the Vendor First and Last Name.</description>
			</attribute>		

            <attribute name="vendorRemitName">
                <label>Remit Name</label>
                <shortLabel>Remit Name</shortLabel>

                <maxLength>45</maxLength>
                <validationPattern><anyCharacter allowWhitespace="true"/></validationPattern>

                <control>
                    <text size="45" />
                </control>

                <summary>vendorRemitName</summary>
                <description>vendorRemitName description...</description>
			</attribute>

            <attribute name="vendorRestrictedReasonText">
                <label>Restricted Reason</label>
                <shortLabel>Restricted Reason</shortLabel>

                <maxLength>60</maxLength>
                <validationPattern><anyCharacter allowWhitespace="true"/></validationPattern>

                <control>
                    <textarea rows="3" cols="60" />
                </control>

                <summary>vendorRestrictedReasonText</summary>
                <description>vendorRestrictedReasonText description...</description>
			</attribute>

            <attribute name="vendorSoldToNumber" forceUppercase="true">
                <label>Sold To Vendor Number</label>
                <shortLabel>Sold To Vendor Num.</shortLabel>

                <maxLength>20</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="22"/>
                </control>

                <summary>vendorSoldToNumber</summary>
                <description>A concatenation of the vendorSoldToGeneratedIdentifier, a dash,
