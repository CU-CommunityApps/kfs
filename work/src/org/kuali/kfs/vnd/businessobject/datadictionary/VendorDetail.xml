            <attributeReference name="vendorAliases.vendorAliasName" sourceClassName="org.kuali.module.vendor.bo.VendorAlias" sourceAttributeName="vendorAliasName" />
            
            <attributeReference name="vendorPhoneNumbers.vendorPhoneTypeCode" sourceClassName="org.kuali.module.vendor.bo.VendorPhoneNumber" sourceAttributeName="vendorPhoneTypeCode"/>
            
            <attributeReference name="vendorPhoneNumbers.vendorPhoneNumber" sourceClassName="org.kuali.module.vendor.bo.VendorPhoneNumber" sourceAttributeName="vendorPhoneNumber"/>
            
            <attributeReference name="vendorPhoneNumbers.vendorPhoneExtensionNumber" sourceClassName="org.kuali.module.vendor.bo.VendorPhoneNumber" sourceAttributeName="vendorPhoneExtensionNumber"/>
            
