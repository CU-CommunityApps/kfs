                <label>Restricted By</label>
                <shortLabel>Restricted By</shortLabel>
                <required>false</required>
                <control>
                    <kualiUser universalIdAttributeName="vendorRestrictedPersonIdentifier" userIdAttributeName="vendorRestrictedPerson.personUserIdentifier" personNameAttributeName="vendorRestrictedPerson.personName"/>
                </control>

            </attributeReference>
            
            <attributeReference name="vendorRestrictedPerson.personName" sourceClassName="org.kuali.core.bo.user.UniversalUser" sourceAttributeName="personName">
                <label>Restricted Person Name</label>
                <shortLabel>Restricted Person Name</shortLabel>
                <required>false</required>
            </attributeReference>
            
            <attributeReference name="vendorRestrictedDate" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="genericDate">
                <label>Restricted Date</label>
                <shortLabel>Restricted Date</shortLabel>

            </attributeReference>
            			
            <attributeReference name="vendorPaymentTermsCode" sourceClassName="org.kuali.module.vendor.bo.PaymentTermType" sourceAttributeName="vendorPaymentTermsCode">
