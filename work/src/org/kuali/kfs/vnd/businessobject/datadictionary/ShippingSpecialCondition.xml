<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "../dataDictionary-1_0.dtd">
<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.module.vendor.bo.ShippingSpecialCondition">

        <inquiry>
            <title>Shipping Special Conditions Inquiry</title>

            <inquirySections><inquirySection title="Header Text"><inquiryFields>
                <field attributeName="vendorShippingSpecialConditionCode" />
                <field attributeName="vendorShippingSpecialConditionDescription" />
                <field attributeName="active" />
            </inquiryFields></inquirySection></inquirySections>
        </inquiry>

        <lookup>
            <title>Shipping Special Conditions Lookup</title>

            <instructions>Lookup a Shipping Special Condition</instructions>

            <defaultSort sortAscending="true">
                <sortAttributes>
                    <sortAttribute attributeName="vendorShippingSpecialConditionDescription" />
                </sortAttributes>
            </defaultSort>

            <lookupFields>
                <lookupField attributeName="vendorShippingSpecialConditionCode" required="false" />
                <lookupField attributeName="vendorShippingSpecialConditionDescription" required="false" />
                <lookupField attributeName="active" required="false" />
           </lookupFields>

            <resultFields>
                <field attributeName="vendorShippingSpecialConditionCode" />
                <field attributeName="vendorShippingSpecialConditionDescription" />
                <field attributeName="active" />
            </resultFields>
        </lookup>

        <titleAttribute>vendorShippingSpecialConditionCode</titleAttribute>

        <attributes>

            <attribute name="vendorShippingSpecialConditionCode" forceUppercase="true">
                <label>Shipping Special Condition Code</label>
                <shortLabel>Code</shortLabel>

                <maxLength>4</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="6" />
                </control>

			</attribute>

            <attribute name="vendorShippingSpecialConditionDescription" forceUppercase="true">
                <label>Shipping Special Condition Description</label>
                <shortLabel>Description</shortLabel>

                <maxLength>45</maxLength>
                <validationPattern><anyCharacter allowWhitespace="true" /></validationPattern>

                <control>
                    <text size="47" />
                </control>

			</attribute>

            <attributeReference name="active" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="activeIndicator" />

            <attributeReference name="versionNumber" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="versionNumber" />

        </attributes>
        <objectLabel>Shipping Special Conditions</objectLabel>
    </businessObject>
</dictionaryEntry>
