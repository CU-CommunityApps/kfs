<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "../dataDictionary-1_0.dtd">
<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.module.vendor.bo.ContractManager">

        <inquiry>
            <title>Purchasing Contract Manager Inquiry</title>

            <inquirySections><inquirySection title="Header Text"><inquiryFields>
                <field attributeName="contractManagerCode" />
                <field attributeName="contractManagerUserIdentifier" />
                <field attributeName="contractManagerName" />
                <field attributeName="contractManagerPhoneNumber" />
                <field attributeName="contractManagerFaxNumber" />
                <field attributeName="contractManagerDelegationDollarLimit" />
            </inquiryFields></inquirySection></inquirySections>
        </inquiry>

        <lookup>
            <title>Purchasing Contract Manager Lookup</title>

            <instructions>Lookup a Contract Manager</instructions>

            <defaultSort sortAscending="true">
                <sortAttributes>
                    <sortAttribute attributeName="contractManagerCode" />
                </sortAttributes>
            </defaultSort>

            <lookupFields>
                <lookupField attributeName="contractManagerCode" required="false" />
                <lookupField attributeName="contractManagerUserIdentifier" required="false" />
                <lookupField attributeName="contractManagerName" required="false" />
                <lookupField attributeName="contractManagerPhoneNumber" required="false" />
                <lookupField attributeName="contractManagerFaxNumber" required="false" />
                <lookupField attributeName="contractManagerDelegationDollarLimit" required="false" />
                <lookupField attributeName="contractManagerSignatureImageLocationDescription" required="false" />
            </lookupFields>

            <resultFields>
                <field attributeName="contractManagerCode" />
                <field attributeName="contractManagerUserIdentifier" />
                <field attributeName="contractManagerName" />
                <field attributeName="contractManagerPhoneNumber" />
                <field attributeName="contractManagerFaxNumber" />
                <field attributeName="contractManagerDelegationDollarLimit" />
                <field attributeName="contractManagerSignatureImageLocationDescription" />
            </resultFields>
        </lookup>

        <titleAttribute>contractManagerCode</titleAttribute>

        <attributes>

            <attribute name="contractManagerCode" forceUppercase="true">
                <label>Purchasing Contract Manager Code</label>
                <shortLabel>Code</shortLabel>

                <maxLength>2</maxLength>
                <validationPattern><numeric /></validationPattern>

                <control>
                    <text size="4" />
                </control>

			</attribute>

            <attribute name="contractManagerDelegationDollarLimit" forceUppercase="false">
                <label>Purchasing Contract Manager Delegation Dollar Limit</label>
                <shortLabel>Limit</shortLabel>

                <maxLength>19</maxLength>
                <validationPattern><floatingPoint /></validationPattern>

                <control>
                    <text size="21" />
                </control>

				<formatterClass>org.kuali.core.web.format.CurrencyFormatter</formatterClass>
			</attribute>

            <attribute name="contractManagerFaxNumber" forceUppercase="true">
                <label>Purchasing Contract Manager Fax Number</label>
                <shortLabel>Number</shortLabel>

                <maxLength>45</maxLength>
                <validationPattern><anyCharacter allowWhitespace="true"/></validationPattern>

                <control>
                    <text size="47" />
                </control>

			</attribute>

            <attribute name="contractManagerName" forceUppercase="true">
                <label>Purchasing Contract Manager Name</label>
                <shortLabel>Name</shortLabel>

                <maxLength>45</maxLength>
                <validationPattern><anyCharacter allowWhitespace="true" /></validationPattern>

                <control>
                    <text size="47" />
                </control>

			</attribute>

            <attribute name="contractManagerPhoneNumber" forceUppercase="true">
                <label>Purchasing Contract Manager Phone Number</label>
                <shortLabel>Number</shortLabel>

                <maxLength>45</maxLength>
                <validationPattern><anyCharacter allowWhitespace="true"/></validationPattern>

                <control>
                    <text size="47" />
                </control>

			</attribute>

            <attribute name="contractManagerSignatureImageLocationDescription" forceUppercase="true">
                <label>Purchasing Contract Manager Signature Image Location Description</label>
                <shortLabel>Description</shortLabel>

                <maxLength>45</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="47" />
                </control>

			</attribute>

            <attribute name="contractManagerUserIdentifier" forceUppercase="true">
                <label>Purchasing Contract Manager User Identifier</label>
                <shortLabel>Identifier</shortLabel>

                <maxLength>11</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="13" />
                </control>

			</attribute>

            <attributeReference name="versionNumber" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="versionNumber" />

        </attributes>
        <objectLabel>Contract Manager</objectLabel>
    </businessObject>
</dictionaryEntry>
