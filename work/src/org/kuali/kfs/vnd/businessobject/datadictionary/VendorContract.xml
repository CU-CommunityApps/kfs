<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "../dataDictionary-1_0.dtd">
<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.module.vendor.bo.VendorContract">

        <inquiry>
            <title>Vendor Contract Inquiry</title>

            <inquirySections><inquirySection title="Header Text"><inquiryFields>
                <field attributeName="vendorContractName" />
                <field attributeName="vendorContractDescription" />
                <field attributeName="vendorCampusCode" />
                <field attributeName="vendorContractBeginningDate" />
                <field attributeName="vendorContractEndDate" />
                <field attributeName="contractManagerCode" />
                <field attributeName="vendorPaymentTermsCode" />
                <field attributeName="vendorContractExtensionDate" />
                <field attributeName="vendorShippingPaymentTermsCode" />
                <field attributeName="vendorShippingTitleCode" />
                <field attributeName="organizationAutomaticPurchaseOrderLimit" />
                <field attributeName="purchaseOrderCostSourceCode" />
                <field attributeName="vendorContractGeneratedIdentifier" />
                <field attributeName="vendorDetailAssignedIdentifier" />
                <field attributeName="vendorHeaderGeneratedIdentifier" />
                <field attributeName="active"/>               
            </inquiryFields></inquirySection></inquirySections>
        </inquiry>

        <lookup>
            <lookupableID>vendorContractLookupable</lookupableID>
            
            <title>Vendor Contract Lookup</title>

            <instructions>Lookup a Vendor Contract</instructions>

            <defaultSort sortAscending="true">
                <sortAttributes>
                    <sortAttribute attributeName="vendorDetail.vendorName" />
                </sortAttributes>
            </defaultSort>

            <lookupFields>
                <lookupField attributeName="vendorCampusCode" required="false" />
                <lookupField attributeName="contractManagerCode" required="false" />
                <lookupField attributeName="vendorContractName" required="false" />
                <lookupField attributeName="vendorContractDescription" required="false" />
                <lookupField attributeName="purchaseOrderCostSourceCode" required="false" />
                <lookupField attributeName="vendorPaymentTermsCode" required="false" />
                <lookupField attributeName="vendorShippingPaymentTermsCode" required="false" />
                <lookupField attributeName="vendorShippingTitleCode" required="false" />
                <lookupField attributeName="active" required="false">
                	<defaultValue>Y</defaultValue>
                </lookupField> 
            </lookupFields>

            <resultFields>
                <field attributeName="vendorDetail.vendorName" />
                <field attributeName="vendorContractDescription" />
                <field attributeName="vendorDetail.vendorHeader.vendorSupplierDiversities" />
                <field attributeName="purchaseOrderCostSource.purchaseOrderCostSourceDescription" />
                <field attributeName="active"/>               
            </resultFields>
        </lookup>

        <attributes>

            <attribute name="vendorContractName" forceUppercase="true">
                <label>Contract Name</label>
                <shortLabel>Contract Name</shortLabel>

                <maxLength>20</maxLength>
                <validationPattern><anyCharacter allowWhitespace="true"/></validationPattern>
                <required>true</required>

				<control>
                    <text size="22" />
                </control>

			</attribute>

            <attribute name="vendorContractDescription" forceUppercase="false">
                <label>Description</label>
                <shortLabel>Description</shortLabel>

                <maxLength>60</maxLength>
                <validationPattern><anyCharacter allowWhitespace="true" /></validationPattern>
                <required>true</required>
    
                <control>
                    <text size="45" />
                </control>

  			</attribute>

			<attributeReference name="vendorCampusCode" sourceClassName="org.kuali.module.vendor.bo.VendorAttributeReferenceDummy" sourceAttributeName="campusCode" >
                <required>true</required>
            </attributeReference>

			<attributeReference name="vendorContractBeginningDate" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="genericDate" >
                <label>Begin Date</label>
                <shortLabel>Begin Date</shortLabel>
                <required>true</required>
			</attributeReference>

			<attributeReference name="vendorContractEndDate" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="genericDate" >
                <label>End Date</label>
                <shortLabel>End Date</shortLabel>
                <required>true</required>
			</attributeReference>

			<attributeReference name="contractManagerCode" sourceClassName="org.kuali.module.vendor.bo.ContractManager" sourceAttributeName="contractManagerCode">
                <label>Contract Manager</label>
                <shortLabel>Contract Manager</shortLabel>
                <required>true</required>
                <control>
                    <select valuesFinderClass="org.kuali.module.vendor.lookup.keyvalues.ContractManagerValuesFinder" />
                </control>
            </attributeReference>			

			<attributeReference name="vendorPaymentTermsCode" sourceClassName="org.kuali.module.vendor.bo.PaymentTermType" sourceAttributeName="vendorPaymentTermsCode">
                <label>Payment Terms</label>
                <shortLabel>Payment Terms</shortLabel>
                <required>true</required>
                <control>
                    <select valuesFinderClass="org.kuali.module.vendor.lookup.keyvalues.PaymentTermsValuesFinder" />
                </control>
            </attributeReference>			

            <attributeReference name="purchaseOrderCostSourceCode" sourceClassName="org.kuali.module.vendor.bo.PurchaseOrderCostSource" sourceAttributeName="purchaseOrderCostSourceCode">
                <label>PO Cost Source</label>
                <shortLabel>PO Cost Source</shortLabel>
                <required>true</required>
                <control>
                    <select valuesFinderClass="org.kuali.module.vendor.lookup.keyvalues.PurchaseOrderCostSourceValuesFinder" />
                </control>
            </attributeReference>

            <attributeReference name="purchaseOrderCostSource.purchaseOrderCostSourceDescription" sourceClassName="org.kuali.module.vendor.bo.PurchaseOrderCostSource" sourceAttributeName="purchaseOrderCostSourceDescription">
                <label>PO Cost Source</label>
                <shortLabel>PO Cost Source</shortLabel>
            </attributeReference>

			<attributeReference name="vendorContractExtensionDate" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="genericDate" >
                <label>Extension Option Date</label>
                <shortLabel>Extension Option Date</shortLabel>
                <required>false</required>
			</attributeReference>

			<attributeReference name="vendorShippingPaymentTermsCode" sourceClassName="org.kuali.module.vendor.bo.ShippingPaymentTerms" sourceAttributeName="vendorShippingPaymentTermsCode" >
                <label>Shipping Terms</label>
                <shortLabel>Shipping Terms</shortLabel>
                <required>false</required>
                <control>
                    <select valuesFinderClass="org.kuali.module.vendor.lookup.keyvalues.ShippingPaymentTermsValuesFinder" />
                </control>
			</attributeReference>

			<attributeReference name="vendorShippingTitleCode" sourceClassName="org.kuali.module.vendor.bo.ShippingTitle" sourceAttributeName="vendorShippingTitleCode" >
                <label>Shipping Title</label>
                <shortLabel>Shipping Title</shortLabel>
                <required>false</required>
                <control>
                    <select valuesFinderClass="org.kuali.module.vendor.lookup.keyvalues.ShippingTitleValuesFinder" />
                </control>
			</attributeReference>

            <attributeReference name="vendorB2bIndicator" sourceClassName="org.kuali.module.vendor.bo.VendorAttributeReferenceDummy" sourceAttributeName="yesNoWithoutBlankIndicator">
                <label>B2B Contract</label>
                <shortLabel>B2B Contract</shortLabel>
                <required>false</required>

			</attributeReference>

            <attributeReference name="organizationAutomaticPurchaseOrderLimit" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="genericAmount">
                <label>Default APO Limit</label>
                <shortLabel>Default APO Limit</shortLabel>
                <required>false</required>
            </attributeReference>
			
			<attribute name="vendorContractGeneratedIdentifier" forceUppercase="true">
                <label>Vendor Contract Generated Identifier</label>
                <shortLabel>Vendor Contract Generated Identifier</shortLabel>
                
				<maxLength>8</maxLength>
                
				<validationPattern><numeric /></validationPattern>
                
				<control>
                    <hidden/>
                </control>

			</attribute>
			
			<attributeReference name="vendorDetail.vendorName" sourceClassName="org.kuali.module.vendor.bo.VendorDetail" sourceAttributeName="vendorName"/>
			<attributeReference name="vendorDetail.vendorHeader.vendorSupplierDiversities.vendorSupplierDiversityCode" sourceClassName="org.kuali.module.vendor.bo.VendorSupplierDiversity" sourceAttributeName="vendorSupplierDiversityCode"/> 
			
			<attributeReference name="newCollectionRecord" 
                                sourceClassName="org.kuali.core.bo.AttributeReferenceDummy"
                                sourceAttributeName="newCollectionRecord" />

            <attributeReference name="vendorHeaderGeneratedIdentifier" sourceClassName="org.kuali.module.vendor.bo.VendorDetail" sourceAttributeName="vendorHeaderGeneratedIdentifier" />
            <attributeReference name="vendorDetailAssignedIdentifier" sourceClassName="org.kuali.module.vendor.bo.VendorDetail" sourceAttributeName="vendorDetailAssignedIdentifier" />
            <attributeReference name="versionNumber" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="versionNumber" />
            <attributeReference name="active" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="activeIndicator"/>
        </attributes>
        
        <collections>
            <collection name="vendorDetail.vendorHeader.vendorSupplierDiversities">
                <label>Supplier Diversities</label>
                <shortLabel>Supplier Diversities</shortLabel>
            </collection>   
        </collections>
    </businessObject>
</dictionaryEntry>
