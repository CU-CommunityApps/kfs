<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

	<bean id="ProjectCodeImportDocument-importLinesValidation" parent="ProjectCodeImportDocument-importLinesValidation-parentBean" scope="prototype" />
  	<bean id="ProjectCodeImportDocument-importLinesValidation-parentBean" parent="CompositeValidation" scope="prototype" abstract="true">
  		<property name="validations">
  			<list>
  				<bean parent="MassImportDocument-massImportLineObjectDataDictionaryValidation">
					<property name="parameterProperties">
						<bean parent="validationFieldConversion">
							<property name="sourceEventProperty" value="importedLines" />
							<property name="targetValidationProperty" value="importedLines" />
						</bean>
					</property>
				</bean>
  			</list>
  		</property>
  	</bean>
  	
	<bean id="ProjectCodeImportDocument-routeDocument-itemsCollectionValidation" parent="ProjectCodeImportDocument-routeDocument-itemsCollectionValidation-parentBean" />
  	<bean id="ProjectCodeImportDocument-routeDocument-itemsCollectionValidation-parentBean" abstract="true" parent="CollectionValidation" scope="prototype">
  		<property name="collectionProperty" value="document.projectCodeImportDetails" />
  		<property name="validations">
  			<list>  					  						
				<bean parent="MassImportDocument-massImportLineObjectDataDictionaryValidation">
					<property name="parameterProperties">
						<bean parent="validationFieldConversion">
							<property name="sourceEventProperty" value="iterationSubject" />
							<property name="targetValidationProperty" value="importedLineForValidation" />
						</bean>
					</property>
				</bean>
				<bean parent="ProjectCodeImportDocument-projectCodeImportLineObjectExistenceValidation">
					<property name="parameterProperties">
						<bean parent="validationFieldConversion">
							<property name="sourceEventProperty" value="iterationSubject" />
							<property name="targetValidationProperty" value="importedLineForValidation" />
						</bean>
					</property>
				</bean>
				<bean parent="ProjectCodeImportDocument-projectManagerValidation">
					<property name="parameterProperties">
						<bean parent="validationFieldConversion">
							<property name="sourceEventProperty" value="iterationSubject" />
							<property name="targetValidationProperty" value="importedLineForValidation" />
						</bean>
					</property>
				</bean>
				<bean parent="ProjectCodeImportDocument-duplicateProjectCodeValidation">
					<property name="parameterProperties">
						<bean parent="validationFieldConversion">
							<property name="sourceEventProperty" value="iterationSubject" />
							<property name="targetValidationProperty" value="importedLineForValidation" />
						</bean>
					</property>
				</bean>
			</list>
		</property>
  	</bean>
  	
  	
	<bean id="ProjectCodeImportDocument-routeDocumentValidation" parent="ProjectCodeImportDocument-routeDocumentValidation-parentBean" scope="prototype" />
  
  	<bean id="ProjectCodeImportDocument-routeDocumentValidation-parentBean" parent="CompositeValidation" scope="prototype" abstract="true">
  		<property name="validations">
  			<list>
  				<bean parent="MassImportDocument-massImportLineRequiredValidation" scope="prototype" />
  				<bean parent="ProjectCodeImportDocument-routeDocument-itemsCollectionValidation" scope="prototype" />
  			</list>
  		</property>
  	</bean>
  	
  	<bean id="ProjectCodeImportDocument-saveDocumentValidation" parent="ProjectCodeImportDocument-saveDocumentValidation-parentBean" scope="prototype" />
  
  	<bean id="ProjectCodeImportDocument-saveDocumentValidation-parentBean" parent="CompositeValidation" scope="prototype" abstract="true">
  		<property name="validations">
			<list>
				<bean parent="ProjectCodeImportDocument-routeDocument-itemsCollectionValidation" scope="prototype" />
			</list>
		</property>
  	</bean>
  
  	<bean id="ProjectCodeImportDocument-approveDocumentValidation" parent="ProjectCodeImportDocument-approveDocumentValidation-parentBean" scope="prototype" />
  
  	<bean id="ProjectCodeImportDocument-approveDocumentValidation-parentBean" parent="CompositeValidation" scope="prototype" abstract="true">
  		<property name="validations">
  			<list>
  			</list>
  		</property>
  	</bean>
  
	<bean id="ProjectCodeImportDocument-blanketApproveDocumentValidation" parent="ProjectCodeImportDocument-blanketApproveDocumentValidation-parentBean" scope="prototype" />
  
  	<bean id="ProjectCodeImportDocument-blanketApproveDocumentValidation-parentBean" parent="CompositeValidation" scope="prototype" abstract="true">
  		<property name="validations">
  			<list>
  			</list>
  		</property>
	</bean>
		
</beans>
