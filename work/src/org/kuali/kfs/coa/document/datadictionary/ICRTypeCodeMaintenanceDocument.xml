<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="ICRTypeCodeMaintenanceDocument" parent="ICRTypeCodeMaintenanceDocument-parentBean" />
  <bean id="ICRTypeCodeMaintenanceDocument-parentBean" abstract="true" parent="MaintenanceDocumentEntry">
    <property name="businessObjectClass" value="org.kuali.kfs.coa.businessobject.ICRTypeCode" />
    <property name="maintainableClass" value="org.kuali.kfs.coa.document.ICRTypeCodeMaintainableImpl" />
    <property name="maintainableSections" >
      <list>
        <ref bean="ICRTypeCodeMaintenanceDocument-EditICRTypeCode" />
        <ref bean="ICRTypeCodeMaintenanceDocument-EditICROtherCodes" />
      </list>
    </property>
    <property name="lockingKeys" >
      <list>
        <value>code</value>
      </list>
    </property>
    <property name="label" value="Indirect Cost Recovery Type" />
    <property name="shortLabel" value="Indirect Cost Recovery Type" />
    <property name="summary" value="ICR Type Code Maintenance Document" />
    <property name="description" value="Maintenance document which allows you to create new ICR Type Codes, or modify existing ICR Type Codes." />
    <property name="helpDefinition" >
      <bean parent="HelpDefinition"
            p:parameterNamespace="KFS-COA"
            p:parameterName="INDIRECT_COST_RECOVERY_TYPE"
            p:parameterDetailType="ICRTypeCode" />
    </property>
    <property name="documentTypeName" value="ICRTypeCodeMaintenanceDocument" />
    <property name="businessRulesClass" value="org.kuali.kfs.coa.document.validation.impl.ICRTypeCodeRule" />
    <property name="documentAuthorizerClass" value="org.kuali.kfs.sys.document.web.struts.FinancialSystemMaintenanceDocumentAuthorizerBase" />
    <property name="documentTypeCode" value="ITYP" />
    <property name="workflowProperties" >
      <ref bean="ICRTypeCodeMaintenanceDocument-workflowProperties" />
    </property>
    <property name="authorizations" >
      <list>
        <bean parent="AuthorizationDefinition">
          <property name="action" value="initiate" />
          <property name="authorizedGroups" >
            <set>
              <value>SY_MAINTENANCE_USERS</value>
            </set>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Maintenance Section Definitions -->

  <bean id="ICRTypeCodeMaintenanceDocument-EditICRTypeCode" parent="ICRTypeCodeMaintenanceDocument-EditICRTypeCode-parentBean" />
  <bean id="ICRTypeCodeMaintenanceDocument-EditICRTypeCode-parentBean" abstract="true" parent="MaintainableSectionDefinition">
    <property name="id" value="Edit ICR Type Code" />
    <property name="title" value="Edit ICR Type Code" />
    <property name="maintainableItems" >
      <list>
        <bean parent="MaintainableFieldDefinition"
              p:name="code"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="versionNumber" />
    	<bean parent="MaintainableFieldDefinition"
    	      p:name="active"
    	      p:defaultValue="true" />
      </list>
    </property>
  </bean>
  
  <bean id="ICRTypeCodeMaintenanceDocument-EditICROtherCodes" parent="ICRTypeCodeMaintenanceDocument-EditICROtherCodes-parentBean" />
  <bean id="ICRTypeCodeMaintenanceDocument-EditICROtherCodes-parentBean" abstract="true" parent="MaintainableSectionDefinition">
    <property name="id" value="Edit ICR Exclusion Codes" />
    <property name="title" value="Edit ICR Exclusion Codes" />
    <property name="maintainableItems" >
      <list>
      	<bean parent="MaintainableCollectionDefinition">
          <property name="name" value="indirectCostRecoveryExclusionTypeCollection" />
          <property name="businessObjectClass" value="org.kuali.kfs.coa.businessobject.IndirectCostRecoveryExclusionType" />
          <property name="sourceClassName" value="org.kuali.kfs.coa.businessobject.ObjectCode" />
          <property name="maintainableFields">
          	<list>
			  <bean parent="MaintainableFieldDefinition"
 		            p:name="chartOfAccountsCode"
 		            p:template="chartOfAccountsCode"
  		  	        p:required="true" />
		      <bean parent="MaintainableFieldDefinition"
    		        p:name="financialObjectCode"
    		        p:template="financialObjectCode"
    	    	    p:required="true" />
		      <bean parent="MaintainableFieldDefinition"
					p:name="versionNumber" />
    		  <bean parent="MaintainableFieldDefinition"
    		        p:name="active"
    		        p:defaultValue="true" />
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Exported Workflow Properties -->

  <bean id="ICRTypeCodeMaintenanceDocument-workflowProperties" parent="ICRTypeCodeMaintenanceDocument-workflowProperties-parentBean" />
  <bean id="ICRTypeCodeMaintenanceDocument-workflowProperties-parentBean" abstract="true" parent="WorkflowProperties">
  </bean>
  
</beans>
