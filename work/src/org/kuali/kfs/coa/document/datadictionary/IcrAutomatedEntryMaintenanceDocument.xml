<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="IcrAutomatedEntryMaintenanceDocument" parent="IcrAutomatedEntryMaintenanceDocument-parentBean" />

  <bean id="IcrAutomatedEntryMaintenanceDocument-parentBean" abstract="true" parent="MaintenanceDocumentEntry">
    <property name="businessObjectClass" value="org.kuali.module.chart.bo.IcrAutomatedEntry" />
    <property name="maintainableClass" value="org.kuali.core.maintenance.KualiMaintainableImpl" />
    <property name="maintainableSections" >
      <list>
        <ref bean="IcrAutomatedEntryMaintenanceDocument-EditICRAutomatedEntry" />
      </list>
    </property>
    <property name="defaultExistenceChecks" >
      <list>
        <bean parent="ReferenceDefinition"
              p:attributeName="universityFiscal"
              p:attributeToHighlightOnFail="universityFiscalYear" />
        <bean parent="ReferenceDefinition"
              p:attributeName="financialBalanceTyp"
              p:attributeToHighlightOnFail="balanceTypeCode" />
      </list>
    </property>
    <property name="lockingKeys" >
      <list>
        <value>universityFiscalYear</value>
        <value>financialIcrSeriesIdentifier</value>
        <value>balanceTypeCode</value>
        <value>awardIndrCostRcvyEntryNbr</value>
      </list>
    </property>
    <property name="label" value="Indirect Cost Recovery Automated Entry" />
    <property name="shortLabel" value="Indirect Cost Recovery Automated Entry" />
    <property name="summary" value="Indirect Cost Recovery Automated Entry Maintenance Document" />
    <property name="description" value="Maintenance document which allows you to create new Indirect Cost Recovery Automated Entries, or modify existing Indirect Cost Recovery Automated Entries." />
    <property name="helpDefinition" >
      <bean parent="HelpDefinition"
            p:parameterNamespace="KFS-CA"
            p:parameterName="INDIRECT_COST_RECOVERY_AUTOMATED_ENTRY"
            p:parameterDetailType="IcrAutomatedEntry" />
    </property>
    <property name="documentTypeName" value="IcrAutomatedEntryMaintenanceDocument" />
    <property name="businessRulesClass" value="org.kuali.module.chart.rules.IcrAutomatedEntryRule" />
    <property name="documentAuthorizerClass" value="org.kuali.core.document.authorization.MaintenanceDocumentAuthorizerBase" />
    <property name="preRulesCheckClass" value="org.kuali.module.chart.rules.IcrAutomatedEntryPreRules" />
    <property name="documentTypeCode" value="ICRE" />
    <property name="workflowProperties" >
      <ref bean="IcrAutomatedEntryMaintenanceDocument-workflowProperties" />
    </property>
    <property name="authorizations" >
      <list>
        <bean parent="AuthorizationDefinition">
          <property name="action" value="initiate" />
          <property name="authorizedGroups" >
            <set>
              <value>SY_MAINTAINENANCE_USERS</value>
            </set>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Maintenance Section Definitions -->


  <bean id="IcrAutomatedEntryMaintenanceDocument-EditICRAutomatedEntry" parent="IcrAutomatedEntryMaintenanceDocument-EditICRAutomatedEntry-parentBean" />

  <bean id="IcrAutomatedEntryMaintenanceDocument-EditICRAutomatedEntry-parentBean" abstract="true" parent="MaintainableSectionDefinition">
    <property name="id" value="Edit ICR Automated Entry" />
    <property name="title" value="Edit ICR Automated Entry" />
    <property name="maintainableItems" >
      <list>
        <bean parent="MaintainableFieldDefinition"
              p:name="universityFiscalYear"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="financialIcrSeriesIdentifier"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="balanceTypeCode"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="awardIndrCostRcvyEntryNbr"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="chartOfAccountsCode"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="accountNumber"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="subAccountNumber" />
        <bean parent="MaintainableFieldDefinition"
              p:name="financialObjectCode"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="financialSubObjectCode" />
        <bean parent="MaintainableFieldDefinition"
              p:name="offsetBalanceSheetObjectCodeNumber"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="transactionDebitIndicator" />
        <bean parent="MaintainableFieldDefinition"
              p:name="awardIndrCostRcvyRatePct"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="versionNumber" />
      </list>
    </property>
  </bean>

<!-- Exported Workflow Properties -->


  <bean id="IcrAutomatedEntryMaintenanceDocument-workflowProperties" parent="IcrAutomatedEntryMaintenanceDocument-workflowProperties-parentBean" />

  <bean id="IcrAutomatedEntryMaintenanceDocument-workflowProperties-parentBean" abstract="true" parent="WorkflowProperties">
  </bean>
</beans>
