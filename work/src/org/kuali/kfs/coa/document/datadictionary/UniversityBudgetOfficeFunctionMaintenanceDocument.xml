<?xml version="1.0" encoding="UTF-8"?><beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p" xmlns:dd="http://rice.kuali.org/dd" xsi:schemaLocation="http://www.springframework.org/schema/beans         http://www.springframework.org/schema/beans/spring-beans-2.0.xsd         http://rice.kuali.org/dd         http://rice.kuali.org/dd/dd.xsd">

  <bean id="UniversityBudgetOfficeFunctionMaintenanceDocument" parent="UniversityBudgetOfficeFunctionMaintenanceDocument-parentBean"/>

  <bean id="UniversityBudgetOfficeFunctionMaintenanceDocument-parentBean" abstract="true" parent="MaintenanceDocumentEntry">
    <property name="businessObjectClass" value="org.kuali.kfs.coa.businessobject.UniversityBudgetOfficeFunction"/>
    <property name="maintainableClass" value="org.kuali.kfs.sys.document.FinancialSystemMaintainable"/>
    <property name="maintainableSections">
      <list>
        <ref bean="UniversityBudgetOfficeFunctionMaintenanceDocument-EditUniversityBudgetOfficeFunction"/>
      </list>
    </property>
    <property name="lockingKeys">
      <list>
        <value>financialUniversityBudgetOfficeFunctionCode</value>
      </list>
    </property>
    
    
    
    
    
    <property name="documentTypeName" value="UFUN"/>
    <property name="documentAuthorizerClass" value="org.kuali.kfs.sys.document.authorization.FinancialSystemMaintenanceDocumentAuthorizerBase"/>
    <property name="workflowAttributes">
    	<ref bean="UniversityBudgetOfficeFunctionMaintenanceDocument-workflowAttributes"/>
    </property>
  </bean>

<!-- Maintenance Section Definitions -->

  <bean id="UniversityBudgetOfficeFunctionMaintenanceDocument-EditUniversityBudgetOfficeFunction" parent="UniversityBudgetOfficeFunctionMaintenanceDocument-EditUniversityBudgetOfficeFunction-parentBean"/>

  <bean id="UniversityBudgetOfficeFunctionMaintenanceDocument-EditUniversityBudgetOfficeFunction-parentBean" abstract="true" parent="MaintainableSectionDefinition">
    <property name="id" value="Edit University Budget Office Function"/>
    <property name="title" value="Edit University Budget Office Function"/>
    <property name="maintainableItems">
      <list>
        <bean parent="MaintainableFieldDefinition" p:name="financialUniversityBudgetOfficeFunctionCode" p:required="true"/>
        <bean parent="MaintainableFieldDefinition" p:name="financialUniversityBudgetOfficeFunctionName" p:required="true"/>
        <bean parent="MaintainableFieldDefinition" p:name="active" p:defaultValue="true"/>
      </list>
    </property>
  </bean>
  
  <!-- workflow attributes -->
  <bean id="UniversityBudgetOfficeFunctionMaintenanceDocument-workflowAttributes" parent="UniversityBudgetOfficeFunctionMaintenanceDocument-workflowAttributes-parentBean"/>
  
  <bean id="UniversityBudgetOfficeFunctionMaintenanceDocument-workflowAttributes-parentBean" class="org.kuali.rice.kns.datadictionary.WorkflowAttributes" abstract="true"/>
</beans>