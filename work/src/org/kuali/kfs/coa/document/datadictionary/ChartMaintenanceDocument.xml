<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "../../../../core/datadictionary/dataDictionary-1_0.dtd">

<dictionaryEntry>
	<maintenanceDocument>
        <businessObjectClass>org.kuali.module.chart.bo.Chart</businessObjectClass>
        <maintainableClass>org.kuali.core.maintenance.KualiMaintainableImpl</maintainableClass>

        <documentAuthorizerClass>org.kuali.core.document.DocumentAuthorizerBase</documentAuthorizerClass>
        <authorizations>
            <authorization action="initiate">
                <workgroups>
                    <workgroup>kualiUniversalGroup</workgroup>
                </workgroups>
            </authorization>
        </authorizations>

		<documentTypeName>KualiChartMaintenanceDocument</documentTypeName>
		<documentTypeCode>COAT</documentTypeCode>

		<label>Chart Maintenance Document</label>
		<shortLabel>ChMainDoc</shortLabel>
		
		<summary>Chart maintenance document</summary>
		<description>Document used to create or update Chart objects</description>

        <lockingKeys>
			<field attributeName="chartOfAccountsCode" />
		</lockingKeys>	         

		<defaultExistenceChecks>
			<reference  attributeName="finCoaManagerUniversal" 
						attributeToHighlightOnFail="finCoaManagerUniversalId" 
						displayFieldName="Chart Manager" />
			<reference  attributeName="finChartOfAcctOrigination" 
						attributeToHighlightOnFail="finChartOfAcctOriginationCd" />
			<reference  attributeName="financialCashObject" 
						attributeToHighlightOnFail="financialCashObjectCode" />
			<reference  attributeName="finAccountsPayableObject" 
						attributeToHighlightOnFail="finAccountsPayableObjectCode" />
			<reference  attributeName="finAccountsReceivableObj" 
						attributeToHighlightOnFail="finAccountsReceivableObjCode" />
			<reference  attributeName="incBdgtEliminationsFinObj" 
						attributeToHighlightOnFail="incBdgtEliminationsFinObjCd" />
			<reference  attributeName="expBdgtEliminationsFinObj" 
						attributeToHighlightOnFail="expBdgtEliminationsFinObjCd" />
			<reference  attributeName="reportsToChartOfAccounts" 
						attributeToHighlightOnFail="reportsToChartOfAccountsCode" />
			<reference  attributeName="finInternalEncumbranceObj" 
						attributeToHighlightOnFail="finInternalEncumbranceObjCd" />
			<reference  attributeName="finExternalEncumbranceObj" 
						attributeToHighlightOnFail="finExternalEncumbranceObjCd" />
			<reference  attributeName="finPreEncumbranceObject" 
						attributeToHighlightOnFail="finPreEncumbranceObjectCode" />
			<reference  attributeName="icrIncomeFinancialObject" 
						attributeToHighlightOnFail="icrIncomeFinancialObjectCode" />
			<reference  attributeName="icrExpenseFinancialObject" 
						attributeToHighlightOnFail="icrExpenseFinancialObjectCd" />
		</defaultExistenceChecks>
		
	    <maintainableSections>
	        <maintainableSection title="Edit Chart">
                <maintainableItems>
					<maintainableField name="chartOfAccountsCode" required="true" />			   
					<maintainableField name="finCoaManagerUniversalId" />
					<maintainableField name="finChartOfAcctOriginationCd" />
					<maintainableField name="finChartOfAccountDescription" />
					<maintainableField name="finChartOfAccountActiveIndicator" />
					<maintainableField name="financialCashObjectCode" />
					<maintainableField name="finAccountsPayableObjectCode" />
					<maintainableField name="incBdgtEliminationsFinObjCd" />
					<maintainableField name="expBdgtEliminationsFinObjCd" />
					<maintainableField name="reportsToChartOfAccountsCode" />
					<maintainableField name="finAccountsReceivableObjCode" />
					<maintainableField name="finInternalEncumbranceObjCd" />
					<maintainableField name="finExternalEncumbranceObjCd" />
					<maintainableField name="finPreEncumbranceObjectCode" />
					<maintainableField name="icrIncomeFinancialObjectCode" />
					<maintainableField name="icrExpenseFinancialObjectCd" />
                </maintainableItems>
	        </maintainableSection>
	    </maintainableSections>
	</maintenanceDocument>
</dictionaryEntry>
