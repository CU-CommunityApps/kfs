<?xml version="1.0" encoding="UTF-8"?><beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p" xsi:schemaLocation="http://www.springframework.org/schema/beans         http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">
  <bean id="ProjectCodeImportDocument" parent="ProjectCodeImportDocument-parentBean"/>
  <bean id="ProjectCodeImportDocument-parentBean" abstract="true" parent="TransactionalDocumentEntry">
    <property name="documentTypeName" value="PCIP"/>
  	<property name="documentClass" value="org.kuali.kfs.coa.document.ProjectCodeImportDocument"/>
    <property name="businessRulesClass" value="org.kuali.kfs.sys.document.validation.impl.AccountingRuleEngineRuleBase"/>
    <property name="documentAuthorizerClass" value="org.kuali.kfs.sys.document.authorization.FinancialSystemTransactionalDocumentAuthorizerBase"/>
    <property name="documentPresentationControllerClass" value="org.kuali.kfs.sys.document.authorization.FinancialSystemTransactionalDocumentPresentationControllerBase"/>
    <property name="workflowAttributes">
      <ref bean="ProjectCodeImportDocument-workflowAttributes"/>
    </property> 
    <property name="attributes">
      <list>
        <ref bean="ProjectCodeImportDocument-documentNumber"/>
      </list>
    </property>
    <property name="validationMap" ref="ProjectCodeImportDocument-validations"/>
  </bean>

<!-- Attribute Definitions -->
  <bean id="ProjectCodeImportDocument-documentNumber" parent="ProjectCodeImportDocument-documentNumber-parentBean"/>

  <bean id="ProjectCodeImportDocument-documentNumber-parentBean" abstract="true" parent="FinancialSystemDocumentHeader-documentNumber">
  </bean>

<!-- workflow attributes -->  	
  <bean id="ProjectCodeImportDocument-workflowAttributes" parent="ProjectCodeImportDocument-workflowAttributes-parentBean"/>
  <bean id="ProjectCodeImportDocument-workflowAttributes-parentBean" abstract="true" parent="WorkflowAttributes">
  </bean>
  
  <!-- Validations -->
  <bean id="ProjectCodeImportDocument-validations" parent="ProjectCodeImportDocument-validations-parentBean"/>
  
  <bean id="ProjectCodeImportDocument-validations-parentBean" class="org.springframework.beans.factory.config.MapFactoryBean">
  	<property name="sourceMap">
		<map key-type="java.lang.Class">
			<entry>
				<key><value>org.kuali.kfs.sys.document.validation.event.AttributedRouteDocumentEvent</value></key>
				<value>ProjectCodeImportDocument-routeDocumentValidation</value>
			</entry>
			<entry>
				<key><value>org.kuali.kfs.sys.document.validation.event.AttributedSaveDocumentEvent</value></key>
				<value>ProjectCodeImportDocument-saveDocumentValidation</value>
			</entry>
			<entry>
				<key><value>org.kuali.kfs.sys.document.validation.event.AttributedApproveDocumentEvent</value></key>
				<value>ProjectCodeImportDocument-approveDocumentValidation</value>
			</entry>
			<entry>
				<key><value>org.kuali.kfs.sys.document.validation.event.AttributedBlanketApproveDocumentEvent</value></key>
				<value>ProjectCodeImportDocument-blanketApproveDocumentValidation</value>
			</entry>
			<entry>
				<key><value>org.kuali.kfs.sys.document.validation.event.AttributedMassImportLineEventBase</value></key>
				<value>ProjectCodeImportDocument-importLinesValidation</value>				
			</entry>
    	</map>
    </property>
  </bean>  
</beans>
