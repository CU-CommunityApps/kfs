<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="OrganizationReversionCategoryMaintenanceDocument" parent="OrganizationReversionCategoryMaintenanceDocument-parentBean" />

  <bean id="OrganizationReversionCategoryMaintenanceDocument-parentBean" abstract="true" parent="MaintenanceDocumentEntry">
    <property name="businessObjectClass" value="org.kuali.module.chart.bo.OrganizationReversionCategory" />
    <property name="maintainableClass" value="org.kuali.core.maintenance.KualiMaintainableImpl" />
    <property name="maintainableSections" >
      <list>
        <ref bean="OrganizationReversionCategoryMaintenanceDocument-EditOrganizationReversionCategory" />
      </list>
    </property>
    <property name="lockingKeys" >
      <list>
        <value>organizationReversionCategoryCode</value>
      </list>
    </property>
    <property name="label" value="Organization Reversion Category" />
    <property name="shortLabel" value="Organization Reversion Category" />
    <property name="summary" value="organizationReversionCategory maintenance document" />
    <property name="description" value="Document used to create or update organizationReversionCategory objects" />
    <property name="helpDefinition" >
      <bean parent="HelpDefinition"
            p:parameterNamespace="KFS-CA"
            p:parameterName="ORGANIZATION_REVERSION_CATEGORY"
            p:parameterDetailType="OrganizationReversionCategory" />
    </property>
    <property name="documentTypeName" value="OrganizationReversionCategoryMaintenanceDocument" />
    <property name="documentAuthorizerClass" value="org.kuali.core.document.authorization.MaintenanceDocumentAuthorizerBase" />
    <property name="documentTypeCode" value="ORGC" />
    <property name="workflowProperties" >
      <ref bean="OrganizationReversionCategoryMaintenanceDocument-workflowProperties" />
    </property>
    <property name="authorizations" >
      <list>
        <bean parent="AuthorizationDefinition">
          <property name="action" value="initiate" />
          <property name="authorizedGroups" >
            <set>
              <value>SY_MAINTENANCE_USERS</value>
            </set>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Maintenance Section Definitions -->


  <bean id="OrganizationReversionCategoryMaintenanceDocument-EditOrganizationReversionCategory" parent="OrganizationReversionCategoryMaintenanceDocument-EditOrganizationReversionCategory-parentBean" />

  <bean id="OrganizationReversionCategoryMaintenanceDocument-EditOrganizationReversionCategory-parentBean" abstract="true" parent="MaintainableSectionDefinition">
    <property name="id" value="Edit Organization Reversion Category" />
    <property name="title" value="Edit Organization Reversion Category" />
    <property name="maintainableItems" >
      <list>
        <bean parent="MaintainableFieldDefinition"
              p:name="organizationReversionCategoryCode"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="organizationReversionCategoryName"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="organizationReversionSortCode"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="organizationReversionCategoryActiveIndicator"
              p:defaultValue="true"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="versionNumber" />
      </list>
    </property>
  </bean>

<!-- Exported Workflow Properties -->


  <bean id="OrganizationReversionCategoryMaintenanceDocument-workflowProperties" parent="OrganizationReversionCategoryMaintenanceDocument-workflowProperties-parentBean" />

  <bean id="OrganizationReversionCategoryMaintenanceDocument-workflowProperties-parentBean" abstract="true" parent="WorkflowProperties">
  </bean>
</beans>
