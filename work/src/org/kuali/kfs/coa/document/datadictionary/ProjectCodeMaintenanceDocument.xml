<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "../../../../core/datadictionary/dataDictionary-1_0.dtd">

<dictionaryEntry>
	<maintenanceDocument>
        <businessObjectClass>org.kuali.module.chart.bo.ProjectCode</businessObjectClass>
        <maintainableClass>org.kuali.core.maintenance.KualiMaintainableImpl</maintainableClass>

        <documentAuthorizerClass>org.kuali.core.document.DocumentAuthorizerBase</documentAuthorizerClass>
        <authorizations>
            <authorization action="initiate">
                <workgroups>
                    <workgroup>kualiUniversalGroup</workgroup>
                </workgroups>
            </authorization>
        </authorizations>

		<documentTypeName>KualiProjectCodeMaintenanceDocument</documentTypeName>
		<documentTypeCode>PROJ</documentTypeCode>

		<label>Project Code Maintenance Document</label>
		<shortLabel>ProjectMaintDoc</shortLabel>
		
		<summary>Project Code maintenance document</summary>
		<description>Document used to create or update Project Code objects</description>

        <lockingKeys>
			<field attributeName="code" />
		</lockingKeys>	         

	    <maintainableSections>
	        <maintainableSection title="Edit Project Code">
                <maintainableItems>
		            <maintainableField name="code" required="true" />
		            <maintainableField name="name" />
		            <maintainableField name="projectManagerUniversalId" required="true"/>
		            <maintainableField name="chartOfAccountsCode" />
		            <maintainableField name="organizationCode" />	     
		            <maintainableField name="projectDescription" />
		            <maintainableField name="active" >
		              <defaultValue>true</defaultValue>	
		            </maintainableField>  
		            <maintainableField name="versionNumber" />
                </maintainableItems>
	        </maintainableSection>
	    </maintainableSections>
	</maintenanceDocument>
</dictionaryEntry>
