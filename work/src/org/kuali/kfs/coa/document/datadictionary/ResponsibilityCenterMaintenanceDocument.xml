<?xml version="1.0" encoding="UTF-8"?><beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p" xmlns:dd="http://rice.kuali.org/dd" xsi:schemaLocation="http://www.springframework.org/schema/beans         http://www.springframework.org/schema/beans/spring-beans-2.0.xsd         http://rice.kuali.org/dd         http://rice.kuali.org/dd/dd.xsd">

  <bean id="ResponsibilityCenterMaintenanceDocument" parent="ResponsibilityCenterMaintenanceDocument-parentBean"/>

  <bean id="ResponsibilityCenterMaintenanceDocument-parentBean" abstract="true" parent="MaintenanceDocumentEntry">
    <property name="businessObjectClass" value="org.kuali.kfs.coa.businessobject.ResponsibilityCenter"/>
    <property name="maintainableClass" value="org.kuali.kfs.sys.document.FinancialSystemMaintainable"/>
    <property name="maintainableSections">
      <list>
        <ref bean="ResponsibilityCenterMaintenanceDocument-EditResponsibilityCenter"/>
      </list>
    </property>
    <property name="lockingKeys">
      <list>
        <value>responsibilityCenterCode</value>
      </list>
    </property>
    
    
    
    
    
    <property name="documentTypeName" value="RCEN"/>
    <property name="documentAuthorizerClass" value="org.kuali.kfs.sys.document.authorization.FinancialSystemMaintenanceDocumentAuthorizerBase"/>
    <property name="workflowAttributes">
    	<ref bean="ResponsibilityCenterMaintenanceDocument-workflowAttributes"/>
    </property>
  </bean>

<!-- Maintenance Section Definitions -->

  <bean id="ResponsibilityCenterMaintenanceDocument-EditResponsibilityCenter" parent="ResponsibilityCenterMaintenanceDocument-EditResponsibilityCenter-parentBean"/>

  <bean id="ResponsibilityCenterMaintenanceDocument-EditResponsibilityCenter-parentBean" abstract="true" parent="MaintainableSectionDefinition">
    <property name="id" value="Edit Responsibility Center"/>
    <property name="title" value="Edit Responsibility Center"/>
    <property name="maintainableItems">
      <list>
        <bean parent="MaintainableFieldDefinition" p:name="responsibilityCenterCode" p:required="true"/>
        <bean parent="MaintainableFieldDefinition" p:name="responsibilityCenterName" p:required="true"/>
        <bean parent="MaintainableFieldDefinition" p:name="responsibilityCenterShortName" p:required="true"/>
        <bean parent="MaintainableFieldDefinition" p:defaultValue="true" p:name="active"/>
      </list>
    </property>
  </bean>
  
  <!-- workflow attributes -->
  <bean id="ResponsibilityCenterMaintenanceDocument-workflowAttributes" parent="ResponsibilityCenterMaintenanceDocument-workflowAttributes-parentBean"/>
  
  <bean id="ResponsibilityCenterMaintenanceDocument-workflowAttributes-parentBean" class="org.kuali.rice.kns.datadictionary.WorkflowAttributes" abstract="true"/>
</beans>