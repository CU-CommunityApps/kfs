<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "../../../../core/datadictionary/dataDictionary-1_0.dtd">

<dictionaryEntry>
	<maintenanceDocument>
		<businessObjectClass>org.kuali.module.chart.bo.OffsetDefinition</businessObjectClass>
		<maintainableClass>org.kuali.core.maintenance.KualiMaintainableImpl</maintainableClass>

        <documentAuthorizerClass>org.kuali.core.document.DocumentAuthorizerBase</documentAuthorizerClass>
        <authorizations>
            <authorization action="initiate">
                <workgroups>
                    <workgroup>kualiUniversalGroup</workgroup>
                </workgroups>
            </authorization>
        </authorizations>

		<documentTypeName>KualiOffsetDefinitionMaintenanceDocument</documentTypeName>
		<documentTypeCode>OFSD</documentTypeCode>
		<label>Offset Definition Maintenance Document</label>
		<shortLabel>OffsetDefMaintDoc</shortLabel>
		<summary>Offset Definition Maintenance Document</summary>
		<description>
			Maintenance document which allows you to create new Offset Definitions, remove exsiting 
			Offset Definitions, or modify existing Offset Definitions.
		</description>
		
        <lockingKeys>
			<field attributeName="universityFiscalYear" />
			<field attributeName="chartOfAccountsCode" />
			<field attributeName="financialDocumentTypeCode" />
			<field attributeName="financialBalanceTypeCode" />				
		</lockingKeys>	  		

		<defaultExistenceChecks>
			<reference  attributeName="chartOfAccounts" 
						activeIndicatorAttributeName="finChartOfAccountActiveIndicator" 
						attributeToHighlightOnFail="chartOfAccountsCode" />
			<reference  attributeName="universityFiscal" 
						attributeToHighlightOnFail="universityFiscalYear" />
			<reference  attributeName="financialObject" 
						attributeToHighlightOnFail="financialObjectCode" />						
		</defaultExistenceChecks>
		
		<maintainableSections>
			<maintainableSection title="Edit Offset Definition">
                <maintainableItems>
					<maintainableField name="universityFiscalYear" required="true" />
					<maintainableField name="chartOfAccountsCode" required="true" />
					<maintainableField name="financialDocumentTypeCode" required="true" />
					<maintainableField name="financialBalanceTypeCode" required="true" />
					<maintainableField name="financialObjectCode" required="true" />
					<maintainableField name="versionNumber" />

                </maintainableItems>
			</maintainableSection>
		</maintainableSections>
	</maintenanceDocument>

</dictionaryEntry>