<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="AICPAFunctionMaintenanceDocument" parent="AICPAFunctionMaintenanceDocument-parentBean" />

  <bean id="AICPAFunctionMaintenanceDocument-parentBean" abstract="true" parent="MaintenanceDocumentEntry">
    <property name="businessObjectClass" value="org.kuali.kfs.coa.businessobject.AICPAFunction" />
    <property name="maintainableClass" value="org.kuali.rice.kns.maintenance.KualiMaintainableImpl" />
    <property name="maintainableSections" >
      <list>
        <ref bean="AICPAFunctionMaintenanceDocument-EditAICPAFunction" />
      </list>
    </property>
    <property name="lockingKeys" >
      <list>
        <value>financialAicpaFunctionCode</value>
      </list>
    </property>
    <property name="label" value="AICPA Function" />
    <property name="shortLabel" value="AICPA Function" />
    <property name="summary" value="AICPA Function Maintenance Document" />
    <property name="description" value="Document used to create or update AICPA Function Maintenance Document objects" />
    <property name="helpDefinition" >
      <bean parent="HelpDefinition" p:parameterClass="org.kuali.kfs.coa.businessobject.AICPAFunction" p:parameterName="AICPA_FUNCTION" />
    </property>
    <property name="documentTypeName" value="AFUN" />
    <property name="documentAuthorizerClass" value="org.kuali.kfs.sys.document.authorization.FinancialSystemMaintenanceDocumentAuthorizerBase" />
    <property name="workflowProperties" >
      <ref bean="AICPAFunctionMaintenanceDocument-workflowProperties" />
    </property>
  </bean>

<!-- Maintenance Section Definitions -->


  <bean id="AICPAFunctionMaintenanceDocument-EditAICPAFunction" parent="AICPAFunctionMaintenanceDocument-EditAICPAFunction-parentBean" />

  <bean id="AICPAFunctionMaintenanceDocument-EditAICPAFunction-parentBean" abstract="true" parent="MaintainableSectionDefinition">
    <property name="id" value="Edit AICPA Function" />
    <property name="title" value="Edit AICPA Function" />
    <property name="maintainableItems" >
      <list>
        <bean parent="MaintainableFieldDefinition"
              p:name="financialAicpaFunctionCode"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="financialAicpaFunctionName"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:defaultValue="true"
              p:name="active" />
      </list>
    </property>
  </bean>

<!-- Exported Workflow Properties -->


  <bean id="AICPAFunctionMaintenanceDocument-workflowProperties" parent="AICPAFunctionMaintenanceDocument-workflowProperties-parentBean" />

  <bean id="AICPAFunctionMaintenanceDocument-workflowProperties-parentBean" abstract="true" parent="WorkflowProperties">
  </bean>
</beans>
