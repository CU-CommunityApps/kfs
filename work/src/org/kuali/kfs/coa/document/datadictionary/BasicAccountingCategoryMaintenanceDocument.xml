<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "../../../../core/datadictionary/dataDictionary-1_0.dtd">

<dictionaryEntry>
    <maintenanceDocument>
        <businessObjectClass>org.kuali.module.chart.bo.BasicAccountingCategory</businessObjectClass>
        <maintainableClass>org.kuali.core.maintenance.KualiMaintainableImpl</maintainableClass>

        <documentAuthorizerClass>org.kuali.core.document.authorization.MaintenanceDocumentAuthorizerBase</documentAuthorizerClass>
        <authorizations>
            <authorization action="initiate">
                <workgroups>
                    <workgroup>SY_MAINTAINENANCE_USERS</workgroup>
                </workgroups>
            </authorization>
        </authorizations>

        <documentTypeName>BasicAccountingCategoryMaintenanceDocument</documentTypeName>
        <documentTypeCode>ACTY</documentTypeCode>

        <label>Basic Accounting Category</label>

        <summary>Basic Accounting Category maintenance document</summary>
        <description>Document used to create or update Basic Accounting Category objects</description>
        <help parameterNamespace="KFS-CA" parameterDetailType="BasicAccountingCategory" parameterName="BASIC_ACCOUNTING_CATEGORY" />

        <lockingKeys>
            <field attributeName="code" />
        </lockingKeys>

        <maintainableSections>
            <maintainableSection title="Edit Basic Accounting Category Code">
                <maintainableItems>
                    <maintainableField name="code" required="true" />
                    <maintainableField name="description" required="true" />
                    <maintainableField name="shortName" required="true" />
                    <maintainableField name="sortCode" required="true" />
					<maintainableField name="active" required="false">
						<defaultValue>true</defaultValue>
					</maintainableField>
                    <maintainableField name="versionNumber" />
                </maintainableItems>
            </maintainableSection>
        </maintainableSections>
    </maintenanceDocument>
</dictionaryEntry>
