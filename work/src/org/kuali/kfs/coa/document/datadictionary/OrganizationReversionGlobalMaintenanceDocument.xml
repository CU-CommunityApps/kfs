<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="OrganizationReversionGlobalMaintenanceDocument" parent="OrganizationReversionGlobalMaintenanceDocument-parentBean" />

  <bean id="OrganizationReversionGlobalMaintenanceDocument-parentBean" abstract="true" parent="MaintenanceDocumentEntry">
    <property name="businessObjectClass" value="org.kuali.kfs.coa.businessobject.OrganizationReversionGlobal" />
    <property name="maintainableClass" value="org.kuali.kfs.coa.document.OrganizationReversionGlobalMaintainableImpl" />
    <property name="maintainableSections" >
      <list>
        <ref bean="OrganizationReversionGlobalMaintenanceDocument-EditGlobalOrganizationReversionOrganizations" />
        <ref bean="OrganizationReversionGlobalMaintenanceDocument-EditGlobalOrganizationReversion" />
        <ref bean="OrganizationReversionGlobalMaintenanceDocument-EditGlobalOrganizationReversionDetails" />
      </list>
    </property>
    <property name="defaultExistenceChecks" >
      <list>
        <bean parent="ReferenceDefinition"
              p:attributeName="universityFiscal"
              p:attributeToHighlightOnFail="universityFiscalYear" />
        <bean parent="ReferenceDefinition"
              p:attributeName="budgetReversionChartOfAccounts"
              p:activeIndicatorAttributeName="active"
              p:attributeToHighlightOnFail="budgetReversionChartOfAccountsCode" />
        <bean parent="ReferenceDefinition"
              p:attributeName="budgetReversionAccount"
              p:activeIndicatorAttributeName="active"
              p:activeIndicatorReversed="true"
              p:attributeToHighlightOnFail="budgetReversionAccountNumber" />
        <bean parent="ReferenceDefinition"
              p:attributeName="cashReversionFinancialChartOfAccounts"
              p:activeIndicatorAttributeName="active"
              p:attributeToHighlightOnFail="cashReversionFinancialChartOfAccountsCode" />
        <bean parent="ReferenceDefinition"
              p:attributeName="cashReversionAccount"
              p:activeIndicatorAttributeName="active"
              p:activeIndicatorReversed="true"
              p:attributeToHighlightOnFail="cashReversionAccountNumber" />
      </list>
    </property>
    <property name="lockingKeys" >
      <list>
        <value>documentNumber</value>
      </list>
    </property>
    <property name="label" value="Organization Reversion Global" />
    <property name="shortLabel" value="Organization Reversion Global" />
    <property name="summary" value="Global Organization Reversion Maintenance Document" />
    <property name="description" value="Global Organization Reversion Maintenance Document" />
    <property name="helpDefinition" >
      <bean parent="HelpDefinition" p:parameterClass="org.kuali.kfs.coa.businessobject.OrganizationReversionGlobal" p:parameterName="ORGANIZATION_REVERSION_GLOBAL" />
    </property>
    <property name="documentTypeName" value="OrganizationReversionGlobalMaintenanceDocument" />
    <property name="businessRulesClass" value="org.kuali.kfs.coa.document.validation.impl.OrganizationReversionGlobalRule" />
    <property name="documentAuthorizerClass" value="org.kuali.kfs.sys.document.authorization.FinancialSystemMaintenanceDocumentAuthorizerBase" />
    <property name="preRulesCheckClass" value="org.kuali.kfs.coa.document.validation.impl.OrganizationReversionGlobalPreRules" />
    <property name="documentTypeCode" value="GORV" />
    <property name="webScriptFiles" >
      <list>
        <value>../dwr/interface/ObjectCodeService.js</value>
        <value>../scripts/chart/orgReversionChangeDocument.js</value>
      </list>
    </property>
    <property name="workflowProperties" >
      <ref bean="OrganizationReversionGlobalMaintenanceDocument-workflowProperties" />
    </property>
    <property name="authorizations" >
      <list>
        <bean parent="AuthorizationDefinition">
          <property name="action" value="initiate" />
          <property name="authorizedGroups" >
            <set>
              <value>SY_MAINTENANCE_USERS</value>
            </set>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Maintenance Section Definitions -->


  <bean id="OrganizationReversionGlobalMaintenanceDocument-EditGlobalOrganizationReversionOrganizations" parent="OrganizationReversionGlobalMaintenanceDocument-EditGlobalOrganizationReversionOrganizations-parentBean" />

  <bean id="OrganizationReversionGlobalMaintenanceDocument-EditGlobalOrganizationReversionOrganizations-parentBean" abstract="true" parent="MaintainableSectionDefinition">
    <property name="id" value="Edit Global Organization Reversion Organizations" />
    <property name="title" value="Edit Global Organization Reversion Organizations" />
    <property name="maintainableItems" >
      <list>
        <bean parent="MaintainableCollectionDefinition">
          <property name="name" value="organizationReversionGlobalOrganizations" />
          <property name="businessObjectClass" value="org.kuali.kfs.coa.businessobject.OrganizationReversionGlobalOrganization" />
          <property name="sourceClassName" value="org.kuali.kfs.coa.businessobject.Org" />
          <property name="summaryTitle" value="Organization" />
          <property name="summaryFields" >
            <list>
              <bean parent="MaintainableFieldDefinition"
                    p:name="chartOfAccountsCode" />
              <bean parent="MaintainableFieldDefinition"
                    p:name="organizationCode" />
            </list>
          </property>
          <property name="maintainableFields" >
            <list>
              <bean parent="MaintainableFieldDefinition"
                    p:name="chartOfAccountsCode"
                    p:required="true"
                    p:template="chartOfAccountsCode" />
              <bean parent="MaintainableFieldDefinition"
                    p:name="organizationCode"
                    p:required="true"
                    p:template="organizationCode" />
              <bean parent="MaintainableFieldDefinition"
                    p:name="newCollectionRecord" />
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>
  <bean id="OrganizationReversionGlobalMaintenanceDocument-EditGlobalOrganizationReversion" parent="OrganizationReversionGlobalMaintenanceDocument-EditGlobalOrganizationReversion-parentBean" />

  <bean id="OrganizationReversionGlobalMaintenanceDocument-EditGlobalOrganizationReversion-parentBean" abstract="true" parent="MaintainableSectionDefinition">
    <property name="id" value="Edit Global Organization Reversion" />
    <property name="title" value="Edit Global Organization Reversion" />
    <property name="maintainableItems" >
      <list>
        <bean parent="MaintainableFieldDefinition"
              p:name="universityFiscalYear"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="budgetReversionChartOfAccountsCode" />
        <bean parent="MaintainableFieldDefinition"
              p:name="budgetReversionAccountNumber" />
        <bean parent="MaintainableFieldDefinition"
              p:name="cashReversionFinancialChartOfAccountsCode" />
        <bean parent="MaintainableFieldDefinition"
              p:name="cashReversionAccountNumber" />
        <bean parent="MaintainableFieldDefinition"
              p:name="carryForwardByObjectCodeIndicator" />
      </list>
    </property>
  </bean>
  <bean id="OrganizationReversionGlobalMaintenanceDocument-EditGlobalOrganizationReversionDetails" parent="OrganizationReversionGlobalMaintenanceDocument-EditGlobalOrganizationReversionDetails-parentBean" />

  <bean id="OrganizationReversionGlobalMaintenanceDocument-EditGlobalOrganizationReversionDetails-parentBean" abstract="true" parent="MaintainableSectionDefinition">
    <property name="id" value="Edit Global Organization Reversion Details" />
    <property name="title" value="Edit Global Organization Reversion Details" />
    <property name="maintainableItems" >
      <list>
        <bean parent="MaintainableCollectionDefinition">
          <property name="name" value="organizationReversionGlobalDetails" />
          <property name="businessObjectClass" value="org.kuali.kfs.coa.businessobject.OrganizationReversionGlobalDetail" />
          <property name="includeAddLine" value="false" />
          <property name="summaryTitle" value="Organization Reversion Detail" />
          <property name="maintainableFields" >
            <list>
              <bean parent="MaintainableFieldDefinition"
                    p:name="organizationReversionCategoryCode"
                    p:readOnly="true"
                    p:required="true" />
              <bean parent="MaintainableFieldDefinition"
                    p:name="organizationReversionObjectCode"
                    p:webUILeaveFieldFunction="updateObjectNames" />
              <bean parent="MaintainableFieldDefinition"
                    p:name="objectCodeNames"
                    p:readOnly="true" />
              <bean parent="MaintainableFieldDefinition"
                    p:name="organizationReversionCode" />
              <bean parent="MaintainableFieldDefinition"
                    p:name="newCollectionRecord" />
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Exported Workflow Properties -->


  <bean id="OrganizationReversionGlobalMaintenanceDocument-workflowProperties" parent="OrganizationReversionGlobalMaintenanceDocument-workflowProperties-parentBean" />

  <bean id="OrganizationReversionGlobalMaintenanceDocument-workflowProperties-parentBean" abstract="true" parent="WorkflowProperties">
    <property name="workflowPropertyGroups" >
      <list>
        <bean parent="WorkflowPropertyGroup">
          <property name="workflowProperties" >
            <list>
              <bean parent="WorkflowProperty"
                    p:path="newMaintainableObject.businessObject.organizationReversionGlobalOrganizations" />
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>
</beans>
