<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="ObjSubTypMaintenanceDocument" parent="ObjSubTypMaintenanceDocument-parentBean" />

  <bean id="ObjSubTypMaintenanceDocument-parentBean" abstract="true" parent="MaintenanceDocumentEntry">
    <property name="businessObjectClass" value="org.kuali.kfs.coa.businessobject.ObjSubTyp" />
    <property name="maintainableClass" value="org.kuali.rice.kns.maintenance.KualiMaintainableImpl" />
    <property name="maintainableSections" >
      <list>
        <ref bean="ObjSubTypMaintenanceDocument-EditObjectSub-Type" />
      </list>
    </property>
    <property name="lockingKeys" >
      <list>
        <value>code</value>
      </list>
    </property>
    <property name="label" value="Object Sub-Type" />
    <property name="shortLabel" value="Object Sub-Type" />
    <property name="summary" value="Object Sub-Type Maintenance Document" />
    <property name="description" value="Maintenance document which allows you to create new Object Sub-Types, or modify existing Object Sub-Types." />
    <property name="helpDefinition" >
      <bean parent="HelpDefinition" p:parameterClass="org.kuali.kfs.coa.businessobject.ObjSubTyp" p:parameterName="OBJECT_SUB_TYPE" />
    </property>
    <property name="documentTypeName" value="ObjSubTypMaintenanceDocument" />
    <property name="documentAuthorizerClass" value="org.kuali.kfs.sys.document.authorization.FinancialSystemMaintenanceDocumentAuthorizerBase" />
    <property name="documentTypeCode" value="OSTY" />
    <property name="workflowProperties" >
      <ref bean="ObjSubTypMaintenanceDocument-workflowProperties" />
    </property>
    <property name="authorizations" >
      <list>
        <bean parent="AuthorizationDefinition">
          <property name="action" value="initiate" />
          <property name="authorizedGroups" >
            <set>
              <value>SY_MAINTENANCE_USERS</value>
            </set>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Maintenance Section Definitions -->


  <bean id="ObjSubTypMaintenanceDocument-EditObjectSub-Type" parent="ObjSubTypMaintenanceDocument-EditObjectSub-Type-parentBean" />

  <bean id="ObjSubTypMaintenanceDocument-EditObjectSub-Type-parentBean" abstract="true" parent="MaintainableSectionDefinition">
    <property name="id" value="Edit Object Sub-Type" />
    <property name="title" value="Edit Object Sub-Type" />
    <property name="maintainableItems" >
      <list>
        <bean parent="MaintainableFieldDefinition"
              p:name="code"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="name"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="active"
              p:defaultValue="true" />
      </list>
    </property>
  </bean>

<!-- Exported Workflow Properties -->


  <bean id="ObjSubTypMaintenanceDocument-workflowProperties" parent="ObjSubTypMaintenanceDocument-workflowProperties-parentBean" />

  <bean id="ObjSubTypMaintenanceDocument-workflowProperties-parentBean" abstract="true" parent="WorkflowProperties">
  </bean>
</beans>
