<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="ObjectConsMaintenanceDocument" parent="ObjectConsMaintenanceDocument-parentBean" />

  <bean id="ObjectConsMaintenanceDocument-parentBean" abstract="true" parent="MaintenanceDocumentEntry">
    <property name="businessObjectClass" value="org.kuali.kfs.coa.businessobject.ObjectCons" />
    <property name="maintainableClass" value="org.kuali.rice.kns.maintenance.KualiMaintainableImpl" />
    <property name="maintainableSections" >
      <list>
        <ref bean="ObjectConsMaintenanceDocument-ObjectConsolidation" />
      </list>
    </property>
    <property name="defaultExistenceChecks" >
      <list>
        <bean parent="ReferenceDefinition"
              p:attributeName="chartOfAccounts"
              p:attributeToHighlightOnFail="chartOfAccountsCode" />
      </list>
    </property>
    <property name="lockingKeys" >
      <list>
        <value>chartOfAccountsCode</value>
        <value>finConsolidationObjectCode</value>
      </list>
    </property>
    <property name="label" value="Object Consolidation" />
    <property name="shortLabel" value="Object Consolidation" />
    <property name="summary" value="Object Consolidation Maintenance Document" />
    <property name="description" value="Maintenance document which allows you to create new Object Consolidations, or modify existing Object Consolidations." />
    <property name="helpDefinition" >
      <bean parent="HelpDefinition" p:parameterClass="org.kuali.kfs.coa.businessobject.ObjectCons" p:parameterName="OBJECT_CONSOLIDATION" />
    </property>
    <property name="documentTypeName" value="ObjectConsMaintenanceDocument" />
    <property name="businessRulesClass" value="org.kuali.kfs.coa.document.validation.impl.ObjectConsRule" />
    <property name="documentAuthorizerClass" value="org.kuali.kfs.sys.document.authorization.FinancialSystemMaintenanceDocumentAuthorizerBase" />
    <property name="documentTypeCode" value="OBJC" />
    <property name="webScriptFiles" >
      <list>
        <value>../dwr/interface/ObjectCodeService.js</value>
        <value>../scripts/chart/objConsDocument.js</value>
      </list>
    </property>
    <property name="workflowProperties" >
      <ref bean="ObjectConsMaintenanceDocument-workflowProperties" />
    </property>
    <property name="authorizations" >
      <list>
        <bean parent="AuthorizationDefinition">
          <property name="action" value="initiate" />
          <property name="authorizedGroups" >
            <set>
              <value>SY_MAINTENANCE_USERS</value>
            </set>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Maintenance Section Definitions -->


  <bean id="ObjectConsMaintenanceDocument-ObjectConsolidation" parent="ObjectConsMaintenanceDocument-ObjectConsolidation-parentBean" />

  <bean id="ObjectConsMaintenanceDocument-ObjectConsolidation-parentBean" abstract="true" parent="MaintainableSectionDefinition">
    <property name="id" value="Object Consolidation" />
    <property name="title" value="Object Consolidation" />
    <property name="maintainableItems" >
      <list>
        <bean parent="MaintainableFieldDefinition"
              p:name="chartOfAccountsCode"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="finConsolidationObjectCode"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="finConsolidationObjShortName"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="finConsolidationObjectName"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="active"
              p:defaultValue="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="financialEliminationsObjectCode"
              p:webUILeaveFieldFunction="updateObjectName" />
        <bean parent="MaintainableFieldDefinition"
              p:name="financialEliminationsObject.financialObjectCodeName"
              p:readOnly="true"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="financialReportingSortCode"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="versionNumber" />
      </list>
    </property>
  </bean>

<!-- Exported Workflow Properties -->


  <bean id="ObjectConsMaintenanceDocument-workflowProperties" parent="ObjectConsMaintenanceDocument-workflowProperties-parentBean" />

  <bean id="ObjectConsMaintenanceDocument-workflowProperties-parentBean" abstract="true" parent="WorkflowProperties">
  </bean>
</beans>
