<dictionaryEntry>
    <maintenanceDocument>
        <businessObjectClass>org.kuali.module.chart.bo.FundGroup</businessObjectClass>
        <maintainableClass>org.kuali.core.maintenance.KualiMaintainableImpl</maintainableClass>

        <documentAuthorizerClass>org.kuali.core.document.MaintenanceDocumentAuthorizerBase</documentAuthorizerClass>
        <authorizations>
            <authorization action="initiate">
                <workgroups>
                    <workgroup>kualiUniversalGroup</workgroup>
                </workgroups>
            </authorization>
        </authorizations>

        <documentTypeName>KualiFundGroupMaintenanceDocument</documentTypeName>
        <documentTypeCode>FGRP</documentTypeCode>

        <label>Fund Group Maintenance Document</label>
        <shortLabel>FundGroupMainDoc</shortLabel>

        <summary>Fund Group maintenance document</summary>
        <description>Document used to create or update Fund Group objects</description>

        <lockingKeys>
            <field attributeName="code" />
        </lockingKeys>

        <maintainableSections>
            <maintainableSection title="Edit Fund Group Code">
                <maintainableItems>
                    <maintainableField name="code" required="true" />
                    <maintainableField name="name" required="true" />
                    <maintainableField name="financialReportingSortCode" required="true" />
                    <maintainableField name="fundGroupBudgetAdjustmentRestrictionLevelCode" required="true" />
                    <maintainableField name="active" required="false" />
                    <maintainableField name="versionNumber" />
                </maintainableItems>
            </maintainableSection>
        </maintainableSections>
    </maintenanceDocument>
