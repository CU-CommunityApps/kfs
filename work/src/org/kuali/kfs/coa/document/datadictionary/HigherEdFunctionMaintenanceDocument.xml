<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="HigherEdFunctionMaintenanceDocument" parent="HigherEdFunctionMaintenanceDocument-parentBean" />

  <bean id="HigherEdFunctionMaintenanceDocument-parentBean" abstract="true" parent="MaintenanceDocumentEntry">
    <property name="businessObjectClass" value="org.kuali.kfs.coa.businessobject.HigherEdFunction" />
    <property name="maintainableClass" value="org.kuali.rice.kns.maintenance.KualiMaintainableImpl" />
    <property name="maintainableSections" >
      <list>
        <ref bean="HigherEdFunctionMaintenanceDocument-EditHigherEducationFunction" />
      </list>
    </property>
    <property name="defaultExistenceChecks" >
      <list>
        <bean parent="ReferenceDefinition"
              p:attributeName="universityBudgetOfficeFunction"
              p:attributeToHighlightOnFail="finUnivBdgtOfficeFunctionCd" />
        <bean parent="ReferenceDefinition"
              p:attributeName="aicpaFunction"
              p:attributeToHighlightOnFail="finAicpaFunctionCode" />
        <bean parent="ReferenceDefinition"
              p:attributeName="federalFunction"
              p:attributeToHighlightOnFail="financialFederalFunctionCode" />
      </list>
    </property>
    <property name="lockingKeys" >
      <list>
        <value>financialHigherEdFunctionCd</value>
      </list>
    </property>
    <property name="label" value="Higher Education Function" />
    <property name="shortLabel" value="Higher Education Function" />
    <property name="summary" value="Higher Education Function maintenance document" />
    <property name="description" value="Document used to create or update Higher Education Function objects" />
    <property name="helpDefinition" >
      <bean parent="HelpDefinition" p:parameterClass="org.kuali.kfs.coa.businessobject.HigherEdFunction" p:parameterName="HIGHER_EDUCATION_FUNCTION" />
    </property>
    <property name="documentTypeName" value="HigherEdFunctionMaintenanceDocument" />
    <property name="documentAuthorizerClass" value="org.kuali.kfs.sys.document.authorization.FinancialSystemMaintenanceDocumentAuthorizerBase" />
    <property name="documentTypeCode" value="HEFN" />
    <property name="workflowProperties" >
      <ref bean="HigherEdFunctionMaintenanceDocument-workflowProperties" />
    </property>
    <property name="authorizations" >
      <list>
        <bean parent="AuthorizationDefinition">
          <property name="action" value="initiate" />
          <property name="authorizedGroups" >
            <set>
              <value>SY_MAINTENANCE_USERS</value>
            </set>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Maintenance Section Definitions -->


  <bean id="HigherEdFunctionMaintenanceDocument-EditHigherEducationFunction" parent="HigherEdFunctionMaintenanceDocument-EditHigherEducationFunction-parentBean" />

  <bean id="HigherEdFunctionMaintenanceDocument-EditHigherEducationFunction-parentBean" abstract="true" parent="MaintainableSectionDefinition">
    <property name="id" value="Edit Higher Education Function" />
    <property name="title" value="Edit Higher Education Function" />
    <property name="maintainableItems" >
      <list>
        <bean parent="MaintainableFieldDefinition"
              p:name="financialHigherEdFunctionCd"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="financialHigherEdFunctionNm"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="finUnivBdgtOfficeFunctionCd"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="finAicpaFunctionCode"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="financialFederalFunctionCode"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:defaultValue="true"
              p:name="active" />
      </list>
    </property>
  </bean>

<!-- Exported Workflow Properties -->


  <bean id="HigherEdFunctionMaintenanceDocument-workflowProperties" parent="HigherEdFunctionMaintenanceDocument-workflowProperties-parentBean" />

  <bean id="HigherEdFunctionMaintenanceDocument-workflowProperties-parentBean" abstract="true" parent="WorkflowProperties">
  </bean>
</beans>
