<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="OrganizationReversionMaintenanceDocument" parent="OrganizationReversionMaintenanceDocument-parentBean" />

  <bean id="OrganizationReversionMaintenanceDocument-parentBean" abstract="true" parent="MaintenanceDocumentEntry">
    <property name="businessObjectClass" value="org.kuali.kfs.coa.businessobject.OrganizationReversion" />
    <property name="maintainableClass" value="org.kuali.kfs.coa.document.OrganizationReversionMaintainableImpl" />
    <property name="maintainableSections" >
      <list>
        <ref bean="OrganizationReversionMaintenanceDocument-EditOrganizationReversion" />
        <ref bean="OrganizationReversionMaintenanceDocument-EditOrganizationReversionDetails" />
      </list>
    </property>
    <property name="defaultExistenceChecks" >
      <list>
        <bean parent="ReferenceDefinition"
              p:attributeName="chartOfAccounts"
              p:attributeToHighlightOnFail="chartOfAccountsCode" />
        <bean parent="ReferenceDefinition"
              p:attributeName="universityFiscal"
              p:attributeToHighlightOnFail="universityFiscalYear" />
        <bean parent="ReferenceDefinition"
              p:attributeName="organization"
              p:attributeToHighlightOnFail="organizationCode" />
        <bean parent="ReferenceDefinition"
              p:attributeName="budgetReversionChartOfAccounts"
              p:attributeToHighlightOnFail="budgetReversionChartOfAccountsCode" />
        <bean parent="ReferenceDefinition"
              p:attributeName="budgetReversionAccount"
              p:attributeToHighlightOnFail="budgetReversionAccountNumber" />
        <bean parent="ReferenceDefinition"
              p:attributeName="cashReversionFinancialChartOfAccounts"
              p:attributeToHighlightOnFail="cashReversionFinancialChartOfAccountsCode" />
        <bean parent="ReferenceDefinition"
              p:attributeName="cashReversionAccount"
              p:attributeToHighlightOnFail="cashReversionAccountNumber" />
      </list>
    </property>
    <property name="lockingKeys" >
      <list>
        <value>universityFiscalYear</value>
        <value>chartOfAccountsCode</value>
        <value>organizationCode</value>
      </list>
    </property>
    <property name="label" value="Organization Reversion" />
    <property name="shortLabel" value="Organization Reversion" />
    <property name="summary" value="Organization Reversion Maintenance Document" />
    <property name="description" value="Maintenance document which allows you to create new Organization Reversions, remove exsiting Organization Reversions, or modify existing Organization Reversions." />
    <property name="helpDefinition" >
      <bean parent="HelpDefinition" p:parameterClass="org.kuali.kfs.coa.businessobject.OrganizationReversion" p:parameterName="ORGANIZATION_REVERSION" />
    </property>
    <property name="documentTypeName" value="OrganizationReversionMaintenanceDocument" />
    <property name="businessRulesClass" value="org.kuali.kfs.coa.document.validation.impl.OrganizationReversionRule" />
    <property name="documentAuthorizerClass" value="org.kuali.kfs.sys.document.authorization.FinancialSystemMaintenanceDocumentAuthorizerBase" />
    <property name="preRulesCheckClass" value="org.kuali.kfs.coa.document.validation.impl.OrganizationReversionPreRules" />
    <property name="documentTypeCode" value="ORGR" />
    <property name="webScriptFiles" >
      <list>
        <value>../dwr/interface/ObjectCodeService.js</value>
        <value>../scripts/chart/orgReversionDocument.js</value>
      </list>
    </property>
    <property name="workflowProperties" >
      <ref bean="OrganizationReversionMaintenanceDocument-workflowProperties" />
    </property>
    <property name="authorizations" >
      <list>
        <bean parent="AuthorizationDefinition">
          <property name="action" value="initiate" />
          <property name="authorizedGroups" >
            <set>
              <value>SY_MAINTENANCE_USERS</value>
            </set>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Maintenance Section Definitions -->


  <bean id="OrganizationReversionMaintenanceDocument-EditOrganizationReversion" parent="OrganizationReversionMaintenanceDocument-EditOrganizationReversion-parentBean" />

  <bean id="OrganizationReversionMaintenanceDocument-EditOrganizationReversion-parentBean" abstract="true" parent="MaintainableSectionDefinition">
    <property name="id" value="Edit Organization Reversion" />
    <property name="title" value="Edit Organization Reversion" />
    <property name="maintainableItems" >
      <list>
        <bean parent="MaintainableFieldDefinition"
              p:name="universityFiscalYear"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="chartOfAccountsCode"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="organizationCode"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="budgetReversionChartOfAccountsCode"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="budgetReversionAccountNumber"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="cashReversionFinancialChartOfAccountsCode"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="cashReversionAccountNumber"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="carryForwardByObjectCodeIndicator" />
        <bean parent="MaintainableFieldDefinition"
              p:defaultValue="true"
              p:name="active" />
      </list>
    </property>
  </bean>
  <bean id="OrganizationReversionMaintenanceDocument-EditOrganizationReversionDetails" parent="OrganizationReversionMaintenanceDocument-EditOrganizationReversionDetails-parentBean" />

  <bean id="OrganizationReversionMaintenanceDocument-EditOrganizationReversionDetails-parentBean" abstract="true" parent="MaintainableSectionDefinition">
    <property name="id" value="Edit Organization Reversion Details" />
    <property name="title" value="Edit Organization Reversion Details" />
    <property name="maintainableItems" >
      <list>
        <bean parent="MaintainableCollectionDefinition">
          <property name="name" value="organizationReversionDetail" />
          <property name="businessObjectClass" value="org.kuali.kfs.coa.businessobject.OrganizationReversionDetail" />
          <property name="includeAddLine" value="false" />
          <property name="maintainableFields" >
            <list>
              <bean parent="MaintainableFieldDefinition"
                    p:name="organizationReversionCategoryCode"
                    p:readOnly="true"
                    p:required="true" />
              <bean parent="MaintainableFieldDefinition"
                    p:name="organizationReversionObjectCode"
                    p:required="true"
                    p:webUILeaveFieldFunction="updateObjectName" />
              <bean parent="MaintainableFieldDefinition"
                    p:name="organizationReversionObject.financialObjectCodeName"
                    p:readOnly="true"
                    p:required="true" />
              <bean parent="MaintainableFieldDefinition"
                    p:name="organizationReversionCode"
                    p:required="true" />
              <bean parent="MaintainableFieldDefinition"
                    p:name="newCollectionRecord" />
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Exported Workflow Properties -->


  <bean id="OrganizationReversionMaintenanceDocument-workflowProperties" parent="OrganizationReversionMaintenanceDocument-workflowProperties-parentBean" />

  <bean id="OrganizationReversionMaintenanceDocument-workflowProperties-parentBean" abstract="true" parent="WorkflowProperties">
  </bean>
</beans>
