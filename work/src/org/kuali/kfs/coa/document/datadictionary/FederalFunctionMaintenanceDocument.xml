<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="FederalFunctionMaintenanceDocument" parent="FederalFunctionMaintenanceDocument-parentBean" />

  <bean id="FederalFunctionMaintenanceDocument-parentBean" abstract="true" parent="MaintenanceDocumentEntry">
    <property name="businessObjectClass" value="org.kuali.module.chart.bo.FederalFunction" />
    <property name="maintainableClass" value="org.kuali.core.maintenance.KualiMaintainableImpl" />
    <property name="maintainableSections" >
      <list>
        <ref bean="FederalFunctionMaintenanceDocument-EditFederalFunction" />
      </list>
    </property>
    <property name="lockingKeys" >
      <list>
        <value>financialFederalFunctionCode</value>
      </list>
    </property>
    <property name="label" value="Federal Function" />
    <property name="shortLabel" value="Federal Function" />
    <property name="summary" value="Federal Function Maintenance Document" />
    <property name="description" value="Document used to create or update Federal Function Maintenance Document objects" />
    <property name="helpDefinition" >
      <bean parent="HelpDefinition"
            p:parameterNamespace="KFS-CA"
            p:parameterName="FEDERAL_FUNCTION"
            p:parameterDetailType="FederalFunction" />
    </property>
    <property name="documentTypeName" value="FederalFunctionMaintenanceDocument" />
    <property name="documentAuthorizerClass" value="org.kuali.core.document.authorization.MaintenanceDocumentAuthorizerBase" />
    <property name="documentTypeCode" value="FFUN" />
    <property name="workflowProperties" >
      <ref bean="FederalFunctionMaintenanceDocument-workflowProperties" />
    </property>
    <property name="authorizations" >
      <list>
        <bean parent="AuthorizationDefinition">
          <property name="action" value="initiate" />
          <property name="authorizedGroups" >
            <set>
              <value>SY_MAINTAINENANCE_USERS</value>
            </set>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Maintenance Section Definitions -->


  <bean id="FederalFunctionMaintenanceDocument-EditFederalFunction" parent="FederalFunctionMaintenanceDocument-EditFederalFunction-parentBean" />

  <bean id="FederalFunctionMaintenanceDocument-EditFederalFunction-parentBean" abstract="true" parent="MaintainableSectionDefinition">
    <property name="id" value="Edit Federal Function" />
    <property name="title" value="Edit Federal Function" />
    <property name="maintainableItems" >
      <list>
        <bean parent="MaintainableFieldDefinition"
              p:name="financialFederalFunctionCode"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="financialFederalFunctionName"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="versionNumber" />
      </list>
    </property>
  </bean>

<!-- Exported Workflow Properties -->


  <bean id="FederalFunctionMaintenanceDocument-workflowProperties" parent="FederalFunctionMaintenanceDocument-workflowProperties-parentBean" />

  <bean id="FederalFunctionMaintenanceDocument-workflowProperties-parentBean" abstract="true" parent="WorkflowProperties">
  </bean>
</beans>
