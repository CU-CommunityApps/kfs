<?xml version="1.0" encoding="UTF-8"?><beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p" xsi:schemaLocation="http://www.springframework.org/schema/beans         http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">
  <bean id="SubObjectCodeImportDocument" parent="SubObjectCodeImportDocument-parentBean"/>
  <bean id="SubObjectCodeImportDocument-parentBean" abstract="true" parent="TransactionalDocumentEntry">
    <property name="documentTypeName" value="SOIP"/>
  	<property name="documentClass" value="org.kuali.kfs.coa.document.SubObjectCodeImportDocument"/>
    <property name="businessRulesClass" value="org.kuali.kfs.sys.document.validation.impl.AccountingRuleEngineRuleBase"/>
    <property name="documentAuthorizerClass" value="org.kuali.kfs.sys.document.authorization.FinancialSystemTransactionalDocumentAuthorizerBase"/>
    <property name="documentPresentationControllerClass" value="org.kuali.kfs.sys.document.authorization.FinancialSystemTransactionalDocumentPresentationControllerBase"/>
    <property name="workflowAttributes">
      <ref bean="SubObjectCodeImportDocument-workflowAttributes"/>
    </property> 
    <property name="attributes">
      <list>
        <ref bean="SubObjectCodeImportDocument-documentNumber"/>
      </list>
    </property>
    <property name="validationMap" ref="SubObjectCodeImportDocument-validations"/>
  </bean>

<!-- Attribute Definitions -->
  <bean id="SubObjectCodeImportDocument-documentNumber" parent="SubObjectCodeImportDocument-documentNumber-parentBean"/>

  <bean id="SubObjectCodeImportDocument-documentNumber-parentBean" abstract="true" parent="FinancialSystemDocumentHeader-documentNumber">
  </bean>

<!-- workflow attributes -->  	
  <bean id="SubObjectCodeImportDocument-workflowAttributes" parent="SubObjectCodeImportDocument-workflowAttributes-parentBean"/>
  <bean id="SubObjectCodeImportDocument-workflowAttributes-parentBean" abstract="true" parent="WorkflowAttributes">
  </bean>
  
  <!-- Validations -->
  <bean id="SubObjectCodeImportDocument-validations" parent="SubObjectCodeImportDocument-validations-parentBean"/>
  
  <bean id="SubObjectCodeImportDocument-validations-parentBean" class="org.springframework.beans.factory.config.MapFactoryBean">
  	<property name="sourceMap">
		<map key-type="java.lang.Class">
			<entry>
				<key><value>org.kuali.kfs.sys.document.validation.event.AttributedRouteDocumentEvent</value></key>
				<value>SubObjectCodeImportDocument-routeDocumentValidation</value>
			</entry>
			<entry>
				<key><value>org.kuali.kfs.sys.document.validation.event.AttributedSaveDocumentEvent</value></key>
				<value>SubObjectCodeImportDocument-saveDocumentValidation</value>
			</entry>
			<entry>
				<key><value>org.kuali.kfs.sys.document.validation.event.AttributedApproveDocumentEvent</value></key>
				<value>SubObjectCodeImportDocument-approveDocumentValidation</value>
			</entry>
			<entry>
				<key><value>org.kuali.kfs.sys.document.validation.event.AttributedBlanketApproveDocumentEvent</value></key>
				<value>SubObjectCodeImportDocument-blanketApproveDocumentValidation</value>
			</entry>
			<entry>
				<key><value>org.kuali.kfs.sys.document.validation.event.AttributedMassImportLineEventBase</value></key>
				<value>SubObjectCodeImportDocument-importLinesValidation</value>				
			</entry>
    	</map>
    </property>
  </bean>  
</beans>
