<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="SubObjCdMaintenanceDocument" parent="SubObjCdMaintenanceDocument-parentBean" />

  <bean id="SubObjCdMaintenanceDocument-parentBean" abstract="true" parent="MaintenanceDocumentEntry">
    <property name="businessObjectClass" value="org.kuali.kfs.coa.businessobject.SubObjCd" />
    <property name="maintainableClass" value="org.kuali.core.maintenance.KualiMaintainableImpl" />
    <property name="maintainableSections" >
      <list>
        <ref bean="SubObjCdMaintenanceDocument-EditSubObjectCode" />
      </list>
    </property>
    <property name="defaultExistenceChecks" >
      <list>
        <bean parent="ReferenceDefinition"
              p:attributeName="universityFiscal"
              p:attributeToHighlightOnFail="universityFiscalYear" />
        <bean parent="ReferenceDefinition"
              p:attributeName="chartOfAccounts"
              p:attributeToHighlightOnFail="chartOfAccountsCode" />
        <bean parent="ReferenceDefinition"
              p:attributeName="account"
              p:activeIndicatorAttributeName="accountClosedIndicator"
              p:activeIndicatorReversed="true"
              p:attributeToHighlightOnFail="accountNumber" />
        <bean parent="ReferenceDefinition"
              p:attributeName="financialObject"
              p:activeIndicatorAttributeName="financialObjectActiveCode"
              p:attributeToHighlightOnFail="financialObjectCode" />
      </list>
    </property>
    <property name="lockingKeys" >
      <list>
        <value>universityFiscalYear</value>
        <value>chartOfAccountsCode</value>
        <value>accountNumber</value>
        <value>financialObjectCode</value>
        <value>financialSubObjectCode</value>
      </list>
    </property>
    <property name="label" value="Sub-Object Code" />
    <property name="shortLabel" value="Sub-Object Code" />
    <property name="summary" value="Sub Object maintenance document" />
    <property name="description" value="Document used to create or update Sub Objects" />
    <property name="helpDefinition" >
      <bean parent="HelpDefinition"
            p:parameterNamespace="KFS-COA"
            p:parameterName="SUB_OBJECT_CODE"
            p:parameterDetailType="SubObjCd" />
    </property>
    <property name="documentTypeName" value="SubObjCdMaintenanceDocument" />
    <property name="businessRulesClass" value="org.kuali.kfs.coa.document.validation.impl.SubObjCdRule" />
    <property name="documentAuthorizerClass" value="org.kuali.kfs.sys.document.web.struts.FinancialSystemMaintenanceDocumentAuthorizerBase" />
    <property name="preRulesCheckClass" value="org.kuali.kfs.coa.document.validation.impl.SubObjectPreRules" />
    <property name="documentTypeCode" value="SOBJ" />
    <property name="workflowProperties" >
      <ref bean="SubObjCdMaintenanceDocument-workflowProperties" />
    </property>
    <property name="authorizations" >
      <list>
        <bean parent="AuthorizationDefinition">
          <property name="action" value="initiate" />
          <property name="authorizedGroups" >
            <set>
              <value>kualiUniversalGroup</value>
            </set>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Maintenance Section Definitions -->


  <bean id="SubObjCdMaintenanceDocument-EditSubObjectCode" parent="SubObjCdMaintenanceDocument-EditSubObjectCode-parentBean" />

  <bean id="SubObjCdMaintenanceDocument-EditSubObjectCode-parentBean" abstract="true" parent="MaintainableSectionDefinition">
    <property name="id" value="Edit Sub Object Code" />
    <property name="title" value="Edit Sub Object Code" />
    <property name="maintainableItems" >
      <list>
        <bean parent="MaintainableFieldDefinition"
              p:name="universityFiscalYear"
              p:required="true"
              p:defaultValueFinderClass="org.kuali.kfs.sys.businessobject.defaultvalue.FiscalYearFinder" />
        <bean parent="MaintainableFieldDefinition"
              p:name="chartOfAccountsCode"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="accountNumber"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="financialObjectCode"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="financialSubObjectCode"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="financialSubObjectCodeName"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="financialSubObjectCdshortNm"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="financialSubObjectActiveIndicator"
              p:defaultValue="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="versionNumber" />
      </list>
    </property>
  </bean>

<!-- Exported Workflow Properties -->


  <bean id="SubObjCdMaintenanceDocument-workflowProperties" parent="SubObjCdMaintenanceDocument-workflowProperties-parentBean" />

  <bean id="SubObjCdMaintenanceDocument-workflowProperties-parentBean" abstract="true" parent="WorkflowProperties">
    <property name="workflowPropertyGroups" >
      <list>
        <bean parent="WorkflowPropertyGroup">
          <property name="workflowProperties" >
            <list>
              <bean parent="WorkflowProperty"
                    p:path="newMaintainableObject.businessObject" />
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>
</beans>
