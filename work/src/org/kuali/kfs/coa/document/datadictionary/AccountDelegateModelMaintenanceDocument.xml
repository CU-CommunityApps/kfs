<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="AccountDelegateModelMaintenanceDocument" parent="AccountDelegateModelMaintenanceDocument-parentBean" />

  <bean id="AccountDelegateModelMaintenanceDocument-parentBean" abstract="true" parent="MaintenanceDocumentEntry">
    <property name="businessObjectClass" value="org.kuali.kfs.coa.businessobject.AccountDelegateModel" />
    <property name="maintainableClass" value="org.kuali.rice.kns.maintenance.KualiMaintainableImpl" />
    <property name="maintainableSections" >
      <list>
        <ref bean="AccountDelegateModelMaintenanceDocument-EditAccountDelegateModel" />
        <ref bean="AccountDelegateModelMaintenanceDocument-EditDelegatesforModel" />
      </list>
    </property>
    <property name="lockingKeys" >
      <list>
        <value>chartOfAccountsCode</value>
        <value>organizationCode</value>
        <value>organizationRoutingModelName</value>
      </list>
    </property>
    <property name="label" value="Account Delegate Model" />
    <property name="shortLabel" value="Account Delegate Model" />
    <property name="summary" value="Account Delegate Model Maintenance Document" />
    <property name="description" value="Account Delegate Model Maintenance Document" />
    <property name="helpDefinition" >
      <bean parent="HelpDefinition" p:parameterClass="org.kuali.kfs.coa.businessobject.AccountDelegateModel" p:parameterName="DELEGATE_MODEL" />
    </property>
    <property name="documentTypeName" value="AccountDelegateModelMaintenanceDocument" />
    <property name="businessRulesClass" value="org.kuali.kfs.coa.document.validation.impl.OrganizationRoutingModelRule" />
    <property name="documentAuthorizerClass" value="org.kuali.kfs.sys.document.authorization.FinancialSystemMaintenanceDocumentAuthorizerBase" />
    <property name="preRulesCheckClass" value="org.kuali.kfs.coa.document.validation.impl.OrganizationRoutingModelPreRules" />
    <property name="documentTypeCode" value="GDLM" />
    <property name="workflowProperties" >
      <ref bean="AccountDelegateModelMaintenanceDocument-workflowProperties" />
    </property>
    <property name="authorizations" >
      <list>
        <bean parent="AuthorizationDefinition">
          <property name="action" value="initiate" />
          <property name="authorizedGroups" >
            <set>
              <value>SY_MAINTENANCE_USERS</value>
            </set>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Maintenance Section Definitions -->


  <bean id="AccountDelegateModelMaintenanceDocument-EditAccountDelegateModel" parent="AccountDelegateModelMaintenanceDocument-EditAccountDelegateModel-parentBean" />

  <bean id="AccountDelegateModelMaintenanceDocument-EditAccountDelegateModel-parentBean" abstract="true" parent="MaintainableSectionDefinition">
    <property name="id" value="Edit Account Delegate  Model" />
    <property name="title" value="Edit Account Delegate  Model" />
    <property name="maintainableItems" >
      <list>
        <bean parent="MaintainableFieldDefinition"
              p:name="chartOfAccountsCode"
              p:required="true"
              p:defaultValueFinderClass="org.kuali.kfs.coa.businessobject.defaultvalue.CurrentUserChartValueFinder" />
        <bean parent="MaintainableFieldDefinition"
              p:name="organizationCode"
              p:required="true"
              p:defaultValueFinderClass="org.kuali.kfs.coa.businessobject.defaultvalue.CurrentUserOrgValueFinder" />
        <bean parent="MaintainableFieldDefinition"
              p:name="organizationRoutingModelName"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:defaultValue="true"
              p:name="active" />
      </list>
    </property>
  </bean>
  <bean id="AccountDelegateModelMaintenanceDocument-EditDelegatesforModel" parent="AccountDelegateModelMaintenanceDocument-EditDelegatesforModel-parentBean" />

  <bean id="AccountDelegateModelMaintenanceDocument-EditDelegatesforModel-parentBean" abstract="true" parent="MaintainableSectionDefinition">
    <property name="id" value="Edit Delegates for Model" />
    <property name="title" value="Edit Delegates for Model" />
    <property name="maintainableItems" >
      <list>
        <bean parent="MaintainableCollectionDefinition">
          <property name="name" value="organizationRoutingModel" />
          <property name="businessObjectClass" value="org.kuali.kfs.coa.businessobject.AccountDelegateModelDetail" />
          <property name="summaryTitle" value="Account Delegate Model Delegate" />
          <property name="summaryFields" >
            <list>
              <bean parent="MaintainableFieldDefinition"
                    p:name="financialDocumentTypeCode" />
              <bean parent="MaintainableFieldDefinition"
                    p:name="accountDelegatePrimaryRoutingIndicator" />
              <bean parent="MaintainableFieldDefinition"
                    p:name="accountDelegate.principalName" />
            </list>
          </property>
          <property name="maintainableFields" >
            <list>
              <bean parent="MaintainableFieldDefinition"
                    p:name="financialDocumentTypeCode"
                    p:required="true" />
              <bean parent="MaintainableFieldDefinition"
                    p:name="accountDelegatePrimaryRoutingIndicator" />
              <bean parent="MaintainableFieldDefinition"
                    p:name="accountDelegateStartDate"
                    p:required="true"
                    p:defaultValueFinderClass="org.kuali.kfs.sys.businessobject.defaultvalue.CurrentDateMMDDYYYYFinder" />
              <bean parent="MaintainableFieldDefinition"
                    p:name="approvalFromThisAmount" />
              <bean parent="MaintainableFieldDefinition"
                    p:name="approvalToThisAmount" />
              <bean parent="MaintainableFieldDefinition"
                    p:name="accountDelegate.principalName"
                    p:required="true" />
              <bean parent="MaintainableFieldDefinition"
                    p:name="accountDelegate.name"
                    p:readOnly="true"
                    p:required="true" />
              <bean parent="MaintainableFieldDefinition"
                    p:name="accountDelegateUniversalId"
                    p:readOnly="true"
                    p:required="true" />
              <bean parent="MaintainableFieldDefinition"
                    p:name="active"
                    p:defaultValue="true" />
              <bean parent="MaintainableFieldDefinition"
                    p:name="newCollectionRecord" />
            </list>
          </property>
          <property name="duplicateIdentificationFields">
          	<list>
          		<bean parent="MaintainableFieldDefinition" p:name="accountDelegate.principalName" />
          		<bean parent="MaintainableFieldDefinition" p:name="financialDocumentTypeCode" />
          	</list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Exported Workflow Properties -->


  <bean id="AccountDelegateModelMaintenanceDocument-workflowProperties" parent="AccountDelegateModelMaintenanceDocument-workflowProperties-parentBean" />

  <bean id="AccountDelegateModelMaintenanceDocument-workflowProperties-parentBean" abstract="true" parent="WorkflowProperties">
    <property name="workflowPropertyGroups" >
      <list>
        <bean parent="WorkflowPropertyGroup">
          <property name="workflowProperties" >
            <list>
              <bean parent="WorkflowProperty"
                    p:path="newMaintainableObject.businessObject" />
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>
</beans>

