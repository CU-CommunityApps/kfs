<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "../../../../core/datadictionary/dataDictionary-1_0.dtd">

<dictionaryEntry>
    <maintenanceDocument>
        <businessObjectClass>org.kuali.module.chart.bo.OrganizationRoutingModelName</businessObjectClass>
        <maintainableClass>org.kuali.core.maintenance.KualiMaintainableImpl</maintainableClass>

        <businessRulesClass>org.kuali.module.chart.rules.OrganizationRoutingModelRule</businessRulesClass>
        <preRulesCheckClass>org.kuali.module.chart.rules.OrganizationRoutingModelPreRules</preRulesCheckClass>
        <documentAuthorizerClass>org.kuali.core.document.authorization.MaintenanceDocumentAuthorizerBase</documentAuthorizerClass>
        <authorizations>
            <authorization action="initiate">
                <workgroups>
                    <workgroup>kualiUniversalGroup</workgroup>
                </workgroups>
            </authorization>
        </authorizations>

        <documentTypeName>OrganizationRoutingModelNameMaintenanceDocument</documentTypeName>
        <documentTypeCode>GDLM</documentTypeCode>

        <label>Account Delegate Model</label>

        <summary>Account Delegate Model Maintenance Document</summary>
        <description>Account Delegate Model Maintenance Document</description>
        <help parameterNamespace="KFS-CA" parameterDetailType="OrganizationRoutingModelName" parameterName="DELEGATE_MODEL" />

        <lockingKeys>
            <field attributeName="chartOfAccountsCode" />
            <field attributeName="organizationCode" />
            <field attributeName="organizationRoutingModelName" />
        </lockingKeys>
        
        <allowsNewOrCopy>true</allowsNewOrCopy>
        
        <maintainableSections>
            <maintainableSection title="Edit Account Delegate  Model">
                <maintainableItems>
                	<maintainableField name="chartOfAccountsCode" required="true">
                		<defaultValueFinderClass>org.kuali.module.chart.lookup.valuefinder.CurrentUserChartValueFinder</defaultValueFinderClass>
                	</maintainableField>
                    <maintainableField name="organizationCode" required="true">
                    	<defaultValueFinderClass>org.kuali.module.chart.lookup.valuefinder.CurrentUserOrgValueFinder</defaultValueFinderClass>
                    </maintainableField>
                    <maintainableField name="organizationRoutingModelName" required="true" />
                    <maintainableField name="versionNumber" />
	            </maintainableItems>
            </maintainableSection>
            <maintainableSection title="Edit Delegates for Model">
            	  <maintainableItems>
                    <maintainableCollection name="organizationRoutingModel" businessObjectClass="org.kuali.module.chart.bo.OrganizationRoutingModel" includeAddLine="true" summaryTitle="Account Delegate Model Delegate">
                        <maintainableField name="financialDocumentTypeCode" required="true"/>
                        <maintainableField name="accountDelegatePrimaryRoutingIndicator" />
                        <maintainableField name="accountDelegateStartDate" required="true">
                        	<defaultValueFinderClass>org.kuali.kfs.lookup.valuefinder.CurrentDateMMDDYYYYFinder</defaultValueFinderClass>
                        </maintainableField>
                        <maintainableField name="approvalFromThisAmount" />
                        <maintainableField name="approvalToThisAmount" />
                        <maintainableField name="accountDelegate.personUserIdentifier" required="true" readOnlyAfterAdd="false" />
                        <maintainableField name="accountDelegate.personName" required="true" readOnly="true" />
                        <maintainableField name="accountDelegateUniversalId" required="true" readOnly="true" />
                        <maintainableField name="active">
                        	<defaultValue>true</defaultValue>
                        </maintainableField>
                        <maintainableField name="newCollectionRecord" />
                        <maintainableField name="versionNumber" />
                        <summaryFields>
                            <summaryField name="financialDocumentTypeCode" />
                            <summaryField name="accountDelegatePrimaryRoutingIndicator" />
                            <summaryField name="accountDelegate.personUserIdentifier" />
                        </summaryFields>
                        <duplicateIdentificationFields>
                        	<duplicateIdentificationField name="accountDelegate.personUserIdentifier" />
                        	<duplicateIdentificationField name="financialDocumentTypeCode" />
                        </duplicateIdentificationFields>
                    </maintainableCollection>
            	  </maintainableItems>
            </maintainableSection>
        </maintainableSections>
    </maintenanceDocument>
</dictionaryEntry>