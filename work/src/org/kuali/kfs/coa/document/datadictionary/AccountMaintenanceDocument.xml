                </maintainableItems>
	        </maintainableSection>
	        <maintainableSection title="Contracts And Grants">
                <maintainableItems>
		            <maintainableField name="contractControlAccount.chartOfAccounts.chartOfAccountsCode" required="false" />
		            <maintainableField name="contractControlAccount.accountNumber" required="false" />
		            <maintainableField name="acctIndirectCostRcvyTypeCd" required="false" />
		            <maintainableField name="financialIcrSeriesIdentifier" required="false" />
		            <maintainableField name="indirectCostRecoveryAcct.chartOfAccounts.chartOfAccountsCode" />    
		            <maintainableField name="indirectCostRecoveryAcct.accountNumber" />
		            <maintainableField name="cgCatlfFedDomestcAssistNbr" />	
                </maintainableItems>
	        </maintainableSection>
	        <maintainableSection title="Guidelines And Purpose">
                <maintainableItems>
		            <maintainableField name="accountGuideline.accountExpenseGuidelineText" />
		            <maintainableField name="accountGuideline.accountIncomeGuidelineText" />
		            <maintainableField name="accountGuideline.accountPurposeText" />
                </maintainableItems>
	        </maintainableSection>
	    </maintainableSections>
	</maintenanceDocument>
</dictionaryEntry>
