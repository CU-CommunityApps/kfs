<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "../../../../core/datadictionary/dataDictionary-1_0.dtd">

<dictionaryEntry>
    <maintenanceDocument>
        <businessObjectClass>org.kuali.module.chart.bo.Account</businessObjectClass>
        <maintainableClass>org.kuali.module.chart.maintenance.KualiAccountMaintainableImpl</maintainableClass>
        <businessRulesClass>org.kuali.module.chart.rules.AccountRule</businessRulesClass>
        <preRulesCheckClass>org.kuali.module.chart.rules.AccountPreRules</preRulesCheckClass>
        <documentAuthorizerClass>org.kuali.core.document.authorization.MaintenanceDocumentAuthorizerBase</documentAuthorizerClass>
        <authorizations>
            <authorization action="initiate">
                <workgroups>
                    <workgroup>kualiUniversalGroup</workgroup>
                </workgroups>
            </authorization>
        </authorizations>

        <documentTypeName>AccountMaintenanceDocument</documentTypeName>
        <documentTypeCode>ACCT</documentTypeCode>

		<webScriptFile>../dwr/interface/SubFundGroupService.js</webScriptFile>
		<webScriptFile>../scripts/chart/accountDocument.js</webScriptFile>

        <label>Account</label>

        <summary>Account maintenance document</summary>
        <description>Document used to create or update Account objects</description>
        <help parameterNamespace="KFS-CA" parameterDetailType="Account" parameterName="ACCOUNT" />

        <lockingKeys>
            <field attributeName="chartOfAccountsCode" />
            <field attributeName="accountNumber" />
        </lockingKeys>

        <defaultExistenceChecks>
            <reference attributeName="organization" activeIndicatorAttributeName="organizationActiveIndicator" attributeToHighlightOnFail="organizationCode" />
            <reference attributeName="accountPhysicalCampus" attributeToHighlightOnFail="accountPhysicalCampusCode" />
            <reference attributeName="accountState" attributeToHighlightOnFail="accountStateCode" />
            <reference attributeName="postalZipCode" attributeToHighlightOnFail="accountZipCode" />
            <reference attributeName="accountType" attributeToHighlightOnFail="accountTypeCode" />
            <reference attributeName="financialHigherEdFunction" attributeToHighlightOnFail="financialHigherEdFunctionCd" />
            <reference attributeName="reportsToAccount" activeIndicatorAttributeName="accountClosedIndicator" activeIndicatorReversed="true" attributeToHighlightOnFail="reportsToAccountNumber" />
            <reference attributeName="continuationAccount" activeIndicatorAttributeName="accountClosedIndicator" activeIndicatorReversed="true" attributeToHighlightOnFail="continuationAccountNumber" />
            <reference attributeName="endowmentIncomeAccount" activeIndicatorAttributeName="accountClosedIndicator" activeIndicatorReversed="true" attributeToHighlightOnFail="endowmentIncomeAccountNumber" />
            <reference attributeName="indirectCostRecoveryAcct" activeIndicatorAttributeName="accountClosedIndicator" activeIndicatorReversed="true" attributeToHighlightOnFail="indirectCostRecoveryAcctNbr" />
            <reference attributeName="sufficientFundsCode" attributeToHighlightOnFail="accountSufficientFundsCode" />
            <reference attributeName="accountFiscalOfficerUser" attributeToHighlightOnFail="accountFiscalOfficerUser.personUserIdentifier" />
            <reference attributeName="accountSupervisoryUser" attributeToHighlightOnFail="accountSupervisoryUser.personUserIdentifier" />
            <reference attributeName="accountManagerUser" attributeToHighlightOnFail="accountManagerUser.personUserIdentifier" />
            <reference attributeName="accountRestrictedStatus" attributeToHighlightOnFail="accountRestrictedStatusCode" />
            <reference attributeName="subFundGroup" attributeToHighlightOnFail="subFundGroupCode" />
            <reference attributeName="budgetRecordingLevel" activeIndicatorAttributeName="active" attributeToHighlightOnFail="budgetRecordingLevelCode" />
            <reference attributeName="acctIndirectCostRcvyType" activeIndicatorAttributeName="active" attributeToHighlightOnFail="acctIndirectCostRcvyTypeCd" />
        </defaultExistenceChecks>

        <maintainableSections>
            <maintainableSection title="Account Maintenance">
                <maintainableItems>
                    <maintainableField name="chartOfAccountsCode" required="true" />
                    <maintainableField name="accountNumber" required="true" />
                    <maintainableField name="accountName" required="true" />
                    <maintainableField name="organizationCode" required="true" />
                    <maintainableField name="accountPhysicalCampusCode" required="true" />
                    <maintainableField name="accountCreateDate" />
                    <maintainableField name="accountEffectiveDate" required="true">
                        <defaultValueFinderClass>org.kuali.kfs.lookup.valuefinder.CurrentDateMMDDYYYYFinder</defaultValueFinderClass>
                    </maintainableField>
                    <maintainableField name="accountExpirationDate" required="false" />
                    <maintainableField name="accountZipCode" required="true" />
                    <maintainableField name="accountCityName" required="true" />
                    <maintainableField name="accountStateCode" required="true" />
                    <maintainableField name="accountStreetAddress" required="true" />
                    <maintainableField name="accountOffCampusIndicator" required="false" />
                    <maintainableField name="accountClosedIndicator" required="false" />
                    <maintainableField name="accountTypeCode" required="true" />
                    <maintainableField name="subFundGroupCode" required="true" webUILeaveFieldFunction="onblur_subFundGroup" webUILeaveFieldCallbackFunction="checkRestrictedStatusCode_Callback" />
                    <maintainableField name="accountsFringesBnftIndicator" required="false">
                        <defaultValue>true</defaultValue>
                    </maintainableField>
                    <maintainableField name="reportsToChartOfAccountsCode" required="false" />
                    <maintainableField name="reportsToAccountNumber" required="false" />
                    <maintainableField name="financialHigherEdFunctionCd" required="true" />
                    <maintainableField name="accountRestrictedStatusCode" required="true"  webUILeaveFieldFunction="onblur_accountRestrictedStatusCode" webUILeaveFieldCallbackFunction="checkRestrictedStatusCode_Callback" />
                    <maintainableField name="accountRestrictedStatusDate" required="false" />
                    <maintainableField name="endowmentIncomeAcctFinCoaCd" required="false" />
                    <maintainableField name="endowmentIncomeAccountNumber" required="false" />
                    <maintainableField name="versionNumber" />
                    <maintainableField name="accountGuideline.versionNumber" />
                    <maintainableField name="accountDescription.versionNumber" />
                </maintainableItems>
            </maintainableSection>
            <maintainableSection title="Account Responsibility">
                <maintainableItems>
                    <maintainableField name="accountFiscalOfficerSystemIdentifier" required="true" readOnly="true" />
                    <maintainableField name="accountFiscalOfficerUser.personUserIdentifier" required="true" />
                    <maintainableField name="accountFiscalOfficerUser.personName" required="false" readOnly="true" />
                    <maintainableField name="accountsSupervisorySystemsIdentifier" required="true" readOnly="true" />
                    <maintainableField name="accountSupervisoryUser.personUserIdentifier" required="true" />
                    <maintainableField name="accountSupervisoryUser.personName" required="false" readOnly="true" />
                    <maintainableField name="accountManagerSystemIdentifier" required="true" readOnly="true" />
                    <maintainableField name="accountManagerUser.personUserIdentifier" required="true" />
                    <maintainableField name="accountManagerUser.personName" required="false" readOnly="true" />
                    <maintainableField name="continuationFinChrtOfAcctCd" required="false" />
                    <maintainableField name="continuationAccountNumber" required="false" />
                    <maintainableField name="incomeStreamFinancialCoaCode" required="false" />
                    <maintainableField name="incomeStreamAccountNumber" required="false" />
                    <maintainableField name="budgetRecordingLevelCode" required="true" />
                    <maintainableField name="accountSufficientFundsCode" required="true" />
                    <maintainableField name="pendingAcctSufficientFundsIndicator" required="false" />
                    <maintainableField name="extrnlFinEncumSufficntFndIndicator" required="false" />
                    <maintainableField name="intrnlFinEncumSufficntFndIndicator" required="false" />
                    <maintainableField name="finPreencumSufficientFundIndicator" required="false" />
                    <maintainableField name="financialObjectivePrsctrlIndicator" required="false" />
                </maintainableItems>
            </maintainableSection>
            <maintainableSection title="Contracts And Grants">
                <maintainableItems>
                    <maintainableField name="contractControlFinCoaCode" required="false" />
                    <maintainableField name="contractControlAccountNumber" required="false" />
                    <maintainableField name="acctIndirectCostRcvyTypeCd" required="false" />
                    <maintainableField name="financialIcrSeriesIdentifier" required="false" >
                         <overrideLookupClass>org.kuali.module.chart.bo.IcrAutomatedEntry</overrideLookupClass>
                         <overrideFieldConversions>financialIcrSeriesIdentifier:financialIcrSeriesIdentifier</overrideFieldConversions>
                    </maintainableField>  
                    <maintainableField name="indirectCostRcvyFinCoaCode" required="false" />
                    <maintainableField name="indirectCostRecoveryAcctNbr" required="false" />
                    <maintainableField name="accountCfdaNumber" required="false" />
                </maintainableItems>
            </maintainableSection>
            <maintainableSection title="Guidelines And Purpose">
                <maintainableItems>
                    <maintainableField name="accountGuideline.accountExpenseGuidelineText" required="true" />
                    <maintainableField name="accountGuideline.accountIncomeGuidelineText" required="true" />
                    <maintainableField name="accountGuideline.accountPurposeText" required="true" />
                </maintainableItems>
            </maintainableSection>
            <maintainableSection title="Account Description">
                <maintainableItems>
                    <maintainableField name="accountDescription.campusDescription" required="false" />
                    <maintainableField name="accountDescription.organizationDescription" required="false" />
                    <maintainableField name="accountDescription.responsibilityCenterDescription" required="false" />
                    <maintainableField name="accountDescription.campusCode" required="false" />
                    <maintainableField name="accountDescription.buildingCode" required="false" />
                </maintainableItems>
            </maintainableSection>
        </maintainableSections>
        
        <workflowProperties>
        	<workflowPropertyGroup>
        		<workflowProperty path="newMaintainableObject.businessObject" />
        		<workflowProperty path="oldMaintainableObject.businessObject" />
        	</workflowPropertyGroup>
        </workflowProperties>
    </maintenanceDocument>
</dictionaryEntry>
