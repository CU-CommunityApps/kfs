<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="AccountMaintenanceDocument" parent="AccountMaintenanceDocument-parentBean" />

  <bean id="AccountMaintenanceDocument-parentBean" abstract="true" parent="MaintenanceDocumentEntry">
    <property name="businessObjectClass" value="org.kuali.module.chart.bo.Account" />
    <property name="maintainableClass" value="org.kuali.module.chart.maintenance.KualiAccountMaintainableImpl" />
    <property name="maintainableSections" >
      <list>
        <ref bean="AccountMaintenanceDocument-AccountMaintenance" />
        <ref bean="AccountMaintenanceDocument-AccountResponsibility" />
        <ref bean="AccountMaintenanceDocument-ContractsAndGrants" />
        <ref bean="AccountMaintenanceDocument-GuidelinesAndPurpose" />
        <ref bean="AccountMaintenanceDocument-AccountDescription" />
      </list>
    </property>
    <property name="defaultExistenceChecks" >
      <list>
        <bean parent="ReferenceDefinition"
              p:attributeName="organization"
              p:activeIndicatorAttributeName="organizationActiveIndicator"
              p:attributeToHighlightOnFail="organizationCode" />
        <bean parent="ReferenceDefinition"
              p:attributeName="accountPhysicalCampus"
              p:attributeToHighlightOnFail="accountPhysicalCampusCode" />
        <bean parent="ReferenceDefinition"
              p:attributeName="accountState"
              p:attributeToHighlightOnFail="accountStateCode" />
        <bean parent="ReferenceDefinition"
              p:attributeName="postalZipCode"
              p:attributeToHighlightOnFail="accountZipCode" />
        <bean parent="ReferenceDefinition"
              p:attributeName="accountType"
              p:attributeToHighlightOnFail="accountTypeCode" />
        <bean parent="ReferenceDefinition"
              p:attributeName="financialHigherEdFunction"
              p:attributeToHighlightOnFail="financialHigherEdFunctionCd" />
        <bean parent="ReferenceDefinition"
              p:attributeName="reportsToAccount"
              p:activeIndicatorAttributeName="accountClosedIndicator"
              p:activeIndicatorReversed="true"
              p:attributeToHighlightOnFail="reportsToAccountNumber" />
        <bean parent="ReferenceDefinition"
              p:attributeName="continuationAccount"
              p:activeIndicatorAttributeName="accountClosedIndicator"
              p:activeIndicatorReversed="true"
              p:attributeToHighlightOnFail="continuationAccountNumber" />
        <bean parent="ReferenceDefinition"
              p:attributeName="endowmentIncomeAccount"
              p:activeIndicatorAttributeName="accountClosedIndicator"
              p:activeIndicatorReversed="true"
              p:attributeToHighlightOnFail="endowmentIncomeAccountNumber" />
        <bean parent="ReferenceDefinition"
              p:attributeName="indirectCostRecoveryAcct"
              p:activeIndicatorAttributeName="accountClosedIndicator"
              p:activeIndicatorReversed="true"
              p:attributeToHighlightOnFail="indirectCostRecoveryAcctNbr" />
        <bean parent="ReferenceDefinition"
              p:attributeName="sufficientFundsCode"
              p:attributeToHighlightOnFail="accountSufficientFundsCode" />
        <bean parent="ReferenceDefinition"
              p:attributeName="accountFiscalOfficerUser"
              p:attributeToHighlightOnFail="accountFiscalOfficerUser.personUserIdentifier" />
        <bean parent="ReferenceDefinition"
              p:attributeName="accountSupervisoryUser"
              p:attributeToHighlightOnFail="accountSupervisoryUser.personUserIdentifier" />
        <bean parent="ReferenceDefinition"
              p:attributeName="accountManagerUser"
              p:attributeToHighlightOnFail="accountManagerUser.personUserIdentifier" />
        <bean parent="ReferenceDefinition"
              p:attributeName="accountRestrictedStatus"
              p:attributeToHighlightOnFail="accountRestrictedStatusCode" />
        <bean parent="ReferenceDefinition"
              p:attributeName="subFundGroup"
              p:attributeToHighlightOnFail="subFundGroupCode" />
        <bean parent="ReferenceDefinition"
              p:attributeName="budgetRecordingLevel"
              p:activeIndicatorAttributeName="active"
              p:attributeToHighlightOnFail="budgetRecordingLevelCode" />
        <bean parent="ReferenceDefinition"
              p:attributeName="acctIndirectCostRcvyType"
              p:activeIndicatorAttributeName="active"
              p:attributeToHighlightOnFail="acctIndirectCostRcvyTypeCd" />
      </list>
    </property>
    <property name="lockingKeys" >
      <list>
        <value>chartOfAccountsCode</value>
        <value>accountNumber</value>
      </list>
    </property>
    <property name="label" value="Account" />
    <property name="shortLabel" value="Account" />
    <property name="summary" value="Account maintenance document" />
    <property name="description" value="Document used to create or update Account objects" />
    <property name="helpDefinition" >
      <bean parent="HelpDefinition"
            p:parameterNamespace="KFS-CA"
            p:parameterName="ACCOUNT"
            p:parameterDetailType="Account" />
    </property>
    <property name="documentTypeName" value="AccountMaintenanceDocument" />
    <property name="businessRulesClass" value="org.kuali.module.chart.rules.AccountRule" />
    <property name="documentAuthorizerClass" value="org.kuali.core.document.authorization.MaintenanceDocumentAuthorizerBase" />
    <property name="preRulesCheckClass" value="org.kuali.module.chart.rules.AccountPreRules" />
    <property name="documentTypeCode" value="ACCT" />
    <property name="webScriptFiles" >
      <list>
        <value>../dwr/interface/SubFundGroupService.js</value>
        <value>../scripts/chart/accountDocument.js</value>
      </list>
    </property>
    <property name="workflowProperties" >
      <ref bean="AccountMaintenanceDocument-workflowProperties" />
    </property>
    <property name="authorizations" >
      <list>
        <bean parent="AuthorizationDefinition">
          <property name="action" value="initiate" />
          <property name="authorizedGroups" >
            <set>
              <value>kualiUniversalGroup</value>
            </set>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Maintenance Section Definitions -->


  <bean id="AccountMaintenanceDocument-AccountMaintenance" parent="AccountMaintenanceDocument-AccountMaintenance-parentBean" />

  <bean id="AccountMaintenanceDocument-AccountMaintenance-parentBean" abstract="true" parent="MaintainableSectionDefinition">
    <property name="id" value="Account Maintenance" />
    <property name="title" value="Account Maintenance" />
    <property name="maintainableItems" >
      <list>
        <bean parent="MaintainableFieldDefinition"
              p:name="chartOfAccountsCode"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="accountNumber"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="accountName"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="organizationCode"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="accountPhysicalCampusCode"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="accountCreateDate" />
        <bean parent="MaintainableFieldDefinition"
              p:name="accountEffectiveDate"
              p:required="true"
              p:defaultValueFinderClass="org.kuali.kfs.lookup.valuefinder.CurrentDateMMDDYYYYFinder" />
        <bean parent="MaintainableFieldDefinition"
              p:name="accountExpirationDate" />
        <bean parent="MaintainableFieldDefinition"
              p:name="accountZipCode"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="accountCityName"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="accountStateCode"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="accountStreetAddress"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="accountOffCampusIndicator" />
        <bean parent="MaintainableFieldDefinition"
              p:name="accountClosedIndicator" />
        <bean parent="MaintainableFieldDefinition"
              p:name="accountTypeCode"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="subFundGroupCode"
              p:required="true"
              p:webUILeaveFieldCallbackFunction="checkRestrictedStatusCode_Callback"
              p:webUILeaveFieldFunction="onblur_subFundGroup" />
        <bean parent="MaintainableFieldDefinition"
              p:name="accountsFringesBnftIndicator"
              p:defaultValue="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="reportsToChartOfAccountsCode" />
        <bean parent="MaintainableFieldDefinition"
              p:name="reportsToAccountNumber" />
        <bean parent="MaintainableFieldDefinition"
              p:name="financialHigherEdFunctionCd"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="accountRestrictedStatusCode"
              p:required="true"
              p:webUILeaveFieldCallbackFunction="checkRestrictedStatusCode_Callback"
              p:webUILeaveFieldFunction="onblur_accountRestrictedStatusCode" />
        <bean parent="MaintainableFieldDefinition"
              p:name="accountRestrictedStatusDate" />
        <bean parent="MaintainableFieldDefinition"
              p:name="endowmentIncomeAcctFinCoaCd" />
        <bean parent="MaintainableFieldDefinition"
              p:name="endowmentIncomeAccountNumber" />
        <bean parent="MaintainableFieldDefinition"
              p:name="versionNumber" />
        <bean parent="MaintainableFieldDefinition"
              p:name="accountGuideline.versionNumber" />
        <bean parent="MaintainableFieldDefinition"
              p:name="accountDescription.versionNumber" />
      </list>
    </property>
  </bean>
  <bean id="AccountMaintenanceDocument-AccountResponsibility" parent="AccountMaintenanceDocument-AccountResponsibility-parentBean" />

  <bean id="AccountMaintenanceDocument-AccountResponsibility-parentBean" abstract="true" parent="MaintainableSectionDefinition">
    <property name="id" value="Account Responsibility" />
    <property name="title" value="Account Responsibility" />
    <property name="maintainableItems" >
      <list>
        <bean parent="MaintainableFieldDefinition"
              p:name="accountFiscalOfficerSystemIdentifier"
              p:readOnly="true"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="accountFiscalOfficerUser.personUserIdentifier"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="accountFiscalOfficerUser.personName"
              p:readOnly="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="accountsSupervisorySystemsIdentifier"
              p:readOnly="true"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="accountSupervisoryUser.personUserIdentifier"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="accountSupervisoryUser.personName"
              p:readOnly="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="accountManagerSystemIdentifier"
              p:readOnly="true"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="accountManagerUser.personUserIdentifier"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="accountManagerUser.personName"
              p:readOnly="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="continuationFinChrtOfAcctCd" />
        <bean parent="MaintainableFieldDefinition"
              p:name="continuationAccountNumber" />
        <bean parent="MaintainableFieldDefinition"
              p:name="incomeStreamFinancialCoaCode" />
        <bean parent="MaintainableFieldDefinition"
              p:name="incomeStreamAccountNumber" />
        <bean parent="MaintainableFieldDefinition"
              p:name="budgetRecordingLevelCode"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="accountSufficientFundsCode"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="pendingAcctSufficientFundsIndicator" />
        <bean parent="MaintainableFieldDefinition"
              p:name="extrnlFinEncumSufficntFndIndicator" />
        <bean parent="MaintainableFieldDefinition"
              p:name="intrnlFinEncumSufficntFndIndicator" />
        <bean parent="MaintainableFieldDefinition"
              p:name="finPreencumSufficientFundIndicator" />
        <bean parent="MaintainableFieldDefinition"
              p:name="financialObjectivePrsctrlIndicator" />
      </list>
    </property>
  </bean>
  <bean id="AccountMaintenanceDocument-ContractsAndGrants" parent="AccountMaintenanceDocument-ContractsAndGrants-parentBean" />

  <bean id="AccountMaintenanceDocument-ContractsAndGrants-parentBean" abstract="true" parent="MaintainableSectionDefinition">
    <property name="id" value="Contracts And Grants" />
    <property name="title" value="Contracts And Grants" />
    <property name="maintainableItems" >
      <list>
        <bean parent="MaintainableFieldDefinition"
              p:name="contractControlFinCoaCode" />
        <bean parent="MaintainableFieldDefinition"
              p:name="contractControlAccountNumber" />
        <bean parent="MaintainableFieldDefinition"
              p:name="acctIndirectCostRcvyTypeCd" />
        <bean parent="MaintainableFieldDefinition"
              p:name="financialIcrSeriesIdentifier"
              p:overrideFieldConversions="financialIcrSeriesIdentifier:financialIcrSeriesIdentifier"
              p:overrideLookupClass="org.kuali.module.chart.bo.IcrAutomatedEntry" />
        <bean parent="MaintainableFieldDefinition"
              p:name="indirectCostRcvyFinCoaCode" />
        <bean parent="MaintainableFieldDefinition"
              p:name="indirectCostRecoveryAcctNbr" />
        <bean parent="MaintainableFieldDefinition"
              p:name="accountCfdaNumber" />
      </list>
    </property>
  </bean>
  <bean id="AccountMaintenanceDocument-GuidelinesAndPurpose" parent="AccountMaintenanceDocument-GuidelinesAndPurpose-parentBean" />

  <bean id="AccountMaintenanceDocument-GuidelinesAndPurpose-parentBean" abstract="true" parent="MaintainableSectionDefinition">
    <property name="id" value="Guidelines And Purpose" />
    <property name="title" value="Guidelines And Purpose" />
    <property name="maintainableItems" >
      <list>
        <bean parent="MaintainableFieldDefinition"
              p:name="accountGuideline.accountExpenseGuidelineText"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="accountGuideline.accountIncomeGuidelineText"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="accountGuideline.accountPurposeText"
              p:required="true" />
      </list>
    </property>
  </bean>
  <bean id="AccountMaintenanceDocument-AccountDescription" parent="AccountMaintenanceDocument-AccountDescription-parentBean" />

  <bean id="AccountMaintenanceDocument-AccountDescription-parentBean" abstract="true" parent="MaintainableSectionDefinition">
    <property name="id" value="Account Description" />
    <property name="title" value="Account Description" />
    <property name="maintainableItems" >
      <list>
        <bean parent="MaintainableFieldDefinition"
              p:name="accountDescription.campusDescription" />
        <bean parent="MaintainableFieldDefinition"
              p:name="accountDescription.organizationDescription" />
        <bean parent="MaintainableFieldDefinition"
              p:name="accountDescription.responsibilityCenterDescription" />
        <bean parent="MaintainableFieldDefinition"
              p:name="accountDescription.campusCode" />
        <bean parent="MaintainableFieldDefinition"
              p:name="accountDescription.buildingCode" />
      </list>
    </property>
  </bean>

<!-- Exported Workflow Properties -->


  <bean id="AccountMaintenanceDocument-workflowProperties" parent="AccountMaintenanceDocument-workflowProperties-parentBean" />

  <bean id="AccountMaintenanceDocument-workflowProperties-parentBean" abstract="true" parent="WorkflowProperties">
    <property name="workflowPropertyGroups" >
      <list>
        <bean parent="WorkflowPropertyGroup">
          <property name="workflowProperties" >
            <list>
              <bean parent="WorkflowProperty"
                    p:path="newMaintainableObject.businessObject" />
              <bean parent="WorkflowProperty"
                    p:path="oldMaintainableObject.businessObject" />
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>
</beans>
