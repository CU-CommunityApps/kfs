<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">
<beans>
    <bean id="balanceTypService" class="org.kuali.module.chart.service.impl.BalanceTypServiceImpl">
        <property name="kualiCodeService">
            <ref bean="kualiCodeService" />
        </property>
    </bean>

    <bean id="accountingPeriodService" class="org.kuali.module.chart.service.impl.AccountingPeriodServiceImpl">
        <property name="businessObjectService">
            <ref bean="businessObjectService" />
        </property>
    </bean>

    <bean id="subFundGroupService" class="org.kuali.module.chart.service.impl.SubFundGroupServiceImpl">
        <property name="subFundGroupDao">
