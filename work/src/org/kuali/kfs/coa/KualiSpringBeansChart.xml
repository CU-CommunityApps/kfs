    <bean id="priorYearAccountService" class="org.kuali.module.chart.service.impl.PrioYearAccountServiceImpl">
        <property name="priorYearAccountDao">
            <ref bean="priorYearAccountDao" />
        </property>
    </bean>

    <bean id="organizationReversionService" class="org.kuali.module.chart.service.impl.OrganizationReversionServiceImpl">
