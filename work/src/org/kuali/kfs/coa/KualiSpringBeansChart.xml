            <ref bean="organizationReversionDao" />
        </property>
        <property name="kualiConfigurationService">
            <ref bean="kualiConfigurationService" />
        </property>
    </bean>

    <bean id="organizationReversionDao" class="org.kuali.module.chart.dao.ojb.OrganizationReversionDaoOjb">
        <property name="jcdAlias">
