<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "../../../core/datadictionary/dataDictionary-1_0.dtd">

<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.module.chart.bo.Chart">


        <inquiry>
            <title>Chart Inquiry</title>

            <inquirySections><inquirySection title="Header Text"><inquiryFields>
                <field attributeName="chartOfAccountsCode" />
                <field attributeName="finCoaManagerUniversal.personName" />
                <field attributeName="finChartOfAccountDescription" />
                <field attributeName="finChartOfAccountActiveIndicator" />
                <field attributeName="financialCashObjectCode" />
                <field attributeName="finAccountsPayableObjectCode" />
                <field attributeName="incBdgtEliminationsFinObjCd" />
                <field attributeName="expBdgtEliminationsFinObjCd" />
                <field attributeName="reportsToChartOfAccountsCode" />
                <field attributeName="finAccountsReceivableObjCode" />
                <field attributeName="finInternalEncumbranceObjCd" />
                <field attributeName="finExternalEncumbranceObjCd" />
                <field attributeName="finPreEncumbranceObjectCode" />
                <field attributeName="icrIncomeFinancialObjectCode" />
                <field attributeName="icrExpenseFinancialObjectCd" />
                <field attributeName="fundBalanceObjectCode" />
            </inquiryFields></inquirySection></inquirySections>
        </inquiry>

        <lookup>
            <title>Chart Lookup</title>

            <instructions>Lookup a Chart</instructions>

            <defaultSort attributeName="chartOfAccountsCode" sortAscending="true" />

            <lookupFields>
                <lookupField attributeName="chartOfAccountsCode" required="false" noLookup="true" />
                <lookupField attributeName="finChartOfAccountDescription" />
                <lookupField attributeName="finChartOfAccountActiveIndicator">
                    <defaultValue>Y</defaultValue>
                </lookupField>
                <lookupField attributeName="financialCashObjectCode" />
                <lookupField attributeName="finAccountsPayableObjectCode" />
            </lookupFields>

            <resultFields>
                <field attributeName="chartOfAccountsCode" />
                <field attributeName="finChartOfAccountDescription" />
                <field attributeName="finChartOfAccountActiveIndicator" />
                <field attributeName="financialCashObjectCode" />
                <field attributeName="finAccountsPayableObjectCode" />
                <field attributeName="finCoaManagerUniversal.personName" />
            </resultFields>
        </lookup>

        <titleAttribute>chartOfAccountsCode</titleAttribute>

        <!--
            Note that this has alot of items that should 
            be converted to <attributeReference> tags, but until the 
            underlying BO is updated to use real sub-objects rather 
            than just strings, doing that will cause this DD to break.
        -->
        <attributes>
            <attribute name="chartOfAccountsCode" forceUppercase="true">
                <label>Chart Code</label>
                <shortLabel>Chart</shortLabel>

                <maxLength>2</maxLength>
                <validationPattern>
                    <alpha exactLength="2" />
                </validationPattern>
                <required>true</required>

                <control>
                    <text size="2" />
                </control>
            </attribute>

            <attribute name="codeAndDescription" forceUppercase="false">
                <label>Chart Code</label>
                <shortLabel>Chart Code</shortLabel>

                <maxLength>43</maxLength>

                <control>
                    <text size="43" />
                </control>
            </attribute>

            <attributeReference name="finCoaManagerUniversalId" sourceClassName="org.kuali.core.bo.user.UniversalUser" sourceAttributeName="personUniversalIdentifier">
                <label>Chart Manager Employee Id</label>
                <shortLabel>Chart Manager</shortLabel>
                <displayLabelAttribute>finCoaManagerUniversal.personUserIdentifier</displayLabelAttribute>
                <required>true</required>
                <control>
                    <hidden />
                </control>
            </attributeReference>

            <attributeReference name="finCoaManagerUniversal.personUserIdentifier" sourceClassName="org.kuali.core.bo.user.UniversalUser" sourceAttributeName="personUserIdentifier">
                <label>Chart Manager UserID</label>
                <shortLabel>UserID</shortLabel>
                <required>false</required>
                <control>
                    <kualiUser universalIdAttributeName="finCoaManagerUniversalId" userIdAttributeName="finCoaManagerUniversal.personUserIdentifier" personNameAttributeName="finCoaManagerUniversal.personName" />
                </control>
            </attributeReference>

            <attributeReference name="finCoaManagerUniversal.personName" sourceClassName="org.kuali.core.bo.user.UniversalUser" sourceAttributeName="personName">
                <label>Chart Manager Name</label>
                <shortLabel>Name</shortLabel>
                <required>false</required>
                <control>
                    <hidden />
                </control>
            </attributeReference>

            <attribute name="finChartOfAccountDescription" forceUppercase="false">
                <label>Chart Description</label>
                <shortLabel>Description</shortLabel>

                <maxLength>40</maxLength>
                <required>true</required>
                <control>
                    <text size="40" />
                </control>
            </attribute>
            <attributeReference name="finChartOfAccountActiveIndicator" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="activeIndicator">
                <label>Chart Active Indicator</label>
                <shortLabel>ChartActive</shortLabel>
                <required>false</required>
            </attributeReference>

            <attributeReference name="financialCashObjectCode" sourceClassName="org.kuali.module.chart.bo.ObjectCode" sourceAttributeName="financialObjectCode">
                <label>Cash Object Code</label>
                <shortLabel>CashObjCode</shortLabel>

                <maxLength>4</maxLength>
                <required>false</required>

                <control>
                    <text size="4" />
                </control>
            </attributeReference>

            <attributeReference name="finAccountsPayableObjectCode" sourceClassName="org.kuali.module.chart.bo.ObjectCode" sourceAttributeName="financialObjectCode">
                <label>Accounts Payable Object Code</label>
                <shortLabel>APObjectCode</shortLabel>

                <maxLength>4</maxLength>
                <required>false</required>

                <control>
                    <text size="4" />
                </control>
            </attributeReference>

            <attributeReference name="incBdgtEliminationsFinObjCd" sourceClassName="org.kuali.module.chart.bo.ObjectCode" sourceAttributeName="financialObjectCode">
                <label>Income Budget Elimination Object Code</label>
                <shortLabel>incBudgElimObjCode</shortLabel>

                <maxLength>4</maxLength>
                <required>false</required>

                <control>
                    <text size="4" />
                </control>
            </attributeReference>

            <attributeReference name="expBdgtEliminationsFinObjCd" sourceClassName="org.kuali.module.chart.bo.ObjectCode" sourceAttributeName="financialObjectCode">
                <label>Expense Budget Elimination Object Code</label>
                <shortLabel>expBudgElimObjCode</shortLabel>

                <maxLength>4</maxLength>
                <required>false</required>

                <control>
                    <text size="4" />
                </control>
            </attributeReference>

            <attributeReference name="reportsToChartOfAccountsCode" sourceClassName="org.kuali.module.chart.bo.Chart" sourceAttributeName="chartOfAccountsCode">
                <label>Reports To Chart of Accounts Code</label>
                <shortLabel>rptsToChartCode</shortLabel>

                <maxLength>2</maxLength>
                <required>true</required>

                <control>
                	<text size="2"/>
                </control>
            </attributeReference>

            <attributeReference name="finAccountsReceivableObjCode" sourceClassName="org.kuali.module.chart.bo.ObjectCode" sourceAttributeName="financialObjectCode">
                <label>Accounts Receivable Object Code</label>
                <shortLabel>acctsReceivObjCode</shortLabel>

                <maxLength>4</maxLength>
                <required>false</required>

                <control>
                    <text size="4" />
                </control>
            </attributeReference>

            <attributeReference name="finInternalEncumbranceObjCd" sourceClassName="org.kuali.module.chart.bo.ObjectCode" sourceAttributeName="financialObjectCode">
                <label>Internal Encumbrances Object Code</label>
                <shortLabel>intEncumObjCode</shortLabel>

                <maxLength>4</maxLength>
                <required>false</required>

                <control>
                    <text size="4" />
                </control>
            </attributeReference>

            <attributeReference name="finExternalEncumbranceObjCd" sourceClassName="org.kuali.module.chart.bo.ObjectCode" sourceAttributeName="financialObjectCode">
                <label>External Encumbrances Object Code</label>
                <shortLabel>exEncumObjCode</shortLabel>

                <maxLength>4</maxLength>
                <required>false</required>

                <control>
                    <text size="4" />
                </control>
            </attributeReference>

            <attributeReference name="finPreEncumbranceObjectCode" sourceClassName="org.kuali.module.chart.bo.ObjectCode" sourceAttributeName="financialObjectCode">
                <label>Pre Encumbrances Object Code</label>
                <shortLabel>PreEncumObjCode</shortLabel>

                <maxLength>4</maxLength>
                <required>false</required>

                <control>
                    <text size="4" />
                </control>
            </attributeReference>

            <attributeReference name="icrIncomeFinancialObjectCode" sourceClassName="org.kuali.module.chart.bo.ObjectCode" sourceAttributeName="financialObjectCode">
                <label>Indirect Cost Recovery Income Object Code</label>
                <shortLabel>IndirecCostRecovIncObjCde</shortLabel>

                <maxLength>4</maxLength>
                <required>false</required>

                <control>
                    <text size="4" />
                </control>
            </attributeReference>
            <attributeReference name="icrExpenseFinancialObjectCd" sourceClassName="org.kuali.module.chart.bo.ObjectCode" sourceAttributeName="financialObjectCode">
                <label>Indirect Cost Recovery Expense Object Code</label>
                <shortLabel>IndirecCostRecovExpObjCde</shortLabel>

                <maxLength>4</maxLength>
                <required>false</required>

                <control>
                    <text size="4" />
                </control>
            </attributeReference>

            <attributeReference name="fundBalanceObjectCode" sourceClassName="org.kuali.module.chart.bo.ObjectCode" sourceAttributeName="financialObjectCode">
                <label>Fund Balance Object Code</label>
                <shortLabel>FndBalObjCde</shortLabel>

                <maxLength>4</maxLength>
                <required>false</required>

                <control>
                    <text size="4" />
                </control>
            </attributeReference>

            <attributeReference name="versionNumber" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="versionNumber" />

        </attributes>

        <relationships>
            <relationship objectAttributeName="finCoaManagerUniversal" >
                <primitiveAttribute sourceName="finCoaManagerUniversalId" targetName="personUniversalIdentifier" />
				<supportAttribute sourceName="finCoaManagerUniversal.personUserIdentifier" targetName="personUserIdentifier" identifier="true" />
				<supportAttribute sourceName="finCoaManagerUniversal.personName" targetName="personName" />
            </relationship>
        </relationships>

        <objectLabel>Chart</objectLabel>
    </businessObject>
</dictionaryEntry>

