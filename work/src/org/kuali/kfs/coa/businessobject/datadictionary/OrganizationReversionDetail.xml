<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "../../../core/datadictionary/dataDictionary-1_0.dtd">
<dictionaryEntry>
	<businessObject businessObjectClass="org.kuali.module.chart.bo.OrganizationReversionDetail">
		<customAttributeEnabled>false</customAttributeEnabled>
		<inquiry>
			<title>Organization Reversion Detail Inquiry</title>
			<inquiryFields>
				<field attributeName="universityFiscalYear" />
				<field attributeName="chartOfAccountsCode" />			
				<field attributeName="organizationCode"  />
				<field attributeName="organizationReversionCategoryCode"  />
				<field attributeName="organizationReversionObjectCode"  />	
				<field attributeName="organizationReversionCode"  />						 				
			</inquiryFields>
		</inquiry>

		<lookup>
			<title>Organization Reversion Detail Lookup</title>

			<menubar><![CDATA[<a href="maintenance.do?methodToCall=start&businessObjectClassName=org.kuali.module.chart.bo.OrganizationReversionDetail">Create New</a>]]></menubar>

			<instructions>Lookup an Organization Reversion Detail</instructions>

			<defaultSort attributeName="organizationCode" sortAscending="true" />

			<lookupFields>
				<lookupField attributeName="universityFiscalYear" required="false" />
				<lookupField attributeName="chartOfAccountsCode" required="false" />
				<lookupField attributeName="organizationCode" required="false" />
				<lookupField attributeName="organizationReversionCategoryCode" required="false" />
				<lookupField attributeName="organizationReversionObjectCode" required="false" />
				<lookupField attributeName="organizationReversionCode" required="false" />							
			</lookupFields>

			<resultFields>
				<field attributeName="universityFiscalYear" />
				<field attributeName="chartOfAccountsCode" />			
				<field attributeName="organizationCode"  />
				<field attributeName="organizationReversionCategoryCode"  />	
				<field attributeName="organizationReversionObjectCode"  />
				<field attributeName="organizationReversionCode"  />							
			</resultFields>
		</lookup>

		<attributes>
			<attributeReference name="universityFiscalYear" 
								sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" 
								sourceAttributeName="genericFiscalYear">
				<required>true</required>
			</attributeReference>
			
			<attributeReference name="chartOfAccountsCode" 
								sourceClassName="org.kuali.module.chart.bo.Chart" 
								sourceAttributeName="chartOfAccountsCode">							
				<required>true</required>
				<control>
					<select valuesFinderClass="org.kuali.core.lookup.keyvalues.ChartValuesFinder"/>					
				</control>
			</attributeReference>

			<attributeReference name="organizationCode" 
								sourceClassName="org.kuali.module.chart.bo.Org" 
								sourceAttributeName="organizationCode" >
				<description>
				The code used to represent an organization.
				</description>
			</attributeReference>
			
			<attributeReference name="organizationReversionCategoryCode" 
								sourceClassName="org.kuali.module.chart.bo.OrganizationReversionCategory" 
								sourceAttributeName="organizationReversionCategoryCode">
				<label>Organization Reversion Category</label>
				<shortLabel>Category</shortLabel>								
				<required>true</required>
				<control>
					<select valuesFinderClass="org.kuali.core.lookup.keyvalues.OrganizationReversionCategoryValuesFinder"/>					
				</control>
			</attributeReference>

			<attributeReference name="organizationReversionObjectCode" 
								sourceClassName="org.kuali.module.chart.bo.ObjectCode" 
								sourceAttributeName="financialObjectCode">
				<label>Organization Reversion Object Code</label>
				<shortLabel>Object Code</shortLabel>
				<required>true</required>
			</attributeReference>

	    	<attribute name="organizationReversionCode">
	            <label>Org Reversion Code</label>
	            <shortLabel>Org Reversion Code</shortLabel>	         
	            <maxLength>2</maxLength>	            
	            <required>true</required>	            
	            <control>
	                <select valuesFinderClass="org.kuali.core.lookup.keyvalues.OrganizationReversionCodeValuesFinder"/>
	            </control>            
	            <summary><![CDATA[&nbsp;]]></summary>
	            <description>Bank Account Type.</description>
	        </attribute>  
						
 			<attributeReference name="versionNumber" 
 				sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" 
 				sourceAttributeName="versionNumber" />

		</attributes>
	</businessObject>
</dictionaryEntry>
