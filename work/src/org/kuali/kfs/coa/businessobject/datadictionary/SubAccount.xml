<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "../../../core/datadictionary/dataDictionary-1_0.dtd">
<dictionaryEntry>
	<businessObject
		businessObjectClass="org.kuali.module.chart.bo.SubAccount">
		<customAttributeEnabled>false</customAttributeEnabled>

		<inquiry>
			<title>Sub-Account Code Inquiry</title>

			<inquiryFields>
				<field attributeName="chartOfAccountsCode" />
				<field attributeName="accountNumber" />			
				<field attributeName="subAccountNumber" />
				<field attributeName="subAccountName" />			
				<field attributeName="financialReportChartCode" />
				<field attributeName="finReportOrganizationCode" />
				<field attributeName="financialReportingCode" />				
				<field attributeName="subAccountActiveIndicator" />
			</inquiryFields>
		</inquiry>	
		
		<lookup>
			<title>Sub-Account Lookup</title>

			<menubar><![CDATA[<a href="maintenance.do?methodToCall=start&businessObjectClassName=org.kuali.module.chart.bo.SubAccount">Create New</a>]]></menubar>

			<instructions>Lookup a Sub-Account</instructions>

			<defaultSort attributeName="subAccountNumber" sortAscending="true" />

			<lookupFields>
				<lookupField attributeName="chartOfAccountsCode" required="false" />
				<lookupField attributeName="accountNumber" required="false" />
				<lookupField attributeName="subAccountNumber" required="false" />
				<lookupField attributeName="subAccountName" required="false" />
				<lookupField attributeName="financialReportChartCode" required="false" />
				<lookupField attributeName="finReportOrganizationCode" required="false" />								
				<lookupField attributeName="financialReportingCode" required="false" />				
			</lookupFields>

			<resultFields>
				<field attributeName="chartOfAccountsCode" />
				<field attributeName="accountNumber" />
				<field attributeName="subAccountNumber" />
				<field attributeName="subAccountName" />				
				<field attributeName="financialReportChartCode" />
				<field attributeName="finReportOrganizationCode" />								
				<field attributeName="financialReportingCode" />				
				<field attributeName="subAccountActiveIndicator" />				
			</resultFields>
		</lookup>
		
		<titleAttribute>subAccountNumber</titleAttribute>
		
		<attributes>
			
			<attributeReference name="chartOfAccountsCode" 
								sourceClassName="org.kuali.module.chart.bo.Chart" 
								sourceAttributeName="chartOfAccountsCode">
				<required>true</required>
			</attributeReference>

			<attributeReference name="accountNumber" 
								sourceClassName="org.kuali.module.chart.bo.Account" 
								sourceAttributeName="accountNumber">
				<required>true</required>
			</attributeReference>
			
			<attribute name="subAccountNumber"	forceUppercase="true">
				<label>Sub-Account Number</label>
				<shortLabel>
					SubAccount
				</shortLabel>

				<maxLength>5</maxLength>
				<validationPattern><anyCharacter maxLength="5" /></validationPattern>
				<required>true</required>
				
				<control>
					<text size="5" />
				</control>

				<summary>sub-Account Number</summary>
				<description>
					Sub-Account Number
				</description>
			</attribute>
		
			<attribute name="subAccountName" forceUppercase="false">
				<label>Sub-Account Name</label>
				<shortLabel>Su-Acct Name</shortLabel>

				<maxLength>40</maxLength>
				<validationPattern><anyCharacter allowWhitespace="true" /></validationPattern>
				<required>true</required>
				
				<control>
					<text size="40" />
				</control>

				<summary>Name</summary>
				<description>Name</description>
			</attribute>
			
			<attributeReference name="financialReportChartCode" 
								sourceClassName="org.kuali.module.chart.bo.Chart" 
								sourceAttributeName="chartOfAccountsCode">
				<label>Financial Reporting Chart Code</label>
				<shortLabel>Fin. Rpt. Chart</shortLabel>
				<required>true</required>
				<summary>Financial Reporting Chart of Accounts Code</summary>
				<description>
					Financial Reporting Chart of Accounts Code
				</description>
			</attributeReference>

			<attributeReference name="finReportOrganizationCode" 
								sourceClassName="org.kuali.module.chart.bo.Org" 
								sourceAttributeName="organizationCode">
				<label>Financial Reporting Org Code</label>
				<shortLabel>Org</shortLabel>
				<required>true</required>
				<summary>Financial Reporting Organization Code</summary>
				<description>
					Financial Reporting Organization Code
				</description>
			</attributeReference>

			<attributeReference name="financialReportingCode"
								sourceClassName="org.kuali.module.chart.bo.ReportingCodes" 
								sourceAttributeName="financialReportingCode">
				<label>Financial Reporting Code</label>
				<shortLabel>
					Fin. Rpt. Code
				</shortLabel>
				<required>true</required>
				<summary>Financial Reporting Code</summary>
				<description>
					Financial Reporting Code
				</description>
			</attributeReference>

			<attributeReference name="subAccountActiveIndicator" 
								sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" 
								sourceAttributeName="activeIndicator" />
		
		    <attributeReference name="versionNumber" 
								sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" 
								sourceAttributeName="versionNumber" />
								
		</attributes>
	</businessObject>
</dictionaryEntry>
			