<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "../../../core/datadictionary/dataDictionary-1_0.dtd">
<dictionaryEntry>
	<businessObject
		businessObjectClass="org.kuali.module.chart.bo.AccountGuideline">

		<customAttributeEnabled>false</customAttributeEnabled>

		<inquiry>
			<title>Account Guideline Inquiry</title>

			<inquiryFields>
				<field attributeName="chartOfAccountsCode" />			
				<field attributeName="accountNumber" />
				<field attributeName="accountExpenseGuidelineText" />			
				<field attributeName="accountIncomeGuidelineText" />
				<field attributeName="accountPurposeText" />
			</inquiryFields>
		</inquiry>

		<lookup>
			<title>Account Guideline Lookup</title>

			<menubar><![CDATA[<a href="maintenance.do?methodToCall=start&businessObjectClassName=org.kuali.module.chart.bo.AccountGuideline">Create New</a>]]></menubar>

			<instructions>Lookup an Account Guideline</instructions>

			<defaultSort attributeName="accountNumber" sortAscending="true" />

			<lookupFields>
				<lookupField attributeName="accountNumber" required="false" />
				<lookupField attributeName="chartOfAccountsCode" required="false" />
			</lookupFields>

			<resultFields>
			    <field attributeName="accountNumber" />
				<field attributeName="chartOfAccountsCode" />
				<field attributeName="accountExpenseGuidelineText" />
				<field attributeName="accountIncomeGuidelineText" />
				<field attributeName="accountPurposeText" />
			</resultFields>
		</lookup>

		<attributes>
			<attributeReference name="chartOfAccountsCode" 
								sourceClassName="org.kuali.module.chart.bo.Chart" 
								sourceAttributeName="chartOfAccountsCode" />
								
			<attributeReference name="accountNumber" 
								sourceClassName="org.kuali.module.chart.bo.Account" 
								sourceAttributeName="accountNumber" />
								
			<attributeReference name="accountExpenseGuidelineText" 
								sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" 
								sourceAttributeName="genericBigText">
				<label>Account Expense Guideline Text</label>
				<shortLabel>ExpGuideTxt</shortLabel>
				<summary>Expense Guidelines</summary>
				<description>A textual description of the Expense Guidelines.</description>
			</attributeReference>
			
			<attributeReference name="accountIncomeGuidelineText" 
								sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" 
								sourceAttributeName="genericBigText">
				<label>Account Income Guideline Text</label>
				<shortLabel>IncGuideTxt</shortLabel>
				<summary>Income Guidelines</summary>
				<description>A textual description of the Income Guidelines.</description>
			</attributeReference>
			
			<attributeReference name="accountPurposeText" 
								sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" 
								sourceAttributeName="genericBigText">
				<label>Account Purpose Text</label>
				<shortLabel>AcctPurpTxt</shortLabel>
				<summary>Account Purpose Text</summary>
				<description>A textual description of the Account Purpose.</description>
			</attributeReference>
			
		</attributes>
	</businessObject>
</dictionaryEntry>

