<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="ObjLevel" parent="ObjLevel-parentBean" />

  <bean id="ObjLevel-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.kfs.coa.businessobject.ObjLevel" />
    <property name="inquiryDefinition" >
      <ref bean="ObjLevel-inquiryDefinition" />
    </property>
    <property name="lookupDefinition" >
      <ref bean="ObjLevel-lookupDefinition" />
    </property>
    <property name="titleAttribute" value="financialObjectLevelCode" />
    <property name="objectLabel" value="Object Level" />
    <property name="attributes" >
      <list>
        <ref bean="ObjLevel-chartOfAccountsCode" />
        <ref bean="ObjLevel-financialObjectLevelCode" />
        <ref bean="ObjLevel-financialObjectLevelName" />
        <ref bean="ObjLevel-financialObjectLevelShortNm" />
        <ref bean="ObjLevel-financialConsolidationObjectCode" />
        <ref bean="ObjLevel-active" />
        <ref bean="ObjLevel-financialReportingSortCode" />
      </list>
    </property>
    <property name="inactivationBlockingDefinitions">
      <list>
        <bean parent="InactivationBlockingDefinition"
          p:blockedReferencePropertyName="financialConsolidationObject"/>
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->


  <bean id="ObjLevel-chartOfAccountsCode" parent="ObjLevel-chartOfAccountsCode-parentBean" />

  <bean id="ObjLevel-chartOfAccountsCode-parentBean" abstract="true" parent="Chart-chartOfAccountsCode">
  </bean>
  <bean id="ObjLevel-financialObjectLevelCode" parent="ObjLevel-financialObjectLevelCode-parentBean" />

  <bean id="ObjLevel-financialObjectLevelCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="financialObjectLevelCode" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Level Code" />
    <property name="shortLabel" value="Level Code" />
    <property name="maxLength" value="4" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern"
            p:allowWhitespace="true" />
    </property>
    <property name="required" value="true" />
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="4" />
    </property>
  </bean>
  <bean id="ObjLevel-financialObjectLevelName" parent="ObjLevel-financialObjectLevelName-parentBean" />

  <bean id="ObjLevel-financialObjectLevelName-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="financialObjectLevelName" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Object Level Name" />
    <property name="shortLabel" value="Name" />
    <property name="maxLength" value="40" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern"
            p:allowWhitespace="true" />
    </property>
    <property name="required" value="true" />
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="40" />
    </property>
  </bean>
  <bean id="ObjLevel-financialObjectLevelShortNm" parent="ObjLevel-financialObjectLevelShortNm-parentBean" />

  <bean id="ObjLevel-financialObjectLevelShortNm-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="financialObjectLevelShortNm" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Object Level Short Name" />
    <property name="shortLabel" value="ObjLevelShortName" />
    <property name="maxLength" value="12" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern"
            p:allowWhitespace="true" />
    </property>
    <property name="required" value="true" />
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="12" />
    </property>
  </bean>
  <bean id="ObjLevel-financialConsolidationObjectCode" parent="ObjLevel-financialConsolidationObjectCode-parentBean" />

  <bean id="ObjLevel-financialConsolidationObjectCode-parentBean" abstract="true" parent="ObjectCons-finConsolidationObjectCode">
    <property name="name" value="financialConsolidationObjectCode" />
    <property name="shortLabel" value="Consolidation Code" />
  </bean>
  <bean id="ObjLevel-active" parent="ObjLevel-active-parentBean" />

  <bean id="ObjLevel-active-parentBean" abstract="true" parent="GenericAttributes-activeIndicator">
    <property name="name" value="active" />
    <property name="required" value="true" />
  </bean>
  <bean id="ObjLevel-financialReportingSortCode" parent="ObjLevel-financialReportingSortCode-parentBean" />

  <bean id="ObjLevel-financialReportingSortCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="financialReportingSortCode" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Report Sort Code" />
    <property name="shortLabel" value="SortCode" />
    <property name="maxLength" value="2" />
    <property name="validationPattern" >
      <bean parent="AlphaValidationPattern"
            p:exactLength="2" />
    </property>
    <property name="required" value="true" />
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="2" />
    </property>
  </bean>

<!-- Business Object Inquiry Definition -->


  <bean id="ObjLevel-inquiryDefinition" parent="ObjLevel-inquiryDefinition-parentBean" />

  <bean id="ObjLevel-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
    <property name="title" value="Object Level Inquiry" />
    <property name="inquirySections" >
      <list>
        <bean parent="InquirySectionDefinition">
          <property name="title" value="" />
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields" >
            <list>
              <bean parent="FieldDefinition"
                    p:attributeName="chartOfAccountsCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="financialObjectLevelCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="financialObjectLevelName" />
              <bean parent="FieldDefinition"
                    p:attributeName="financialObjectLevelShortNm" />
              <bean parent="FieldDefinition"
                    p:attributeName="financialConsolidationObjectCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="active" />
              <bean parent="FieldDefinition"
                    p:attributeName="financialReportingSortCode" />
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Business Object Lookup Definition -->


  <bean id="ObjLevel-lookupDefinition" parent="ObjLevel-lookupDefinition-parentBean" />

  <bean id="ObjLevel-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
    <property name="title" value="Object Level Lookup" />
    <property name="instructions" value="Lookup an Object Level" />
    <property name="defaultSort" >
      <bean parent="SortDefinition">
        <property name="attributeNames" >
          <list>
            <value>financialObjectLevelCode</value>
          </list>
        </property>
        <property name="sortAscending" value="false" />
      </bean>
    </property>
    <property name="lookupFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="chartOfAccountsCode" />
        <bean parent="FieldDefinition"
              p:attributeName="financialObjectLevelCode" />
        <bean parent="FieldDefinition"
              p:attributeName="financialObjectLevelName" />
        <bean parent="FieldDefinition"
              p:attributeName="financialObjectLevelShortNm" />
        <bean parent="FieldDefinition"
              p:attributeName="financialConsolidationObjectCode" />
        <bean parent="FieldDefinition"
              p:defaultValue="Y"
              p:attributeName="active" />
      </list>
    </property>
    <property name="resultFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="chartOfAccountsCode" />
        <bean parent="FieldDefinition"
              p:attributeName="financialObjectLevelCode" />
        <bean parent="FieldDefinition"
              p:attributeName="financialObjectLevelName" />
        <bean parent="FieldDefinition"
              p:attributeName="financialObjectLevelShortNm" />
        <bean parent="FieldDefinition"
              p:attributeName="financialConsolidationObjectCode" />
        <bean parent="FieldDefinition"
              p:attributeName="active" />
      </list>
    </property>
  </bean>
</beans>
