<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="OrganizationRoutingModel" parent="OrganizationRoutingModel-parentBean" />

  <bean id="OrganizationRoutingModel-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.kfs.coa.businessobject.OrganizationRoutingModel" />
    <property name="objectLabel" value="Organization Routing Model Delegate" />
    <property name="attributes" >
      <list>
        <ref bean="OrganizationRoutingModel-financialDocumentTypeCode" />
        <ref bean="OrganizationRoutingModel-chartOfAccountsCode" />
        <ref bean="OrganizationRoutingModel-organizationCode" />
        <ref bean="OrganizationRoutingModel-organizationRoutingModelName" />
        <ref bean="OrganizationRoutingModel-accountDelegatePrimaryRoutingIndicator" />
        <ref bean="OrganizationRoutingModel-accountDelegateStartDate" />
        <ref bean="OrganizationRoutingModel-accountDelegateUniversalId" />
        <ref bean="OrganizationRoutingModel-accountDelegate.principalName" />
        <ref bean="OrganizationRoutingModel-accountDelegate.name" />
        <ref bean="OrganizationRoutingModel-approvalFromThisAmount" />
        <ref bean="OrganizationRoutingModel-approvalToThisAmount" />
        <ref bean="OrganizationRoutingModel-active" />
        <ref bean="OrganizationRoutingModel-newCollectionRecord" />
      </list>
    </property>
    <property name="relationships" >
      <list>
        <bean parent="RelationshipDefinition">
          <property name="objectAttributeName" value="accountDelegate" />
          <property name="primitiveAttributes" >
            <list>
              <bean parent="PrimitiveAttributeDefinition"
                    p:sourceName="accountDelegateUniversalId"
                    p:targetName="principalId" />
            </list>
          </property>
          <property name="supportAttributes" >
            <list>
              <bean parent="SupportAttributeDefinition"
                    p:identifier="true"
                    p:sourceName="accountDelegate.principalName"
                    p:targetName="principalName" />
              <bean parent="SupportAttributeDefinition"
                    p:sourceName="accountDelegate.name"
                    p:targetName="name" />
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->


  <bean id="OrganizationRoutingModel-financialDocumentTypeCode" parent="OrganizationRoutingModel-financialDocumentTypeCode-parentBean" />

  <bean id="OrganizationRoutingModel-financialDocumentTypeCode-parentBean" abstract="true" parent="DocumentType-documentTypeCode">
    <property name="name" value="financialDocumentTypeCode" />
  </bean>
  <bean id="OrganizationRoutingModel-chartOfAccountsCode" parent="OrganizationRoutingModel-chartOfAccountsCode-parentBean" />

  <bean id="OrganizationRoutingModel-chartOfAccountsCode-parentBean" abstract="true" parent="Chart-chartOfAccountsCode">
  </bean>
  <bean id="OrganizationRoutingModel-organizationCode" parent="OrganizationRoutingModel-organizationCode-parentBean" />

  <bean id="OrganizationRoutingModel-organizationCode-parentBean" abstract="true" parent="Org-organizationCode">
  </bean>
  <bean id="OrganizationRoutingModel-organizationRoutingModelName" parent="OrganizationRoutingModel-organizationRoutingModelName-parentBean" />

  <bean id="OrganizationRoutingModel-organizationRoutingModelName-parentBean" abstract="true" parent="OrganizationRoutingModelName-organizationRoutingModelName">
    <property name="required" value="true" />
  </bean>
  <bean id="OrganizationRoutingModel-accountDelegatePrimaryRoutingIndicator" parent="OrganizationRoutingModel-accountDelegatePrimaryRoutingIndicator-parentBean" />

  <bean id="OrganizationRoutingModel-accountDelegatePrimaryRoutingIndicator-parentBean" abstract="true" parent="GenericAttributes-genericBoolean">
    <property name="name" value="accountDelegatePrimaryRoutingIndicator" />
    <property name="label" value="Account Delegate Primary Route" />
    <property name="shortLabel" value="PrimaryRoute" />
  </bean>
  <bean id="OrganizationRoutingModel-accountDelegateStartDate" parent="OrganizationRoutingModel-accountDelegateStartDate-parentBean" />

  <bean id="OrganizationRoutingModel-accountDelegateStartDate-parentBean" abstract="true" parent="GenericAttributes-genericTimestamp">
    <property name="name" value="accountDelegateStartDate" />
    <property name="label" value="Account Delegate Start Date" />
    <property name="shortLabel" value="DelegateStartDt" />
    <property name="required" value="true" />
  </bean>
  <bean id="OrganizationRoutingModel-accountDelegateUniversalId" parent="OrganizationRoutingModel-accountDelegateUniversalId-parentBean" />

  <bean id="OrganizationRoutingModel-accountDelegateUniversalId-parentBean" abstract="true" parent="PersonImpl-principalId">
    <property name="name" value="accountDelegateUniversalId" />
    <property name="label" value="Account Delegate Unvl ID" />
    <property name="shortLabel" value="AccDelegateID" />
    <property name="control" >
      <bean parent="HiddenControlDefinition" />
    </property>
    <property name="displayLabelAttribute" value="accountDelegate.principalName" />
  </bean>
  <bean id="OrganizationRoutingModel-accountDelegate.principalName" parent="OrganizationRoutingModel-accountDelegate.principalName-parentBean" />

  <bean id="OrganizationRoutingModel-accountDelegate.principalName-parentBean" abstract="true" parent="PersonImpl-principalName">
    <property name="name" value="accountDelegate.principalName" />
    <property name="label" value="Account Delegate UserID" />
    <property name="shortLabel" value="UserID" />
    <property name="required" value="false" />
    <property name="control" >
      <bean parent="KualiUserControlDefinition"
            p:personNameAttributeName="accountDelegate.name"
            p:universalIdAttributeName="accountDelegateUniversalId"
            p:userIdAttributeName="accountDelegate.principalName" />
    </property>
  </bean>
  <bean id="OrganizationRoutingModel-accountDelegate.name" parent="OrganizationRoutingModel-accountDelegate.name-parentBean" />

  <bean id="OrganizationRoutingModel-accountDelegate.name-parentBean" abstract="true" parent="PersonImpl-name">
    <property name="name" value="accountDelegate.name" />
    <property name="label" value="Account Delegate User Name" />
    <property name="required" value="false" />
  </bean>
  <bean id="OrganizationRoutingModel-approvalFromThisAmount" parent="OrganizationRoutingModel-approvalFromThisAmount-parentBean" />

  <bean id="OrganizationRoutingModel-approvalFromThisAmount-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="approvalFromThisAmount" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Approval From This Amount" />
    <property name="shortLabel" value="Amount" />
    <property name="maxLength" value="19" />
    <property name="validationPattern" >
      <bean parent="FloatingPointValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="21" />
    </property>
  </bean>
  <bean id="OrganizationRoutingModel-approvalToThisAmount" parent="OrganizationRoutingModel-approvalToThisAmount-parentBean" />

  <bean id="OrganizationRoutingModel-approvalToThisAmount-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="approvalToThisAmount" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Approval To This Amount" />
    <property name="shortLabel" value="Amount" />
    <property name="maxLength" value="19" />
    <property name="validationPattern" >
      <bean parent="FloatingPointValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="21" />
    </property>
  </bean>
  <bean id="OrganizationRoutingModel-active" parent="OrganizationRoutingModel-active-parentBean" />

  <bean id="OrganizationRoutingModel-active-parentBean" abstract="true" parent="GenericAttributes-genericBoolean">
    <property name="name" value="active" />
    <property name="label" value="Active" />
    <property name="shortLabel" value="Act." />
  </bean>
  <bean id="OrganizationRoutingModel-newCollectionRecord" parent="OrganizationRoutingModel-newCollectionRecord-parentBean" />

  <bean id="OrganizationRoutingModel-newCollectionRecord-parentBean" abstract="true" parent="GenericAttributes-newCollectionRecord">
  </bean>
</beans>

