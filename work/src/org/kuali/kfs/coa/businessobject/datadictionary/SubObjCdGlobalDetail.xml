<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="SubObjCdGlobalDetail" parent="SubObjCdGlobalDetail-parentBean" />

  <bean id="SubObjCdGlobalDetail-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.kfs.coa.businessobject.SubObjCdGlobalDetail" />
    <property name="objectLabel" value="Object Code Change Detail" />
    <property name="attributes" >
      <list>
        <ref bean="SubObjCdGlobalDetail-universityFiscalYear" />
        <ref bean="SubObjCdGlobalDetail-chartOfAccountsCode" />
        <ref bean="SubObjCdGlobalDetail-financialObjectCode" />
        <ref bean="SubObjCdGlobalDetail-newCollectionRecord" />
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->


  <bean id="SubObjCdGlobalDetail-universityFiscalYear" parent="SubObjCdGlobalDetail-universityFiscalYear-parentBean" />

  <bean id="SubObjCdGlobalDetail-universityFiscalYear-parentBean" abstract="true" parent="GenericAttributes-genericFiscalYear">
    <property name="name" value="universityFiscalYear" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Fiscal Year" />
    <property name="shortLabel" value="FY" />
    <property name="required" value="false" />
  </bean>
  <bean id="SubObjCdGlobalDetail-chartOfAccountsCode" parent="SubObjCdGlobalDetail-chartOfAccountsCode-parentBean" />

  <bean id="SubObjCdGlobalDetail-chartOfAccountsCode-parentBean" abstract="true" parent="Chart-chartOfAccountsCode">
    <property name="required" value="false" />
    <property name="forceUppercase" value="true" />
    <property name="control" >
      <bean parent="SelectControlDefinition"
            p:valuesFinderClass="org.kuali.kfs.coa.businessobject.options.ChartValuesFinder"
            p:includeKeyInLabel="false" />
    </property>
  </bean>
  <bean id="SubObjCdGlobalDetail-financialObjectCode" parent="SubObjCdGlobalDetail-financialObjectCode-parentBean" />

  <bean id="SubObjCdGlobalDetail-financialObjectCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="financialObjectCode" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Object Code" />
    <property name="shortLabel" value="Object" />
    <property name="maxLength" value="4" />
    <property name="validationPattern" >
      <bean parent="AlphaNumericValidationPattern" />
    </property>
    <property name="required" value="true" />
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="4" />
    </property>
  </bean>
  <bean id="SubObjCdGlobalDetail-newCollectionRecord" parent="SubObjCdGlobalDetail-newCollectionRecord-parentBean" />

  <bean id="SubObjCdGlobalDetail-newCollectionRecord-parentBean" abstract="true" parent="GenericAttributes-newCollectionRecord">
  </bean>
</beans>
