<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "../../../core/datadictionary/dataDictionary-1_0.dtd">

<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.module.chart.bo.ObjectCodeGlobal">


       
        <attributes>
            <attributeReference name="universityFiscalYear" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="genericFiscalYear">
                <label>Fiscal Year</label>
                <shortLabel>FY</shortLabel>
                <required>false</required>
            </attributeReference>

            <attributeReference name="chartOfAccountsCode" sourceClassName="org.kuali.module.chart.bo.Chart" sourceAttributeName="chartOfAccountsCode">
                <required>false</required>
                <control>
                    <select valuesFinderClass="org.kuali.module.chart.lookup.keyvalues.ChartValuesFinder" />
                </control>

            </attributeReference>

            <attribute name="financialObjectCode" forceUppercase="true">
                <label>Object Code</label>
                <shortLabel>Object</shortLabel>

                <maxLength>4</maxLength>
                <validationPattern>
                    <alphaNumeric />
                </validationPattern>
                <required>true</required>
                <control>
                    <text size="4" />
                </control>
            </attribute>

            <attribute name="financialObjectCodeName" forceUppercase="true">
                <label>Object Code Name</label>
                <shortLabel>ObjCodeName</shortLabel>

                <maxLength>40</maxLength>
                <validationPattern>
                    <anyCharacter allowWhitespace="true" />
                </validationPattern>
                <required>true</required>
                <control>
                    <text size="40" />
                </control>
            </attribute>

            <attribute name="financialObjectCodeShortName" forceUppercase="true">
                <label>Object Code Short Name</label>
                <shortLabel>ObjCodeShortName</shortLabel>

                <maxLength>12</maxLength>
                <validationPattern>
                    <anyCharacter allowWhitespace="true" />
                </validationPattern>
                <required>true</required>
                <control>
                    <text size="12" />
                </control>
            </attribute>
            <attribute name="financialObjectLevelCode" forceUppercase="true">
                <label>Object Level Code</label>
                <shortLabel>ObjLevlCde</shortLabel>

                <maxLength>4</maxLength>
                <validationPattern>
                    <anyCharacter allowWhitespace="true" />
                </validationPattern>
                <required>true</required>
                <control>
                    <text size="4" />
                </control>
            </attribute>

            <attribute name="reportsToFinancialObjectCode" forceUppercase="true">
                <label>Reports To Object Code</label>
                <shortLabel>RptsToObjCode</shortLabel>

                <maxLength>4</maxLength>
                <validationPattern>
                    <alphaNumeric />
                </validationPattern>
                <required>true</required>
                <control>
                    <text size="4" />
                </control>
            </attribute>
 
            <attribute name="financialObjectTypeCode" forceUppercase="true">
                <label>Object Type Code</label>
                <shortLabel>Object Type</shortLabel>

                <maxLength>2</maxLength>
                <validationPattern>
                    <anyCharacter />
                </validationPattern>
                <required>true</required>
                <control>
                    <text size="2" />
                </control>
            </attribute>

            <attribute name="financialObjectSubTypeCode" forceUppercase="true">
                <label>Object Sub-Type Code</label>
                <shortLabel>ObjSubTypCode</shortLabel>

                <maxLength>2</maxLength>
                <validationPattern>
                    <anyCharacter />
                </validationPattern>
                <required>true</required>
                <control>
                    <text size="2" />
                </control>
            </attribute>

            <attribute name="historicalFinancialObjectCode" forceUppercase="true">
                <label>Historical Financial Object Code</label>
                <shortLabel>HistFinObjCde</shortLabel>

                <maxLength>4</maxLength>
                <validationPattern>
                    <anyCharacter />
                </validationPattern>
                <control>
                    <text size="4" />
                </control>
            </attribute>

            <attribute name="financialObjectActiveIndicator" forceUppercase="false">
                <label>Active Code</label>
                <shortLabel>Ac.</shortLabel>

                <maxLength>1</maxLength>

                <control>
                    <checkbox />
                </control>
                <formatterClass>org.kuali.core.web.format.BooleanFormatter</formatterClass>
            </attribute>

            <attribute name="financialBudgetAggregationCd" forceUppercase="true">
                <label>Budget Aggregation Code</label>
                <shortLabel>BudgetAggCde</shortLabel>

                <maxLength>1</maxLength>
                <required>true</required>
                <control>
                    <text size="1" />
                </control>                
            </attribute>

            <attribute name="finObjMandatoryTrnfrOrElimCd" forceUppercase="true">
                <label>Mandatory Transfer Or Eliminations Code</label>
                <shortLabel>ManTranOrElimCode</shortLabel>

                <maxLength>1</maxLength>

                <control>
                    <text size="1" />
                </control>
            </attribute>
            <attribute name="financialFederalFundedCode" forceUppercase="true">
                <label>Federal Funded Code</label>
                <shortLabel>FedFundCd</shortLabel>

                <maxLength>1</maxLength>
                <control>
                    <select valuesFinderClass="org.kuali.module.chart.lookup.keyvalues.FederalFundedCodeValuesFinder" />
                </control>
            </attribute>

            <attribute name="nextYearFinancialObjectCode" forceUppercase="true">
                <label>Next Year Object Code</label>
                <shortLabel>NextYearObjCode</shortLabel>

                <maxLength>4</maxLength>
                <validationPattern>
                    <alphaNumeric />
                </validationPattern>

                <control>
                    <text size="4" />
                </control>
            </attribute>

            <attributeReference name="versionNumber" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="versionNumber" />

        </attributes>
        
        <objectLabel>Object Code Global</objectLabel>
    </businessObject>
</dictionaryEntry>
