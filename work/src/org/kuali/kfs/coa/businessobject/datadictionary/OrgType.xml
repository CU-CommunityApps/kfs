<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "../../../core/datadictionary/dataDictionary-1_0.dtd">
<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.module.chart.bo.OrgType">
        <customAttributeEnabled>false</customAttributeEnabled>

        <inquiry>
            <title>Organization Type Inquiry</title>

            <inquiryFields>
                <field attributeName="organizationTypeCode" />
                <field attributeName="organizationTypeName" />
            </inquiryFields>
        </inquiry>

        <lookup>
            <title>Organization Type Lookup</title>

            <menubar>
                <![CDATA[<a href="maintenance.do?methodToCall=start&businessObjectClassName=org.kuali.module.chart.bo.OrgType">Create New</a>]]>
            </menubar>

            <instructions>Lookup an Organization Type</instructions>

            <defaultSort attributeName="organizationTypeCode" sortAscending="true" />

            <lookupFields>
                <lookupField attributeName="organizationTypeCode" required="false" />
                <lookupField attributeName="organizationTypeName" required="false" />
            </lookupFields>

            <resultFields>
                <field attributeName="organizationTypeCode" />
                <field attributeName="organizationTypeName" />
            </resultFields>
        </lookup>

        <titleAttribute>organizationTypeCode</titleAttribute>

        <attributes>
            <attribute name="organizationTypeCode" forceUppercase="true">
                <label>Organization Type Code</label>
                <shortLabel>Organization Type</shortLabel>

                <maxLength>1</maxLength>
                <validationPattern>
                    <alphaNumeric />
                </validationPattern>
                <required>true</required>
                <control>
                    <text size="1" />
                </control>

                <summary>Organization Type Code</summary>
                <description>The type of organization.</description>
            </attribute>

            <attribute name="organizationTypeName" forceUppercase="true">
                <label>Organization Type Name</label>
                <shortLabel>Name</shortLabel>

                <maxLength>40</maxLength>

                <required>true</required>
                <control>
                    <text size="40" />
                </control>

                <summary>Organization Type Name</summary>
                <description>Organization Type Name...</description>
            </attribute>

            <attributeReference name="objectId" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="objectId" />

            <attributeReference name="versionNumber" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="versionNumber" />
        </attributes>
    </businessObject>
</dictionaryEntry>
