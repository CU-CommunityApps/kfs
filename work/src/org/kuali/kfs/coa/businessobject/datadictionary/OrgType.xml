<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="OrgType" parent="OrgType-parentBean" />

  <bean id="OrgType-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.module.chart.bo.OrgType" />
    <property name="inquiryDefinition" >
      <ref bean="OrgType-inquiryDefinition" />
    </property>
    <property name="lookupDefinition" >
      <ref bean="OrgType-lookupDefinition" />
    </property>
    <property name="titleAttribute" value="organizationTypeCode" />
    <property name="objectLabel" value="Organization Type" />
    <property name="attributes" >
      <list>
        <ref bean="OrgType-organizationTypeCode" />
        <ref bean="OrgType-organizationTypeName" />
        <ref bean="OrgType-objectId" />
        <ref bean="OrgType-versionNumber" />
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->


  <bean id="OrgType-organizationTypeCode" parent="OrgType-organizationTypeCode-parentBean" />

  <bean id="OrgType-organizationTypeCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="organizationTypeCode" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Organization Type Code" />
    <property name="shortLabel" value="Organization Type" />
    <property name="maxLength" value="1" />
    <property name="validationPattern" >
      <bean parent="AlphaNumericValidationPattern" />
    </property>
    <property name="required" value="true" />
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="1" />
    </property>
  </bean>
  <bean id="OrgType-organizationTypeName" parent="OrgType-organizationTypeName-parentBean" />

  <bean id="OrgType-organizationTypeName-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="organizationTypeName" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Organization Type Name" />
    <property name="shortLabel" value="Name" />
    <property name="maxLength" value="40" />
    <property name="required" value="true" />
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="40" />
    </property>
  </bean>
  <bean id="OrgType-objectId" parent="OrgType-objectId-parentBean" />

  <bean id="OrgType-objectId-parentBean" abstract="true" parent="AttributeReferenceDummy-objectId">
  </bean>
  <bean id="OrgType-versionNumber" parent="OrgType-versionNumber-parentBean" />

  <bean id="OrgType-versionNumber-parentBean" abstract="true" parent="AttributeReferenceDummy-versionNumber">
  </bean>

<!-- Business Object Inquiry Definition -->


  <bean id="OrgType-inquiryDefinition" parent="OrgType-inquiryDefinition-parentBean" />

  <bean id="OrgType-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
    <property name="title" value="Organization Type Inquiry" />
    <property name="inquirySections" >
      <list>
        <bean parent="InquirySectionDefinition">
          <property name="title" value="Header Text" />
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields" >
            <list>
              <bean parent="FieldDefinition"
                    p:attributeName="organizationTypeCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="organizationTypeName" />
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Business Object Lookup Definition -->


  <bean id="OrgType-lookupDefinition" parent="OrgType-lookupDefinition-parentBean" />

  <bean id="OrgType-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
    <property name="title" value="Organization Type Lookup" />
    <property name="instructions" value="Lookup an Organization Type" />
    <property name="defaultSort" >
      <bean parent="SortDefinition">
        <property name="attributeNames" >
          <list>
            <value>organizationTypeCode</value>
          </list>
        </property>
      </bean>
    </property>
    <property name="lookupFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="organizationTypeCode" />
        <bean parent="FieldDefinition"
              p:attributeName="organizationTypeName" />
      </list>
    </property>
    <property name="resultFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="organizationTypeCode" />
        <bean parent="FieldDefinition"
              p:attributeName="organizationTypeName" />
      </list>
    </property>
  </bean>
</beans>
