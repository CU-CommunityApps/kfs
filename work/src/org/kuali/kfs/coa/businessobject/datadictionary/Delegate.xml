<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "../../../core/datadictionary/dataDictionary-1_0.dtd">
<dictionaryEntry>
	<businessObject
		businessObjectClass="org.kuali.module.chart.bo.Delegate">
		<customAttributeEnabled>false</customAttributeEnabled>

		<inquiry>
			<title>Account Delegate Inquiry</title>

			<inquiryFields>
				<field attributeName="chartOfAccounts.chartOfAccountsCode" />			
				<field attributeName="account.accountNumber" />
				<field attributeName="financialDocumentTypeCode" />			
				<field attributeName="accountDelegateSystemId" />
				<field attributeName="finDocApprovalFromThisAmt" />
				<field attributeName="finDocApprovalToThisAmount" />
				<field attributeName="accountsDelegatePrmrtIndicator" />
				<field attributeName="accountDelegateActiveIndicator" />
				<field attributeName="accountDelegateStartDate" />
			</inquiryFields>
		</inquiry>

		<lookup>
			<title>Account Delegate Lookup</title>

			<menubar><![CDATA[<a href="maintenance.do?methodToCall=start&businessObjectClassName=org.kuali.module.chart.bo.Delegate">Create New</a>]]></menubar>

			<instructions>Lookup an Account Delegate</instructions>

			<defaultSort attributeName="account.accountNumber" sortAscending="true" />

			<lookupFields>
				<lookupField attributeName="chartOfAccounts.chartOfAccountsCode" required="false" />
				<lookupField attributeName="account.accountNumber" required="false" />
				<lookupField attributeName="financialDocumentTypeCode" required="false" />
				<lookupField attributeName="accountDelegateSystemId" required="false" />
			</lookupFields>

			<resultFields>
				<field attributeName="chartOfAccounts.chartOfAccountsCode" />			
				<field attributeName="account.accountNumber" />
				<field attributeName="financialDocumentTypeCode" />			
				<field attributeName="accountDelegateSystemId" />
				<field attributeName="finDocApprovalFromThisAmt" />
				<field attributeName="finDocApprovalToThisAmount" />
				<field attributeName="accountsDelegatePrmrtIndicator" />
				<field attributeName="accountDelegateActiveIndicator" />
				<field attributeName="accountDelegateStartDate" />
			</resultFields>
		</lookup>

		<attributes>
			<attributeReference name="chartOfAccounts.chartOfAccountsCode" 
								sourceClassName="org.kuali.module.chart.bo.Chart" 
								sourceAttributeName="chartOfAccountsCode" />
								
			<attributeReference name="account.accountNumber" 
								sourceClassName="org.kuali.module.chart.bo.Account" 
								sourceAttributeName="accountNumber" />
								
			<attribute name="financialDocumentTypeCode" forceUppercase="true">
				<label>Document Type Code</label>
				<shortLabel>DocType</shortLabel>

				<maxLength>4</maxLength>
				<validationPattern><alphaNumeric /></validationPattern>

				<control>
					<text size="4" />
				</control>

				<summary>Document Type Code</summary>
				<description>Document Type Code is a unique identifier of the type of a document.</description>
			</attribute>
			
			<attributeReference name="accountDelegateSystemId" 
								sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" 
								sourceAttributeName="genericSystemId">
				<label>Account Delegate System ID</label>
				<shortLabel>SysID</shortLabel>
				<summary>Account Delegate System ID</summary>
				<description>
					The SystemID (ie, unique identifier) of the Account Delegate.
				</description>
			</attributeReference>
			
			<attributeReference name="finDocApprovalFromThisAmt" 
								sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" 
								sourceAttributeName="genericAmount">
				<label>From Amount</label>
				<shortLabel>FromAmt</shortLabel>
				<summary>Total Amount on the 'From' side of the document.</summary>
				<description>Total Amount on the 'From' side of the document.</description>
			</attributeReference>
			
			<attributeReference name="finDocApprovalToThisAmount" 
								sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" 
								sourceAttributeName="genericAmount">
				<label>To Amount</label>
				<shortLabel>ToAmt</shortLabel>
				<summary>Total Amount on the 'To' side of the document.</summary>
				<description>Total Amount on the 'To' side of the document.</description>
			</attributeReference>
			
			<attributeReference name="accountsDelegatePrmrtIndicator" 
								sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" 
								sourceAttributeName="genericBoolean">
				<label>Account Delegate Primary Route</label>
				<shortLabel>PrimaryRoute</shortLabel>
				<summary>Account Delegate Primary Route Flag</summary>
				<description>Account Delegate Primary Route Flag</description>
			</attributeReference>
			
			<attributeReference name="accountDelegateActiveIndicator" 
								sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" 
								sourceAttributeName="genericBoolean">
				<label>Account Delegate Active</label>
				<shortLabel>DelegateActive</shortLabel>
				<summary>Account Delegate Active Flag</summary>
				<description>Account Delegate Active Flag</description>
			</attributeReference>
			
			<attributeReference name="accountDelegateStartDate" 
								sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" 
								sourceAttributeName="genericTimestamp">
				<label>Account Delegate Start Date</label>
				<shortLabel>DelegateStartDt</shortLabel>
				<summary>Account Delegate Start Date</summary>
				<description>Account Delegate Start Date</description>
			</attributeReference>
			
		</attributes>
	</businessObject>
</dictionaryEntry>

