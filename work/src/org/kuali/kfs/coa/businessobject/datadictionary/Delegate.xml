<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "../../../core/datadictionary/dataDictionary-1_0.dtd">
<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.module.chart.bo.Delegate">


        <inquiry>
            <title>Account Delegate Inquiry</title>

            <inquirySections><inquirySection title="Header Text"><inquiryFields>
                <field attributeName="chartOfAccountsCode" />
                <field attributeName="accountNumber" />
                <field attributeName="financialDocumentTypeCode" />
                <field attributeName="accountDelegate.personUserIdentifier" />
                <field attributeName="accountDelegate.personName" />
                <field attributeName="finDocApprovalFromThisAmt" />
                <field attributeName="finDocApprovalToThisAmount" />
                <field attributeName="accountsDelegatePrmrtIndicator" />
                <field attributeName="accountDelegateActiveIndicator" />
                <field attributeName="accountDelegateStartDate" />
            </inquiryFields></inquirySection></inquirySections>
        </inquiry>

        <lookup>
            <title>Account Delegate Lookup</title>

			<menubar>
                <![CDATA[<a href="maintenance.do?methodToCall=start&businessObjectClassName=org.kuali.module.chart.bo.DelegateGlobal"><img src="${kr.externalizable.images.url}tinybutton-createnewglobal.gif" alt="create new global" width="98" height="15"/></a>&nbsp;<a href="lookup.do?businessObjectClassName=org.kuali.module.chart.bo.OrganizationRoutingModelName&conversionFields=chartOfAccountsCode%3AmodelChartOfAccountsCode%2CorganizationCode%3AmodelOrganizationCode%2CorganizationRoutingModelName%3AmodelName&returnLocation=portal.do&docFormKey=88888888"><img src="${externalizable.images.url}tinybutton-crtnewglob.gif" alt="create new global from model" width="144" height="15"/></a>]]>
            </menubar>
            
            <instructions>Lookup an Account Delegate</instructions>

            <defaultSort attributeName="accountNumber" sortAscending="true" />

            <lookupFields>
                <lookupField attributeName="chartOfAccountsCode" required="false" />
                <lookupField attributeName="accountNumber" required="false" />
                <lookupField attributeName="financialDocumentTypeCode" required="false" />
                <lookupField attributeName="accountDelegateSystemId" required="false" />
                <lookupField attributeName="accountDelegate.personUserIdentifier" required="false" />
                <lookupField attributeName="accountDelegate.personName" required="false" />
                <lookupField attributeName="accountDelegateActiveIndicator" required="false">
                    <defaultValue>Y</defaultValue>
                </lookupField>
            </lookupFields>

            <resultFields>
                <field attributeName="chartOfAccountsCode" />
                <field attributeName="accountNumber" />
                <field attributeName="financialDocumentTypeCode" />
                <field attributeName="accountDelegate.personUserIdentifier" />
                <field attributeName="accountDelegateActiveIndicator" />                
                <field attributeName="finDocApprovalFromThisAmt" />
                <field attributeName="finDocApprovalToThisAmount" />
                <field attributeName="accountsDelegatePrmrtIndicator" />

                <field attributeName="accountDelegateStartDate" />
            </resultFields>
        </lookup>

        <attributes>
            <attributeReference name="chartOfAccountsCode" sourceClassName="org.kuali.module.chart.bo.Chart" sourceAttributeName="chartOfAccountsCode">
                <required>true</required>
                <control>
                    <text size="2" />
                </control>

                <description>The code for the Chart of Accounts for an Account. It is part of the primary key. Kuali accommodates more than one Chart of Accounts</description>

            </attributeReference>

            <attributeReference name="accountNumber" sourceClassName="org.kuali.module.chart.bo.Account" sourceAttributeName="accountNumber">
                <required>true</required>
                <description>Identifier for a pool of funds assigned to a specific university entity for a specific function.</description>

            </attributeReference>

            <attributeReference name="financialDocumentTypeCode" 
            					sourceClassName="org.kuali.core.bo.DocumentType" 
            					sourceAttributeName="financialDocumentTypeCode" />

            <attributeReference name="accountDelegateSystemId" sourceClassName="org.kuali.core.bo.user.UniversalUser" sourceAttributeName="personUniversalIdentifier">
                <label>Account Delegate Universal ID</label>
                <shortLabel>UnvlID</shortLabel>
                <displayLabelAttribute>accountDelegate.personUserIdentifier</displayLabelAttribute>
                <required>true</required>
                <control>
                    <hidden />
                </control>
                <summary>
                    <![CDATA[&nbsp;]]>
                </summary>
                <description>The Universal ID (ie, unique identifier) of the Account Delegate.</description>
            </attributeReference>

            <attributeReference name="accountDelegate.personUserIdentifier" sourceClassName="org.kuali.core.bo.user.UniversalUser" sourceAttributeName="personUserIdentifier">
                <label>Account Delegate UserID</label>
                <shortLabel>UserID</shortLabel>
                <required>false</required>
                <control>
                    <kualiUser universalIdAttributeName="accountDelegateSystemId" userIdAttributeName="accountDelegate.personUserIdentifier" personNameAttributeName="accountDelegate.personName" />
                </control>
                <summary>
                    <![CDATA[&nbsp;]]>
                </summary>
                <description>The user ID of the designated delegate for the Fiscal Officer for the Account.</description>
            </attributeReference>

            <attributeReference name="accountDelegate.personName" sourceClassName="org.kuali.core.bo.user.UniversalUser" sourceAttributeName="personName">
                <label>Account Delegate Name</label>
                <shortLabel>Name</shortLabel>
                <required>false</required>
                <control>
                    <hidden />
                </control>
                <summary>
                    <![CDATA[&nbsp;]]>
                </summary>
                <description>The Name of the Account Delegate.</description>
            </attributeReference>

            <attributeReference name="finDocApprovalFromThisAmt" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="genericAmount">
                <label>Approval From This Amount</label>
                <shortLabel>FromAmt</shortLabel>
                <summary>
                    <![CDATA[&nbsp;]]>
                </summary>
                <description>Total Amount on the 'From' side of the document.</description>
                <formatterClass>org.kuali.core.web.format.CurrencyFormatter</formatterClass>
            </attributeReference>

            <attributeReference name="finDocApprovalToThisAmount" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="genericAmount">
                <label>Approval To This Amount</label>
                <shortLabel>ToAmt</shortLabel>
                <summary>
                    <![CDATA[&nbsp;]]>
                </summary>
                <description>Total Amount on the 'To' side of the document.</description>
                <formatterClass>org.kuali.core.web.format.CurrencyFormatter</formatterClass>
            </attributeReference>

            <attributeReference name="accountsDelegatePrmrtIndicator" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="genericBoolean">
                <label>Account Delegate Primary Route</label>
                <shortLabel>PrimaryRoute</shortLabel>
                <summary>
                    <![CDATA[&nbsp;]]>
                </summary>
                <description>
                    <![CDATA[&nbsp;]]>
                </description>
            </attributeReference>

            <attributeReference name="accountDelegateActiveIndicator" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="genericBoolean">
                <label>Account Delegate Active</label>
                <shortLabel>DelegateActive</shortLabel>
                <summary>
                    <![CDATA[&nbsp;]]>
                </summary>
                <description>Radioboxes in the entry form that indicate if the Account delegation is active or inactive. If "Both" is checked in the Lookup, then both "Yes" (active) or "No" (inactive) values are listed in the search.</description>
            </attributeReference>

            <attributeReference name="accountDelegateStartDate" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="genericTimestamp">
                <label>Account Delegate Start Date</label>
                <shortLabel>DelegateStartDt</shortLabel>
                <required>true</required>
                <summary>
                    <![CDATA[&nbsp;]]>
                </summary>
                <description>
                    <![CDATA[&nbsp;]]>
                </description>
            </attributeReference>

            <attributeReference name="versionNumber" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="versionNumber" />

        </attributes>

        <relationships>
            <relationship objectAttributeName="accountDelegate" >
                <primitiveAttribute sourceName="accountDelegateSystemId" targetName="personUniversalIdentifier" />
				<supportAttribute sourceName="accountDelegate.personUserIdentifier" targetName="personUserIdentifier" identifier="true" />
				<supportAttribute sourceName="accountDelegate.personName" targetName="personName" />
            </relationship>
        </relationships>

		<objectLabel>Account Delegate</objectLabel>
    </businessObject>
</dictionaryEntry>

