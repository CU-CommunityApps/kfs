<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "../../../core/datadictionary/dataDictionary-1_0.dtd">
<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.module.chart.bo.ReportingCodes">
        <customAttributeEnabled>true</customAttributeEnabled>

        <inquiry>
            <title>Financial Reporting Code Inquiry</title>

            <inquirySections><inquirySection title="Header Text"><inquiryFields>
                <field attributeName="chartOfAccountsCode" />
                <field attributeName="organizationCode" />
                <field attributeName="financialReportingCode" />
                <field attributeName="financialReportingCodeDescription" />
                <field attributeName="universalUser.personName" />
                <field attributeName="financialReportsToReportingCode" />
            </inquiryFields></inquirySection></inquirySections>
        </inquiry>

        <lookup>
            <title>Financial Reporting Code Lookup</title>

            <instructions>Lookup Financial Reporting Code</instructions>

            <defaultSort attributeName="chartOfAccountsCode" sortAscending="true" />

            <lookupFields>
                <lookupField attributeName="chartOfAccountsCode" />
                <lookupField attributeName="organizationCode" />
                <lookupField attributeName="financialReportingCode" />
            </lookupFields>

            <resultFields>
                <field attributeName="chartOfAccountsCode" />
                <field attributeName="organizationCode" />
                <field attributeName="financialReportingCode" />
                <field attributeName="financialReportingCodeDescription" />
                <field attributeName="universalUser.personName" />
                <field attributeName="financialReportsToReportingCode" />
            </resultFields>
        </lookup>

        <titleAttribute>financialReportingCode</titleAttribute>

        <attributes>

            <attributeReference name="chartOfAccountsCode" sourceClassName="org.kuali.module.chart.bo.Chart" sourceAttributeName="chartOfAccountsCode">
                <required>true</required>
            </attributeReference>


            <attributeReference name="organizationCode" sourceClassName="org.kuali.module.chart.bo.Org" sourceAttributeName="organizationCode">
                <required>true</required>
            </attributeReference>


            <attribute name="financialReportingCode" forceUppercase="true">
                <label>Financial Reporting Code</label>
                <shortLabel>Financial Reporting Code</shortLabel>

                <maxLength>10</maxLength>
                <validationPattern>
                    <anyCharacter />
                </validationPattern>
                <required>true</required>
                <control>
                    <text size="10" />
                </control>

                <summary>financialReportingCode</summary>
                <description>Financial Reporting Code is a unique identifier of the type of a document.</description>
            </attribute>

            <attribute name="financialReportingCodeDescription" forceUppercase="true">
                <label>Financial Reporting Code Description</label>
                <shortLabel>financialReportingCodeDescription</shortLabel>

                <maxLength>40</maxLength>
                <validationPattern>
                    <anyCharacter allowWhitespace="true" />
                </validationPattern>
                <required>true</required>
                <control>
                    <text size="40" />
                </control>

                <summary>financialReportingCodeDescription</summary>
                <description>financialReportingCodeDescription is a unique identifier of the type of a document.</description>
            </attribute>

            <attribute name="financialReportingCodeMgrId" forceUppercase="true">
                <label>Financial Reporting Code Manager Id</label>
                <shortLabel>Manager Id</shortLabel>

                <maxLength>10</maxLength>
                <validationPattern>
                    <alphaNumeric />
                </validationPattern>

                <required>true</required>
                <control>
                    <hidden />
                </control>

                <summary>financialReportingCodeMgrId</summary>
                <description>Financial Reporting Code Manager Id is a unique identifier of the type of a document.</description>
            </attribute>

            <attributeReference name="universalUser.personUserIdentifier" sourceClassName="org.kuali.core.bo.user.UniversalUser" sourceAttributeName="personUserIdentifier">
                <label>Financial Reporting Code Manager UserID</label>
                <shortLabel>UserID</shortLabel>
                <required>false</required>
                <control>
                    <kualiUser universalIdAttributeName="financialReportingCodeMgrId" userIdAttributeName="universalUser.personUserIdentifier" personNameAttributeName="universalUser.personName" />
                </control>
                <summary>Reporting Code Manager User ID</summary>
                <description>The UserID of the Reporting Code Manager.</description>
            </attributeReference>

            <attributeReference name="universalUser.personName" sourceClassName="org.kuali.core.bo.user.UniversalUser" sourceAttributeName="personName">
                <label>Financial Reporting Code Manager Name</label>
                <shortLabel>Name</shortLabel>
                <required>false</required>
                <control>
                    <hidden />
                </control>
                <summary>Reporting Code Manager Name</summary>
                <description>The Name of the Reporting Code Manager.</description>
            </attributeReference>
            <attributeReference name="financialReportsToReportingCode" sourceClassName="org.kuali.module.chart.bo.ReportingCodes" sourceAttributeName="financialReportingCode">
                <label>Financial Reports To Reporting Code</label>
                <shortLabel>Reports To Code</shortLabel>
                <required>true</required>
                <control>
                    <text size="10" />
                </control>
                <summary>Financial Reports To Reporting Code</summary>
                <description>Financial Reports To Reporting Code is a unique identifier of the type of a document.</description>
            </attributeReference>

            <attributeReference name="versionNumber" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="versionNumber" />

            <attributeReference name="objectId" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="objectId" />

        </attributes>

        <relationships>
            <relationship objectAttributeName="universalUser" >
                <primitiveAttribute sourceName="financialReportingCodeMgrId" targetName="personUniversalIdentifier" />
            </relationship>
        </relationships>

		<objectLabel>Financial Reporting Code</objectLabel>
    </businessObject>
</dictionaryEntry>

