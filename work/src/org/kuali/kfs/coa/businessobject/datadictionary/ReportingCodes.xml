<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="ReportingCodes" parent="ReportingCodes-parentBean" />

  <bean id="ReportingCodes-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.kfs.coa.businessobject.ReportingCodes" />
    <property name="inquiryDefinition" >
      <ref bean="ReportingCodes-inquiryDefinition" />
    </property>
    <property name="lookupDefinition" >
      <ref bean="ReportingCodes-lookupDefinition" />
    </property>
    <property name="titleAttribute" value="financialReportingCode" />
    <property name="objectLabel" value="Financial Reporting Code" />
    <property name="attributes" >
      <list>
        <ref bean="ReportingCodes-chartOfAccountsCode" />
        <ref bean="ReportingCodes-organizationCode" />
        <ref bean="ReportingCodes-financialReportingCode" />
        <ref bean="ReportingCodes-financialReportingCodeDescription" />
        <ref bean="ReportingCodes-financialReportingCodeMgrId" />
        <ref bean="ReportingCodes-person.principalName" />
        <ref bean="ReportingCodes-person.name" />
        <ref bean="ReportingCodes-financialReportsToReportingCode" />
        <ref bean="ReportingCodes-active" />
      </list>
    </property>
    <property name="relationships" >
      <list>
        <bean parent="RelationshipDefinition">
          <property name="objectAttributeName" value="person" />
          <property name="primitiveAttributes" >
            <list>
              <bean parent="PrimitiveAttributeDefinition"
                    p:sourceName="financialReportingCodeMgrId"
                    p:targetName="principalId" />
            </list>
          </property>
          <property name="supportAttributes" >
            <list>
              <bean parent="SupportAttributeDefinition"
                    p:identifier="true"
                    p:sourceName="person.principalName"
                    p:targetName="principalName" />
              <bean parent="SupportAttributeDefinition"
                    p:sourceName="person.name"
                    p:targetName="name" />
            </list>
          </property>
        </bean>
      </list>
    </property>
    <property name="inactivationBlockingDefinitions">
      <list>
        <bean parent="InactivationBlockingDefinition"
          p:blockedReferencePropertyName="reportingCodes"/>
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->


  <bean id="ReportingCodes-chartOfAccountsCode" parent="ReportingCodes-chartOfAccountsCode-parentBean" />

  <bean id="ReportingCodes-chartOfAccountsCode-parentBean" abstract="true" parent="Chart-chartOfAccountsCode">
  </bean>
  <bean id="ReportingCodes-organizationCode" parent="ReportingCodes-organizationCode-parentBean" />

  <bean id="ReportingCodes-organizationCode-parentBean" abstract="true" parent="Org-organizationCode">
  </bean>
  <bean id="ReportingCodes-financialReportingCode" parent="ReportingCodes-financialReportingCode-parentBean" />

  <bean id="ReportingCodes-financialReportingCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="financialReportingCode" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Financial Reporting Code" />
    <property name="shortLabel" value="Financial Reporting Code" />
    <property name="maxLength" value="10" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="required" value="true" />
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="10" />
    </property>
  </bean>
  <bean id="ReportingCodes-financialReportingCodeDescription" parent="ReportingCodes-financialReportingCodeDescription-parentBean" />

  <bean id="ReportingCodes-financialReportingCodeDescription-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="financialReportingCodeDescription" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Financial Reporting Code Description" />
    <property name="shortLabel" value="financialReportingCodeDescription" />
    <property name="maxLength" value="40" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern"
            p:allowWhitespace="true" />
    </property>
    <property name="required" value="true" />
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="40" />
    </property>
  </bean>
  <bean id="ReportingCodes-financialReportingCodeMgrId" parent="ReportingCodes-financialReportingCodeMgrId-parentBean" />

  <bean id="ReportingCodes-financialReportingCodeMgrId-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="financialReportingCodeMgrId" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Financial Reporting Code Manager Id" />
    <property name="shortLabel" value="Manager Id" />
    <property name="maxLength" value="10" />
    <property name="validationPattern" >
      <bean parent="AlphaNumericValidationPattern" />
    </property>
    <property name="required" value="true" />
    <property name="control" >
      <bean parent="HiddenControlDefinition" />
    </property>
  </bean>
  <bean id="ReportingCodes-person.principalName" parent="ReportingCodes-person.principalName-parentBean" />

  <bean id="ReportingCodes-person.principalName-parentBean" abstract="true" parent="PersonImpl-principalName">
    <property name="name" value="person.principalName" />
    <property name="label" value="Financial Reporting Code Manager UserID" />
    <property name="shortLabel" value="UserID" />
    <property name="required" value="false" />
    <property name="control" >
      <bean parent="KualiUserControlDefinition"
            p:personNameAttributeName="person.name"
            p:universalIdAttributeName="financialReportingCodeMgrId"
            p:userIdAttributeName="person.principalName" />
    </property>
  </bean>
  <bean id="ReportingCodes-person.name" parent="ReportingCodes-person.name-parentBean" />

  <bean id="ReportingCodes-person.name-parentBean" abstract="true" parent="PersonImpl-name">
    <property name="name" value="person.name" />
    <property name="label" value="Financial Reporting Code Manager Name" />
    <property name="required" value="false" />
  </bean>
  <bean id="ReportingCodes-financialReportsToReportingCode" parent="ReportingCodes-financialReportsToReportingCode-parentBean" />

  <bean id="ReportingCodes-financialReportsToReportingCode-parentBean" abstract="true" parent="ReportingCodes-financialReportingCode">
    <property name="name" value="financialReportsToReportingCode" />
    <property name="label" value="Financial Reports To Reporting Code" />
    <property name="shortLabel" value="Reports To Code" />
  </bean>
  <bean id="ReportingCodes-active" parent="ReportingCodes-active-parentBean" />

  <bean id="ReportingCodes-active-parentBean" abstract="true" parent="GenericAttributes-activeIndicator">
    <property name="name" value="active"/>
  </bean>

<!-- Business Object Inquiry Definition -->


  <bean id="ReportingCodes-inquiryDefinition" parent="ReportingCodes-inquiryDefinition-parentBean" />

  <bean id="ReportingCodes-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
    <property name="title" value="Financial Reporting Code Inquiry" />
    <property name="inquirySections" >
      <list>
        <bean parent="InquirySectionDefinition">
          <property name="title" value="" />
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields" >
            <list>
              <bean parent="FieldDefinition"
                    p:attributeName="chartOfAccountsCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="organizationCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="financialReportingCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="financialReportingCodeDescription" />
              <bean parent="FieldDefinition"
                    p:attributeName="person.name" />
              <bean parent="FieldDefinition"
                    p:attributeName="financialReportsToReportingCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="active" />
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Business Object Lookup Definition -->


  <bean id="ReportingCodes-lookupDefinition" parent="ReportingCodes-lookupDefinition-parentBean" />

  <bean id="ReportingCodes-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
    <property name="title" value="Financial Reporting Code Lookup" />
    <property name="instructions" value="Lookup Financial Reporting Code" />
    <property name="defaultSort" >
      <bean parent="SortDefinition">
        <property name="attributeNames" >
          <list>
            <value>chartOfAccountsCode</value>
          </list>
        </property>
      </bean>
    </property>
    <property name="lookupFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="chartOfAccountsCode" />
        <bean parent="FieldDefinition"
              p:attributeName="organizationCode" />
        <bean parent="FieldDefinition"
              p:attributeName="financialReportingCode" />
        <bean parent="FieldDefinition"
              p:attributeName="active" />
      </list>
    </property>
    <property name="resultFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="chartOfAccountsCode" />
        <bean parent="FieldDefinition"
              p:attributeName="organizationCode" />
        <bean parent="FieldDefinition"
              p:attributeName="financialReportingCode" />
        <bean parent="FieldDefinition"
              p:attributeName="financialReportingCodeDescription" />
        <bean parent="FieldDefinition"
              p:attributeName="person.name" />
        <bean parent="FieldDefinition"
              p:attributeName="financialReportsToReportingCode" />
        <bean parent="FieldDefinition"
              p:attributeName="active" />
      </list>
    </property>
  </bean>
</beans>

