<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "../../../core/datadictionary/dataDictionary-1_0.dtd">
<dictionaryEntry>
	<businessObject
		businessObjectClass="org.kuali.module.chart.bo.ReportingCodes">
		<customAttributeEnabled>false</customAttributeEnabled>

		<inquiry>
			<title>Reporting Codes Inquiry</title>

			<inquiryFields>
				<field attributeName="chartOfAccountsCode" />			
				<field attributeName="organizationCode" />
				<field attributeName="financialReportingCode" />			
				<field attributeName="financialReportingCodeDescription" />
				<field attributeName="financialReportingCodeMgrId" />
				<field attributeName="financialReportsToReportingCode" />
			</inquiryFields>
		</inquiry>

		<lookup>
			<title>Reporting Codes Lookup</title>

			<menubar><![CDATA[<a href="maintenance.do?methodToCall=start&businessObjectClassName=org.kuali.module.chart.bo.ReportingCodes">Create New</a>]]></menubar>

			<instructions>Lookup Reporting Codes</instructions>

			<defaultSort attributeName="chartOfAccountsCode" sortAscending="true" />

			<lookupFields>
				<lookupField attributeName="chartOfAccountsCode" />
				<lookupField attributeName="organizationCode" />
				<lookupField attributeName="financialReportingCode" />	
			</lookupFields>

			<resultFields>
				<field attributeName="chartOfAccountsCode" />			
				<field attributeName="organizationCode" />
				<field attributeName="financialReportingCode" />			
				<field attributeName="financialReportingCodeDescription" />
				<field attributeName="financialReportingCodeMgrId" />
				<field attributeName="financialReportsToReportingCode" />
			</resultFields>
		</lookup>

		<attributes>							

			<attributeReference name="chartOfAccountsCode" 
								sourceClassName="org.kuali.module.chart.bo.Chart" 
								sourceAttributeName="chartOfAccountsCode" />
			
            <attributeReference name="organizationCode"
                                sourceClassName="org.kuali.module.chart.bo.Org" 
				                sourceAttributeName="organizationCode" />
         
			<attribute name="financialReportingCode" forceUppercase="true">
				<label>Financial Reporting Code</label>
				<shortLabel>financialReportingCode</shortLabel>

				<maxLength>10</maxLength>
				<validationPattern><alphaNumeric /></validationPattern>

				<control>
					<text size="4" />
				</control>

				<summary>financialReportingCode</summary>
				<description>Financial Reporting Code is a unique identifier of the type of a document.</description>
			</attribute>
			
			<attribute name="financialReportingCodeDescription" forceUppercase="true">
				<label>Financial Reporting Code Description</label>
				<shortLabel>financialReportingCodeDescription</shortLabel>

				<maxLength>40</maxLength>
				<validationPattern><alphaNumeric /></validationPattern>

				<control>
					<text size="4" />
				</control>

				<summary>financialReportingCodeDescription</summary>
				<description>financialReportingCodeDescription is a unique identifier of the type of a document.</description>
			</attribute>
			
			<attribute name="financialReportingCodeMgrId" forceUppercase="true">
				<label>Financial Reporting Code Manager Id</label>
				<shortLabel>financialReportingCodeMgrId</shortLabel>

				<maxLength>10</maxLength>
				<validationPattern><alphaNumeric /></validationPattern>

				<control>
					<text size="4" />
				</control>

				<summary>financialReportingCodeMgrId</summary>
				<description>Financial Reporting Code Manager Id is a unique identifier of the type of a document.</description>
			</attribute>
			
			<attribute name="financialReportsToReportingCode" forceUppercase="true">
				<label>Financial Reports To Reporting Code</label>
				<shortLabel>financialReportsToReportingCode</shortLabel>

				<maxLength>10</maxLength>
				<validationPattern><alphaNumeric /></validationPattern>

				<control>
					<text size="4" />
				</control>

				<summary>Financial Reports To Reporting Code</summary>
				<description>Financial Reports To Reporting Code is a unique identifier of the type of a document.</description>
			</attribute>

		    <attributeReference name="versionNumber" 
								sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" 
								sourceAttributeName="versionNumber" />
			
		    <attributeReference name="objectId" 
								sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" 
								sourceAttributeName="objectId" />
			
		</attributes>
	</businessObject>
</dictionaryEntry>

