<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="BalanceTyp" parent="BalanceTyp-parentBean" />

  <bean id="BalanceTyp-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.module.chart.bo.codes.BalanceTyp" />
    <property name="inquiryDefinition" >
      <ref bean="BalanceTyp-inquiryDefinition" />
    </property>
    <property name="lookupDefinition" >
      <ref bean="BalanceTyp-lookupDefinition" />
    </property>
    <property name="objectLabel" value="Balance Type" />
    <property name="attributes" >
      <list>
        <ref bean="BalanceTyp-code" />
        <ref bean="BalanceTyp-name" />
        <ref bean="BalanceTyp-financialBalanceTypeShortNm" />
        <ref bean="BalanceTyp-financialOffsetGenerationIndicator" />
        <ref bean="BalanceTyp-finBalanceTypeEncumIndicator" />
        <ref bean="BalanceTyp-objectId" />
        <ref bean="BalanceTyp-versionNumber" />
        <ref bean="BalanceTyp-active" />
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->


  <bean id="BalanceTyp-code" parent="BalanceTyp-code-parentBean" />

  <bean id="BalanceTyp-code-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="code" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Balance Type Code" />
    <property name="shortLabel" value="Balance Type" />
    <property name="maxLength" value="2" />
    <property name="validationPattern" >
      <bean parent="AlphaNumericValidationPattern" />
    </property>
    <property name="required" value="true" />
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="2" />
    </property>
  </bean>
  <bean id="BalanceTyp-name" parent="BalanceTyp-name-parentBean" />

  <bean id="BalanceTyp-name-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="name" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Balance Type Name" />
    <property name="shortLabel" value="Name" />
    <property name="maxLength" value="40" />
    <property name="required" value="true" />
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="40" />
    </property>
  </bean>
  <bean id="BalanceTyp-financialBalanceTypeShortNm" parent="BalanceTyp-financialBalanceTypeShortNm-parentBean" />

  <bean id="BalanceTyp-financialBalanceTypeShortNm-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="financialBalanceTypeShortNm" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Short Name" />
    <property name="shortLabel" value="Short Name" />
    <property name="maxLength" value="12" />
    <property name="required" value="true" />
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="12" />
    </property>
  </bean>
  <bean id="BalanceTyp-financialOffsetGenerationIndicator" parent="BalanceTyp-financialOffsetGenerationIndicator-parentBean" />

  <bean id="BalanceTyp-financialOffsetGenerationIndicator-parentBean" abstract="true" parent="GenericAttributes-genericBoolean">
    <property name="name" value="financialOffsetGenerationIndicator" />
    <property name="label" value="Offset Generation" />
    <property name="shortLabel" value="Offset Gen" />
  </bean>
  <bean id="BalanceTyp-finBalanceTypeEncumIndicator" parent="BalanceTyp-finBalanceTypeEncumIndicator-parentBean" />

  <bean id="BalanceTyp-finBalanceTypeEncumIndicator-parentBean" abstract="true" parent="GenericAttributes-genericBoolean">
    <property name="name" value="finBalanceTypeEncumIndicator" />
    <property name="label" value="Encumbrance" />
    <property name="shortLabel" value="Encumbrance" />
  </bean>
  <bean id="BalanceTyp-objectId" parent="BalanceTyp-objectId-parentBean" />

  <bean id="BalanceTyp-objectId-parentBean" abstract="true" parent="GenericAttributes-objectId">
  </bean>
  <bean id="BalanceTyp-versionNumber" parent="BalanceTyp-versionNumber-parentBean" />

  <bean id="BalanceTyp-versionNumber-parentBean" abstract="true" parent="GenericAttributes-versionNumber">
  </bean>
  <bean id="BalanceTyp-active" parent="BalanceTyp-active-parentBean" />

  <bean id="BalanceTyp-active-parentBean" abstract="true" parent="GenericAttributes-activeIndicator">
    <property name="name" value="active" />
  </bean>

<!-- Business Object Inquiry Definition -->


  <bean id="BalanceTyp-inquiryDefinition" parent="BalanceTyp-inquiryDefinition-parentBean" />

  <bean id="BalanceTyp-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
    <property name="title" value="Balance Type Inquiry" />
    <property name="inquirySections" >
      <list>
        <bean parent="InquirySectionDefinition">
          <property name="title" value="Header Text" />
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields" >
            <list>
              <bean parent="FieldDefinition"
                    p:attributeName="code" />
              <bean parent="FieldDefinition"
                    p:attributeName="name" />
              <bean parent="FieldDefinition"
                    p:attributeName="financialBalanceTypeShortNm" />
              <bean parent="FieldDefinition"
                    p:attributeName="financialOffsetGenerationIndicator" />
              <bean parent="FieldDefinition"
                    p:attributeName="finBalanceTypeEncumIndicator" />
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Business Object Lookup Definition -->


  <bean id="BalanceTyp-lookupDefinition" parent="BalanceTyp-lookupDefinition-parentBean" />

  <bean id="BalanceTyp-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
    <property name="title" value="Balance Type Lookup" />
    <property name="instructions" value="Lookup a Balance Type" />
    <property name="defaultSort" >
      <bean parent="SortDefinition">
        <property name="attributeNames" >
          <list>
            <value>code</value>
          </list>
        </property>
      </bean>
    </property>
    <property name="lookupFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="code" />
        <bean parent="FieldDefinition"
              p:attributeName="name" />
        <bean parent="FieldDefinition"
              p:attributeName="financialBalanceTypeShortNm" />
        <bean parent="FieldDefinition"
              p:attributeName="financialOffsetGenerationIndicator" />
        <bean parent="FieldDefinition"
              p:attributeName="finBalanceTypeEncumIndicator" />
      </list>
    </property>
    <property name="resultFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="code" />
        <bean parent="FieldDefinition"
              p:attributeName="name" />
        <bean parent="FieldDefinition"
              p:attributeName="financialBalanceTypeShortNm" />
        <bean parent="FieldDefinition"
              p:attributeName="financialOffsetGenerationIndicator" />
        <bean parent="FieldDefinition"
              p:attributeName="finBalanceTypeEncumIndicator" />
      </list>
    </property>
  </bean>
</beans>
