<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="IndirectCostRecoveryExclusionType" parent="IndirectCostRecoveryExclusionType-parentBean" />
  <bean id="IndirectCostRecoveryExclusionType-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.kfs.coa.businessobject.IndirectCostRecoveryExclusionType" />
    <property name="inquiryDefinition" >
      <ref bean="IndirectCostRecoveryExclusionType-inquiryDefinition" />
    </property>
    <property name="lookupDefinition" >
      <ref bean="IndirectCostRecoveryExclusionType-lookupDefinition" />
    </property>
    <property name="objectLabel" value="Indirect Cost Recovery Exclusion By Type" />
    <property name="attributes" >
      <list>
        <ref bean="IndirectCostRecoveryExclusionType-accountIndirectCostRecoveryTypeCode" />
        <ref bean="IndirectCostRecoveryExclusionType-chartOfAccountsCode" />
        <ref bean="IndirectCostRecoveryExclusionType-financialObjectCode" />
        <ref bean="IndirectCostRecoveryExclusionType-versionNumber" />
        <ref bean="IndirectCostRecoveryExclusionType-active" />
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->

  <bean id="IndirectCostRecoveryExclusionType-accountIndirectCostRecoveryTypeCode" parent="IndirectCostRecoveryExclusionType-accountIndirectCostRecoveryTypeCode-parentBean" />
  <bean id="IndirectCostRecoveryExclusionType-accountIndirectCostRecoveryTypeCode-parentBean" abstract="true" parent="IndirectCostRecoveryType-code">
    <property name="name" value="accountIndirectCostRecoveryTypeCode" />
    <property name="label" value="Type Code" />
    <property name="shortLabel" value="Type" />
    <property name="maxLength" value="2" />
  </bean>
  
  <bean id="IndirectCostRecoveryExclusionType-chartOfAccountsCode" parent="IndirectCostRecoveryExclusionType-chartOfAccountsCode-parentBean" />
  <bean id="IndirectCostRecoveryExclusionType-chartOfAccountsCode-parentBean" abstract="true" parent="Chart-chartOfAccountsCode">
    <property name="name" value="chartOfAccountsCode" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Chart Code" />
    <property name="shortLabel" value="Chart" />
    <property name="maxLength" value="2" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="required" value="true" />
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="2" />
    </property>
  </bean>
  
  <bean id="IndirectCostRecoveryExclusionType-financialObjectCode" parent="IndirectCostRecoveryExclusionType-financialObjectCode-parentBean" />
  <bean id="IndirectCostRecoveryExclusionType-financialObjectCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="financialObjectCode" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Object Code" />
    <property name="shortLabel" value="Object" />
    <property name="maxLength" value="4" />
    <property name="required" value="true" />
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="4" />
    </property>
  </bean>
  
  <bean id="IndirectCostRecoveryExclusionType-versionNumber" parent="IndirectCostRecoveryExclusionType-versionNumber-parentBean" />
  <bean id="IndirectCostRecoveryExclusionType-versionNumber-parentBean" abstract="true" parent="GenericAttributes-versionNumber">
  </bean>
  
  <bean id="IndirectCostRecoveryExclusionType-active" parent="IndirectCostRecoveryExclusionType-active-parentBean" />
  <bean id="IndirectCostRecoveryExclusionType-active-parentBean" abstract="true" parent="AttributeReferenceDummy-activeIndicator">
    <property name="name" value="active" />
  </bean>

<!-- Business Object Inquiry Definition -->

  <bean id="IndirectCostRecoveryExclusionType-inquiryDefinition" parent="IndirectCostRecoveryExclusionType-inquiryDefinition-parentBean" />
  <bean id="IndirectCostRecoveryExclusionType-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
    <property name="title" value="Indirect Cost Recovery Exclusion By Type Inquiry" />
    <property name="inquirySections" >
      <list>
        <bean parent="InquirySectionDefinition">
          <property name="title" value="" />
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields" >
            <list>
              <bean parent="FieldDefinition"
                    p:attributeName="accountIndirectCostRecoveryTypeCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="chartOfAccountsCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="objectCodeCurrent.financialObjectCode" />
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Business Object Lookup Definition -->

  <bean id="IndirectCostRecoveryExclusionType-lookupDefinition" parent="IndirectCostRecoveryExclusionType-lookupDefinition-parentBean" />
  <bean id="IndirectCostRecoveryExclusionType-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
    <property name="title" value="Indirect Cost Recovery Exclusion By Type Lookup" />
    <property name="lookupableID" value="indirectCostRecoveryExclusionByTypeLookupable" />
    <property name="instructions" value="Lookup an Indirect Cost Recovery Exclusion By type" />
    <property name="defaultSort" >
      <bean parent="SortDefinition">
        <property name="attributeNames" >
          <list>
            <value>accountIndirectCostRecoveryTypeCode</value>
          </list>
        </property>
      </bean>
    </property>
    <property name="lookupFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="accountIndirectCostRecoveryTypeCode" />
        <bean parent="FieldDefinition"
              p:attributeName="chartOfAccountsCode" />
        <bean parent="FieldDefinition"
              p:attributeName="financialObjectCode" />
      </list>
    </property>
    <property name="resultFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="accountIndirectCostRecoveryTypeCode" />
        <bean parent="FieldDefinition"
              p:attributeName="chartOfAccountsCode" />
        <bean parent="FieldDefinition"
              p:attributeName="financialObjectCode" />
      </list>
    </property>
  </bean>
  
</beans>