<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "../../../core/datadictionary/dataDictionary-1_0.dtd">
<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.module.chart.bo.UniversityBudgetOfficeFunction">
        <customAttributeEnabled>false</customAttributeEnabled>

        <inquiry>
            <title>University Budget Office Function Inquiry</title>

            <inquiryFields>
                <field attributeName="financialUniversityBudgetOfficeFunctionCode" />
                <field attributeName="financialUniversityBudgetOfficeFunctionName" />
            </inquiryFields>
        </inquiry>

        <lookup>
            <title>University Budget Office Function Lookup</title>

            <menubar>
                <![CDATA[<a href="maintenance.do?methodToCall=start&businessObjectClassName=org.kuali.module.chart.bo.UniversityBudgetOfficeFunction"><img src="images/tinybutton-createnew.gif" alt="create new" width="70" height="15"/></a>]]>
            </menubar>

            <instructions>Lookup a University Budget Office Function</instructions>

            <defaultSort attributeName="financialUniversityBudgetOfficeFunctionCode" sortAscending="true" />

            <lookupFields>
                <lookupField attributeName="financialUniversityBudgetOfficeFunctionCode" />
                <lookupField attributeName="financialUniversityBudgetOfficeFunctionName" />
            </lookupFields>

            <resultFields>
                <field attributeName="financialUniversityBudgetOfficeFunctionCode" />
                <field attributeName="financialUniversityBudgetOfficeFunctionName" />
            </resultFields>
        </lookup>

        <titleAttribute>financialUniversityBudgetOfficeFunctionCode</titleAttribute>

        <attributes>

            <attribute name="financialUniversityBudgetOfficeFunctionCode" forceUppercase="true">
                <label>University Budget Office Function Code</label>
                <shortLabel>UBOC</shortLabel>

                <maxLength>3</maxLength>
                <validationPattern>
                    <anyCharacter />
                </validationPattern>
                <required>true</required>

                <control>
                    <text size="3" />
                </control>

                <summary>University Budget Office Function Code</summary>
                <description>The code uniquely identifying a particular University Budget Office Function Code.</description>
            </attribute>

            <attribute name="financialUniversityBudgetOfficeFunctionName" forceUppercase="true">
                <label>University Budget Office Function Name</label>
                <shortLabel>Name</shortLabel>

                <maxLength>40</maxLength>
                <required>true</required>

                <control>
                    <text size="40" />
                </control>

                <summary>University Budget Office Function Name</summary>
                <description>University Budget Office Function Name...</description>
            </attribute>


            <attributeReference name="versionNumber" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="versionNumber" />

        </attributes>
    </businessObject>
</dictionaryEntry>

