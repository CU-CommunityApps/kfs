<dictionaryEntry>
	<businessObject businessObjectClass="org.kuali.module.chart.bo.OrganizationExtension">

		<attributes>
			<attributeReference name="chartOfAccountsCode" 
						sourceClassName="org.kuali.module.chart.bo.Chart" 
						sourceAttributeName="chartOfAccountsCode" />
								
     		<attributeReference name="organizationCode"
				        sourceClassName="org.kuali.module.chart.bo.Org"
				        sourceAttributeName="organizationCode" />

      		<attribute name="organizationExtension.hrmsCompany" forceUppercase="false">
				<label>HRMS Company</label>
				<shortLabel>HRMS Company</shortLabel>

				<maxLength>3</maxLength>
				<validationPattern>
				<alpha />
        		</validationPattern>
				<required>false</required>
		
		        <control>
		          <text size="3" />
		        </control>

		        <summary><![CDATA[&nbsp;]]></summary>
		        <description>The company in the HRMS system to which this organization belongs.</description>
		      </attribute>

								
			<attribute name="hrmsIuOrganizationAddress2" forceUppercase="false">
        		<label>Address-1</label>
        		<shortLabel>Address-1</shortLabel>
        		
        		<maxLength>30</maxLength>
        		<validationPattern><anyCharacter maxLength="30" allowWhitespace="true" /></validationPattern>
        		
				<control>
					<text size="30" />
				</control>

        		<summary><![CDATA[&nbsp;]]></summary>
        		<description>The first line of address where correspondence can be sent related to HRMS issues for this organization.</description>
			</attribute>
			
			<attribute name="hrmsIuOrganizationAddress3" forceUppercase="false">
        		<label>Address-2</label>
        		<shortLabel>Address-2</shortLabel>
        		
        		<maxLength>30</maxLength>
        		<validationPattern><anyCharacter maxLength="30" allowWhitespace="true" /></validationPattern>
        		
				<control>
					<text size="30" />
				</control>

        		<summary><![CDATA[&nbsp;]]></summary>
        		<description>The second line of address where correspondence can be sent related to HRMS issues for this organization.</description>
			</attribute>

			
			<attribute name="hrmsIuCampusCode" forceUppercase="false">
        		<label>Campus</label>
        		<shortLabel>Campus</shortLabel>
        		
        		<maxLength>2</maxLength>
        		<validationPattern><anyCharacter maxLength="2" /></validationPattern>
        		
				<control>
					<select valuesFinderClass="org.kuali.core.lookup.keyvalues.CampusValuesFinder" />
				</control>

        		<summary><![CDATA[&nbsp;]]></summary>
        		<description>The campus where correspondence can be sent related to HRMS issues for this organization.</description>
			</attribute>
			
			<attribute name="hrmsIuCampusBuilding" forceUppercase="false">
        		<label>Building</label>
        		<shortLabel>Bldg</shortLabel>
        		
        		<maxLength>10</maxLength>
        		<validationPattern><anyCharacter maxLength="10" /></validationPattern>
        		
				<control>
					<text size="10" />
				</control>

        		<summary><![CDATA[&nbsp;]]></summary>
        		<description>The building where correspondence can be sent related to HRMS issues for this organization.</description>
			</attribute>
			
			<attribute name="hrmsIuCampusRoom" forceUppercase="false">
        		<label>Room</label>
        		<shortLabel>Room</shortLabel>
        		
        		<maxLength>8</maxLength>
        		<validationPattern><anyCharacter maxLength="8" /></validationPattern>
        		
				<control>
					<text size="8" />
				</control>

        		<summary><![CDATA[&nbsp;]]></summary>
        		<description>The room where correspondence can be sent related to HRMS issues for this organization.</description>
			</attribute>
			
