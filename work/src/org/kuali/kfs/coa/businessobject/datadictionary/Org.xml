<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.module.chart.bo.Org">


        <inquiry>
            <title>Organization Code Inquiry</title>
			<inquirableClass>org.kuali.module.chart.maintenance.OrgInquirable</inquirableClass>

            <inquirySections><inquirySection title="Header Text"><inquiryFields>

                <field attributeName="chartOfAccountsCode" />
                <field attributeName="organizationCode" />
                <field attributeName="organizationName" />
                <field attributeName="organizationManagerUniversalId" />
                <field attributeName="organizationManagerUniversal.personUserIdentifier" />
                <field attributeName="organizationManagerUniversal.personName" />
                <field attributeName="responsibilityCenterCode" />
                <field attributeName="organizationPhysicalCampusCode" />
                <field attributeName="organizationTypeCode" />
                <field attributeName="organizationDefaultAccountNumber" />
                <field attributeName="organizationZipCode" />
                <field attributeName="organizationLine1Address" />
                <field attributeName="organizationLine2Address" />             
                <field attributeName="organizationCityName" />
                <field attributeName="organizationStateCode" />
                <field attributeName="organizationCountryCode" />               
                <field attributeName="organizationBeginDate" />
                <field attributeName="organizationEndDate" />
                <field attributeName="reportsToChartOfAccountsCode" />
                <field attributeName="reportsToOrganizationCode" />
                <field attributeName="organizationActiveIndicator" />

                <field attributeName="editPlantAccountsSectionBlank" />
                <field attributeName="editPlantAccountsSection" />

                <field attributeName="organizationPlantChartCode" />
                <field attributeName="organizationPlantAccountNumber" />
                <field attributeName="campusPlantChartCode" />
                <field attributeName="campusPlantAccountNumber" />

		        <field attributeName="editHrmsUnitSectionBlank" />
		        <field attributeName="editHrmsUnitSection" />     
		        <field attributeName="organizationExtension.hrmsCompany" />       
		        <field attributeName="organizationExtension.hrmsIuOrganizationAddress2" />       
		        <field attributeName="organizationExtension.hrmsIuOrganizationAddress3" />       
