		        <field attributeName="organizationExtension.hrmsIuCampusBuilding" />              
		        <field attributeName="organizationExtension.hrmsIuCampusRoom" />       
		        <field attributeName="organizationExtension.hrmsIuPositionAllowedFlag" />
		        <field attributeName="organizationExtension.hrmsIuTenureAllowedFlag" />
		        <field attributeName="organizationExtension.hrmsIuTitleAllowedFlag" />
		        <field attributeName="organizationExtension.hrmsIuOccupationalUnitAllowedFlag" />
		
		        <field attributeName="organizationExtension.hrmsPersonnelApproverUniversalId" />
		        <field attributeName="organizationExtension.hrmsPersonnelApproverUniversal.personUserIdentifier" />
		        <field attributeName="organizationExtension.hrmsPersonnelApproverUniversal.personName" />
		        
		        <field attributeName="organizationExtension.fiscalApproverUniversalId" />
		        <field attributeName="organizationExtension.fiscalApproverUniversal.personUserIdentifier" />
		        <field attributeName="organizationExtension.fiscalApproverUniversal.personName" />
		        
		        <field attributeName="organizationExtension.hrmsLastUpdateDate" />
            </inquiryFields></inquirySection></inquirySections>
        </inquiry>

        <lookup>
            <title>Organization Code Lookup</title>

            <instructions>Lookup an Organization</instructions>

            <defaultSort attributeName="chartOfAccountsCode" sortAscending="false" />

            <lookupFields>
                <lookupField attributeName="organizationName" required="false" />
                <lookupField attributeName="chartOfAccountsCode" required="false" />
                <lookupField attributeName="organizationCode" required="false" forceLookup="true" />
                <lookupField attributeName="responsibilityCenterCode" required="false" />
                <lookupField attributeName="organizationTypeCode" required="false" />
                <lookupField attributeName="reportsToChartOfAccountsCode" required="false"  />
                <lookupField attributeName="reportsToOrganizationCode" required="false" />
                <lookupField attributeName="organizationActiveIndicator" required="false">
                    <defaultValue>Y</defaultValue>
                </lookupField>

            </lookupFields>

            <resultFields>
                <field attributeName="chartOfAccountsCode"  />
                <field attributeName="organizationCode" />
                <field attributeName="responsibilityCenterCode" />
                <field attributeName="organizationTypeCode" />
                <field attributeName="organizationName" />
                <field attributeName="reportsToChartOfAccountsCode" />
                <field attributeName="reportsToOrganizationCode" />
                <field attributeName="organizationActiveIndicator" />               
                <field attributeName="organizationManagerUniversal.personName" />
            </resultFields>
        </lookup>

        <titleAttribute>organizationCode</titleAttribute>

        <attributes>

            <attributeReference name="chartOfAccountsCode" sourceClassName="org.kuali.module.chart.bo.Chart" sourceAttributeName="chartOfAccountsCode">
                <required>true</required>
                <control>
                    <text size="2" />
                </control>
            </attributeReference>

            <attribute name="organizationCode" forceUppercase="true">
                <label>Organization Code</label>
                <shortLabel>Org</shortLabel>

                <maxLength>4</maxLength>
                <validationPattern>
                    <anyCharacter />
                </validationPattern>
                <required>true</required>

                <control>
                    <text size="4" />
                </control>
            </attribute>

            <attribute name="organizationName" forceUppercase="true">
                <label>Organization Name</label>
                <shortLabel>Org Name</shortLabel>

                <maxLength>40</maxLength>
                <validationPattern>
                    <anyCharacter allowWhitespace="true" />
                </validationPattern>
                <required>true</required>

                <control>
                    <text size="40" />
                </control>
            </attribute>


            <attribute name="codeAndDescription" forceUppercase="false">
                <label>Organization Code and Description</label>
                <shortLabel>Org Code and Des</shortLabel>

                <maxLength>45</maxLength>

                <control>
                    <text size="45" />
                </control>
            </attribute>


            <attributeReference name="organizationManagerUniversalId" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="genericSystemId">
                <label>Organization Manager UserID</label>
                <shortLabel>Manager UserID</shortLabel>
                <required>true</required>
                <control>
                    <hidden />
                </control>
            </attributeReference>

            <attributeReference name="organizationManagerUniversal.personUserIdentifier" sourceClassName="org.kuali.core.bo.user.UniversalUser" sourceAttributeName="personUserIdentifier">
                <label>Organization Manager UserID</label>
                <shortLabel>OrgMgrUserId</shortLabel>
                <required>false</required>
                <control>
                    <kualiUser universalIdAttributeName="organizationManagerUniversalId" userIdAttributeName="organizationManagerUniversal.personUserIdentifier" personNameAttributeName="organizationManagerUniversal.personName" />
                </control>
            </attributeReference>

            <attributeReference name="organizationManagerUniversal.personName" sourceClassName="org.kuali.core.bo.user.UniversalUser" sourceAttributeName="personName">
                <label>Organization Manager Name</label>
                <shortLabel>Name</shortLabel>
                <required>false</required>
                <control>
                    <hidden />
                </control>
            </attributeReference>

            <attribute name="responsibilityCenterCode" forceUppercase="true">
                <label>Responsibility Center Code</label>
                <shortLabel>RC</shortLabel>

                <maxLength>2</maxLength>
                <validationPattern>
                    <anyCharacter exactLength="2" />
                </validationPattern>
                <required>true</required>

                <control>
                    <text size="2" />
                </control>
            </attribute>

            <attribute name="organizationPhysicalCampusCode" forceUppercase="true">
                <label>Organization Physical Campus Code</label>
                <shortLabel>Physical Campus</shortLabel>

                <maxLength>2</maxLength>
                <validationPattern>
                    <alpha exactLength="2" />
                </validationPattern>
                <required>true</required>

                <control>
                    <text size="2" />
                </control>
            </attribute>

            <attribute name="organizationTypeCode" forceUppercase="false">
                <label>Organization Type</label>
                <shortLabel>Type</shortLabel>

                <maxLength>1</maxLength>
                <validationPattern>
                    <alpha exactLength="1" />
                </validationPattern>
                <required>true</required>

                <control>
                    <select valuesFinderClass="org.kuali.module.chart.lookup.keyvalues.OrgTypeValuesFinder" />
                </control>
            </attribute>

            <attributeReference name="organizationDefaultAccountNumber" sourceClassName="org.kuali.module.chart.bo.Account" sourceAttributeName="accountNumber">
                <label>Organization Default Account Number</label>
                <shortLabel>Default Acct</shortLabel>
                <required>false</required>
            </attributeReference>

            <attributeReference name="organizationPlantAccountNumber" sourceClassName="org.kuali.module.chart.bo.Account" sourceAttributeName="accountNumber">
                <label>Plant Account Number</label>
                <shortLabel>Plant Acct</shortLabel>
                <required>false</required>
            </attributeReference>

            <attributeReference name="organizationPlantChartCode" sourceClassName="org.kuali.module.chart.bo.Chart" sourceAttributeName="chartOfAccountsCode">
                <label>Organization Plant Chart</label>
                <shortLabel>Plant Chart</shortLabel>
                <required>false</required>
                <control>
                    <select valuesFinderClass="org.kuali.module.chart.lookup.keyvalues.ChartValuesFinder" />
                </control>
            </attributeReference>

            <attribute name="organizationZipCode" forceUppercase="false">
                <label>Organization Postal Code</label>
                <shortLabel>Postal Code</shortLabel>

                <maxLength>11</maxLength>
                <validationPattern>
                    <zipcode />
                </validationPattern>
                <required>true</required>

                <control>
                    <text size="11" />
                </control>
            </attribute>

            <attribute name="organizationStateCode" forceUppercase="true">
                <label>Organization State/Province Code</label>
                <shortLabel>St Cd</shortLabel>

                <maxLength>2</maxLength>
                <validationPattern>
                    <alpha exactLength="2" />
                </validationPattern>
                <required>true</required>

                <control>
                    <text size="2" />
                </control>
            </attribute>

            <attribute name="organizationCountryCode" forceUppercase="true">
                <label>Organization Country Code</label>
                <shortLabel>Country Cd</shortLabel>

                <maxLength>2</maxLength>
                <validationPattern>
                    <alpha exactLength="2" />
                </validationPattern>
                <required>true</required>

                <control>
                    <text size="2" />
                </control>
            </attribute>
            
            <attribute name="organizationLine1Address" forceUppercase="false">
                <label>Organization Address Line 1</label>
                <shortLabel>Street</shortLabel>

                <maxLength>30</maxLength>
                <validationPattern>
                    <anyCharacter allowWhitespace="true" />
                </validationPattern>
                <required>true</required>

                <control>
                    <text size="30" />
                </control>
            </attribute>

            <attribute name="organizationLine2Address" forceUppercase="false">
                <label>Organization Address Line 2</label>
                <shortLabel>Street</shortLabel>

                <maxLength>30</maxLength>
                <validationPattern>
                    <anyCharacter allowWhitespace="true" />
                </validationPattern>
                <required>false</required>

                <control>
                    <text size="30" />
                </control>
            </attribute>
            <attribute name="organizationCityName" forceUppercase="false">
                <label>Organization City Name</label>
                <shortLabel>City</shortLabel>

                <maxLength>25</maxLength>
                <validationPattern>
                    <anyCharacter allowWhitespace="true" />
                </validationPattern>
                <required>true</required>

                <control>
                    <text size="25" />
                </control>
            </attribute>

            <attributeReference name="reportsToChartOfAccountsCode" sourceClassName="org.kuali.module.chart.bo.Chart" sourceAttributeName="chartOfAccountsCode">
                <label>Reports to Chart Code</label>
                <shortLabel>Rpts to Chart</shortLabel>
                <required>true</required>
       	 		<control>
        	 	 <text size="2" />
        		</control>
            </attributeReference>

            <attributeReference name="reportsToOrganizationCode" sourceClassName="org.kuali.module.chart.bo.Org" sourceAttributeName="organizationCode">
                <label>Reports to Organization</label>
                <shortLabel>Rpts to Org</shortLabel>
                <required>true</required>
            </attributeReference>

            <attributeReference name="organizationBeginDate" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="genericTimestamp">
                <label>Organization Begin Date</label>
                <shortLabel>Begin Date</shortLabel>
                <required>true</required>
                <formatterClass>org.kuali.core.web.format.DateFormatter</formatterClass>
            </attributeReference>

            <attributeReference name="organizationEndDate" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="genericTimestamp">
                <label>Organization End Date</label>
                <shortLabel>End Date</shortLabel>
            </attributeReference>

            <attributeReference name="organizationActiveIndicator" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="genericBoolean">
                <label>Active</label>
                <shortLabel>Act.</shortLabel>
            </attributeReference>

            <attributeReference name="campusPlantAccountNumber" sourceClassName="org.kuali.module.chart.bo.Account" sourceAttributeName="accountNumber">
                <label>Campus Plant Account Number</label>
                <shortLabel>Plant Acct</shortLabel>
                <required>false</required>
            </attributeReference>

            <attributeReference name="campusPlantChartCode" sourceClassName="org.kuali.module.chart.bo.Chart" sourceAttributeName="chartOfAccountsCode">
                <label>Campus Plant Chart Code</label>
                <shortLabel>Chart Code</shortLabel>
                <required>false</required>
                <control>
                    <select valuesFinderClass="org.kuali.module.chart.lookup.keyvalues.ChartValuesFinder" />
                </control>
            </attributeReference>

            <attribute name="organizationInFinancialProcessingIndicator" forceUppercase="false">
                <label>Organization In Financial Processing</label>
                <shortLabel>In Fin Processing</shortLabel>

                <maxLength>1</maxLength>

                <control>
                    <checkbox />
                </control>
            </attribute>

            <attribute name="editPlantAccountsSection" forceUppercase="false">
                <label>EDIT PLANT ACCOUNT SECTION</label>
                <maxLength>50</maxLength>
                <control>
                    <checkbox />
                </control>
            </attribute>

            <attributeReference name="versionNumber" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="versionNumber" />

		<attribute name="editHrmsUnitSection" forceUppercase="false">
			<label>EDIT HRMS UNIT SECTION</label>
			<maxLength>50</maxLength>
       		<control><checkbox /></control>
		</attribute>

		<attribute name="organizationExtension.hrmsCompany" forceUppercase="false">
	        <label>HRMS Company</label>
	        <shortLabel>HRMS Company</shortLabel>
	
	        <maxLength>3</maxLength>
	        <validationPattern>
	          <alpha />
	        </validationPattern>
			<required>true</required>
		
       	 	<control>
        	  <text size="3" />
        	</control>
		</attribute>

		<attributeReference name="organizationExtension.hrmsIuOrganizationAddress2" 
								sourceClassName="org.kuali.module.chart.bo.OrganizationExtension" 
								sourceAttributeName="hrmsIuOrganizationAddress2" />
								
		<attributeReference name="organizationExtension.hrmsIuOrganizationAddress3" 
								sourceClassName="org.kuali.module.chart.bo.OrganizationExtension" 
								sourceAttributeName="hrmsIuOrganizationAddress3" />

		<attributeReference name="organizationExtension.hrmsIuCampusCode" 
								sourceClassName="org.kuali.module.chart.bo.OrganizationExtension" 
								sourceAttributeName="hrmsIuCampusCode" />
								
		<attributeReference name="organizationExtension.hrmsIuCampusBuilding" 
								sourceClassName="org.kuali.module.chart.bo.OrganizationExtension" 
								sourceAttributeName="hrmsIuCampusBuilding" />
      
		<attributeReference name="organizationExtension.hrmsIuCampusRoom" 
								sourceClassName="org.kuali.module.chart.bo.OrganizationExtension" 
								sourceAttributeName="hrmsIuCampusRoom" />
								
      <attributeReference name="organizationExtension.hrmsPersonnelApproverUniversalId"
                          sourceClassName="org.kuali.core.bo.user.UniversalUser"
                          sourceAttributeName="personUniversalIdentifier">
        <label>Personnel Approver UserID</label>
        <shortLabel>Personnel Approver UserID</shortLabel>
        <required>false</required>
        <control><hidden /></control>
      </attributeReference>

	  <attributeReference name="organizationExtension.hrmsPersonnelApproverUniversal.personUserIdentifier" 
	  					  sourceClassName="org.kuali.core.bo.user.UniversalUser" 
	  					  sourceAttributeName="personUserIdentifier">
			<label>Personnel Approver UserID</label>
			<shortLabel>Personnel Approver</shortLabel>
			<required>false</required>
			<control>
				<kualiUser universalIdAttributeName="organizationExtension.hrmsPersonnelApproverUniversalId" 
							userIdAttributeName="organizationExtension.hrmsPersonnelApproverUniversal.personUserIdentifier" 
							personNameAttributeName="organizationExtension.hrmsPersonnelApproverUniversal.personName" />
			</control>
	  </attributeReference>
	  
		<attributeReference name="organizationExtension.hrmsPersonnelApproverUniversal.personName" 
							sourceClassName="org.kuali.core.bo.user.UniversalUser" 
							sourceAttributeName="personName">
			<label>Personnel Approver Name</label>
			<shortLabel>Name</shortLabel>
			<required>false</required>
			<control> <hidden /></control>
		</attributeReference>

      <attributeReference name="organizationExtension.fiscalApproverUniversalId"
                          sourceClassName="org.kuali.core.bo.user.UniversalUser"
                          sourceAttributeName="personUniversalIdentifier">
        <label>Fiscal Approver UserID</label>
        <shortLabel>Fiscal Approver UserID</shortLabel>
        <required>false</required>
        <control><hidden /></control>
      </attributeReference>

	  <attributeReference name="organizationExtension.fiscalApproverUniversal.personUserIdentifier" 
	  					  sourceClassName="org.kuali.core.bo.user.UniversalUser" 
	  					  sourceAttributeName="personUserIdentifier">
			<label>Fiscal Approver UserID</label>
			<shortLabel>Fiscal Approver</shortLabel>
			<required>false</required>
			<control>
				<kualiUser universalIdAttributeName="organizationExtension.fiscalApproverUniversalId" 
							userIdAttributeName="organizationExtension.fiscalApproverUniversal.personUserIdentifier" 
							personNameAttributeName="organizationExtension.fiscalApproverUniversal.personName" />
			</control>
	  </attributeReference>
	  
		<attributeReference name="organizationExtension.fiscalApproverUniversal.personName" 
							sourceClassName="org.kuali.core.bo.user.UniversalUser" 
							sourceAttributeName="personName">
			<label>Fiscal Approver Name</label>
			<shortLabel>Name</shortLabel>
			<required>false</required>
			<control><hidden /></control>
		</attributeReference>

		<attributeReference name="organizationExtension.hrmsIuPositionAllowedFlag" 
					sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" 
					sourceAttributeName="genericBoolean">
			<label>Position Allowed</label>
			<shortLabel>Position Allowed</shortLabel>
			<required>true</required>
		</attributeReference>
		
		<attributeReference name="organizationExtension.hrmsIuTenureAllowedFlag" 
					sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" 
					sourceAttributeName="genericBoolean">
			<label>Tenure Allowed</label>
			<shortLabel>Tenure Allowed</shortLabel>
			<required>true</required>
		</attributeReference>
						
		<attributeReference name="organizationExtension.hrmsIuTitleAllowedFlag" 
					sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" 
					sourceAttributeName="genericBoolean">
			<label>Title Allowed</label>
			<shortLabel>Title Allowed</shortLabel>
			<required>true</required>
		</attributeReference>
						
		<attributeReference name="organizationExtension.hrmsIuOccupationalUnitAllowedFlag" 
					sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" 
					sourceAttributeName="genericBoolean">
			<label>Occupational Unit Allowed</label>
			<shortLabel>Occupational Unit Allowed</shortLabel>
			<required>true</required>
		</attributeReference>

		<attributeReference name="organizationExtension.hrmsLastUpdateDate" 
					sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" 
					sourceAttributeName="genericTimestamp">
			<label>Last Update Date</label>
			<shortLabel>Last Update Date</shortLabel>
		</attributeReference>
												      
		<attributeReference name="organizationExtension.versionNumber" 
    		sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" 
   			sourceAttributeName="versionNumber" />
   			
        </attributes>

        <relationships>
            <relationship objectAttributeName="organizationManagerUniversal" >
                <primitiveAttribute sourceName="organizationManagerUniversalId" targetName="personUniversalIdentifier" />
				<supportAttribute sourceName="organizationManagerUniversal.personUserIdentifier" targetName="personUserIdentifier" identifier="true" />
				<supportAttribute sourceName="organizationManagerUniversal.personName" targetName="personName" />
            </relationship>

            <relationship objectAttributeName="organizationExtension.hrmsPersonnelApproverUniversal" >
                <primitiveAttribute sourceName="organizationExtension.hrmsPersonnelApproverUniversalId" targetName="personUniversalIdentifier" />
                <supportAttribute sourceName="organizationExtension.hrmsPersonnelApproverUniversal.personUserIdentifier" targetName="personUserIdentifier" identifier="true" />
                <supportAttribute sourceName="organizationExtension.hrmsPersonnelApproverUniversal.personName" targetName="personName" />
            </relationship>

            <relationship objectAttributeName="organizationExtension.fiscalApproverUniversal" >
                <primitiveAttribute sourceName="organizationExtension.fiscalApproverUniversalId" targetName="personUniversalIdentifier" />
                <supportAttribute sourceName="organizationExtension.fiscalApproverUniversal.personUserIdentifier" targetName="personUserIdentifier" identifier="true" />
                <supportAttribute sourceName="organizationExtension.fiscalApproverUniversal.personName" targetName="personName" />
            </relationship> 
        </relationships>

		<objectLabel>Organization Code</objectLabel>
    </businessObject>
</dictionaryEntry>

