<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "../../../core/datadictionary/dataDictionary-1_0.dtd">
<dictionaryEntry>
  <businessObject businessObjectClass="org.kuali.module.chart.bo.Org">
    <customAttributeEnabled>false</customAttributeEnabled>

    <inquiry>
      <title>Organization Inquiry</title>

      <inquiryFields>
        <field attributeName="organizationStateCode" />
        <field attributeName="organizationActiveIndicator" />
        <field attributeName="hrisOrganizationCode" />
        <field attributeName="organizationTypeCode" />
        <field attributeName="chartOfAccountsCode" noInquiry="true" />
        <field attributeName="organizationName" />
        <field attributeName="reportsToChartOfAccountsCode" />
        <field attributeName="organizationZipCode" />
        <field attributeName="organizationPlantChartCode" />
        <field attributeName="campusPlantAccountNumber" />
        <field attributeName="reportsToOrganizationCode" />
        <field attributeName="organizationCode" noInquiry="true" />
        <field attributeName="campusPlantChartCode" />
        <field attributeName="organizationCityName" />
        <field attributeName="responsibilityCenterCode" />
        <field attributeName="organizationBeginDate" />
        <field attributeName="organizationManagerUniversal.personUserIdentifier" />
        <field attributeName="organizationManagerUniversal.personName" />
        <field attributeName="organizationStreetAddress" />
      </inquiryFields>
    </inquiry>

    <lookup>
      <title>Organization Lookup</title>

      <menubar>
        <![CDATA[<a href="maintenance.do?methodToCall=start&businessObjectClassName=org.kuali.module.chart.bo.Org">Create New</a>]]>
      </menubar>

      <instructions>Lookup an Organization</instructions>

      <defaultSort
        attributeName="chartOfAccountsCode"
        sortAscending="false" />

      <lookupFields>
        <lookupField attributeName="chartOfAccountsCode" required="false" />
        <lookupField attributeName="organizationCode" required="false" forceLookup="true" />
        <lookupField attributeName="responsibilityCenterCode" required="false" />
        <lookupField attributeName="organizationTypeCode" required="false" />
        <lookupField attributeName="organizationName" required="false" />
        <lookupField attributeName="reportsToChartOfAccountsCode" required="false" noLookup="true" />
        <lookupField attributeName="reportsToOrganizationCode" required="false" />
      </lookupFields>

      <resultFields>
        <field attributeName="chartOfAccountsCode" noInquiry="true" />
        <field attributeName="organizationCode" />
        <field attributeName="responsibilityCenterCode" />
        <field attributeName="organizationTypeCode" />
        <field attributeName="organizationName" />
        <field attributeName="organizationManagerUniversal.personName" />
        <field attributeName="organizationActiveIndicator" />
      </resultFields>
    </lookup>

    <titleAttribute>organizationCode</titleAttribute>

    <attributes>

      <attributeReference name="chartOfAccountsCode"
				        sourceClassName="org.kuali.module.chart.bo.Chart"
				        sourceAttributeName="chartOfAccountsCode">
		<required>true</required>
		<control>
			<select valuesFinderClass="org.kuali.core.lookup.keyvalues.ChartValuesFinder" />
		</control> 
      </attributeReference>

      <attribute name="organizationCode" forceUppercase="true">
        <label>Organization Code</label>
        <shortLabel>Org</shortLabel>

        <maxLength>4</maxLength>
        <validationPattern>
          <anyCharacter />
        </validationPattern>
		<required>true</required>
		
        <control>
          <text size="4" />
        </control>

        <summary>Organization Code</summary>
        <description>The code used to represent an organization.</description>
      </attribute>

      <attribute name="organizationName" forceUppercase="false">
        <label>Organization Name</label>
        <shortLabel>Org Name</shortLabel>

        <maxLength>40</maxLength>
        <validationPattern>
          <anyCharacter allowWhitespace="true" />
        </validationPattern>
		<required>true</required>
		
        <control>
          <text size="40" />
        </control>

        <summary>Organization Name</summary>
        <description>Organization Name</description>
      </attribute>

      <attributeReference name="organizationManagerUniversalId"
				        sourceClassName="org.kuali.core.bo.AttributeReferenceDummy"
				        sourceAttributeName="genericSystemId">
        <label>Manager Universal Id</label>
        <shortLabel>Unvl Id</shortLabel>
        <required>true</required>
        <control><hidden /></control>
        <summary>Manager Universal Id</summary>
        <description>Manager Universal Id</description>
      </attributeReference>

	  <attributeReference name="organizationManagerUniversal.personUserIdentifier" 
	  					  sourceClassName="org.kuali.core.bo.user.KualiUser" 
	  					  sourceAttributeName="personUserIdentifier">
			<label>Organization Manager UserID</label>
			<shortLabel>OrgMgrUserId</shortLabel>
			<required>false</required>
			<control>
				<kualiUser universalIdAttributeName="organizationManagerUniversalId" 
							userIdAttributeName="organizationManagerUniversal.personUserIdentifier" 
							personNameAttributeName="organizationManagerUniversal.personName" />
			</control>
			<summary>Organization Manager User ID</summary>
			<description>
				The UserID of the Organization Manager.
			</description>
	  </attributeReference>
	  
		<attributeReference name="organizationManagerUniversal.personName" 
							sourceClassName="org.kuali.core.bo.user.UniversalUser" 
							sourceAttributeName="personName">
			<label>Organization Manager Name</label>
			<shortLabel>Name</shortLabel>
			<required>false</required>
			<control><hidden /></control>
			<summary>Organization Manager Name</summary>
			<description>
				The Name of the Organization Manager.
			</description>
		</attributeReference>
		
      <attribute name="responsibilityCenterCode" forceUppercase="false">
        <label>Responsibility Center Code</label>
        <shortLabel>RC</shortLabel>

        <maxLength>2</maxLength>
        <validationPattern>
          <anyCharacter exactLength="2" />
        </validationPattern>
		<required>true</required>
		
        <control>
          <text size="2" />
        </control>

        <summary>Responsibility Center Code</summary>
        <description>Responsibility Center Code</description>
      </attribute>

      <attribute name="organizationPhysicalCampusCode" forceUppercase="false">
        <label>Organization Physical Campus Code</label>
        <shortLabel>Physical Campus</shortLabel>

        <maxLength>2</maxLength>
        <validationPattern>
          <alpha exactLength="2" />
        </validationPattern>
		<required>true</required>
		
        <control>
          <text size="2" />
        </control>

        <summary>Organization Physical Campus Code</summary>
        <description>Organization Physical Campus Code</description>
      </attribute>

      <attribute name="organizationTypeCode" forceUppercase="false">
        <label>Organization Type</label>
        <shortLabel>Type</shortLabel>

        <maxLength>1</maxLength>
        <validationPattern>
          <alpha exactLength="1" />
        </validationPattern>
		<required>true</required>
		
        <control>
          <select valuesFinderClass="org.kuali.core.lookup.keyvalues.OrgTypeValuesFinder" />
        </control>

        <summary>Organization Type</summary>
        <description>Organization Type</description>
      </attribute>

      <attributeReference name="organizationDefaultAccountNumber"
				        sourceClassName="org.kuali.module.chart.bo.Account"
				        sourceAttributeName="accountNumber">
        <label>Organization Default Account Number</label>
        <shortLabel>Default Acct</shortLabel>
        <required>true</required>
        <summary>Organization Default Account Number</summary>
        <description>Organization Default Account Number</description>
      </attributeReference>

      <attributeReference name="organizationPlantAccountNumber"
				        sourceClassName="org.kuali.module.chart.bo.Account"
				        sourceAttributeName="accountNumber">
        <label>Plant Account Number</label>
        <shortLabel>Plant Acct</shortLabel>
        <required>false</required>
        <summary>Plant Account Number</summary>
        <description>Plant Account Number</description>
      </attributeReference>

      <attributeReference name="organizationPlantChartCode"
				        sourceClassName="org.kuali.module.chart.bo.Chart"
				        sourceAttributeName="chartOfAccountsCode">
        <label>Organization Plant Chart</label>
        <shortLabel>Plant Chart</shortLabel>
        <required>false</required>
        <control>
			<select valuesFinderClass="org.kuali.core.lookup.keyvalues.ChartValuesFinder" />
		</control> 
        <summary>Organization Plant Chart</summary>
        <description>Organization Plant Chart</description>
      </attributeReference>

      <attribute name="organizationZipCode" forceUppercase="false">
        <label>Organization Zip Code</label>
        <shortLabel>Zip</shortLabel>

        <maxLength>11</maxLength>
        <validationPattern>
          <zipcode />
        </validationPattern>
		<required>true</required>
		
        <control>
          <text size="11" />
        </control>

        <summary>Organization Zip Code</summary>
        <description>Organization Zip Code</description>
      </attribute>

      <attribute name="organizationStateCode" forceUppercase="false">
        <label>Organization State Code</label>
        <shortLabel>St Cd</shortLabel>

        <maxLength>2</maxLength>
        <validationPattern>
          <alpha exactLength="2" />
        </validationPattern>
		<required>true</required>
		
        <control>
          <text size="2" />
        </control>

        <summary>Organization State Code</summary>
        <description>Organization State Code</description>
      </attribute>

      <attribute name="organizationStreetAddress" forceUppercase="false">
        <label>Organization Street</label>
        <shortLabel>Street</shortLabel>

        <maxLength>30</maxLength>
        <validationPattern>
          <anyCharacter allowWhitespace="true" />
        </validationPattern>
		<required>true</required>
		
        <control>
          <text size="30" />
        </control>

        <summary>Organization Street</summary>
        <description>Organization Street</description>
      </attribute>
      
      <attribute name="organizationCityName" forceUppercase="false">
        <label>Organization City Name</label>
        <shortLabel>City</shortLabel>

        <maxLength>25</maxLength>
        <validationPattern>
          <anyCharacter allowWhitespace="true" />
        </validationPattern>
		<required>true</required>
		
        <control>
          <text size="25" />
        </control>

        <summary>Organization City Name</summary>
        <description>Organization City Name</description>
      </attribute>

      <attributeReference name="reportsToChartOfAccountsCode"
				        sourceClassName="org.kuali.module.chart.bo.Chart"
				        sourceAttributeName="chartOfAccountsCode">
        <label>Reports to Chart Code</label>
        <shortLabel>Rpts to Chart</shortLabel>
		<required>true</required>
		<control>
			<select valuesFinderClass="org.kuali.core.lookup.keyvalues.ChartValuesFinder" />
		</control> 
        <summary>Reports to Chart Code</summary>
        <description>Reports to Chart Code</description>
      </attributeReference>

      <attributeReference name="reportsToOrganizationCode"
				        sourceClassName="org.kuali.module.chart.bo.Org"
				        sourceAttributeName="organizationCode">
        <label>Reports to Organization</label>
        <shortLabel>Rpts to Org</shortLabel>
		<required>true</required>
        <summary>Reports to Organization</summary>
        <description>Reports to Organization</description>
      </attributeReference>

      <attributeReference name="organizationBeginDate"
				        sourceClassName="org.kuali.core.bo.AttributeReferenceDummy"
				        sourceAttributeName="genericTimestamp">
        <label>Organization Begin Date</label>
        <shortLabel>Begin Date</shortLabel>
        <required>true</required>
        <summary>Organization Begin Date</summary>
        <description>Organization Begin Date</description>
        <formatterClass>org.kuali.core.web.format.DateFormatter</formatterClass>
      </attributeReference>

      <attributeReference
	        name="hrisOrganization.organizationCode"
	        sourceClassName="org.kuali.module.chart.bo.Org"
	        sourceAttributeName="organizationCode">
        <label>Human Resources Organization Code</label>
        <shortLabel>HR Org Code</shortLabel>
        <summary>Human Resources Organization Code</summary>
        <description>Human Resources Organization Code</description>
      </attributeReference>

      <attributeReference name="organizationEndDate"
				sourceClassName="org.kuali.core.bo.AttributeReferenceDummy"
				sourceAttributeName="genericTimestamp">
        <label>Organization End Date</label>
        <shortLabel>End Date</shortLabel>
        <summary>Organization End Date</summary>
        <description>Organization End Date</description>
      </attributeReference>

      <attributeReference
	        name="organizationActiveIndicator"
	        sourceClassName="org.kuali.core.bo.AttributeReferenceDummy"
	        sourceAttributeName="genericBoolean">
        <label>Active</label>
        <shortLabel>Act.</shortLabel>
        <summary>Active</summary>
        <description>Action</description>
      </attributeReference>

      <attributeReference
	        name="campusPlantAccountNumber"
	        sourceClassName="org.kuali.module.chart.bo.Account"
	        sourceAttributeName="accountNumber">
        <label>Campus Plant Account Number</label>
        <shortLabel>Plant Acct</shortLabel>
        <required>false</required>
        <summary>Campus Plant Account Number</summary>
        <description>Campus Plant Account Number</description>
      </attributeReference>

      <attributeReference
	        name="campusPlantChartCode"
	        sourceClassName="org.kuali.module.chart.bo.Chart"
	        sourceAttributeName="chartOfAccountsCode">
        <label>Campus Plant Chart Code</label>
        <shortLabel>Chart Code</shortLabel>
        <required>false</required>
        <control>
			<select valuesFinderClass="org.kuali.core.lookup.keyvalues.ChartValuesFinder" />
		</control> 
        <summary>Campus Plant Chart Code</summary>
        <description>Campus Plant Chart Code</description>
      </attributeReference>

      <attribute name="organizationInFinancialProcessingIndicator" forceUppercase="false">
        <label>Organization In Financial Processing</label>
        <shortLabel>In Fin Processing</shortLabel>

        <maxLength>1</maxLength>

        <control>
          <checkbox />
        </control>

        <summary>Organization In Financial Processing</summary>
        <description>Organization In Financial Processing</description>
      </attribute>

 	<attributeReference name="versionNumber" 
 			sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" 
 			sourceAttributeName="versionNumber" />

    </attributes>
  </businessObject>
</dictionaryEntry>

