<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "../../../core/datadictionary/dataDictionary-1_0.dtd">
<dictionaryEntry>
	<businessObject
		businessObjectClass="org.kuali.module.chart.bo.A21SubAccount">
		<customAttributeEnabled>false</customAttributeEnabled>

		<inquiry>
			<title>A21 Sub-Account Code Inquiry</title>

			<inquiryFields>
				<field attributeName="chart.chartOfAccountsCode" />
				<field attributeName="account.accountNumber" />			
				<field attributeName="subAccount.subAccountNumber" />
				<field attributeName="subAccountTypeCode" />	
				<field attributeName="indirectCostRecoveryType.code" />			
				<field attributeName="financialIcrSeriesIdentifier" />
				<field attributeName="indirectCostRecoveryChartOfAccounts.chartOfAccountsCode" />
				<field attributeName="indirectCostRecoveryAccount.accountNumber" />				
				<field attributeName="offCampusCode" />
				<field attributeName="costShareChartOfAccount.chartOfAccountsCode" />
				<field attributeName="costShareSourceAccount.accountNumber" />			
				<field attributeName="costShareSourceSubAccount.subAccountNumber" />
			</inquiryFields>
		</inquiry>	
		
		<lookup>
			<title>A21 Sub-Account Lookup</title>

			<menubar><![CDATA[<a href="maintenance.do?methodToCall=start&businessObjectClassName=org.kuali.module.chart.bo.A21SubAccount">Create New</a>]]></menubar>

			<instructions>Lookup an A21 Sub-Account</instructions>

			<defaultSort attributeName="subAccount.subAccountNumber" sortAscending="true" />

			<lookupFields>
			
				<lookupField attributeName="chart.chartOfAccountsCode" required="false" />
				<lookupField attributeName="account.accountNumber"  required="false" />			
				<lookupField attributeName="subAccount.subAccountNumber"  required="false" />
				<lookupField attributeName="subAccountTypeCode"  required="false" />				
				<lookupField attributeName="indirectCostRecoveryType.code" required="false"  />			
				<lookupField attributeName="financialIcrSeriesIdentifier" required="false"  />
				<lookupField attributeName="indirectCostRecoveryChartOfAccounts.chartOfAccountsCode"  required="false" />
				<lookupField attributeName="indirectCostRecoveryAccount.accountNumber"  required="false" />				
				<lookupField attributeName="offCampusCode"  required="false" />
				<lookupField attributeName="costShareChartOfAccount.chartOfAccountsCode"  required="false" />
				<lookupField attributeName="costShareSourceAccount.accountNumber"  required="false" />			
				<lookupField attributeName="costShareSourceSubAccount.subAccountNumber"  required="false" />
			</lookupFields>

			<resultFields>
				<field attributeName="chart.chartOfAccountsCode" />
				<field attributeName="account.accountNumber" />			
				<field attributeName="subAccount.subAccountNumber" />
				<field attributeName="subAccountTypeCode" />
				<field attributeName="indirectCostRecoveryType.code" />			
				<field attributeName="financialIcrSeriesIdentifier" />
				<field attributeName="indirectCostRecoveryChartOfAccounts.chartOfAccountsCode" />
				<field attributeName="indirectCostRecoveryAccount.accountNumber" />				
				<field attributeName="offCampusCode" />
				<field attributeName="costShareChartOfAccount.chartOfAccountsCode" />
				<field attributeName="costShareSourceAccount.accountNumber" />			
				<field attributeName="costShareSourceSubAccount.subAccountNumber" />
			</resultFields>
		</lookup>
		
		<titleAttribute>A21SubAccountNumber</titleAttribute>
		
		<attributes>
			<attributeReference name="chart.chartOfAccountsCode" 
								sourceClassName="org.kuali.module.chart.bo.Chart" 
								sourceAttributeName="chartOfAccountsCode" />

			<attributeReference name="account.accountNumber" 
								sourceClassName="org.kuali.module.chart.bo.Account" 
								sourceAttributeName="accountNumber" />
			
			<attributeReference name="subAccount.subAccountNumber" 
								sourceClassName="org.kuali.module.chart.bo.SubAccount" 
								sourceAttributeName="subAccountNumber" />

			<attribute name="subAccountTypeCode" forceUppercase="true">
				<label>Sub-Account Type Code</label>
				<shortLabel>Type Code</shortLabel>

				<maxLength>2</maxLength>
				<validationPattern><alphaNumeric /></validationPattern>

				<control>
					<text size="2" />
				</control>

				<summary>Sub-Account Type Code</summary>
				<description>Sub-Account Type Code should be EX or CS indicating Indirect Cost Recovery or Cost Sharing respectively.</description>
			</attribute>

			<attributeReference name="indirectCostRecoveryType.code" 
				sourceClassName="org.kuali.module.chart.bo.codes.ICRTypeCode" 
				sourceAttributeName="code" />


			<attribute name="financialIcrSeriesIdentifier" forceUppercase="true">
				<label>Series Id</label>
				<shortLabel>Series Id</shortLabel>

				<maxLength>3</maxLength>
				<validationPattern><alphaNumeric /></validationPattern>

				<control>
					<text size="3" />
				</control>

				<summary>Series Id</summary>
				<description>Series Id</description>
			</attribute>

			<attributeReference name="indirectCostRecoveryChartOfAccounts.chartOfAccountsCode" 
								sourceClassName="org.kuali.module.chart.bo.Chart" 
								sourceAttributeName="chartOfAccountsCode">
								<label>ICR Chart</label>
				                <shortLabel>ICR Chart</shortLabel>
				                <summary>ICR Chart</summary>
								<description>ICR Chart...</description>	                
			</attributeReference>					

			<attributeReference name="indirectCostRecoveryAccount.accountNumber" 
								sourceClassName="org.kuali.module.chart.bo.Account" 
								sourceAttributeName="accountNumber" >
								<label>ICR Account</label>
				                <shortLabel>ICR Account</shortLabel>
				                <summary>ICR Account</summary>
								<description>ICR Account...</description>	                
			</attributeReference>			

			<attributeReference name="offCampusCode" 
								sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" 
								sourceAttributeName="oneDigitTextCode">
								<label>Off Campus Indicator</label>
								<shortLabel>Off Campus</shortLabel>
								<summary>Off Campus Indicator</summary>
								<description>Off Campus Indicator</description>
			</attributeReference>

			<attributeReference name="costShareChartOfAccount.chartOfAccountsCode" 
								sourceClassName="org.kuali.module.chart.bo.Chart" 
								sourceAttributeName="chartOfAccountsCode" >
								<label>Cost Sharing Chart of Accounts Code</label>
								<shortLabel>Cost Sharting Chart Code</shortLabel>
								<summary>Cost Sharing Chart of Accounts Code</summary>
								<description>Cost Sharing Chart of Accounts Code</description>
			</attributeReference>
								

			<attributeReference name="costShareSourceAccount.accountNumber" 
								sourceClassName="org.kuali.module.chart.bo.Account" 
								sourceAttributeName="accountNumber" >
								<label>Cost Sharing Account Number</label>
								<shortLabel>Cost Sharing Account</shortLabel>
								<summary>Cost Sharing Account Number</summary>
								<description>Cost Sharing Account Number</description>
			</attributeReference>

			
			<attributeReference name="costShareSourceSubAccount.subAccountNumber" 
								sourceClassName="org.kuali.module.chart.bo.SubAccount" 
								sourceAttributeName="subAccountNumber" >
								<label>Cost Sharing Sub-Account Number</label>
								<shortLabel>Cost Sharing Sub-Account</shortLabel>
								<summary>Cost Sharing Sub-Account Number</summary>
								<description>Cost Sharing Sub-Account Number</description>
			</attributeReference>

		
		    <attributeReference name="versionNumber" 
								sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" 
								sourceAttributeName="versionNumber" />
								
		</attributes>
	</businessObject>
</dictionaryEntry>
			