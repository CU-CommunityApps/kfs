<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="ObjectCons" parent="ObjectCons-parentBean" />

  <bean id="ObjectCons-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.kfs.coa.businessobject.ObjectCons" />
    <property name="inquiryDefinition" >
      <ref bean="ObjectCons-inquiryDefinition" />
    </property>
    <property name="lookupDefinition" >
      <ref bean="ObjectCons-lookupDefinition" />
    </property>
    <property name="titleAttribute" value="finConsolidationObjectCode" />
    <property name="objectLabel" value="Object Consolidation" />
    <property name="attributes" >
      <list>
        <ref bean="ObjectCons-chartOfAccountsCode" />
        <ref bean="ObjectCons-finConsolidationObjectCode" />
        <ref bean="ObjectCons-finConsolidationObjectName" />
        <ref bean="ObjectCons-finConsolidationObjShortName" />
        <ref bean="ObjectCons-finConsolidationObjActiveIndicator" />
        <ref bean="ObjectCons-financialEliminationsObject.financialObjectCodeName" />
        <ref bean="ObjectCons-financialEliminationsObjectCode" />
        <ref bean="ObjectCons-financialReportingSortCode" />
        <ref bean="ObjectCons-versionNumber" />
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->


  <bean id="ObjectCons-chartOfAccountsCode" parent="ObjectCons-chartOfAccountsCode-parentBean" />

  <bean id="ObjectCons-chartOfAccountsCode-parentBean" abstract="true" parent="Chart-chartOfAccountsCode">
  </bean>
  <bean id="ObjectCons-finConsolidationObjectCode" parent="ObjectCons-finConsolidationObjectCode-parentBean" />

  <bean id="ObjectCons-finConsolidationObjectCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="finConsolidationObjectCode" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Consolidation Code" />
    <property name="shortLabel" value="Cons Code" />
    <property name="maxLength" value="4" />
    <property name="required" value="true" />
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="4" />
    </property>
  </bean>
  <bean id="ObjectCons-finConsolidationObjectName" parent="ObjectCons-finConsolidationObjectName-parentBean" />

  <bean id="ObjectCons-finConsolidationObjectName-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="finConsolidationObjectName" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Consolidation Object Name" />
    <property name="shortLabel" value="Long Name" />
    <property name="maxLength" value="40" />
    <property name="required" value="true" />
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="40" />
    </property>
  </bean>
  <bean id="ObjectCons-finConsolidationObjShortName" parent="ObjectCons-finConsolidationObjShortName-parentBean" />

  <bean id="ObjectCons-finConsolidationObjShortName-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="finConsolidationObjShortName" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Consolidation Object Short Name" />
    <property name="shortLabel" value="Short Name" />
    <property name="maxLength" value="12" />
    <property name="required" value="true" />
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="12" />
    </property>
  </bean>
  <bean id="ObjectCons-finConsolidationObjActiveIndicator" parent="ObjectCons-finConsolidationObjActiveIndicator-parentBean" />

  <bean id="ObjectCons-finConsolidationObjActiveIndicator-parentBean" abstract="true" parent="GenericAttributes-activeIndicator">
    <property name="name" value="finConsolidationObjActiveIndicator" />
  </bean>
  <bean id="ObjectCons-financialEliminationsObject.financialObjectCodeName" parent="ObjectCons-financialEliminationsObject.financialObjectCodeName-parentBean" />

  <bean id="ObjectCons-financialEliminationsObject.financialObjectCodeName-parentBean" abstract="true" parent="ObjectCode-financialObjectCodeName">
    <property name="name" value="financialEliminationsObject.financialObjectCodeName" />
  </bean>
  <bean id="ObjectCons-financialEliminationsObjectCode" parent="ObjectCons-financialEliminationsObjectCode-parentBean" />

  <bean id="ObjectCons-financialEliminationsObjectCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="financialEliminationsObjectCode" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Elimination Object Code" />
    <property name="shortLabel" value="Elim Obj Code" />
    <property name="maxLength" value="4" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern"
            p:allowWhitespace="true" />
    </property>
    <property name="required" value="true" />
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="4" />
    </property>
  </bean>
  <bean id="ObjectCons-financialReportingSortCode" parent="ObjectCons-financialReportingSortCode-parentBean" />

  <bean id="ObjectCons-financialReportingSortCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="financialReportingSortCode" />
    <property name="forceUppercase" value="false" />
    <property name="label" value="Reporting Sort Code" />
    <property name="shortLabel" value="Sort Code" />
    <property name="maxLength" value="2" />
    <property name="validationPattern" >
      <bean parent="NumericValidationPattern" />
    </property>
    <property name="required" value="true" />
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="2" />
    </property>
  </bean>
  <bean id="ObjectCons-versionNumber" parent="ObjectCons-versionNumber-parentBean" />

  <bean id="ObjectCons-versionNumber-parentBean" abstract="true" parent="GenericAttributes-versionNumber">
  </bean>

<!-- Business Object Inquiry Definition -->


  <bean id="ObjectCons-inquiryDefinition" parent="ObjectCons-inquiryDefinition-parentBean" />

  <bean id="ObjectCons-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
    <property name="title" value="Object Consolidation Inquiry" />
    <property name="inquirySections" >
      <list>
        <bean parent="InquirySectionDefinition">
          <property name="title" value="Header Text" />
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields" >
            <list>
              <bean parent="FieldDefinition"
                    p:attributeName="chartOfAccountsCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="finConsolidationObjectCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="finConsolidationObjectName" />
              <bean parent="FieldDefinition"
                    p:attributeName="finConsolidationObjShortName" />
              <bean parent="FieldDefinition"
                    p:attributeName="finConsolidationObjActiveIndicator" />
              <bean parent="FieldDefinition"
                    p:attributeName="financialEliminationsObjectCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="financialReportingSortCode" />
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Business Object Lookup Definition -->


  <bean id="ObjectCons-lookupDefinition" parent="ObjectCons-lookupDefinition-parentBean" />

  <bean id="ObjectCons-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
    <property name="title" value="Object Consolidation Lookup" />
    <property name="instructions" value="Lookup an Object Consolidation" />
    <property name="defaultSort" >
      <bean parent="SortDefinition">
        <property name="attributeNames" >
          <list>
            <value>finConsolidationObjectCode</value>
          </list>
        </property>
      </bean>
    </property>
    <property name="lookupFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="chartOfAccountsCode" />
        <bean parent="FieldDefinition"
              p:attributeName="finConsolidationObjectCode" />
        <bean parent="FieldDefinition"
              p:attributeName="finConsolidationObjectName" />
        <bean parent="FieldDefinition"
              p:attributeName="finConsolidationObjShortName" />
        <bean parent="FieldDefinition"
              p:attributeName="financialEliminationsObjectCode" />
        <bean parent="FieldDefinition"
              p:attributeName="finConsolidationObjActiveIndicator" />
      </list>
    </property>
    <property name="resultFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="chartOfAccountsCode" />
        <bean parent="FieldDefinition"
              p:attributeName="finConsolidationObjectCode" />
        <bean parent="FieldDefinition"
              p:attributeName="finConsolidationObjectName" />
        <bean parent="FieldDefinition"
              p:attributeName="finConsolidationObjShortName" />
        <bean parent="FieldDefinition"
              p:attributeName="financialEliminationsObjectCode" />
        <bean parent="FieldDefinition"
              p:attributeName="financialReportingSortCode" />
        <bean parent="FieldDefinition"
              p:attributeName="finConsolidationObjActiveIndicator" />
      </list>
    </property>
  </bean>
</beans>
