<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "../dataDictionary-1_0.dtd">
<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.module.chart.bo.DelegateGlobalDetail">


        <inquiry>
            <title>Delegate Change Document Inquiry</title>

            <inquirySections><inquirySection title="Header Text"><inquiryFields>
                <field attributeName="financialDocumentTypeCode" />
                <field attributeName="accountDelegatePrimaryRoutingIndicator" />
                <field attributeName="accountDelegateStartDate" />
                <field attributeName="accountDelegateUniversalId" />
                <field attributeName="approvalFromThisAmount" />
                <field attributeName="approvalToThisAmount" />
                <field attributeName="documentNumber" />
                <field attributeName="versionNumber" />
            </inquiryFields></inquirySection></inquirySections>
        </inquiry>

        <lookup>
            <title>Delegate Change Document Lookup</title>

            <instructions>Lookup an Delegate Change Document</instructions>

            <defaultSort sortAscending="true">
                <sortAttributes>
                    <sortAttribute attributeName="documentNumber" />
                </sortAttributes>
            </defaultSort>

            <lookupFields>
                <lookupField attributeName="financialDocumentTypeCode" required="false" />
                <lookupField attributeName="accountDelegatePrimaryRoutingIndicator" required="false" />
                <lookupField attributeName="accountDelegateStartDate" required="false" />
                <lookupField attributeName="accountDelegateUniversalId" required="false" />
                <lookupField attributeName="approvalFromThisAmount" required="false" />
                <lookupField attributeName="approvalToThisAmount" required="false" />
                <lookupField attributeName="documentNumber" required="false" />
                <lookupField attributeName="versionNumber" required="false" />
            </lookupFields>

            <resultFields>
                <field attributeName="financialDocumentTypeCode" />
                <field attributeName="accountDelegatePrimaryRoutingIndicator" />
                <field attributeName="accountDelegateStartDate" />
                <field attributeName="accountDelegateUniversalId" />
                <field attributeName="approvalFromThisAmount" />
                <field attributeName="approvalToThisAmount" />
                <field attributeName="documentNumber" />
                <field attributeName="versionNumber" />
            </resultFields>
        </lookup>

        <attributes>

            <attributeReference name="financialDocumentTypeCode" 
            					sourceClassName="org.kuali.core.bo.DocumentType" 
            					sourceAttributeName="financialDocumentTypeCode" />
            <attributeReference name="accountDelegatePrimaryRoutingIndicator" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="genericBoolean">
                <label>Account Delegate Primary Route</label>
                <shortLabel>PrimaryRoute</shortLabel>
            </attributeReference>
            <attributeReference name="accountDelegateStartDate" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="genericTimestamp">
                <label>Account Delegate Start Date</label>
                <shortLabel>DelegateStartDt</shortLabel>
                <required>true</required>
            </attributeReference>
            
             <attributeReference name="accountDelegateUniversalId" sourceClassName="org.kuali.core.bo.user.UniversalUser" sourceAttributeName="personUniversalIdentifier">
                <label>Account Delegate Unvl ID</label>
                <shortLabel>AccDelegateID</shortLabel>
                <displayLabelAttribute>accountDelegate.personUserIdentifier</displayLabelAttribute>
                <required>true</required>
                <control>
                    <hidden />
                </control>
            </attributeReference>

            <attributeReference name="accountDelegate.personUserIdentifier" sourceClassName="org.kuali.core.bo.user.UniversalUser" sourceAttributeName="personUserIdentifier">
                <label>Account Delegate UserID</label>
                <shortLabel>UserID</shortLabel>
                <required>false</required>
                <control>
                    <kualiUser universalIdAttributeName="accountDelegateUniversalId" userIdAttributeName="accountDelegate.personUserIdentifier" personNameAttributeName="accountDelegate.personName" />
                </control>
            </attributeReference>

            <attributeReference name="accountDelegate.personName" sourceClassName="org.kuali.core.bo.user.UniversalUser" sourceAttributeName="personName">
                <label>Account Delegate User Name</label>
                <shortLabel>Name</shortLabel>
                <required>false</required>
                <control>
                    <hidden />
                </control>
            </attributeReference>

            <attribute name="approvalFromThisAmount" forceUppercase="true">
                <label>Approval From This Amount</label>
                <shortLabel>Amount</shortLabel>

                <maxLength>19</maxLength>
                <validationPattern>
                    <floatingPoint />
                </validationPattern>

                <control>
                    <text size="21" />
                </control>
            </attribute>

            <attribute name="approvalToThisAmount" forceUppercase="true">
                <label>Approval To This Amount</label>
                <shortLabel>Amount</shortLabel>

                <maxLength>19</maxLength>
                <validationPattern>
                    <floatingPoint />
                </validationPattern>

                <control>
                    <text size="21" />
                </control>
            </attribute>

            <attribute name="documentNumber" forceUppercase="true">
                <label>Document Number</label>
                <shortLabel>Number</shortLabel>

                <maxLength>14</maxLength>
                <validationPattern>
                    <anyCharacter />
                </validationPattern>

                <control>
                    <text size="16" />
                </control>
            </attribute>
            
            <attributeReference name="newCollectionRecord" 
                                sourceClassName="org.kuali.core.bo.AttributeReferenceDummy"
                                sourceAttributeName="newCollectionRecord" />

            <attributeReference name="versionNumber" 
            					sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" 
            					sourceAttributeName="versionNumber" />

        </attributes>

        <relationships>
            <relationship objectAttributeName="accountDelegate" >
                <primitiveAttribute sourceName="accountDelegateUniversalId" targetName="personUniversalIdentifier" />
				<supportAttribute sourceName="accountDelegate.personUserIdentifier" targetName="personUserIdentifier" identifier="true" />
				<supportAttribute sourceName="accountDelegate.personName" targetName="personName" />
            </relationship>
        </relationships>

		<objectLabel>Delegate Change Document</objectLabel>
    </businessObject>
</dictionaryEntry>
