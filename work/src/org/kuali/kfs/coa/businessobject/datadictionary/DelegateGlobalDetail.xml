<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="DelegateGlobalDetail" parent="DelegateGlobalDetail-parentBean" />

  <bean id="DelegateGlobalDetail-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.kfs.coa.businessobject.DelegateGlobalDetail" />
    <property name="inquiryDefinition" >
      <ref bean="DelegateGlobalDetail-inquiryDefinition" />
    </property>
    <property name="lookupDefinition" >
      <ref bean="DelegateGlobalDetail-lookupDefinition" />
    </property>
    <property name="objectLabel" value="Delegate Change Document" />
    <property name="attributes" >
      <list>
        <ref bean="DelegateGlobalDetail-financialDocumentTypeCode" />
        <ref bean="DelegateGlobalDetail-accountDelegatePrimaryRoutingIndicator" />
        <ref bean="DelegateGlobalDetail-accountDelegateStartDate" />
        <ref bean="DelegateGlobalDetail-accountDelegateUniversalId" />
        <ref bean="DelegateGlobalDetail-accountDelegate.principalName" />
        <ref bean="DelegateGlobalDetail-accountDelegate.name" />
        <ref bean="DelegateGlobalDetail-approvalFromThisAmount" />
        <ref bean="DelegateGlobalDetail-approvalToThisAmount" />
        <ref bean="DelegateGlobalDetail-documentNumber" />
        <ref bean="DelegateGlobalDetail-newCollectionRecord" />
      </list>
    </property>
    <property name="relationships" >
      <list>
        <bean parent="RelationshipDefinition">
          <property name="objectAttributeName" value="accountDelegate" />
          <property name="primitiveAttributes" >
            <list>
              <bean parent="PrimitiveAttributeDefinition"
                    p:sourceName="accountDelegateUniversalId"
                    p:targetName="principalId" />
            </list>
          </property>
          <property name="supportAttributes" >
            <list>
              <bean parent="SupportAttributeDefinition"
                    p:identifier="true"
                    p:sourceName="accountDelegate.principalName"
                    p:targetName="principalName" />
              <bean parent="SupportAttributeDefinition"
                    p:sourceName="accountDelegate.name"
                    p:targetName="name" />
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->


  <bean id="DelegateGlobalDetail-financialDocumentTypeCode" parent="DelegateGlobalDetail-financialDocumentTypeCode-parentBean" />

  <bean id="DelegateGlobalDetail-financialDocumentTypeCode-parentBean" abstract="true" parent="DocumentType-documentTypeCode">
    <property name="name" value="financialDocumentTypeCode" />
  </bean>
  <bean id="DelegateGlobalDetail-accountDelegatePrimaryRoutingIndicator" parent="DelegateGlobalDetail-accountDelegatePrimaryRoutingIndicator-parentBean" />

  <bean id="DelegateGlobalDetail-accountDelegatePrimaryRoutingIndicator-parentBean" abstract="true" parent="GenericAttributes-genericBoolean">
    <property name="name" value="accountDelegatePrimaryRoutingIndicator" />
    <property name="label" value="Account Delegate Primary Route" />
    <property name="shortLabel" value="PrimaryRoute" />
  </bean>
  <bean id="DelegateGlobalDetail-accountDelegateStartDate" parent="DelegateGlobalDetail-accountDelegateStartDate-parentBean" />

  <bean id="DelegateGlobalDetail-accountDelegateStartDate-parentBean" abstract="true" parent="GenericAttributes-genericTimestamp">
    <property name="name" value="accountDelegateStartDate" />
    <property name="label" value="Account Delegate Start Date" />
    <property name="shortLabel" value="DelegateStartDt" />
    <property name="required" value="true" />
  </bean>
  <bean id="DelegateGlobalDetail-accountDelegateUniversalId" parent="DelegateGlobalDetail-accountDelegateUniversalId-parentBean" />

  <bean id="DelegateGlobalDetail-accountDelegateUniversalId-parentBean" abstract="true" parent="PersonImpl-principalId">
    <property name="name" value="accountDelegateUniversalId" />
    <property name="label" value="Account Delegate Unvl ID" />
    <property name="shortLabel" value="AccDelegateID" />
    <property name="control" >
      <bean parent="HiddenControlDefinition" />
    </property>
    <property name="displayLabelAttribute" value="accountDelegate.principalName" />
  </bean>
  <bean id="DelegateGlobalDetail-accountDelegate.principalName" parent="DelegateGlobalDetail-accountDelegate.principalName-parentBean" />

  <bean id="DelegateGlobalDetail-accountDelegate.principalName-parentBean" abstract="true" parent="PersonImpl-principalName">
    <property name="name" value="accountDelegate.principalName" />
    <property name="label" value="Account Delegate UserID" />
    <property name="shortLabel" value="UserID" />
    <property name="required" value="false" />
    <property name="control" >
      <bean parent="KualiUserControlDefinition"
            p:personNameAttributeName="accountDelegate.name"
            p:universalIdAttributeName="accountDelegateUniversalId"
            p:userIdAttributeName="accountDelegate.principalName" />
    </property>
  </bean>
  <bean id="DelegateGlobalDetail-accountDelegate.name" parent="DelegateGlobalDetail-accountDelegate.name-parentBean" />

  <bean id="DelegateGlobalDetail-accountDelegate.name-parentBean" abstract="true" parent="PersonImpl-name">
    <property name="name" value="accountDelegate.name" />
    <property name="label" value="Account Delegate User Name" />
    <property name="required" value="false" />
  </bean>
  <bean id="DelegateGlobalDetail-approvalFromThisAmount" parent="DelegateGlobalDetail-approvalFromThisAmount-parentBean" />

  <bean id="DelegateGlobalDetail-approvalFromThisAmount-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="approvalFromThisAmount" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Approval From This Amount" />
    <property name="shortLabel" value="Amount" />
    <property name="maxLength" value="19" />
    <property name="validationPattern" >
      <bean parent="FloatingPointValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="21" />
    </property>
  </bean>
  <bean id="DelegateGlobalDetail-approvalToThisAmount" parent="DelegateGlobalDetail-approvalToThisAmount-parentBean" />

  <bean id="DelegateGlobalDetail-approvalToThisAmount-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="approvalToThisAmount" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Approval To This Amount" />
    <property name="shortLabel" value="Amount" />
    <property name="maxLength" value="19" />
    <property name="validationPattern" >
      <bean parent="FloatingPointValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="21" />
    </property>
  </bean>
  <bean id="DelegateGlobalDetail-documentNumber" parent="DelegateGlobalDetail-documentNumber-parentBean" />

  <bean id="DelegateGlobalDetail-documentNumber-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="documentNumber" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Document Number" />
    <property name="shortLabel" value="Number" />
    <property name="maxLength" value="14" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="16" />
    </property>
  </bean>
  <bean id="DelegateGlobalDetail-newCollectionRecord" parent="DelegateGlobalDetail-newCollectionRecord-parentBean" />

  <bean id="DelegateGlobalDetail-newCollectionRecord-parentBean" abstract="true" parent="GenericAttributes-newCollectionRecord">
  </bean>

<!-- Business Object Inquiry Definition -->


  <bean id="DelegateGlobalDetail-inquiryDefinition" parent="DelegateGlobalDetail-inquiryDefinition-parentBean" />

  <bean id="DelegateGlobalDetail-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
    <property name="title" value="Delegate Change Document Inquiry" />
    <property name="inquirySections" >
      <list>
        <bean parent="InquirySectionDefinition">
          <property name="title" value="" />
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields" >
            <list>
              <bean parent="FieldDefinition"
                    p:attributeName="financialDocumentTypeCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="accountDelegatePrimaryRoutingIndicator" />
              <bean parent="FieldDefinition"
                    p:attributeName="accountDelegateStartDate" />
              <bean parent="FieldDefinition"
                    p:attributeName="accountDelegateUniversalId" />
              <bean parent="FieldDefinition"
                    p:attributeName="approvalFromThisAmount" />
              <bean parent="FieldDefinition"
                    p:attributeName="approvalToThisAmount" />
              <bean parent="FieldDefinition"
                    p:attributeName="documentNumber" />
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Business Object Lookup Definition -->


  <bean id="DelegateGlobalDetail-lookupDefinition" parent="DelegateGlobalDetail-lookupDefinition-parentBean" />

  <bean id="DelegateGlobalDetail-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
    <property name="title" value="Delegate Change Document Lookup" />
    <property name="instructions" value="Lookup an Delegate Change Document" />
    <property name="defaultSort" >
      <bean parent="SortDefinition">
      </bean>
    </property>
    <property name="lookupFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="financialDocumentTypeCode" />
        <bean parent="FieldDefinition"
              p:attributeName="accountDelegatePrimaryRoutingIndicator" />
        <bean parent="FieldDefinition"
              p:attributeName="accountDelegateStartDate" />
        <bean parent="FieldDefinition"
              p:attributeName="accountDelegateUniversalId" />
        <bean parent="FieldDefinition"
              p:attributeName="approvalFromThisAmount" />
        <bean parent="FieldDefinition"
              p:attributeName="approvalToThisAmount" />
        <bean parent="FieldDefinition"
              p:attributeName="documentNumber" />
      </list>
    </property>
    <property name="resultFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="financialDocumentTypeCode" />
        <bean parent="FieldDefinition"
              p:attributeName="accountDelegatePrimaryRoutingIndicator" />
        <bean parent="FieldDefinition"
              p:attributeName="accountDelegateStartDate" />
        <bean parent="FieldDefinition"
              p:attributeName="accountDelegateUniversalId" />
        <bean parent="FieldDefinition"
              p:attributeName="approvalFromThisAmount" />
        <bean parent="FieldDefinition"
              p:attributeName="approvalToThisAmount" />
        <bean parent="FieldDefinition"
              p:attributeName="documentNumber" />
      </list>
    </property>
  </bean>
</beans>

