<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "../../../core/datadictionary/dataDictionary-1_0.dtd">
<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.module.chart.bo.AccountGlobal">


        <attributes>
            <attribute name="documentNumber" forceUppercase="true">
                <label>Document Number</label>
                <shortLabel>Number</shortLabel>

                <maxLength>14</maxLength>
                <validationPattern>
                    <anyCharacter />
                </validationPattern>

                <control>
                    <hidden />
                </control>

                <summary>documentNumber</summary>
                <description>documentNumber description...</description>
            </attribute>

            <attributeReference name="accountExpirationDate" sourceClassName="org.kuali.module.chart.bo.Account" sourceAttributeName="accountExpirationDate"/>

            <attributeReference name="subFundGroupCode" sourceClassName="org.kuali.module.chart.bo.SubFundGroup" sourceAttributeName="subFundGroupCode">
                <label>Sub-Fund Group Code</label>
                <shortLabel>SubFundGrpCd</shortLabel>
                <required>false</required>
                <summary>
                    <![CDATA[&nbsp;]]>
                </summary>
                <description>The code used to categorize Accounts for internal resource management, and to relate the Account to a Fund through the Sub-Fund Groups.</description>
            </attributeReference>

            <attributeReference name="organizationCode" sourceClassName="org.kuali.module.chart.bo.Org" sourceAttributeName="organizationCode">
                <required>false</required>
                <description>The code used to represent an organization.</description>
            </attributeReference>



            <attribute name="accountCityName" forceUppercase="true">
                <label>Account City Name</label>
                <shortLabel>CityName</shortLabel>

                <maxLength>25</maxLength>
                <validationPattern>
                    <anyCharacter allowWhitespace="true" />
                </validationPattern>
                <required>false</required>

                <control>
                    <text size="25" />
                </control>

                <summary>
                    <![CDATA[&nbsp;]]>
                </summary>
                <description>The city in which the account is managed.</description>
            </attribute>

            <attributeReference name="accountStateCode" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="twoDigitTextCode">
                <label>Account State Code</label>
                <shortLabel>StateCd</shortLabel>
                <required>false</required>
                <summary>
                    <![CDATA[&nbsp;]]>
                </summary>
                <description>A 2-character code for the state in which the Account is managed.</description>
            </attributeReference>

            <attribute name="accountStreetAddress" forceUppercase="true">
                <label>Account Street Address</label>
                <shortLabel>StreetAddr</shortLabel>

                <maxLength>30</maxLength>
                <validationPattern>
                    <anyCharacter allowWhitespace="true" />
                </validationPattern>
                <required>false</required>

                <control>
                    <text size="30" />
                </control>

                <summary>
                    <![CDATA[&nbsp;]]>
                </summary>
                <description>The street address where the Account is managed. It is also the location to which standard financial reports are to be sent.</description>
            </attribute>

            <attributeReference name="accountZipCode" sourceClassName="org.kuali.kfs.bo.PostalZipCode" sourceAttributeName="postalZipCode">
                <label>Account Postal Code</label>
                <shortLabel>ZipCode</shortLabel>
                <required>false</required>
                <summary>
                    <![CDATA[&nbsp;]]>
                </summary>
                <description>The Zip code assigned by the US Postal Service for the city in which the Account is managed.</description>
            </attributeReference>

            <attributeReference name="accountSufficientFundsCode" sourceClassName="org.kuali.module.chart.bo.codes.SufficientFundsCode" sourceAttributeName="code">
                <label>Account Sufficient Funds Code</label>
                <shortLabel>SuffFndsCd</shortLabel>
                <required>false</required>
                <control>
                    <select valuesFinderClass="org.kuali.module.chart.lookup.keyvalues.SufficientFundsCodeValuesFinder" />
                </control>
                <summary>
                    <![CDATA[&nbsp;]]>
                </summary>
                <description>The code for the level at which the account is going to be checked for sufficient funds in the transaction processing environment.</description>
            </attributeReference>

            <attributeReference name="accountCfdaNumber" sourceClassName="org.kuali.module.chart.bo.Account" sourceAttributeName="accountCfdaNumber">
                <required>false</required>
            </attributeReference>


            <attributeReference name="continuationAccountNumber" sourceClassName="org.kuali.module.chart.bo.Account" sourceAttributeName="accountNumber">
                <label>Continuation Account Number</label>
                <shortLabel>ContAcct</shortLabel>
                <required>false</required>
                <summary>
                    <![CDATA[&nbsp;]]>
                </summary>
                <description>The numeric identifier for the Continuation Account. It will accept transactions after the original account has expired.</description>
            </attributeReference>

            <attributeReference name="continuationFinChrtOfAcctCd" sourceClassName="org.kuali.module.chart.bo.Chart" sourceAttributeName="chartOfAccountsCode">
                <label>Continuation Chart Of Accounts Code</label>
                <shortLabel>ContChartCd</shortLabel>
                <required>false</required>
                <control>
                    <select valuesFinderClass="org.kuali.module.chart.lookup.keyvalues.ChartValuesFinder" />
                </control>
                <summary>
                    <![CDATA[&nbsp;]]>
                </summary>
                <description>The code for the Chart of Accounts for the Continuation Account. It will accept transactions after the original account has expired.</description>
            </attributeReference>

            <attribute name="financialHigherEdFunctionCd" forceUppercase="true">
                <label>Higher Education Function Code</label>
                <shortLabel>Higher Ed Func Cd</shortLabel>

                <maxLength>4</maxLength>
                <validationPattern>
                    <anyCharacter />
                </validationPattern>

                <control>
                    <text size="6" />
                </control>

                <summary>financialHigherEdFunctionCd</summary>
                <description>The Higher Education Function Code which classifies the purpose of the account.</description>
            </attribute>

            <attributeReference name="incomeStreamAccountNumber" sourceClassName="org.kuali.module.chart.bo.Account" sourceAttributeName="accountNumber">
                <label>Income Stream Account Number</label>
                <shortLabel>IncStrAcct</shortLabel>
                <required>false</required>
                <summary>
                    <![CDATA[&nbsp;]]>
                </summary>
                <description>The numeric identifier for the Income Stream Account. It is the account that is linked to receive any offsetting entries, in order to balance responsibility center activity.</description>
            </attributeReference>

            <attributeReference name="incomeStreamFinancialCoaCode" sourceClassName="org.kuali.module.chart.bo.Chart" sourceAttributeName="chartOfAccountsCode">
                <label>Income Stream Chart Of Accounts Code</label>
                <shortLabel>IncStrChart</shortLabel>
                <required>false</required>
                <control>
                    <select valuesFinderClass="org.kuali.module.chart.lookup.keyvalues.ChartValuesFinder" />
                </control>
                <summary>
                    <![CDATA[&nbsp;]]>
                </summary>
                <description>The code for the Chart of Accounts for the Income Stream Account. It is the account that is linked to receive any offsetting entries, in order to balance responsibility center activity.</description>
            </attributeReference>

            <attribute name="pendingAcctSufficientFundsIndicator" forceUppercase="false">
                <label>TP Sufficient Funds Check</label>
                <shortLabel>Indicator</shortLabel>

                <maxLength>$attribute.maxLength</maxLength>

                <control>
                    <select valuesFinderClass="org.kuali.core.lookup.keyvalues.IndicatorYNNullValuesFinder" />
                </control>

                <summary>TP Sufficient Funds Check.</summary>
                <description>TP Sufficient Funds Check description...</description>
                <formatterClass>org.kuali.core.web.format.BooleanFormatter</formatterClass>
            </attribute>
            <attributeReference name="accountFiscalOfficerSystemIdentifier" sourceClassName="org.kuali.core.bo.user.UniversalUser" sourceAttributeName="personUniversalIdentifier">
                <label>Fiscal Officer Unvl ID</label>
                <shortLabel>FiscOffId</shortLabel>
                <displayLabelAttribute>accountFiscalOfficerUser.personUserIdentifier</displayLabelAttribute>
                <required>false</required>
                <control>
                    <hidden />
                </control>
            </attributeReference>

            <attributeReference name="accountFiscalOfficerUser.personUserIdentifier" sourceClassName="org.kuali.core.bo.user.UniversalUser" sourceAttributeName="personUserIdentifier">
                <label>Fiscal Officer UserID</label>
                <shortLabel>UserID</shortLabel>
                <required>false</required>
                <control>
                    <kualiUser universalIdAttributeName="accountFiscalOfficerSystemIdentifier" userIdAttributeName="accountFiscalOfficerUser.personUserIdentifier" personNameAttributeName="accountFiscalOfficerUser.personName" />
                </control>
                <summary>
                    <![CDATA[&nbsp;]]>
                </summary>
                <description>The username of the Fiscal Officer for the Account. Fiscal Officers are responsible for the accuracy of transactions in the Account. All transaction documents route to Fiscal Officers or their delegates.</description>
            </attributeReference>

            <attributeReference name="accountFiscalOfficerUser.personName" sourceClassName="org.kuali.core.bo.user.UniversalUser" sourceAttributeName="personName">
                <label>Fiscal Officer Name</label>
                <shortLabel>Name</shortLabel>
                <required>false</required>
                <control>
                    <hidden />
                </control>
                <summary>
                    <![CDATA[&nbsp;]]>
                </summary>
                <description>The Name of the Fiscal Officer.</description>
            </attributeReference>

            <attributeReference name="accountsSupervisorySystemsIdentifier" sourceClassName="org.kuali.core.bo.user.UniversalUser" sourceAttributeName="personUniversalIdentifier">
                <label>Supervisor Unvl ID</label>
                <shortLabel>SprvsrId</shortLabel>
                <displayLabelAttribute>accountSupervisoryUser.personUserIdentifier</displayLabelAttribute>
                <required>false</required>
                <control>
                    <hidden />
                </control>
            </attributeReference>

            <attributeReference name="accountSupervisoryUser.personUserIdentifier" sourceClassName="org.kuali.module.chart.bo.Account" sourceAttributeName="accountSupervisoryUser.personUserIdentifier"/>

            <attributeReference name="accountSupervisoryUser.personName" sourceClassName="org.kuali.core.bo.user.UniversalUser" sourceAttributeName="personName">
                <label>Account Supervisor Name</label>
                <shortLabel>Name</shortLabel>
                <required>false</required>
                <control>
                    <hidden />
                </control>
                <summary>
                    <![CDATA[&nbsp;]]>
                </summary>
                <description>The Name of the Account Supervisor.</description>
            </attributeReference>

            <attributeReference name="accountManagerSystemIdentifier" sourceClassName="org.kuali.core.bo.user.UniversalUser" sourceAttributeName="personUniversalIdentifier">
                <label>Manager Unvl ID</label>
                <shortLabel>MgrId</shortLabel>
                <displayLabelAttribute>accountManagerUser.personUserIdentifier</displayLabelAttribute>
                <required>false</required>
                <control>
                    <hidden />
                </control>
            </attributeReference>

            <attributeReference name="accountManagerUser.personUserIdentifier" sourceClassName="org.kuali.core.bo.user.UniversalUser" sourceAttributeName="personUserIdentifier">
                <label>Account Manager UserID</label>
                <shortLabel>UserID</shortLabel>
                <required>false</required>
                <control>
                    <kualiUser universalIdAttributeName="accountManagerSystemIdentifier" userIdAttributeName="accountManagerUser.personUserIdentifier" personNameAttributeName="accountManagerUser.personName" />
                </control>
                <summary>
                    <![CDATA[&nbsp;]]>
                </summary>
                <description>The username of the Account Manager. Account Managers generally provide the account management role for the Account. They develop or establish the account budget, determine how funds are being spent, and monitor the remaining balance.</description>
            </attributeReference>

            <attributeReference name="accountManagerUser.personName" sourceClassName="org.kuali.core.bo.user.UniversalUser" sourceAttributeName="personName">
                <label>Account Manager Name</label>
                <shortLabel>Name</shortLabel>
                <required>false</required>
                <control>
                    <hidden />
                </control>
                <summary>
                    <![CDATA[&nbsp;]]>
                </summary>
                <description>The Name of the Account Manager.</description>
            </attributeReference>

            <attributeReference name="versionNumber" 
            					sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" 
            					sourceAttributeName="versionNumber" />

        </attributes>

        <relationships>
            <relationship objectAttributeName="accountFiscalOfficerUser" >
                <primitiveAttribute sourceName="accountFiscalOfficerSystemIdentifier" targetName="personUniversalIdentifier" />
				<supportAttribute sourceName="accountFiscalOfficerUser.personUserIdentifier" targetName="personUserIdentifier" identifier="true" />
				<supportAttribute sourceName="accountFiscalOfficerUser.personName" targetName="personName" />
            </relationship>

            <relationship objectAttributeName="accountSupervisoryUser" >
                <primitiveAttribute sourceName="accountsSupervisorySystemsIdentifier" targetName="personUniversalIdentifier" />
				<supportAttribute sourceName="accountSupervisoryUser.personUserIdentifier" targetName="personUserIdentifier" identifier="true" />
				<supportAttribute sourceName="accountSupervisoryUser.personName" targetName="personName" />
            </relationship>

            <relationship objectAttributeName="accountManagerUser" >
                <primitiveAttribute sourceName="accountManagerSystemIdentifier" targetName="personUniversalIdentifier" />
				<supportAttribute sourceName="accountManagerUser.personUserIdentifier" targetName="personUserIdentifier" identifier="true" />
				<supportAttribute sourceName="accountManagerUser.personName" targetName="personName" />
            </relationship>
        </relationships>

		<objectLabel>Account Change Document</objectLabel>
    </businessObject>
</dictionaryEntry>
