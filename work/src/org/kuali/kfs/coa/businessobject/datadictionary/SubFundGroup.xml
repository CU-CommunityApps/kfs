<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "../../../core/datadictionary/dataDictionary-1_0.dtd">
<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.module.chart.bo.SubFundGroup">


        <inquiry>
            <title>Sub-Fund Group Inquiry</title>

            <inquirySections><inquirySection title="Header Text"><inquiryFields>
                <field attributeName="subFundGroupCode" />
                <field attributeName="subFundGroupDescription" />
                <field attributeName="subFundGroupTypeCode" />
                <field attributeName="fundGroupCode" />
                <field attributeName="subFundGroupWagesIndicator" />
                <field attributeName="financialReportingSortCode" />
                <field attributeName="subfundgrpActivityIndicator" />
                <field attributeName="fundGroupBudgetAdjustmentRestrictionLevelCode" />
                <field attributeName="accountRestrictedStatusCode" />
            </inquiryFields></inquirySection></inquirySections>
        </inquiry>

        <lookup>
            <title>Sub-Fund Group Lookup</title>

            <instructions>Lookup an Sub-Fund Group</instructions>

            <defaultSort attributeName="subFundGroupCode" sortAscending="false" />

            <lookupFields>
                <lookupField attributeName="subFundGroupCode" />
                <lookupField attributeName="subFundGroupDescription" />
                <lookupField attributeName="subFundGroupTypeCode" />
                <lookupField attributeName="fundGroupCode" />
                <lookupField attributeName="subFundGroupWagesIndicator" />
                <lookupField attributeName="financialReportingSortCode" />
                <lookupField attributeName="subfundgrpActivityIndicator">
                    <defaultValue>Y</defaultValue>
                </lookupField>

            </lookupFields>

            <resultFields>
                <field attributeName="subFundGroupCode" />
                <field attributeName="subFundGroupDescription" />
                <field attributeName="subFundGroupTypeCode" />
                <field attributeName="fundGroupCode" />
                <field attributeName="subFundGroupWagesIndicator" />
                <field attributeName="financialReportingSortCode" />
                <field attributeName="subfundgrpActivityIndicator" />
                <field attributeName="fundGroupBudgetAdjustmentRestrictionLevelCode" />
            </resultFields>
        </lookup>

        <titleAttribute>subFundGroupCode</titleAttribute>

        <attributes>
            <attribute name="subFundGroupCode" forceUppercase="true">
                <label>Sub-Fund Group Code</label>
                <shortLabel>Sub-Fund Group Code</shortLabel>

                <maxLength>6</maxLength>

                <validationPattern>
                    <alpha maxLength="6" />
                </validationPattern>
                <required>true</required>

                <control>
                    <text size="8" />
                </control>
            </attribute>


            <attribute name="codeAndDescription" forceUppercase="false">
                <label>Sub-Fund Group Code</label>
                <shortLabel>Sub-Fund Group Code</shortLabel>

                <maxLength>47</maxLength>

                <control>
                    <text size="47" />
                </control>
            </attribute>
            <attributeReference name="subfundgrpActivityIndicator" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="activeIndicator" />

            <attributeReference name="subFundGroupWagesIndicator" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="activeIndicator">
                <label>Wage Indicator</label>
                <shortLabel>Indicator</shortLabel>
            </attributeReference>

            <attributeReference name="objectId" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="objectId" />

            <attributeReference name="fundGroupCode" sourceClassName="org.kuali.module.chart.bo.FundGroup" sourceAttributeName="code">
                <required>true</required>
            </attributeReference>

            <attribute name="subFundGroupTypeCode" forceUppercase="true">
                <label>Sub-Fund Type Code</label>
                <shortLabel>Sub-Fund Type Code</shortLabel>

                <maxLength>1</maxLength>
                <required>true</required>
                <control>
                    <select valuesFinderClass="org.kuali.module.chart.lookup.keyvalues.SubFundGroupTypeValuesFinder" />
                </control>
            </attribute>

            <attribute name="subFundGroupDescription" forceUppercase="true">
                <label>Description</label>
                <shortLabel>Desc.</shortLabel>

                <maxLength>40</maxLength>

                <validationPattern>
                    <anyCharacter allowWhitespace="true" />
                </validationPattern>
                <required>true</required>
                <control>
                    <text size="42" />
                </control>
            </attribute>

              <attributeReference name="accountRestrictedStatusCode" sourceClassName="org.kuali.module.chart.bo.RestrictedStatus" sourceAttributeName="accountRestrictedStatusCode">
                 <label>Default Account Restricted Status Code</label>
                 <shortLabel>DefaultRstrcStatCd</shortLabel>
                 <control>
                     <select valuesFinderClass="org.kuali.module.chart.lookup.keyvalues.RestrictedStatusValuesFinder" />
                 </control>
             </attributeReference>
            

            <attribute name="financialReportingSortCode" forceUppercase="false">
                <label>Financial Reporting Sort Code</label>
                <shortLabel>Fin. Rpt. Code</shortLabel>

                <maxLength>2</maxLength>

                <validationPattern>
                    <numeric exactLength="2" />
                </validationPattern>
                <required>true</required>
                <control>
                    <text size="4" />
                </control>
            </attribute>
            
            <attributeReference name="fundGroupBudgetAdjustmentRestrictionLevelCode" 
                                sourceClassName="org.kuali.module.chart.bo.FundGroup" 
                                sourceAttributeName="fundGroupBudgetAdjustmentRestrictionLevelCode" >
                <control>
                    <select valuesFinderClass="org.kuali.module.financial.lookup.keyvalues.BudgetAdjustmentSubFundRestrictionsValuesFinder"/>
                </control>
            </attributeReference>    

            <attributeReference name="versionNumber" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="versionNumber" />

        </attributes>
        
        <objectLabel>Sub-Fund Group</objectLabel>
    </businessObject>
</dictionaryEntry>
