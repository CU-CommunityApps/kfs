<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="ICRTypeCode" parent="ICRTypeCode-parentBean" />

  <bean id="ICRTypeCode-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.module.chart.bo.codes.ICRTypeCode" />
    <property name="inquiryDefinition" >
      <ref bean="ICRTypeCode-inquiryDefinition" />
    </property>
    <property name="lookupDefinition" >
      <ref bean="ICRTypeCode-lookupDefinition" />
    </property>
    <property name="titleAttribute" value="code" />
    <property name="objectLabel" value="Indirect Cost Recovery Type" />
    <property name="attributes" >
      <list>
        <ref bean="ICRTypeCode-code" />
        <ref bean="ICRTypeCode-name" />
        <ref bean="ICRTypeCode-codeAndDescription" />
        <ref bean="ICRTypeCode-versionNumber" />
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->


  <bean id="ICRTypeCode-code" parent="ICRTypeCode-code-parentBean" />

  <bean id="ICRTypeCode-code-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="code" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Indirect Cost Recovery Type Code" />
    <property name="shortLabel" value="Indirect Cost Recovery Type Code" />
    <property name="maxLength" value="2" />
    <property name="validationPattern" >
      <bean parent="AlphaNumericValidationPattern" />
    </property>
    <property name="required" value="true" />
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="2" />
    </property>
  </bean>
  <bean id="ICRTypeCode-name" parent="ICRTypeCode-name-parentBean" />

  <bean id="ICRTypeCode-name-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="name" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Indirect Cost Recovery Type Name" />
    <property name="shortLabel" value="Name" />
    <property name="maxLength" value="40" />
    <property name="required" value="true" />
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="40" />
    </property>
  </bean>
  <bean id="ICRTypeCode-codeAndDescription" parent="ICRTypeCode-codeAndDescription-parentBean" />

  <bean id="ICRTypeCode-codeAndDescription-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="codeAndDescription" />
    <property name="forceUppercase" value="false" />
    <property name="label" value="Indirect Cost Recovery Type Code" />
    <property name="shortLabel" value="Indirect Cost Recovery Type Code" />
    <property name="maxLength" value="45" />
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="45" />
    </property>
  </bean>
  <bean id="ICRTypeCode-versionNumber" parent="ICRTypeCode-versionNumber-parentBean" />

  <bean id="ICRTypeCode-versionNumber-parentBean" abstract="true" parent="GenericAttributes-versionNumber">
  </bean>

<!-- Business Object Inquiry Definition -->


  <bean id="ICRTypeCode-inquiryDefinition" parent="ICRTypeCode-inquiryDefinition-parentBean" />

  <bean id="ICRTypeCode-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
    <property name="title" value="Indirect Cost Recovery Type Inquiry" />
    <property name="inquirySections" >
      <list>
        <bean parent="InquirySectionDefinition">
          <property name="title" value="Header Text" />
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields" >
            <list>
              <bean parent="FieldDefinition"
                    p:attributeName="code" />
              <bean parent="FieldDefinition"
                    p:attributeName="name" />
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Business Object Lookup Definition -->


  <bean id="ICRTypeCode-lookupDefinition" parent="ICRTypeCode-lookupDefinition-parentBean" />

  <bean id="ICRTypeCode-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
    <property name="title" value="Indirect Cost Recovery Type Lookup" />
    <property name="instructions" value="Lookup an Indirect Cost Recovery Type Code" />
    <property name="defaultSort" >
      <bean parent="SortDefinition">
        <property name="attributeNames" >
          <list>
            <value>code</value>
          </list>
        </property>
      </bean>
    </property>
    <property name="lookupFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="code" />
        <bean parent="FieldDefinition"
              p:attributeName="name" />
      </list>
    </property>
    <property name="resultFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="code" />
        <bean parent="FieldDefinition"
              p:attributeName="name" />
      </list>
    </property>
  </bean>
</beans>
