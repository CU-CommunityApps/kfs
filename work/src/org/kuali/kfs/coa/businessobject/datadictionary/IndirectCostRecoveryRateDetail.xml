<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="IndirectCostRecoveryRateDetail" parent="IndirectCostRecoveryRateDetail-parentBean" />
  <bean id="IndirectCostRecoveryRateDetail-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.kfs.coa.businessobject.IndirectCostRecoveryRateDetail" />
    <property name="inquiryDefinition" >
      <ref bean="IndirectCostRecoveryRateDetail-inquiryDefinition" />
    </property>
    <property name="lookupDefinition" >
      <ref bean="IndirectCostRecoveryRateDetail-lookupDefinition" />
    </property>
    <property name="objectLabel" value="Indirect Cost Recovery Rate Detail" />
    <property name="attributes" >
      <list>
        <ref bean="IndirectCostRecoveryRateDetail-universityFiscalYear" />
        <ref bean="IndirectCostRecoveryRateDetail-financialIcrSeriesIdentifier" />
        <ref bean="IndirectCostRecoveryRateDetail-awardIndrCostRcvyEntryNbr" />
        <ref bean="IndirectCostRecoveryRateDetail-chartOfAccountsCode" />
        <ref bean="IndirectCostRecoveryRateDetail-accountNumber" />
        <ref bean="IndirectCostRecoveryRateDetail-subAccountNumber" />
        <ref bean="IndirectCostRecoveryRateDetail-financialObjectCode" />
        <ref bean="IndirectCostRecoveryRateDetail-financialSubObjectCode" />
        <ref bean="IndirectCostRecoveryRateDetail-transactionDebitIndicator" />
        <ref bean="IndirectCostRecoveryRateDetail-awardIndrCostRcvyRatePct" />
        <ref bean="IndirectCostRecoveryRateDetail-active" />
      </list>
    </property>
    <property name="inactivationBlockingDefinitions">
      <list>
        <bean parent="InactivationBlockingDefinition"
          p:blockedBusinessObjectClass="org.kuali.kfs.coa.businessobject.Account"
          p:inactivationBlockingDetectionServiceBeanName="indirectCostRecoveryRateDetailInactivationBlockingDetectionService"/>
        <bean parent="InactivationBlockingDefinition"
          p:blockedBusinessObjectClass="org.kuali.kfs.coa.businessobject.ObjectCode"
          p:inactivationBlockingDetectionServiceBeanName="indirectCostRecoveryRateDetailInactivationBlockingDetectionService"/>
        <bean parent="InactivationBlockingDefinition"
          p:blockedBusinessObjectClass="org.kuali.kfs.coa.businessobject.SubObjCd"
          p:inactivationBlockingDetectionServiceBeanName="indirectCostRecoveryRateDetailInactivationBlockingDetectionService"/>
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->

  <bean id="IndirectCostRecoveryRateDetail-universityFiscalYear" parent="IndirectCostRecoveryRateDetail-universityFiscalYear-parentBean" />
  <bean id="IndirectCostRecoveryRateDetail-universityFiscalYear-parentBean" abstract="true" parent="GenericAttributes-genericFiscalYear">
    <property name="name" value="universityFiscalYear" />
    <property name="required" value="true" />
  </bean>
  
  <bean id="IndirectCostRecoveryRateDetail-financialIcrSeriesIdentifier" parent="IndirectCostRecoveryRateDetail-financialIcrSeriesIdentifier-parentBean" />
  <bean id="IndirectCostRecoveryRateDetail-financialIcrSeriesIdentifier-parentBean" abstract="true" parent="IndirectCostRecoveryRate-financialIcrSeriesIdentifier">
    <property name="name" value="financialIcrSeriesIdentifier" />
  </bean>
  
  <bean id="IndirectCostRecoveryRateDetail-awardIndrCostRcvyEntryNbr" parent="IndirectCostRecoveryRateDetail-awardIndrCostRcvyEntryNbr-parentBean" />
  <bean id="IndirectCostRecoveryRateDetail-awardIndrCostRcvyEntryNbr-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="awardIndrCostRcvyEntryNbr" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="ICR Entry Number" />
    <property name="shortLabel" value="Number" />
    <property name="maxLength" value="2" />
    <property name="validationPattern" >
      <bean parent="NumericValidationPattern" />
    </property>
    <property name="required" value="true" />
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="3" />
    </property>
  </bean>
  
  <bean id="IndirectCostRecoveryRateDetail-chartOfAccountsCode" parent="IndirectCostRecoveryRateDetail-chartOfAccountsCode-parentBean" />
  <bean id="IndirectCostRecoveryRateDetail-chartOfAccountsCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="chartOfAccountsCode" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Chart Code" />
    <property name="shortLabel" value="Chart" />
    <property name="maxLength" value="2" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="required" value="true" />
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="2" />
    </property>
  </bean>
  
  <bean id="IndirectCostRecoveryRateDetail-accountNumber" parent="IndirectCostRecoveryRateDetail-accountNumber-parentBean" />
  <bean id="IndirectCostRecoveryRateDetail-accountNumber-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="accountNumber" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Account" />
    <property name="shortLabel" value="ACCT" />
    <property name="maxLength" value="7" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="required" value="true" />
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="7" />
    </property>
  </bean>
  
  <bean id="IndirectCostRecoveryRateDetail-subAccountNumber" parent="IndirectCostRecoveryRateDetail-subAccountNumber-parentBean" />
  <bean id="IndirectCostRecoveryRateDetail-subAccountNumber-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="subAccountNumber" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Sub-Account" />
    <property name="shortLabel" value="SubAcct" />
    <property name="maxLength" value="5" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="required" value="false" />
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="5" />
    </property>
  </bean>
  
  <bean id="IndirectCostRecoveryRateDetail-financialObjectCode" parent="IndirectCostRecoveryRateDetail-financialObjectCode-parentBean" />
  <bean id="IndirectCostRecoveryRateDetail-financialObjectCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="financialObjectCode" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Object Code" />
    <property name="shortLabel" value="Object" />
    <property name="maxLength" value="4" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="required" value="true" />
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="4" />
    </property>
  </bean>
  
  <bean id="IndirectCostRecoveryRateDetail-financialSubObjectCode" parent="IndirectCostRecoveryRateDetail-financialSubObjectCode-parentBean" />
  <bean id="IndirectCostRecoveryRateDetail-financialSubObjectCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="financialSubObjectCode" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Sub-Object Code" />
    <property name="shortLabel" value="SubObject" />
    <property name="maxLength" value="3" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="required" value="false" />
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="3" />
    </property>
  </bean>
  
  <bean id="IndirectCostRecoveryRateDetail-transactionDebitIndicator" parent="IndirectCostRecoveryRateDetail-transactionDebitIndicator-parentBean" />
  <bean id="IndirectCostRecoveryRateDetail-transactionDebitIndicator-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="transactionDebitIndicator" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Transaction Debit Credit Code" />
    <property name="shortLabel" value="D/C" />
    <property name="maxLength" value="1" />
    <property name="validationPattern" >
      <bean parent="CharsetValidationPattern"
            p:validChars="CD" />
    </property>
    <property name="required" value="true" />
    <property name="control" >
      <bean parent="SelectControlDefinition"
            p:valuesFinderClass="org.kuali.kfs.sys.businessobject.options.DebitCreditValuesFinder"
            p:includeKeyInLabel="false" />
    </property>
  </bean>
  
  <bean id="IndirectCostRecoveryRateDetail-awardIndrCostRcvyRatePct" parent="IndirectCostRecoveryRateDetail-awardIndrCostRcvyRatePct-parentBean" />
  <bean id="IndirectCostRecoveryRateDetail-awardIndrCostRcvyRatePct-parentBean" abstract="true" parent="GenericAttributes-percent">
    <property name="name" value="awardIndrCostRcvyRatePct" />
    <property name="label" value="ICR Percent" />
    <property name="shortLabel" value="Rate%" />
    <property name="required" value="true" />
  </bean>
  
  
  <bean id="IndirectCostRecoveryRateDetail-active" parent="IndirectCostRecoveryRateDetail-active-parentBean" />
  <bean id="IndirectCostRecoveryRateDetail-active-parentBean" abstract="true" parent="GenericAttributes-activeIndicator">
  	<property name="name" value="active" />
  </bean>

<!-- Business Object Inquiry Definition -->

  <bean id="IndirectCostRecoveryRateDetail-inquiryDefinition" parent="IndirectCostRecoveryRateDetail-inquiryDefinition-parentBean" />
  <bean id="IndirectCostRecoveryRateDetail-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
    <property name="title" value="Indirect Cost Recovery Rate Detail Inquiry" />
    <property name="inquirySections" >
      <list>
        <bean parent="InquirySectionDefinition">
          <property name="title" value="Header Text" />
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields" >
            <list>
              <bean parent="FieldDefinition"
                    p:attributeName="universityFiscalYear" />
              <bean parent="FieldDefinition"
                    p:attributeName="financialIcrSeriesIdentifier" />
              <bean parent="FieldDefinition"
                    p:attributeName="awardIndrCostRcvyEntryNbr" />
              <bean parent="FieldDefinition"
                    p:attributeName="chartOfAccountsCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="accountNumber" />
              <bean parent="FieldDefinition"
                    p:attributeName="subAccountNumber" />
              <bean parent="FieldDefinition"
                    p:attributeName="financialObjectCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="financialSubObjectCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="transactionDebitIndicator" />
              <bean parent="FieldDefinition"
                    p:attributeName="awardIndrCostRcvyRatePct" />
              <bean parent="FieldDefinition"
                    p:attributeName="active" />
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Business Object Lookup Definition -->

  <bean id="IndirectCostRecoveryRateDetail-lookupDefinition" parent="IndirectCostRecoveryRateDetail-lookupDefinition-parentBean" />
  <bean id="IndirectCostRecoveryRateDetail-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
    <property name="title" value="Indirect Cost Recovery Rate Detail Lookup" />
    <property name="instructions" value="Lookup an Indirect Cost Recovery Rate Detail" />
    <property name="defaultSort" >
      <bean parent="SortDefinition">
        <property name="attributeNames" >
          <list>
            <value>accountNumber</value>
          </list>
        </property>
      </bean>
    </property>
    <property name="lookupFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="universityFiscalYear" />
        <bean parent="FieldDefinition"
              p:attributeName="financialIcrSeriesIdentifier" />
        <bean parent="FieldDefinition"
              p:attributeName="chartOfAccountsCode" />
        <bean parent="FieldDefinition"
              p:attributeName="accountNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="financialObjectCode" />
        <bean parent="FieldDefinition"
              p:attributeName="active" />
      </list>
    </property>
    <property name="resultFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="universityFiscalYear" />
        <bean parent="FieldDefinition"
              p:attributeName="financialIcrSeriesIdentifier" />
        <bean parent="FieldDefinition"
              p:attributeName="chartOfAccountsCode" />
        <bean parent="FieldDefinition"
              p:attributeName="accountNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="financialObjectCode" />
        <bean parent="FieldDefinition"
              p:attributeName="financialSubObjectCode" />
        <bean parent="FieldDefinition"
              p:attributeName="subAccountNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="awardIndrCostRcvyEntryNbr" />
        <bean parent="FieldDefinition"
              p:attributeName="transactionDebitIndicator" />
        <bean parent="FieldDefinition"
              p:attributeName="awardIndrCostRcvyRatePct" />
        <bean parent="FieldDefinition"
              p:attributeName="active" />
      </list>
    </property>
  </bean>
</beans>
