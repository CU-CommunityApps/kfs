<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="SubObjCd" parent="SubObjCd-parentBean" />

  <bean id="SubObjCd-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.module.chart.bo.SubObjCd" />
    <property name="inquiryDefinition" >
      <ref bean="SubObjCd-inquiryDefinition" />
    </property>
    <property name="lookupDefinition" >
      <ref bean="SubObjCd-lookupDefinition" />
    </property>
    <property name="titleAttribute" value="financialSubObjectCode" />
    <property name="objectLabel" value="Sub-Object Code" />
    <property name="attributes" >
      <list>
        <ref bean="SubObjCd-universityFiscalYear" />
        <ref bean="SubObjCd-chartOfAccountsCode" />
        <ref bean="SubObjCd-financialObjectCode" />
        <ref bean="SubObjCd-accountNumber" />
        <ref bean="SubObjCd-financialSubObjectCode" />
        <ref bean="SubObjCd-financialSubObjectCodeName" />
        <ref bean="SubObjCd-financialSubObjectCdshortNm" />
        <ref bean="SubObjCd-financialSubObjectActiveIndicator" />
        <ref bean="SubObjCd-versionNumber" />
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->


  <bean id="SubObjCd-universityFiscalYear" parent="SubObjCd-universityFiscalYear-parentBean" />

  <bean id="SubObjCd-universityFiscalYear-parentBean" abstract="true" parent="GenericAttributes-genericFiscalYear">
    <property name="name" value="universityFiscalYear" />
    <property name="required" value="true" />
  </bean>
  <bean id="SubObjCd-chartOfAccountsCode" parent="SubObjCd-chartOfAccountsCode-parentBean" />

  <bean id="SubObjCd-chartOfAccountsCode-parentBean" abstract="true" parent="Chart-chartOfAccountsCode">
  </bean>
  <bean id="SubObjCd-financialObjectCode" parent="SubObjCd-financialObjectCode-parentBean" />

  <bean id="SubObjCd-financialObjectCode-parentBean" abstract="true" parent="ObjectCode-financialObjectCode">
  </bean>
  <bean id="SubObjCd-accountNumber" parent="SubObjCd-accountNumber-parentBean" />

  <bean id="SubObjCd-accountNumber-parentBean" abstract="true" parent="Account-accountNumber">
  </bean>
  <bean id="SubObjCd-financialSubObjectCode" parent="SubObjCd-financialSubObjectCode-parentBean" />

  <bean id="SubObjCd-financialSubObjectCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="financialSubObjectCode" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Sub-Object Code" />
    <property name="shortLabel" value="Sub-Object" />
    <property name="maxLength" value="3" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern"
            p:allowWhitespace="true" />
    </property>
    <property name="required" value="true" />
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="3" />
    </property>
  </bean>
  <bean id="SubObjCd-financialSubObjectCodeName" parent="SubObjCd-financialSubObjectCodeName-parentBean" />

  <bean id="SubObjCd-financialSubObjectCodeName-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="financialSubObjectCodeName" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Sub-Object Code Name" />
    <property name="shortLabel" value="SubObjCodeName" />
    <property name="maxLength" value="40" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern"
            p:allowWhitespace="true" />
    </property>
    <property name="required" value="true" />
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="40" />
    </property>
  </bean>
  <bean id="SubObjCd-financialSubObjectCdshortNm" parent="SubObjCd-financialSubObjectCdshortNm-parentBean" />

  <bean id="SubObjCd-financialSubObjectCdshortNm-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="financialSubObjectCdshortNm" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Sub-Object Code Short Name" />
    <property name="shortLabel" value="SubObjCodeShortName" />
    <property name="maxLength" value="12" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern"
            p:allowWhitespace="true" />
    </property>
    <property name="required" value="true" />
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="12" />
    </property>
  </bean>
  <bean id="SubObjCd-financialSubObjectActiveIndicator" parent="SubObjCd-financialSubObjectActiveIndicator-parentBean" />

  <bean id="SubObjCd-financialSubObjectActiveIndicator-parentBean" abstract="true" parent="GenericAttributes-activeIndicator">
    <property name="name" value="financialSubObjectActiveIndicator" />
  </bean>
  <bean id="SubObjCd-versionNumber" parent="SubObjCd-versionNumber-parentBean" />

  <bean id="SubObjCd-versionNumber-parentBean" abstract="true" parent="GenericAttributes-versionNumber">
  </bean>

<!-- Business Object Inquiry Definition -->


  <bean id="SubObjCd-inquiryDefinition" parent="SubObjCd-inquiryDefinition-parentBean" />

  <bean id="SubObjCd-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
    <property name="title" value="Sub-Object Code Inquiry" />
    <property name="inquirySections" >
      <list>
        <bean parent="InquirySectionDefinition">
          <property name="title" value="Header Text" />
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields" >
            <list>
              <bean parent="FieldDefinition"
                    p:attributeName="universityFiscalYear" />
              <bean parent="FieldDefinition"
                    p:attributeName="accountNumber" />
              <bean parent="FieldDefinition"
                    p:attributeName="chartOfAccountsCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="financialObjectCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="financialSubObjectCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="financialSubObjectCodeName" />
              <bean parent="FieldDefinition"
                    p:attributeName="financialSubObjectActiveIndicator" />
              <bean parent="FieldDefinition"
                    p:attributeName="financialSubObjectCdshortNm" />
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Business Object Lookup Definition -->


  <bean id="SubObjCd-lookupDefinition" parent="SubObjCd-lookupDefinition-parentBean" />

  <bean id="SubObjCd-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
    <property name="title" value="Sub-Object Code Lookup" />
    <property name="menubar" value="&lt;a href=&quot;maintenance.do?methodToCall=start&amp;businessObjectClassName=org.kuali.module.chart.bo.SubObjCdGlobal&quot;&gt;&lt;img src=&quot;${kr.externalizable.images.url}tinybutton-createnewglobal.gif&quot; alt=&quot;create new global&quot; width=&quot;98&quot; height=&quot;15&quot;/&gt;&lt;/a&gt;" />
    <property name="instructions" value="Lookup a Sub-Object Code" />
    <property name="defaultSort" >
      <bean parent="SortDefinition">
        <property name="attributeNames" >
          <list>
            <value>financialSubObjectCode</value>
          </list>
        </property>
      </bean>
    </property>
    <property name="lookupFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="universityFiscalYear"
              p:defaultValueFinderClass="org.kuali.kfs.lookup.valuefinder.FiscalYearFinder" />
        <bean parent="FieldDefinition"
              p:attributeName="accountNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="chartOfAccountsCode" />
        <bean parent="FieldDefinition"
              p:attributeName="financialObjectCode" />
        <bean parent="FieldDefinition"
              p:attributeName="financialSubObjectCode" />
        <bean parent="FieldDefinition"
              p:attributeName="financialSubObjectCodeName" />
        <bean parent="FieldDefinition"
              p:attributeName="financialSubObjectActiveIndicator" />
      </list>
    </property>
    <property name="resultFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="universityFiscalYear" />
        <bean parent="FieldDefinition"
              p:attributeName="accountNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="chartOfAccountsCode" />
        <bean parent="FieldDefinition"
              p:attributeName="financialObjectCode" />
        <bean parent="FieldDefinition"
              p:attributeName="financialSubObjectCode" />
        <bean parent="FieldDefinition"
              p:attributeName="financialSubObjectCodeName" />
        <bean parent="FieldDefinition"
              p:attributeName="financialSubObjectActiveIndicator" />
        <bean parent="FieldDefinition"
              p:attributeName="financialSubObjectCdshortNm" />
      </list>
    </property>
  </bean>
</beans>
