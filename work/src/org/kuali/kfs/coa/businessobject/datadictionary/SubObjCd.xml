<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "../../../core/datadictionary/dataDictionary-1_0.dtd">
    
<dictionaryEntry>
	<businessObject
		businessObjectClass="org.kuali.module.chart.bo.SubObjCd">
		<customAttributeEnabled>false</customAttributeEnabled>

		<inquiry>
			<title>Sub-Object Code Inquiry</title>

			<inquiryFields>
				<field attributeName="universityFiscal.universityFiscalYear" />
				<field attributeName="account.accountNumber" />
				<field attributeName="chartOfAccounts.chartOfAccountsCode" />			
				<field attributeName="financialObject.financialObjectCode" />
				<field attributeName="financialSubObjectCode" />
				<field attributeName="financialSubObjectCodeName" />
				<field attributeName="financialSubObjectActiveIndicator" />			
				<field attributeName="financialSubObjectCdshortNm" />
			</inquiryFields>
		</inquiry>

		<lookup>
			<title>Sub-Object Code Lookup</title>

			<menubar><![CDATA[<a href="maintenance.do?methodToCall=start&businessObjectClassName=org.kuali.module.chart.bo.SubObjCd">Create New</a>]]></menubar>

			<instructions>Lookup a Sub-Object Code</instructions>

            <defaultSort attributeName="financialSubObjectCode" sortAscending="true" /> 

			<lookupFields>
				<lookupField attributeName="universityFiscal.universityFiscalYear" required="false" >
				  <defaultValueFinderClass>org.kuali.core.lookup.valueFinder.FiscalYearFinder</defaultValueFinderClass>
				</lookupField>
				<lookupField attributeName="account.accountNumber" />
				<lookupField attributeName="chartOfAccounts.chartOfAccountsCode" />			
				<lookupField attributeName="financialObject.financialObjectCode" />
				<lookupField attributeName="financialSubObjectCode" />
				<lookupField attributeName="financialSubObjectCodeName" />
			</lookupFields>

			<resultFields>
				<field attributeName="universityFiscal.universityFiscalYear" />
				<field attributeName="account.accountNumber" />
				<field attributeName="chartOfAccounts.chartOfAccountsCode" />			
				<field attributeName="financialObject.financialObjectCode" />
				<field attributeName="financialSubObjectCode" />
				<field attributeName="financialSubObjectCodeName" />
				<field attributeName="financialSubObjectActiveIndicator" />			
				<field attributeName="financialSubObjectCdshortNm" />
			</resultFields>
		</lookup>
		
	    <titleAttribute>financialSubObjectCode</titleAttribute>

		<attributes>
			<attributeReference name="universityFiscal.universityFiscalYear" 
								sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" 
								sourceAttributeName="genericFiscalYear" />

			<attributeReference name="chartOfAccounts.chartOfAccountsCode" 
								sourceClassName="org.kuali.module.chart.bo.Chart" 
								sourceAttributeName="chartOfAccountsCode" />

			<attributeReference name="financialObject.financialObjectCode" 
								sourceClassName="org.kuali.module.chart.bo.ObjectCode" 
								sourceAttributeName="financialObjectCode" />

			<attributeReference name="account.accountNumber" 
								sourceClassName="org.kuali.module.chart.bo.Account" 
								sourceAttributeName="accountNumber" />

			<attribute name="financialSubObjectCode" forceUppercase="true">
				<label>SubObject Code</label>
				<shortLabel>SubObject</shortLabel>

				<maxLength>3</maxLength>
				<validationPattern><anyCharacter allowWhitespace="true" /></validationPattern>

				<control>
					<text size="3" />
				</control>

				<summary>Sub-Object Code</summary>
				<description>Sub-Object Code.</description>
			</attribute>	

			<attribute name="financialSubObjectCodeName" forceUppercase="false">
				<label>Sub-Object Code Name</label>
				<shortLabel>SubObjCodeName</shortLabel>

				<maxLength>40</maxLength>
				<validationPattern><anyCharacter allowWhitespace="true" /></validationPattern>

				<control>
					<text size="40" />
				</control>

				<summary>Sub-Object Code Name</summary>
				<description>
					Sub-Object Code Name
				</description>
			</attribute>
			
			<attribute name="financialSubObjectCdshortNm" forceUppercase="false">
				<label>Sub-Object Code Short Name</label>
				<shortLabel>SubObjCodeShortName</shortLabel>

				<maxLength>12</maxLength>
				<validationPattern><anyCharacter allowWhitespace="true" /></validationPattern>

				<control>
					<text size="12" />
				</control>

				<summary>Sub-Object Code Short Name</summary>
				<description>
					Sub-Object Code Short Name
				</description>
			</attribute>
			
 			<attributeReference name="versionNumber" 
 				sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" 
 				sourceAttributeName="versionNumber" />
			
		</attributes>
	</businessObject>
</dictionaryEntry>

