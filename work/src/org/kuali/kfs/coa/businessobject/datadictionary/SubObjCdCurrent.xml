<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="SubObjCdCurrent" parent="SubObjCdCurrent-parentBean" />

  <bean id="SubObjCdCurrent-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.module.chart.bo.SubObjCdCurrent" />
    <property name="inquiryDefinition" >
      <ref bean="SubObjCdCurrent-inquiryDefinition" />
    </property>
    <property name="lookupDefinition" >
      <ref bean="SubObjCdCurrent-lookupDefinition" />
    </property>
    <property name="titleAttribute" value="financialSubObjectCode" />
    <property name="objectLabel" value="Sub-Object Code Current" />
    <property name="attributes" >
      <list>
        <ref bean="SubObjCdCurrent-universityFiscalYear" />
        <ref bean="SubObjCdCurrent-chartOfAccountsCode" />
        <ref bean="SubObjCdCurrent-financialObjectCode" />
        <ref bean="SubObjCdCurrent-accountNumber" />
        <ref bean="SubObjCdCurrent-financialSubObjectCode" />
        <ref bean="SubObjCdCurrent-financialSubObjectCodeName" />
        <ref bean="SubObjCdCurrent-financialSubObjectCdshortNm" />
        <ref bean="SubObjCdCurrent-financialSubObjectActiveIndicator" />
        <ref bean="SubObjCdCurrent-versionNumber" />
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->


  <bean id="SubObjCdCurrent-universityFiscalYear" parent="SubObjCdCurrent-universityFiscalYear-parentBean" />

  <bean id="SubObjCdCurrent-universityFiscalYear-parentBean" abstract="true" parent="GenericAttributes-genericFiscalYear">
    <property name="name" value="universityFiscalYear" />
    <property name="required" value="true" />
  </bean>
  <bean id="SubObjCdCurrent-chartOfAccountsCode" parent="SubObjCdCurrent-chartOfAccountsCode-parentBean" />

  <bean id="SubObjCdCurrent-chartOfAccountsCode-parentBean" abstract="true" parent="Chart-chartOfAccountsCode">
    <property name="control" >
      <bean parent="SelectControlDefinition"
            p:valuesFinderClass="org.kuali.module.chart.lookup.keyvalues.ChartValuesFinder"
            p:includeKeyInLabel="false" />
    </property>
  </bean>
  <bean id="SubObjCdCurrent-financialObjectCode" parent="SubObjCdCurrent-financialObjectCode-parentBean" />

  <bean id="SubObjCdCurrent-financialObjectCode-parentBean" abstract="true" parent="ObjectCode-financialObjectCode">
  </bean>
  <bean id="SubObjCdCurrent-accountNumber" parent="SubObjCdCurrent-accountNumber-parentBean" />

  <bean id="SubObjCdCurrent-accountNumber-parentBean" abstract="true" parent="Account-accountNumber">
  </bean>
  <bean id="SubObjCdCurrent-financialSubObjectCode" parent="SubObjCdCurrent-financialSubObjectCode-parentBean" />

  <bean id="SubObjCdCurrent-financialSubObjectCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="financialSubObjectCode" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Sub-Object Code" />
    <property name="shortLabel" value="Sub-Object" />
    <property name="maxLength" value="3" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern"
            p:allowWhitespace="true" />
    </property>
    <property name="required" value="true" />
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="3" />
    </property>
  </bean>
  <bean id="SubObjCdCurrent-financialSubObjectCodeName" parent="SubObjCdCurrent-financialSubObjectCodeName-parentBean" />

  <bean id="SubObjCdCurrent-financialSubObjectCodeName-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="financialSubObjectCodeName" />
    <property name="forceUppercase" value="false" />
    <property name="label" value="Sub-Object Code Name" />
    <property name="shortLabel" value="SubObjCodeName" />
    <property name="maxLength" value="40" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern"
            p:allowWhitespace="true" />
    </property>
    <property name="required" value="true" />
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="40" />
    </property>
  </bean>
  <bean id="SubObjCdCurrent-financialSubObjectCdshortNm" parent="SubObjCdCurrent-financialSubObjectCdshortNm-parentBean" />

  <bean id="SubObjCdCurrent-financialSubObjectCdshortNm-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="financialSubObjectCdshortNm" />
    <property name="forceUppercase" value="false" />
    <property name="label" value="Sub-Object Code Short Name" />
    <property name="shortLabel" value="SubObjCodeShortName" />
    <property name="maxLength" value="12" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern"
            p:allowWhitespace="true" />
    </property>
    <property name="required" value="true" />
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="12" />
    </property>
  </bean>
  <bean id="SubObjCdCurrent-financialSubObjectActiveIndicator" parent="SubObjCdCurrent-financialSubObjectActiveIndicator-parentBean" />

  <bean id="SubObjCdCurrent-financialSubObjectActiveIndicator-parentBean" abstract="true" parent="GenericAttributes-activeIndicator">
    <property name="name" value="financialSubObjectActiveIndicator" />
  </bean>
  <bean id="SubObjCdCurrent-versionNumber" parent="SubObjCdCurrent-versionNumber-parentBean" />

  <bean id="SubObjCdCurrent-versionNumber-parentBean" abstract="true" parent="GenericAttributes-versionNumber">
  </bean>

<!-- Business Object Inquiry Definition -->


  <bean id="SubObjCdCurrent-inquiryDefinition" parent="SubObjCdCurrent-inquiryDefinition-parentBean" />

  <bean id="SubObjCdCurrent-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
    <property name="title" value="Sub-Object Code Inquiry" />
    <property name="inquirySections" >
      <list>
        <bean parent="InquirySectionDefinition">
          <property name="title" value="Header Text" />
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields" >
            <list>
              <bean parent="FieldDefinition"
                    p:attributeName="universityFiscalYear" />
              <bean parent="FieldDefinition"
                    p:attributeName="accountNumber" />
              <bean parent="FieldDefinition"
                    p:attributeName="chartOfAccountsCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="financialObjectCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="financialSubObjectCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="financialSubObjectCodeName" />
              <bean parent="FieldDefinition"
                    p:attributeName="financialSubObjectActiveIndicator" />
              <bean parent="FieldDefinition"
                    p:attributeName="financialSubObjectCdshortNm" />
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Business Object Lookup Definition -->


  <bean id="SubObjCdCurrent-lookupDefinition" parent="SubObjCdCurrent-lookupDefinition-parentBean" />

  <bean id="SubObjCdCurrent-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
    <property name="title" value="Sub-Object Code Lookup" />
    <property name="instructions" value="Lookup a Sub-Object Code" />
    <property name="defaultSort" >
      <bean parent="SortDefinition">
        <property name="attributeNames" >
          <list>
            <value>financialSubObjectCode</value>
          </list>
        </property>
      </bean>
    </property>
    <property name="lookupFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="universityFiscalYear"
              p:defaultValueFinderClass="org.kuali.kfs.lookup.valuefinder.FiscalYearFinder" />
        <bean parent="FieldDefinition"
              p:attributeName="accountNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="chartOfAccountsCode" />
        <bean parent="FieldDefinition"
              p:attributeName="financialObjectCode" />
        <bean parent="FieldDefinition"
              p:attributeName="financialSubObjectCode" />
        <bean parent="FieldDefinition"
              p:attributeName="financialSubObjectCodeName" />
      </list>
    </property>
    <property name="resultFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="universityFiscalYear" />
        <bean parent="FieldDefinition"
              p:attributeName="accountNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="chartOfAccountsCode" />
        <bean parent="FieldDefinition"
              p:attributeName="financialObjectCode" />
        <bean parent="FieldDefinition"
              p:attributeName="financialSubObjectCode" />
        <bean parent="FieldDefinition"
              p:attributeName="financialSubObjectCodeName" />
        <bean parent="FieldDefinition"
              p:attributeName="financialSubObjectActiveIndicator" />
        <bean parent="FieldDefinition"
              p:attributeName="financialSubObjectCdshortNm" />
      </list>
    </property>
  </bean>
</beans>
