<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "../../../core/datadictionary/dataDictionary-1_0.dtd">
<!-- 
    NOTE!!!
    This is essentially a copy of SubObjCd.xml, if you make any changes 
    they need to be made to both documents
-->

<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.module.chart.bo.SubObjCdCurrent">
        <customAttributeEnabled>true</customAttributeEnabled>

        <inquiry>
            <title>Sub-Object Code Inquiry</title>

            <inquiryFields>
                <field attributeName="universityFiscalYear" />
                <field attributeName="accountNumber" />
                <field attributeName="chartOfAccountsCode" />
                <field attributeName="financialObjectCode" />
                <field attributeName="financialSubObjectCode" />
                <field attributeName="financialSubObjectCodeName" />
                <field attributeName="financialSubObjectActiveIndicator" />
                <field attributeName="financialSubObjectCdshortNm" />
            </inquiryFields>
        </inquiry>

        <lookup>
            <title>Sub-Object Code Lookup</title>

            <menubar>
                <![CDATA[<a href="maintenance.do?methodToCall=start&businessObjectClassName=org.kuali.module.chart.bo.SubObjCd"><img src="images/tinybutton-createnew.gif" alt="create new" width="70" height="15"/></a>]]>
            </menubar>

            <instructions>Lookup a Sub-Object Code</instructions>

            <defaultSort attributeName="financialSubObjectCode" sortAscending="true" />

            <lookupFields>
                <lookupField attributeName="universityFiscalYear" required="false">
                    <defaultValueFinderClass>org.kuali.core.lookup.valueFinder.FiscalYearFinder</defaultValueFinderClass>
                </lookupField>
                <lookupField attributeName="accountNumber" />
                <lookupField attributeName="chartOfAccountsCode" />
                <lookupField attributeName="financialObjectCode" />
                <lookupField attributeName="financialSubObjectCode" />
                <lookupField attributeName="financialSubObjectCodeName" />
            </lookupFields>

            <resultFields>
                <field attributeName="universityFiscalYear" />
                <field attributeName="accountNumber" />
                <field attributeName="chartOfAccountsCode" />
                <field attributeName="financialObjectCode" />
                <field attributeName="financialSubObjectCode" />
                <field attributeName="financialSubObjectCodeName" />
                <field attributeName="financialSubObjectActiveIndicator" />
                <field attributeName="financialSubObjectCdshortNm" />
            </resultFields>
        </lookup>

        <titleAttribute>financialSubObjectCode</titleAttribute>

        <attributes>
            <attributeReference name="universityFiscalYear" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="genericFiscalYear">
                <required>true</required>
            </attributeReference>

            <attributeReference name="chartOfAccountsCode" sourceClassName="org.kuali.module.chart.bo.Chart" sourceAttributeName="chartOfAccountsCode">
                <required>true</required>
                <control>
                    <select valuesFinderClass="org.kuali.core.lookup.keyvalues.ChartValuesFinder" />
                </control>

            </attributeReference>

            <attributeReference name="financialObjectCode" sourceClassName="org.kuali.module.chart.bo.ObjectCode" sourceAttributeName="financialObjectCode">
                <required>true</required>
            </attributeReference>

            <attributeReference name="accountNumber" sourceClassName="org.kuali.module.chart.bo.Account" sourceAttributeName="accountNumber">
                <required>true</required>
            </attributeReference>

            <attribute name="financialSubObjectCode" forceUppercase="true">
                <label>Sub-Object Code</label>
                <shortLabel>Sub-Object</shortLabel>

                <maxLength>3</maxLength>
                <validationPattern>
                    <anyCharacter allowWhitespace="true" />
                </validationPattern>
                <required>true</required>

                <control>
                    <text size="3" />
                </control>

                <summary>
                    <![CDATA[&nbsp;]]>
                </summary>
                <description>
                    The code to define this sub-object. This is the code that should be entered in the sub-object field of the accounting line on financial documents.
                </description>
            </attribute>

            <attribute name="financialSubObjectCodeName" forceUppercase="false">
                <label>Sub-Object Code Name</label>
                <shortLabel>SubObjCodeName</shortLabel>

                <maxLength>40</maxLength>
                <validationPattern>
                    <anyCharacter allowWhitespace="true" />
                </validationPattern>
                <required>true</required>

                <control>
                    <text size="40" />
                </control>

                <summary>
                    <![CDATA[&nbsp;]]>
                </summary>
                <description>
                    The long descriptive name of this Sub-object Code. This name will appear on the accounting line in financial documents as well as in searches and reports.
                </description>
            </attribute>

            <attribute name="financialSubObjectCdshortNm" forceUppercase="false">
                <label>Sub-Object Code Short Name</label>
                <shortLabel>SubObjCodeShortName</shortLabel>

                <maxLength>12</maxLength>
                <validationPattern>
                    <anyCharacter allowWhitespace="true" />
                </validationPattern>
                <required>true</required>

                <control>
                    <text size="12" />
                </control>

                <summary>
                    <![CDATA[&nbsp;]]>
                </summary>
                <description>
                    The abbreviated name for this Sub-object Code. This name may be used in reports where there is not enough space to accommodate the Sub-object Code Name.
                </description>
            </attribute>

            <attributeReference name="financialSubObjectActiveIndicator" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="activeIndicator" />

            <attributeReference name="versionNumber" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="versionNumber" />

        </attributes>
    </businessObject>
</dictionaryEntry>

