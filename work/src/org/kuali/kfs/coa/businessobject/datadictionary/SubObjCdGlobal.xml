<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "../../../core/datadictionary/dataDictionary-1_0.dtd">

<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.module.chart.bo.SubObjCdGlobal">


       
        <attributes>
            <attributeReference name="universityFiscalYear" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="genericFiscalYear">
                <label>Fiscal Year</label>
                <shortLabel>FY</shortLabel>
                <required>false</required>
            </attributeReference>

            <attributeReference name="chartOfAccountsCode" sourceClassName="org.kuali.module.chart.bo.Chart" sourceAttributeName="chartOfAccountsCode">
                <required>false</required>
                <control>
                    <select valuesFinderClass="org.kuali.module.chart.lookup.keyvalues.ChartValuesFinder" />
                </control>

            </attributeReference>

            <attribute name="financialSubObjectCode" forceUppercase="true">
                <label>Sub-Object Code</label>
                <shortLabel>Sub-Object</shortLabel>

                <maxLength>3</maxLength>
                <validationPattern>
                    <alphaNumeric />
                </validationPattern>
                <required>true</required>
                <control>
                    <text size="3" />
                </control>
            </attribute>

            <attribute name="financialSubObjectCodeName" forceUppercase="true">
                <label>Sub-Object Code Name</label>
                <shortLabel>SubObjCodeName</shortLabel>

                <maxLength>40</maxLength>
                <validationPattern>
                    <anyCharacter allowWhitespace="true" />
                </validationPattern>
                <required>true</required>
                <control>
                    <text size="40" />
                </control>
            </attribute>

            <attribute name="financialSubObjectCodeShortName" forceUppercase="true">
                <label>Sub Object Code Short Name</label>
                <shortLabel>SubObjCodeShortName</shortLabel>

                <maxLength>12</maxLength>
                <validationPattern>
                    <anyCharacter allowWhitespace="true" />
                </validationPattern>
                <required>true</required>
                <control>
                    <text size="12" />
                </control>
            </attribute>

            <attribute name="financialSubObjectActiveIndicator" forceUppercase="false">
                <label>Active Code</label>
                <shortLabel>Ac.</shortLabel>

                <maxLength>1</maxLength>

                <control>
                    <checkbox />
                </control>
                <formatterClass>org.kuali.core.web.format.BooleanFormatter</formatterClass>
            </attribute>

            <attributeReference name="versionNumber" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="versionNumber" />

        </attributes>
        
        <objectLabel>Sub-Object Code Global</objectLabel>
    </businessObject>
</dictionaryEntry>
