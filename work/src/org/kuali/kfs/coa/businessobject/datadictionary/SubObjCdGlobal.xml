<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="SubObjCdGlobal" parent="SubObjCdGlobal-parentBean" />

  <bean id="SubObjCdGlobal-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.kfs.coa.businessobject.SubObjCdGlobal" />
    <property name="objectLabel" value="Sub-Object Code Global" />
    <property name="attributes" >
      <list>
        <ref bean="SubObjCdGlobal-universityFiscalYear" />
        <ref bean="SubObjCdGlobal-chartOfAccountsCode" />
        <ref bean="SubObjCdGlobal-financialSubObjectCode" />
        <ref bean="SubObjCdGlobal-financialSubObjectCodeName" />
        <ref bean="SubObjCdGlobal-financialSubObjectCodeShortName" />
        <ref bean="SubObjCdGlobal-active" />
        <ref bean="SubObjCdGlobal-versionNumber" />
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->


  <bean id="SubObjCdGlobal-universityFiscalYear" parent="SubObjCdGlobal-universityFiscalYear-parentBean" />

  <bean id="SubObjCdGlobal-universityFiscalYear-parentBean" abstract="true" parent="GenericAttributes-genericFiscalYear">
    <property name="name" value="universityFiscalYear" />
    <property name="label" value="Fiscal Year" />
    <property name="shortLabel" value="FY" />
    <property name="required" value="false" />
  </bean>
  <bean id="SubObjCdGlobal-chartOfAccountsCode" parent="SubObjCdGlobal-chartOfAccountsCode-parentBean" />

  <bean id="SubObjCdGlobal-chartOfAccountsCode-parentBean" abstract="true" parent="Chart-chartOfAccountsCode">
    <property name="required" value="false" />
    <property name="control" >
      <bean parent="SelectControlDefinition"
            p:valuesFinderClass="org.kuali.kfs.coa.businessobject.options.ChartValuesFinder"
            p:includeKeyInLabel="false" />
    </property>
  </bean>
  <bean id="SubObjCdGlobal-financialSubObjectCode" parent="SubObjCdGlobal-financialSubObjectCode-parentBean" />

  <bean id="SubObjCdGlobal-financialSubObjectCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="financialSubObjectCode" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Sub-Object Code" />
    <property name="shortLabel" value="Sub-Object" />
    <property name="maxLength" value="3" />
    <property name="validationPattern" >
      <bean parent="AlphaNumericValidationPattern" />
    </property>
    <property name="required" value="true" />
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="3" />
    </property>
  </bean>
  <bean id="SubObjCdGlobal-financialSubObjectCodeName" parent="SubObjCdGlobal-financialSubObjectCodeName-parentBean" />

  <bean id="SubObjCdGlobal-financialSubObjectCodeName-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="financialSubObjectCodeName" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Sub-Object Code Name" />
    <property name="shortLabel" value="SubObjCodeName" />
    <property name="maxLength" value="40" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern"
            p:allowWhitespace="true" />
    </property>
    <property name="required" value="true" />
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="40" />
    </property>
  </bean>
  <bean id="SubObjCdGlobal-financialSubObjectCodeShortName" parent="SubObjCdGlobal-financialSubObjectCodeShortName-parentBean" />

  <bean id="SubObjCdGlobal-financialSubObjectCodeShortName-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="financialSubObjectCodeShortName" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Sub Object Code Short Name" />
    <property name="shortLabel" value="SubObjCodeShortName" />
    <property name="maxLength" value="12" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern"
            p:allowWhitespace="true" />
    </property>
    <property name="required" value="true" />
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="12" />
    </property>
  </bean>
  <bean id="SubObjCdGlobal-active" parent="SubObjCdGlobal-active-parentBean" />

  <bean id="SubObjCdGlobal-active-parentBean" abstract="true" parent="GenericAttributes-activeIndicator">
    <property name="name" value="active" />
  </bean>
  <bean id="SubObjCdGlobal-versionNumber" parent="SubObjCdGlobal-versionNumber-parentBean" />

  <bean id="SubObjCdGlobal-versionNumber-parentBean" abstract="true" parent="GenericAttributes-versionNumber">
  </bean>
</beans>
