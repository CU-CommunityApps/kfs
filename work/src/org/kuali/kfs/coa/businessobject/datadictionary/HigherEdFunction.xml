<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="HigherEdFunction" parent="HigherEdFunction-parentBean" />

  <bean id="HigherEdFunction-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.kfs.coa.businessobject.HigherEdFunction" />
    <property name="inquiryDefinition" >
      <ref bean="HigherEdFunction-inquiryDefinition" />
    </property>
    <property name="lookupDefinition" >
      <ref bean="HigherEdFunction-lookupDefinition" />
    </property>
    <property name="titleAttribute" value="financialHigherEdFunctionCd" />
    <property name="objectLabel" value="Higher Education Function" />
    <property name="attributes" >
      <list>
        <ref bean="HigherEdFunction-financialHigherEdFunctionCd" />
        <ref bean="HigherEdFunction-financialHigherEdFunctionNm" />
        <ref bean="HigherEdFunction-codeAndDescription" />
        <ref bean="HigherEdFunction-finUnivBdgtOfficeFunctionCd" />
        <ref bean="HigherEdFunction-finAicpaFunctionCode" />
        <ref bean="HigherEdFunction-financialFederalFunctionCode" />
        <ref bean="HigherEdFunction-active" />
      </list>
    </property>
    <property name="inactivationBlockingDefinitions">
      <list>
        <bean parent="InactivationBlockingDefinition"
          p:blockedReferencePropertyName="aicpaFunction"/>
        <bean parent="InactivationBlockingDefinition"
          p:blockedReferencePropertyName="federalFunction"/>
        <bean parent="InactivationBlockingDefinition"
          p:blockedReferencePropertyName="universityBudgetOfficeFunction"/>
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->


  <bean id="HigherEdFunction-financialHigherEdFunctionCd" parent="HigherEdFunction-financialHigherEdFunctionCd-parentBean" />

  <bean id="HigherEdFunction-financialHigherEdFunctionCd-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="financialHigherEdFunctionCd" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Higher Education Function Code" />
    <property name="shortLabel" value="Code" />
    <property name="maxLength" value="4" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="required" value="true" />
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="4" />
    </property>
  </bean>
  <bean id="HigherEdFunction-financialHigherEdFunctionNm" parent="HigherEdFunction-financialHigherEdFunctionNm-parentBean" />

  <bean id="HigherEdFunction-financialHigherEdFunctionNm-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="financialHigherEdFunctionNm" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Higher Education Function Name" />
    <property name="shortLabel" value="Name" />
    <property name="maxLength" value="40" />
    <property name="required" value="true" />
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="40" />
    </property>
  </bean>
  <bean id="HigherEdFunction-codeAndDescription" parent="HigherEdFunction-codeAndDescription-parentBean" />

  <bean id="HigherEdFunction-codeAndDescription-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="codeAndDescription" />
    <property name="forceUppercase" value="false" />
    <property name="label" value="Higher Education Function Code" />
    <property name="shortLabel" value="Higher Education Function Code" />
    <property name="maxLength" value="47" />
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="47" />
    </property>
  </bean>
  <bean id="HigherEdFunction-finUnivBdgtOfficeFunctionCd" parent="HigherEdFunction-finUnivBdgtOfficeFunctionCd-parentBean" />

  <bean id="HigherEdFunction-finUnivBdgtOfficeFunctionCd-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="finUnivBdgtOfficeFunctionCd" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="University Budget Office Function Code" />
    <property name="shortLabel" value="UBO" />
    <property name="maxLength" value="3" />
    <property name="required" value="true" />
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="3" />
    </property>
  </bean>
  <bean id="HigherEdFunction-finAicpaFunctionCode" parent="HigherEdFunction-finAicpaFunctionCode-parentBean" />

  <bean id="HigherEdFunction-finAicpaFunctionCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="finAicpaFunctionCode" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="AICPA Function Code" />
    <property name="shortLabel" value="AICPA" />
    <property name="maxLength" value="3" />
    <property name="required" value="true" />
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="3" />
    </property>
  </bean>
  <bean id="HigherEdFunction-financialFederalFunctionCode" parent="HigherEdFunction-financialFederalFunctionCode-parentBean" />

  <bean id="HigherEdFunction-financialFederalFunctionCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="financialFederalFunctionCode" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Federal Function Code" />
    <property name="shortLabel" value="Fed" />
    <property name="maxLength" value="3" />
    <property name="required" value="true" />
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="3" />
    </property>
  </bean>
  <bean id="HigherEdFunction-active" parent="HigherEdFunction-active-parentBean" />

  <bean id="HigherEdFunction-active-parentBean" abstract="true" parent="GenericAttributes-activeIndicator">
    <property name="name" value="active" />
  </bean>


<!-- Business Object Inquiry Definition -->


  <bean id="HigherEdFunction-inquiryDefinition" parent="HigherEdFunction-inquiryDefinition-parentBean" />

  <bean id="HigherEdFunction-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
    <property name="title" value="Higher Education Function Inquiry" />
    <property name="inquirySections" >
      <list>
        <bean parent="InquirySectionDefinition">
          <property name="title" value="" />
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields" >
            <list>
              <bean parent="FieldDefinition"
                    p:attributeName="financialHigherEdFunctionCd" />
              <bean parent="FieldDefinition"
                    p:attributeName="financialHigherEdFunctionNm" />
              <bean parent="FieldDefinition"
                    p:attributeName="finUnivBdgtOfficeFunctionCd" />
              <bean parent="FieldDefinition"
                    p:attributeName="finAicpaFunctionCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="financialFederalFunctionCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="active" />
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Business Object Lookup Definition -->


  <bean id="HigherEdFunction-lookupDefinition" parent="HigherEdFunction-lookupDefinition-parentBean" />

  <bean id="HigherEdFunction-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
    <property name="title" value="Higher Education Function Lookup" />
    <property name="instructions" value="Lookup a Higher Education Function" />
    <property name="defaultSort" >
      <bean parent="SortDefinition">
        <property name="attributeNames" >
          <list>
            <value>financialHigherEdFunctionCd</value>
          </list>
        </property>
      </bean>
    </property>
    <property name="lookupFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="financialHigherEdFunctionCd" />
        <bean parent="FieldDefinition"
              p:attributeName="financialHigherEdFunctionNm" />
        <bean parent="FieldDefinition"
              p:attributeName="active" />
      </list>
    </property>
    <property name="resultFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="financialHigherEdFunctionCd" />
        <bean parent="FieldDefinition"
              p:attributeName="financialHigherEdFunctionNm" />
        <bean parent="FieldDefinition"
              p:attributeName="finUnivBdgtOfficeFunctionCd" />
        <bean parent="FieldDefinition"
              p:attributeName="finAicpaFunctionCode" />
        <bean parent="FieldDefinition"
              p:attributeName="financialFederalFunctionCode" />
        <bean parent="FieldDefinition"
              p:attributeName="active" />
      </list>
    </property>
  </bean>
</beans>
