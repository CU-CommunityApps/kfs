<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="AccountingPeriod" parent="AccountingPeriod-parentBean" />

  <bean id="AccountingPeriod-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.module.chart.bo.AccountingPeriod" />
    <property name="inquiryDefinition" >
      <ref bean="AccountingPeriod-inquiryDefinition" />
    </property>
    <property name="lookupDefinition" >
      <ref bean="AccountingPeriod-lookupDefinition" />
    </property>
    <property name="objectLabel" value="Accounting Period" />
    <property name="attributes" >
      <list>
        <ref bean="AccountingPeriod-budgetRolloverIndicator" />
        <ref bean="AccountingPeriod-universityFiscalPeriodName" />
        <ref bean="AccountingPeriod-universityFiscalPeriodStatusCode" />
        <ref bean="AccountingPeriod-universityFiscalPeriodCode" />
        <ref bean="AccountingPeriod-universityFiscalPeriodEndDate" />
        <ref bean="AccountingPeriod-universityFiscalYear" />
        <ref bean="AccountingPeriod-versionNumber" />
        <ref bean="AccountingPeriod-objectId" />
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->


  <bean id="AccountingPeriod-budgetRolloverIndicator" parent="AccountingPeriod-budgetRolloverIndicator-parentBean" />

  <bean id="AccountingPeriod-budgetRolloverIndicator-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="budgetRolloverIndicator" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Budget Rollover" />
    <property name="shortLabel" value="Bdgt. Rollover" />
    <property name="maxLength" value="0" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="required" value="true" />
    <property name="control" >
      <bean parent="CheckboxControlDefinition" />
    </property>
  </bean>
  <bean id="AccountingPeriod-universityFiscalPeriodName" parent="AccountingPeriod-universityFiscalPeriodName-parentBean" />

  <bean id="AccountingPeriod-universityFiscalPeriodName-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="universityFiscalPeriodName" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="University Fiscal Period Name" />
    <property name="shortLabel" value="Name" />
    <property name="maxLength" value="10" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern"
            p:allowWhitespace="true" />
    </property>
    <property name="required" value="true" />
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="10" />
    </property>
  </bean>
  <bean id="AccountingPeriod-universityFiscalPeriodStatusCode" parent="AccountingPeriod-universityFiscalPeriodStatusCode-parentBean" />

  <bean id="AccountingPeriod-universityFiscalPeriodStatusCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="universityFiscalPeriodStatusCode" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="University Fiscal Period Status Code" />
    <property name="shortLabel" value="Status" />
    <property name="maxLength" value="1" />
    <property name="validationPattern" >
      <bean parent="CharsetValidationPattern"
            p:validChars="CO" />
    </property>
    <property name="required" value="true" />
    <property name="control" >
      <bean parent="SelectControlDefinition"
            p:valuesFinderClass="org.kuali.core.lookup.keyvalues.OpenCloseValuesFinder"
            p:includeKeyInLabel="false" />
    </property>
  </bean>
  <bean id="AccountingPeriod-universityFiscalPeriodCode" parent="AccountingPeriod-universityFiscalPeriodCode-parentBean" />

  <bean id="AccountingPeriod-universityFiscalPeriodCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="universityFiscalPeriodCode" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="University Fiscal Period Code" />
    <property name="shortLabel" value="Fiscal Period Code" />
    <property name="maxLength" value="2" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern"
            p:exactLength="2" />
    </property>
    <property name="required" value="true" />
    <property name="control" >
      <bean parent="SelectControlDefinition"
            p:valuesFinderClass="org.kuali.kfs.lookup.keyvalues.FiscalPeriodValuesFinder"
            p:includeKeyInLabel="false" />
    </property>
  </bean>
  <bean id="AccountingPeriod-universityFiscalPeriodEndDate" parent="AccountingPeriod-universityFiscalPeriodEndDate-parentBean" />

  <bean id="AccountingPeriod-universityFiscalPeriodEndDate-parentBean" abstract="true" parent="AttributeReferenceDummy-genericDate">
    <property name="name" value="universityFiscalPeriodEndDate" />
    <property name="label" value="University Fiscal Period End Date" />
    <property name="shortLabel" value="End Date" />
    <property name="required" value="true" />
  </bean>
  <bean id="AccountingPeriod-universityFiscalYear" parent="AccountingPeriod-universityFiscalYear-parentBean" />

  <bean id="AccountingPeriod-universityFiscalYear-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="universityFiscalYear" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="University Fiscal Year" />
    <property name="shortLabel" value="Fiscal Year" />
    <property name="maxLength" value="4" />
    <property name="validationPattern" >
      <bean parent="YearValidationPattern" />
    </property>
    <property name="required" value="true" />
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="4" />
    </property>
  </bean>
  <bean id="AccountingPeriod-versionNumber" parent="AccountingPeriod-versionNumber-parentBean" />

  <bean id="AccountingPeriod-versionNumber-parentBean" abstract="true" parent="AttributeReferenceDummy-versionNumber">
  </bean>
  <bean id="AccountingPeriod-objectId" parent="AccountingPeriod-objectId-parentBean" />

  <bean id="AccountingPeriod-objectId-parentBean" abstract="true" parent="AttributeReferenceDummy-objectId">
  </bean>

<!-- Business Object Inquiry Definition -->


  <bean id="AccountingPeriod-inquiryDefinition" parent="AccountingPeriod-inquiryDefinition-parentBean" />

  <bean id="AccountingPeriod-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
    <property name="title" value="Accounting Period Inquiry" />
    <property name="inquirySections" >
      <list>
        <bean parent="InquirySectionDefinition">
          <property name="title" value="Header Text" />
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields" >
            <list>
              <bean parent="FieldDefinition"
                    p:attributeName="universityFiscalYear" />
              <bean parent="FieldDefinition"
                    p:attributeName="universityFiscalPeriodCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="universityFiscalPeriodName" />
              <bean parent="FieldDefinition"
                    p:attributeName="universityFiscalPeriodStatusCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="universityFiscalPeriodEndDate" />
              <bean parent="FieldDefinition"
                    p:attributeName="budgetRolloverIndicator" />
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Business Object Lookup Definition -->


  <bean id="AccountingPeriod-lookupDefinition" parent="AccountingPeriod-lookupDefinition-parentBean" />

  <bean id="AccountingPeriod-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
    <property name="title" value="Accounting Period Lookup" />
    <property name="instructions" value="Lookup an Accounting Period" />
    <property name="defaultSort" >
      <bean parent="SortDefinition">
        <property name="sortAscending" value="false" />
      </bean>
    </property>
    <property name="lookupFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="universityFiscalYear" />
        <bean parent="FieldDefinition"
              p:attributeName="universityFiscalPeriodCode" />
        <bean parent="FieldDefinition"
              p:attributeName="universityFiscalPeriodName" />
        <bean parent="FieldDefinition"
              p:attributeName="universityFiscalPeriodEndDate" />
        <bean parent="FieldDefinition"
              p:attributeName="universityFiscalPeriodStatusCode" />
        <bean parent="FieldDefinition"
              p:attributeName="budgetRolloverIndicator" />
      </list>
    </property>
    <property name="resultFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="universityFiscalYear" />
        <bean parent="FieldDefinition"
              p:attributeName="universityFiscalPeriodCode" />
        <bean parent="FieldDefinition"
              p:attributeName="universityFiscalPeriodName" />
        <bean parent="FieldDefinition"
              p:attributeName="universityFiscalPeriodEndDate" />
        <bean parent="FieldDefinition"
              p:attributeName="universityFiscalPeriodStatusCode" />
        <bean parent="FieldDefinition"
              p:attributeName="budgetRolloverIndicator" />
      </list>
    </property>
  </bean>
</beans>
