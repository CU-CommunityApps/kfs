<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "../../../core/datadictionary/dataDictionary-1_0.dtd">
<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.module.chart.bo.FederalFunction">
        <customAttributeEnabled>false</customAttributeEnabled>

        <inquiry>
            <title>Federal Function Inquiry</title>

            <inquiryFields>
                <field attributeName="financialFederalFunctionCode" />
                <field attributeName="financialFederalFunctionName" />
            </inquiryFields>
        </inquiry>

        <lookup>
            <title>Federal Function Lookup</title>

            <menubar>
                <![CDATA[<a href="maintenance.do?methodToCall=start&businessObjectClassName=org.kuali.module.chart.bo.FederalFunction"><img src="images/tinybutton-createnew.gif" alt="create new" width="70" height="15"/></a>]]>
            </menubar>

            <instructions>Lookup a Federal Function</instructions>

            <defaultSort attributeName="financialFederalFunctionCode" sortAscending="true" />

            <lookupFields>
                <lookupField attributeName="financialFederalFunctionCode" />
                <lookupField attributeName="financialFederalFunctionName" />
            </lookupFields>

            <resultFields>
                <field attributeName="financialFederalFunctionCode" forceInquiry="true" />
                <field attributeName="financialFederalFunctionName" />
            </resultFields>
        </lookup>

       <titleAttribute>financialFederalFunctionCode</titleAttribute>

        <attributes>

            <attribute name="financialFederalFunctionCode" forceUppercase="true">
                <label>Federal Function Code</label>
                <shortLabel>Fed Function Code</shortLabel>

                <maxLength>3</maxLength>
                <validationPattern>
                    <anyCharacter />
                </validationPattern>
                <required>true</required>

                <control>
                    <text size="3" />
                </control>

                <summary>Federal Function Code</summary>
                <description>The code uniquely identifying a particular Federal Function.</description>
            </attribute>

            <attribute name="financialFederalFunctionName" forceUppercase="false">
                <label>Federal Function Name</label>
                <shortLabel>Name</shortLabel>

                <maxLength>40</maxLength>
                <required>true</required>

                <control>
                    <text size="40" />
                </control>

                <summary>Federal Function Name</summary>
                <description>Federal Function Name...</description>
            </attribute>


            <attributeReference name="versionNumber" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="versionNumber" />

        </attributes>
    </businessObject>
</dictionaryEntry>

