<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="AccountDelegateModel" parent="AccountDelegateModel-parentBean" />

  <bean id="AccountDelegateModel-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.kfs.coa.businessobject.AccountDelegateModel" />
    <property name="inquiryDefinition" >
      <ref bean="AccountDelegateModel-inquiryDefinition" />
    </property>
    <property name="lookupDefinition" >
      <ref bean="AccountDelegateModel-lookupDefinition" />
    </property>
    <property name="titleAttribute" value="accountDelegateModel" />
    <property name="objectLabel" value="Account Delegate Model" />
    <property name="attributes" >
      <list>
        <ref bean="AccountDelegateModel-chartOfAccountsCode" />
        <ref bean="AccountDelegateModel-organizationCode" />
        <ref bean="AccountDelegateModel-organizationRoutingModelName" />
        <ref bean="AccountDelegateModel-active" />
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->


  <bean id="AccountDelegateModel-chartOfAccountsCode" parent="AccountDelegateModel-chartOfAccountsCode-parentBean" />

  <bean id="AccountDelegateModel-chartOfAccountsCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="chartOfAccountsCode" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Chart Of Accounts Code" />
    <property name="shortLabel" value="Code" />
    <property name="maxLength" value="2" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="4" />
    </property>
  </bean>
  <bean id="AccountDelegateModel-organizationCode" parent="AccountDelegateModel-organizationCode-parentBean" />

  <bean id="AccountDelegateModel-organizationCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="organizationCode" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Organization Code" />
    <property name="shortLabel" value="Code" />
    <property name="maxLength" value="4" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="6" />
    </property>
  </bean>
  <bean id="AccountDelegateModel-organizationRoutingModelName" parent="AccountDelegateModel-organizationRoutingModelName-parentBean" />

  <bean id="AccountDelegateModel-organizationRoutingModelName-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="organizationRoutingModelName" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Account Delegate Model Name" />
    <property name="shortLabel" value="Name" />
    <property name="maxLength" value="10" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern"
            p:allowWhitespace="true" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="12" />
    </property>
  </bean>
  <bean id="AccountDelegateModel-active" parent="AccountDelegateModel-active-parentBean" />

  <bean id="AccountDelegateModel-active-parentBean" abstract="true" parent="GenericAttributes-activeIndicator">
    <property name="name" value="active" />
  </bean>


<!-- Business Object Inquiry Definition -->


  <bean id="AccountDelegateModel-inquiryDefinition" parent="AccountDelegateModel-inquiryDefinition-parentBean" />

  <bean id="AccountDelegateModel-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
    <property name="title" value="Account Delegate Model Inquiry" />
    <property name="inquirySections" >
      <list>
        <bean parent="InquirySectionDefinition">
          <property name="title" value="Account Delegate Model Name" />
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields" >
            <list>
              <bean parent="FieldDefinition"
                    p:attributeName="chartOfAccountsCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="organizationCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="organizationRoutingModelName" />
              <bean parent="FieldDefinition"
                    p:attributeName="active" />
            </list>
          </property>
        </bean>
        <bean parent="InquirySectionDefinition">
          <property name="title" value="Account Delegate Model Delegates" />
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields" >
            <list>
              <bean parent="InquiryCollectionDefinition">
                <property name="attributeName" value="organizationRoutingModel" />
                <property name="businessObjectClass" value="org.kuali.kfs.coa.businessobject.AccountDelegateModelDetail" />
                <property name="numberOfColumns" value="1" />
                <property name="inquiryFields" >
                  <list>
                    <bean parent="FieldDefinition"
                          p:attributeName="financialDocumentTypeCode" />
                    <bean parent="FieldDefinition"
                          p:attributeName="accountDelegatePrimaryRoutingIndicator" />
                    <bean parent="FieldDefinition"
                          p:attributeName="approvalFromThisAmount" />
                    <bean parent="FieldDefinition"
                          p:attributeName="approvalToThisAmount" />
                    <bean parent="FieldDefinition"
                          p:attributeName="accountDelegate.name" />
                  </list>
                </property>
              </bean>
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Business Object Lookup Definition -->


  <bean id="AccountDelegateModel-lookupDefinition" parent="AccountDelegateModel-lookupDefinition-parentBean" />

  <bean id="AccountDelegateModel-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
    <property name="title" value="Account Delegate Model Lookup" />
    <property name="lookupableID" value="organizationRoutingModelLookupable" />
    <property name="instructions" value="Look up an Account Delegate Model Name" />
    <property name="defaultSort" >
      <bean parent="SortDefinition">
      </bean>
    </property>
    <property name="lookupFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="chartOfAccountsCode" />
        <bean parent="FieldDefinition"
              p:attributeName="organizationCode" />
        <bean parent="FieldDefinition"
              p:attributeName="organizationRoutingModelName" />
        <bean parent="FieldDefinition"
              p:attributeName="active" />
      </list>
    </property>
    <property name="resultFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="chartOfAccountsCode" />
        <bean parent="FieldDefinition"
              p:attributeName="organizationCode" />
        <bean parent="FieldDefinition"
              p:attributeName="organizationRoutingModelName"
              p:forceInquiry="true" />
        <bean parent="FieldDefinition"
              p:attributeName="active" />
      </list>
    </property>
  </bean>
</beans>

