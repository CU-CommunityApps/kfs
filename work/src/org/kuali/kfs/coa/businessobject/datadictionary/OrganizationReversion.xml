                <required>true</required>
            </attributeReference>

            <attributeReference name="budgetReversionAccountNumber" sourceClassName="org.kuali.module.chart.bo.Account" sourceAttributeName="accountNumber">
                <label>Budget Reversion Account</label>
                <shortLabel>Budget Reversion Account</shortLabel>
                <required>true</required>
                <description>Budget Reversion Account</description>
            </attributeReference>

            <attributeReference name="cashReversionFinancialChartOfAccountsCode" sourceClassName="org.kuali.module.chart.bo.Chart" sourceAttributeName="chartOfAccountsCode">
