            <inquirySections><inquirySection title="Header Text"><inquiryFields>
                <field attributeName="universityFiscalYear" />
                <field attributeName="chartOfAccountsCode" />
                <field attributeName="organizationCode" />
                <field attributeName="budgetReversionChartOfAccountsCode" />
                <field attributeName="budgetReversionAccountNumber" />
                <field attributeName="cashReversionFinancialChartOfAccountsCode" />
                <field attributeName="cashReversionAccountNumber" />
                <field attributeName="carryForwardByObjectCodeIndicator" />



            </inquiryFields></inquirySection></inquirySections>
        </inquiry>

        <lookup>
