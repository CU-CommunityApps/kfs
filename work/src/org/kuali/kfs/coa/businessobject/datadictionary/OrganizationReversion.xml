            <instructions>Lookup an Organization Reversion</instructions>

            <defaultSort attributeName="organizationCode" sortAscending="true" />

            <lookupFields>
                <lookupField attributeName="universityFiscalYear" required="false" />
                <lookupField attributeName="chartOfAccountsCode" required="false" />
                <lookupField attributeName="organizationCode" required="false" />
                <lookupField attributeName="budgetReversionChartOfAccountsCode" required="false" />
                <lookupField attributeName="budgetReversionAccountNumber" required="false" />
                <lookupField attributeName="cashReversionFinancialChartOfAccountsCode" required="false" />
                <lookupField attributeName="cashReversionAccountNumber" required="false" />


