<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="ObjectCodeCurrent" parent="ObjectCodeCurrent-parentBean" />

  <bean id="ObjectCodeCurrent-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.module.chart.bo.ObjectCodeCurrent" />
    <property name="inquiryDefinition" >
      <ref bean="ObjectCodeCurrent-inquiryDefinition" />
    </property>
    <property name="lookupDefinition" >
      <ref bean="ObjectCodeCurrent-lookupDefinition" />
    </property>
    <property name="titleAttribute" value="financialObjectCode" />
    <property name="objectLabel" value="Object Code Current" />
    <property name="attributes" >
      <list>
        <ref bean="ObjectCodeCurrent-universityFiscalYear" />
        <ref bean="ObjectCodeCurrent-chartOfAccountsCode" />
        <ref bean="ObjectCodeCurrent-financialObjectCode" />
        <ref bean="ObjectCodeCurrent-financialObjectCodeName" />
        <ref bean="ObjectCodeCurrent-financialObjectCodeShortName" />
        <ref bean="ObjectCodeCurrent-financialObjectLevelCode" />
        <ref bean="ObjectCodeCurrent-reportsToChartOfAccountsCode" />
        <ref bean="ObjectCodeCurrent-reportsToFinancialObjectCode" />
        <ref bean="ObjectCodeCurrent-financialObjectTypeCode" />
        <ref bean="ObjectCodeCurrent-financialObjectSubTypeCode" />
        <ref bean="ObjectCodeCurrent-historicalFinancialObjectCode" />
        <ref bean="ObjectCodeCurrent-financialObjectActiveCode" />
        <ref bean="ObjectCodeCurrent-financialBudgetAggregationCd" />
        <ref bean="ObjectCodeCurrent-finObjMandatoryTrnfrelimCd" />
        <ref bean="ObjectCodeCurrent-financialFederalFundedCode" />
        <ref bean="ObjectCodeCurrent-nextYearFinancialObjectCode" />
        <ref bean="ObjectCodeCurrent-versionNumber" />
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->


  <bean id="ObjectCodeCurrent-universityFiscalYear" parent="ObjectCodeCurrent-universityFiscalYear-parentBean" />

  <bean id="ObjectCodeCurrent-universityFiscalYear-parentBean" abstract="true" parent="GenericAttributes-genericFiscalYear">
    <property name="name" value="universityFiscalYear" />
    <property name="label" value="Fiscal Year" />
    <property name="shortLabel" value="FY" />
    <property name="required" value="true" />
  </bean>
  <bean id="ObjectCodeCurrent-chartOfAccountsCode" parent="ObjectCodeCurrent-chartOfAccountsCode-parentBean" />

  <bean id="ObjectCodeCurrent-chartOfAccountsCode-parentBean" abstract="true" parent="Chart-chartOfAccountsCode">
    <property name="control" >
      <bean parent="SelectControlDefinition"
            p:valuesFinderClass="org.kuali.module.chart.lookup.keyvalues.ChartValuesFinder"
            p:includeKeyInLabel="false" />
    </property>
  </bean>
  <bean id="ObjectCodeCurrent-financialObjectCode" parent="ObjectCodeCurrent-financialObjectCode-parentBean" />

  <bean id="ObjectCodeCurrent-financialObjectCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="financialObjectCode" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Object Code" />
    <property name="shortLabel" value="Object" />
    <property name="maxLength" value="4" />
    <property name="validationPattern" >
      <bean parent="AlphaNumericValidationPattern" />
    </property>
    <property name="required" value="true" />
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="4" />
    </property>
  </bean>
  <bean id="ObjectCodeCurrent-financialObjectCodeName" parent="ObjectCodeCurrent-financialObjectCodeName-parentBean" />

  <bean id="ObjectCodeCurrent-financialObjectCodeName-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="financialObjectCodeName" />
    <property name="forceUppercase" value="false" />
    <property name="label" value="Object Code Name" />
    <property name="shortLabel" value="ObjCodeName" />
    <property name="maxLength" value="40" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern"
            p:allowWhitespace="true" />
    </property>
    <property name="required" value="true" />
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="40" />
    </property>
  </bean>
  <bean id="ObjectCodeCurrent-financialObjectCodeShortName" parent="ObjectCodeCurrent-financialObjectCodeShortName-parentBean" />

  <bean id="ObjectCodeCurrent-financialObjectCodeShortName-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="financialObjectCodeShortName" />
    <property name="forceUppercase" value="false" />
    <property name="label" value="Object Code Short Name" />
    <property name="shortLabel" value="ObjCodeShortName" />
    <property name="maxLength" value="12" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern"
            p:allowWhitespace="true" />
    </property>
    <property name="required" value="true" />
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="12" />
    </property>
  </bean>
  <bean id="ObjectCodeCurrent-financialObjectLevelCode" parent="ObjectCodeCurrent-financialObjectLevelCode-parentBean" />

  <bean id="ObjectCodeCurrent-financialObjectLevelCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="financialObjectLevelCode" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Level Code" />
    <property name="shortLabel" value="Level Code" />
    <property name="maxLength" value="4" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern"
            p:allowWhitespace="true" />
    </property>
    <property name="required" value="true" />
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="4" />
    </property>
  </bean>
  <bean id="ObjectCodeCurrent-reportsToChartOfAccountsCode" parent="ObjectCodeCurrent-reportsToChartOfAccountsCode-parentBean" />

  <bean id="ObjectCodeCurrent-reportsToChartOfAccountsCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="reportsToChartOfAccountsCode" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Reports to Chart Code" />
    <property name="shortLabel" value="RptsToChart" />
    <property name="maxLength" value="2" />
    <property name="validationPattern" >
      <bean parent="AlphaValidationPattern"
            p:exactLength="2" />
    </property>
    <property name="required" value="true" />
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="2" />
    </property>
  </bean>
  <bean id="ObjectCodeCurrent-reportsToFinancialObjectCode" parent="ObjectCodeCurrent-reportsToFinancialObjectCode-parentBean" />

  <bean id="ObjectCodeCurrent-reportsToFinancialObjectCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="reportsToFinancialObjectCode" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Reports To Object Code" />
    <property name="shortLabel" value="RptsToObjCode" />
    <property name="maxLength" value="4" />
    <property name="validationPattern" >
      <bean parent="AlphaNumericValidationPattern" />
    </property>
    <property name="required" value="true" />
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="4" />
    </property>
  </bean>
  <bean id="ObjectCodeCurrent-financialObjectTypeCode" parent="ObjectCodeCurrent-financialObjectTypeCode-parentBean" />

  <bean id="ObjectCodeCurrent-financialObjectTypeCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="financialObjectTypeCode" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Object Type Code" />
    <property name="shortLabel" value="Object Type" />
    <property name="maxLength" value="2" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="required" value="true" />
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="2" />
    </property>
  </bean>
  <bean id="ObjectCodeCurrent-financialObjectSubTypeCode" parent="ObjectCodeCurrent-financialObjectSubTypeCode-parentBean" />

  <bean id="ObjectCodeCurrent-financialObjectSubTypeCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="financialObjectSubTypeCode" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Object Sub-Type Code" />
    <property name="shortLabel" value="Object Sub-Type Code" />
    <property name="maxLength" value="2" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="required" value="true" />
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="2" />
    </property>
  </bean>
  <bean id="ObjectCodeCurrent-historicalFinancialObjectCode" parent="ObjectCodeCurrent-historicalFinancialObjectCode-parentBean" />

  <bean id="ObjectCodeCurrent-historicalFinancialObjectCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="historicalFinancialObjectCode" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Historical Financial Object Code" />
    <property name="shortLabel" value="HistFinObjCde" />
    <property name="maxLength" value="4" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="required" value="true" />
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="4" />
    </property>
  </bean>
  <bean id="ObjectCodeCurrent-financialObjectActiveCode" parent="ObjectCodeCurrent-financialObjectActiveCode-parentBean" />

  <bean id="ObjectCodeCurrent-financialObjectActiveCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="financialObjectActiveCode" />
    <property name="forceUppercase" value="false" />
    <property name="label" value="Active Code" />
    <property name="shortLabel" value="Ac." />
    <property name="maxLength" value="1" />
    <property name="required" value="true" />
    <property name="control" >
      <bean parent="CheckboxControlDefinition" />
    </property>
    <property name="formatterClass" value="org.kuali.core.web.format.BooleanFormatter" />
  </bean>
  <bean id="ObjectCodeCurrent-financialBudgetAggregationCd" parent="ObjectCodeCurrent-financialBudgetAggregationCd-parentBean" />

  <bean id="ObjectCodeCurrent-financialBudgetAggregationCd-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="financialBudgetAggregationCd" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Budget Aggregation Code" />
    <property name="shortLabel" value="BudgetAggCde" />
    <property name="maxLength" value="1" />
    <property name="required" value="true" />
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="1" />
    </property>
  </bean>
  <bean id="ObjectCodeCurrent-finObjMandatoryTrnfrelimCd" parent="ObjectCodeCurrent-finObjMandatoryTrnfrelimCd-parentBean" />

  <bean id="ObjectCodeCurrent-finObjMandatoryTrnfrelimCd-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="finObjMandatoryTrnfrelimCd" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Mandatory Transfer Or Eliminations Code" />
    <property name="shortLabel" value="ManTranOrElimCode" />
    <property name="maxLength" value="1" />
    <property name="required" value="true" />
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="1" />
    </property>
  </bean>
  <bean id="ObjectCodeCurrent-financialFederalFundedCode" parent="ObjectCodeCurrent-financialFederalFundedCode-parentBean" />

  <bean id="ObjectCodeCurrent-financialFederalFundedCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="financialFederalFundedCode" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Federal Funded Code" />
    <property name="shortLabel" value="FedFundCd" />
    <property name="maxLength" value="1" />
    <property name="required" value="true" />
    <property name="control" >
      <bean parent="SelectControlDefinition"
            p:valuesFinderClass="org.kuali.module.chart.lookup.keyvalues.FederalFundedCodeValuesFinder"
            p:includeKeyInLabel="false" />
    </property>
  </bean>
  <bean id="ObjectCodeCurrent-nextYearFinancialObjectCode" parent="ObjectCodeCurrent-nextYearFinancialObjectCode-parentBean" />

  <bean id="ObjectCodeCurrent-nextYearFinancialObjectCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="nextYearFinancialObjectCode" />
    <property name="forceUppercase" value="true" />
    <property name="label" value="Next Year Object Code" />
    <property name="shortLabel" value="NextYearObjCode" />
    <property name="maxLength" value="4" />
    <property name="validationPattern" >
      <bean parent="AlphaNumericValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="4" />
    </property>
  </bean>
  <bean id="ObjectCodeCurrent-versionNumber" parent="ObjectCodeCurrent-versionNumber-parentBean" />

  <bean id="ObjectCodeCurrent-versionNumber-parentBean" abstract="true" parent="GenericAttributes-versionNumber">
  </bean>

<!-- Business Object Inquiry Definition -->


  <bean id="ObjectCodeCurrent-inquiryDefinition" parent="ObjectCodeCurrent-inquiryDefinition-parentBean" />

  <bean id="ObjectCodeCurrent-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
    <property name="title" value="Object Code Inquiry" />
    <property name="inquirySections" >
      <list>
        <bean parent="InquirySectionDefinition">
          <property name="title" value="Header Text" />
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields" >
            <list>
              <bean parent="FieldDefinition"
                    p:attributeName="universityFiscalYear" />
              <bean parent="FieldDefinition"
                    p:attributeName="chartOfAccountsCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="financialObjectCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="financialObjectCodeName" />
              <bean parent="FieldDefinition"
                    p:attributeName="financialObjectCodeShortName" />
              <bean parent="FieldDefinition"
                    p:attributeName="reportsToChartOfAccountsCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="reportsToFinancialObjectCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="financialObjectTypeCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="financialObjectLevelCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="financialObjectSubTypeCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="historicalFinancialObjectCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="financialObjectActiveCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="financialBudgetAggregationCd" />
              <bean parent="FieldDefinition"
                    p:attributeName="finObjMandatoryTrnfrelimCd" />
              <bean parent="FieldDefinition"
                    p:attributeName="financialFederalFundedCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="nextYearFinancialObjectCode" />
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Business Object Lookup Definition -->


  <bean id="ObjectCodeCurrent-lookupDefinition" parent="ObjectCodeCurrent-lookupDefinition-parentBean" />

  <bean id="ObjectCodeCurrent-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
    <property name="title" value="Object Code Lookup" />
    <property name="instructions" value="Lookup an Object Code" />
    <property name="defaultSort" >
      <bean parent="SortDefinition">
        <property name="attributeNames" >
          <list>
            <value>financialObjectCode</value>
          </list>
        </property>
      </bean>
    </property>
    <property name="lookupFields" >
      <list>
        <bean parent="FieldDefinition"
              p:required="true"
              p:attributeName="universityFiscalYear"
              p:defaultValueFinderClass="org.kuali.kfs.lookup.valuefinder.FiscalYearFinder" />
        <bean parent="FieldDefinition"
              p:attributeName="chartOfAccountsCode" />
        <bean parent="FieldDefinition"
              p:attributeName="financialObjectCode" />
        <bean parent="FieldDefinition"
              p:attributeName="financialObjectCodeName" />
        <bean parent="FieldDefinition"
              p:attributeName="financialObjectCodeShortName" />
        <bean parent="FieldDefinition"
              p:attributeName="financialObjectLevelCode" />
        <bean parent="FieldDefinition"
              p:attributeName="financialObjectTypeCode" />
        <bean parent="FieldDefinition"
              p:attributeName="financialObjectSubTypeCode" />
        <bean parent="FieldDefinition"
              p:attributeName="historicalFinancialObjectCode" />
        <bean parent="FieldDefinition"
              p:attributeName="financialBudgetAggregationCd" />
        <bean parent="FieldDefinition"
              p:defaultValue="Y"
              p:attributeName="financialObjectActiveCode" />
      </list>
    </property>
    <property name="resultFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="universityFiscalYear" />
        <bean parent="FieldDefinition"
              p:attributeName="chartOfAccountsCode" />
        <bean parent="FieldDefinition"
              p:attributeName="financialObjectCode" />
        <bean parent="FieldDefinition"
              p:attributeName="financialObjectCodeName" />
        <bean parent="FieldDefinition"
              p:attributeName="financialObjectCodeShortName" />
        <bean parent="FieldDefinition"
              p:attributeName="financialObjectLevelCode" />
        <bean parent="FieldDefinition"
              p:attributeName="financialObjectTypeCode" />
        <bean parent="FieldDefinition"
              p:attributeName="financialObjectSubTypeCode" />
        <bean parent="FieldDefinition"
              p:attributeName="historicalFinancialObjectCode" />
        <bean parent="FieldDefinition"
              p:attributeName="financialBudgetAggregationCd" />
        <bean parent="FieldDefinition"
              p:attributeName="financialObjectActiveCode" />
      </list>
    </property>
  </bean>
</beans>
