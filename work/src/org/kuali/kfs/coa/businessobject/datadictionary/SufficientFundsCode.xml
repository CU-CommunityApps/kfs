<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "../dataDictionary-1_0.dtd">
<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.module.chart.bo.codes.SufficientFundsCode">
 
        <inquiry>
            <title>Sufficient Funds Code Inquiry</title>
             
            <inquirySections>
                <inquirySection title="Sufficient Funds Code Details">
                    <inquiryFields>
                        <field attributeName="code" />
                        <field attributeName="name" />
                        <field attributeName="active" />
                    </inquiryFields>
                </inquirySection>
            </inquirySections>  
        </inquiry>

        <lookup>
            <title>Sufficient Funds Code Lookup</title>

            <instructions>Look up an Sufficient Funds Code</instructions>

            <defaultSort sortAscending="true">
                <sortAttributes>
                    <sortAttribute attributeName="code" />
                </sortAttributes>
            </defaultSort>

            <lookupFields>
                <lookupField attributeName="code" required="false" />
                <lookupField attributeName="name" required="false" />
                <lookupField attributeName="active" required="false" />
            </lookupFields>

            <resultFields>
                <field attributeName="code" />
                <field attributeName="name" />
                <field attributeName="active" />
            </resultFields>
        </lookup>

        <attributes>

            <attribute name="code" forceUppercase="true">
                <label>Account Sufficient Funds Code</label>
                <shortLabel>Code</shortLabel>

                <maxLength>1</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="3" />
                </control>
			</attribute>

            <attribute name="name" forceUppercase="false">
                <label>Account Sufficient Funds Name</label>
                <shortLabel>Name</shortLabel>

                <maxLength>40</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="42" />
                </control>
			</attribute>

            <attribute name="active" forceUppercase="true">
                <label>Active</label>
                <shortLabel>Active</shortLabel>

                <maxLength>1</maxLength>

                <control>
                    <checkbox />
                </control>
			</attribute>

            <attributeReference name="versionNumber" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="versionNumber" />

        </attributes>
    </businessObject>
</dictionaryEntry>
