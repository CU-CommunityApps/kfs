<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "../../../core/datadictionary/dataDictionary-1_0.dtd">
<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.module.chart.bo.codes.SufficientFundsCode">
        <customAttributeEnabled>false</customAttributeEnabled>

        <inquiry>
            <title>Sufficient Funds Code</title>

            <inquiryFields>
                <field attributeName="code" />
                <field attributeName="name" />
                <field attributeName="active" />
            </inquiryFields>
        </inquiry>

        <titleAttribute>code</titleAttribute>

        <attributes>
            <attribute name="code" forceUppercase="true">
                <label>Sufficient Funds Code</label>
                <shortLabel>Code</shortLabel>

                <maxLength>1</maxLength>
                <validationPattern>
                    <alpha exactLength="1" />
                </validationPattern>
                <required>true</required>

                <control>
                    <text size="1" />
                </control>

                <summary>Sufficient Funds Code</summary>
                <description>Sufficient Funds Code</description>
            </attribute>

            <attribute name="name" forceUppercase="false">
                <label>Sufficient Funds Name</label>
                <shortLabel>Name</shortLabel>

                <maxLength>50</maxLength>
                <validationPattern>
                    <anyCharacter allowWhitespace="true" />
                </validationPattern>
                <required>true</required>

                <control>
                    <text size="50" />
                </control>

                <summary>Sufficient Funds Name</summary>
                <description>Sufficient Funds Name</description>
            </attribute>

            <attributeReference name="active" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="activeIndicator"></attributeReference>

        </attributes>
    </businessObject>
</dictionaryEntry>

