<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE descriptor-repository SYSTEM "repository.dtd" [
<!ENTITY core SYSTEM "OJB-repository-core.xml">
<!ENTITY ar SYSTEM "org/kuali/module/ar/OJB-repository-ar.xml">
<!ENTITY cams SYSTEM "org/kuali/module/cams/OJB-repository-cams.xml">
<!ENTITY cg SYSTEM "org/kuali/module/cg/OJB-repository-cg.xml">
<!ENTITY chart SYSTEM "org/kuali/module/chart/OJB-repository-chart.xml">
<!ENTITY financial SYSTEM "org/kuali/module/financial/OJB-repository-financial.xml">
<!ENTITY gl SYSTEM "org/kuali/module/gl/OJB-repository-gl.xml">
<!ENTITY kra SYSTEM "org/kuali/module/kra/OJB-repository-kra.xml">
<!ENTITY labor SYSTEM "org/kuali/module/labor/OJB-repository-labor.xml">
<!ENTITY cache SYSTEM "OJB-repository-cache.xml">
<!ENTITY codes SYSTEM "org/kuali/module/chart/OJB-repository-codes.xml">
]>
<descriptor-repository version="1.0">
    &core;
    &ar;
    &cams;
    &cg;
    &chart;
    &financial;
    &gl;
    &kra;
    &labor;
    &codes;
    <jdbc-connection-descriptor jcd-alias="dataSource" default-connection="true" platform="Oracle9i" jdbc-level="3.0" useAutoCommit="1">
        <sequence-manager className="org.apache.ojb.broker.util.sequence.SequenceManagerNextValImpl" />
        &cache;
    </jdbc-connection-descriptor>
