<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">
<beans>
	<bean id="riceConfigurer" class="org.kuali.kfs.PropertyLoader">
		<property name="propertyFiles">
			<list>
				<value>classpath:configuration.properties</value>
				<value>file:/opt/sa_forms/java/dev/kuali/datasource.properties</value>				
				<value>file:/opt/sa_forms/java/dev/kuali/encryption.properties</value>				
				<value>classpath:test-configuration.properties</value>
			</list>
		</property>
	</bean>
	
	<bean id="mailService" class="org.kuali.test.NonSendingMailService">
		<property name="mailSender">
			<ref bean="mailSender" />
		</property>
		<property name="batchMailingList" value="${mailing.list.batch}" />
	</bean>

	<bean id="unitTestSqlDao" parent="platformAwareDao" class="org.kuali.core.util.UnitTestSqlDaoOjb" />
</beans>