<xsd:schema elementFormDefault="qualified" xmlns:xsd="http://www.w3.org/2001/XMLSchema">

    <xsd:element name="importBy" type="importBy" />    	

    <!-- Agency Information -->
    <xsd:element name="agencyDataId" type="xsd:integer" />
    <xsd:element name="creditCardOrAgencyCode" type="creditCardOrAgencyCode" />
    <xsd:element name="agency" type="agency" />    
    <xsd:element name="otherCompanyName" type="otherCompanyName" />
    <xsd:element name="agencyFileName" type="agencyFileName" />
    <xsd:element name="merchantName" type="merchantName" />
    <xsd:element name="billingCycleDate" type="xsd:date" />
    
    <!-- Trip Information -->
    
    <xsd:element name="tripId" type="tripId" />
    <xsd:element name="tripInvoiceNumber" type="tripInvoiceNumber" />
    <xsd:element name="tripTravelerTypeId" type="tripTravelerTypeId" />
    <xsd:element name="otherAmount" type="xsd:decimal" />
    
    <!-- Traveler Information -->
    
    <xsd:element name="travelerName" type="travelerName" />
    <xsd:element name="travelerId" type="travelerId" />
    <xsd:element name="travelerNetworkId" type="travelerNetworkId" />
    <xsd:element name="tripExpenseAmount" type="xsd:decimal" />
    <xsd:element name="alternateTripId" type="alternateTripId" />
    <xsd:element name="tripArrangerName" type="tripArrangerName" />
    
    <!-- Accounting Information -->
    <xsd:element name="accountingInfo" type="accountingInfo" />   
    <xsd:element name="groupObjectCode" type="groupObjectCode" />
    <xsd:element name="distributionCode" type="distributionCode" />
    
    <!-- Airline Information -->
    
    <xsd:element name="tripDepartureDate" type="xsd:date" />
    <xsd:element name="tripReturnDate" type="xsd:date" />
    <xsd:element name="fareSaverCode" type="fareSaverCode" />
    <xsd:element name="airBookDate" type="xsd:date" />
    <xsd:element name="airCarrierCode" type="airCarrierCode" />
    <xsd:element name="airTicketNumber" type="airTicketNumber" />
    <xsd:element name="pnrNumber" type="pnrNumber" />
    <xsd:element name="airTicketClass" type="airTicketClass" />
    <xsd:element name="airTransactionAmount" type="xsd:decimal" />
    <xsd:element name="airBaseFareAmount" type="xsd:decimal" />
    <xsd:element name="airTaxAmount" type="xsd:decimal" />
    <xsd:element name="airLowFareAmount" type="xsd:decimal" />
    <xsd:element name="airReasonCode" type="airReasonCode" />
    <xsd:element name="airSegmentId" type="airSegmentId" />
    <xsd:element name="airDestinationCode" type="airDestinationCode" />
    <xsd:element name="airServiceFeeNumber" type="airServiceFeeNumber" />
    <xsd:element name="airServiceFeeAmount" type="xsd:decimal" />
    <xsd:element name="transactionUniqueId" type="transactionUniqueId" />
    
    <!-- Lodging information -->
    
    <xsd:element name="lodgingItineraryNumber" type="lodgingItineraryNumber" />
    <xsd:element name="lodgingPrepayDate" type="xsd:date" />
    <xsd:element name="lodgingAmount" type="xsd:decimal" />
    <xsd:element name="lodgingPrepayDaysNumber" type="lodgingPrepayDaysNumber" />
    <xsd:element name="lodgingPropName" type="lodgingPropName" />
    <xsd:element name="tripLodgingArrivalDate" type="xsd:date" />
    <xsd:element name="lodgingDepartureDate" type="xsd:date" />
    <xsd:element name="lodgingBookingDate" type="xsd:date" />
    <xsd:element name="lodgingPropertyCityName" type="lodgingPropertyCityName" />  
    <xsd:element name="lodgingPropertyStateCode" type="lodgingPropertyStateCode" />
    <xsd:element name="lodgingCountryName" type="lodgingCountryName" />
    
    <!-- Rental Car Information -->
    
    <xsd:element name="rentalCarItineraryNumber" type="rentalCarItineraryNumber" />
    <xsd:element name="rentalCarAmount" type="xsd:decimal" />
    <xsd:element name="rentalCarNumberOfDays" type="rentalCarNumberOfDays" />
    <xsd:element name="rentalCarCompanyName" type="rentalCarCompanyName" />
    <xsd:element name="rentalCarOpenDate" type="xsd:date" />
    <xsd:element name="rentalCarCloseDate" type="xsd:date" />
    <xsd:element name="rentalCarFuelAmount" type="xsd:decimal" />
    <xsd:element name="rentalCarAdditionalAmount" type="xsd:decimal" />
    <xsd:element name="rentalCarTaxAmount" type="xsd:decimal" />
    <xsd:element name="rentalCarSurchargeAmount" type="xsd:decimal" />
    <xsd:element name="rentalCarGovernmentSurchargeAmount" type="xsd:decimal" />
    <xsd:element name="rentalCarBillingAmount" type="xsd:decimal" />
    <xsd:element name="rentalCarDetailText" type="rentalCarDetailText" />
    
    <!-- Conference Registration -->
    <xsd:element name="registrationCompanyName" type="registrationCompanyName" />
    <xsd:element name="registrationAmount" type="xsd:decimal" />
    
    <!-- Processing Information -->
    <xsd:element name="transactionPostingDate" type="xsd:date" />
    <xsd:element name="objectVersionNumber" type="xsd:integer" />
    <xsd:element name="creationTimestamp" type="xsd:dateTime" /> 
    <xsd:element name="processingTimestamp" type="xsd:dateTime" /> 
    <xsd:element name="moveToHistoryIndicator" type="moveToHistoryIndicator" />    
    
    <xsd:simpleType name="importBy">
        <xsd:restriction base="xsd:normalizedString">
            <xsd:maxLength value="3" />
            <xsd:whiteSpace value="replace"/>
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:simpleType name="creditCardOrAgencyCode">
        <xsd:restriction base="xsd:normalizedString">
            <xsd:maxLength value="4"/>
            <xsd:whiteSpace value="replace"/>
        </xsd:restriction>
    </xsd:simpleType>
    
    <xsd:simpleType name="agency">
        <xsd:restriction base="xsd:normalizedString">
            <xsd:maxLength value="20"/>
            <xsd:whiteSpace value="replace"/>
        </xsd:restriction>
    </xsd:simpleType>
    
    
    <xsd:simpleType name="otherCompanyName">
        <xsd:restriction base="xsd:normalizedString">
            <xsd:maxLength value="30"/>
            <xsd:whiteSpace value="replace"/>
        </xsd:restriction>
    </xsd:simpleType>
    
    <xsd:simpleType name="agencyFileName">
        <xsd:restriction base="xsd:normalizedString">
            <xsd:maxLength value="50"/>
            <xsd:whiteSpace value="replace"/>
        </xsd:restriction>
    </xsd:simpleType>
    
    <xsd:simpleType name="merchantName">
        <xsd:restriction base="xsd:normalizedString">
            <xsd:maxLength value="40"/>
            <xsd:whiteSpace value="replace"/>
        </xsd:restriction>
    </xsd:simpleType>
    
    <xsd:simpleType name="tripId">
        <xsd:restriction base="xsd:normalizedString">
            <xsd:maxLength value="12"/>
            <xsd:whiteSpace value="replace"/>
        </xsd:restriction>
    </xsd:simpleType>
    
    <xsd:simpleType name="tripInvoiceNumber">
        <xsd:restriction base="xsd:normalizedString">
            <xsd:maxLength value="20"/>
            <xsd:whiteSpace value="replace"/>
        </xsd:restriction>
    </xsd:simpleType>
    
    <xsd:simpleType name="tripTravelerTypeId">
        <xsd:restriction base="xsd:normalizedString">
            <xsd:maxLength value="1"/>
            <xsd:whiteSpace value="replace"/>
        </xsd:restriction>
    </xsd:simpleType>
    
    <xsd:simpleType name="travelerName">
        <xsd:restriction base="xsd:normalizedString">
            <xsd:maxLength value="50"/>
            <xsd:whiteSpace value="replace"/>
        </xsd:restriction>
    </xsd:simpleType>
    
    <xsd:simpleType name="travelerId">
        <xsd:restriction base="xsd:normalizedString">
            <xsd:maxLength value="40"/>
            <xsd:whiteSpace value="replace"/>
        </xsd:restriction>
    </xsd:simpleType>
    
    <xsd:simpleType name="travelerNetworkId">
        <xsd:restriction base="xsd:normalizedString">
            <xsd:maxLength value="9"/>
            <xsd:whiteSpace value="replace"/>
        </xsd:restriction>
    </xsd:simpleType>
    
    <xsd:simpleType name="alternateTripId">
        <xsd:restriction base="xsd:normalizedString">
            <xsd:maxLength value="20"/>
            <xsd:whiteSpace value="replace"/>
        </xsd:restriction>
    </xsd:simpleType>
    
    <xsd:simpleType name="tripArrangerName">
        <xsd:restriction base="xsd:normalizedString">
            <xsd:maxLength value="50"/>
            <xsd:whiteSpace value="replace"/>
        </xsd:restriction>
    </xsd:simpleType>
    
    <xsd:simpleType name="tripChartCode">
        <xsd:restriction base="xsd:normalizedString">
            <xsd:maxLength value="2"/>
            <xsd:whiteSpace value="replace"/>
        </xsd:restriction>
    </xsd:simpleType>
    
    <xsd:simpleType name="tripAccountNumber">
        <xsd:restriction base="xsd:normalizedString">
            <xsd:maxLength value="7"/>
            <xsd:whiteSpace value="replace"/>
        </xsd:restriction>
    </xsd:simpleType>
    
    <xsd:simpleType name="tripSubAccountNumber">
        <xsd:restriction base="xsd:normalizedString">
            <xsd:maxLength value="5"/>
            <xsd:whiteSpace value="replace"/>
        </xsd:restriction>
    </xsd:simpleType>
    
    <xsd:simpleType name="objectCode">
        <xsd:restriction base="xsd:normalizedString">
            <xsd:maxLength value="4"/>
            <xsd:whiteSpace value="replace"/>
        </xsd:restriction>
    </xsd:simpleType>
    
    <xsd:simpleType name="subObjectCode">
        <xsd:restriction base="xsd:normalizedString">
            <xsd:maxLength value="3"/>
            <xsd:whiteSpace value="replace"/>
        </xsd:restriction>
    </xsd:simpleType>
    
    <xsd:simpleType name="projectCode">
        <xsd:restriction base="xsd:normalizedString">
            <xsd:maxLength value="10"/>
            <xsd:whiteSpace value="replace"/>
        </xsd:restriction>
    </xsd:simpleType>
    
    <xsd:simpleType name="organizationReference">
        <xsd:restriction base="xsd:normalizedString">
            <xsd:maxLength value="8"/>
            <xsd:whiteSpace value="replace"/>
        </xsd:restriction>
    </xsd:simpleType>

	<xsd:complexType name="accountingInfo">
		<xsd:sequence>
		    <xsd:element name="tripChartCode" type="tripChartCode" minOccurs="0" maxOccurs="1" />
		    <xsd:element name="tripAccountNumber" type="tripAccountNumber" minOccurs="0" maxOccurs="1" />
		    <xsd:element name="tripSubAccountNumber" type="tripSubAccountNumber" minOccurs="0" maxOccurs="1" />
		    <xsd:element name="objectCode" type="objectCode" minOccurs="0" maxOccurs="1" />
		    <xsd:element name="subObjectCode" type="subObjectCode" minOccurs="0" maxOccurs="1" />
		    <xsd:element name="projectCode" type="projectCode" minOccurs="0" maxOccurs="1" />
		    <xsd:element name="organizationReference" type="organizationReference" minOccurs="0" maxOccurs="1" />
		</xsd:sequence>
	</xsd:complexType>
        
    <xsd:simpleType name="groupObjectCode">
        <xsd:restriction base="xsd:normalizedString">
            <xsd:maxLength value="20"/>
            <xsd:whiteSpace value="replace"/>
        </xsd:restriction>
    </xsd:simpleType>
    
    <xsd:simpleType name="distributionCode">
        <xsd:restriction base="xsd:normalizedString">
            <xsd:maxLength value="4"/>
            <xsd:whiteSpace value="replace"/>
        </xsd:restriction>
    </xsd:simpleType>
    
    <xsd:simpleType name="fareSaverCode">
        <xsd:restriction base="xsd:normalizedString">
            <xsd:maxLength value="1"/>
            <xsd:whiteSpace value="replace"/>
        </xsd:restriction>
    </xsd:simpleType>
    
    <xsd:simpleType name="airCarrierCode">
        <xsd:restriction base="xsd:normalizedString">
            <xsd:maxLength value="3"/>
            <xsd:whiteSpace value="replace"/>
        </xsd:restriction>
    </xsd:simpleType>
    
    <xsd:simpleType name="airTicketNumber">
        <xsd:restriction base="xsd:normalizedString">
            <xsd:maxLength value="20"/>
            <xsd:whiteSpace value="replace"/>
        </xsd:restriction>
    </xsd:simpleType>
    
    <xsd:simpleType name="pnrNumber">
        <xsd:restriction base="xsd:normalizedString">
            <xsd:maxLength value="20"/>
            <xsd:whiteSpace value="replace"/>
        </xsd:restriction>
    </xsd:simpleType>
    
    <xsd:simpleType name="airTicketClass">
        <xsd:restriction base="xsd:normalizedString">
            <xsd:maxLength value="20"/>
            <xsd:whiteSpace value="replace"/>
        </xsd:restriction>
    </xsd:simpleType>
    
    <xsd:simpleType name="airReasonCode">
        <xsd:restriction base="xsd:normalizedString">
            <xsd:maxLength value="1"/>
            <xsd:whiteSpace value="replace"/>
        </xsd:restriction>
    </xsd:simpleType>
    
    <xsd:simpleType name="airSegmentId">
        <xsd:restriction base="xsd:normalizedString">
            <xsd:maxLength value="100"/>
            <xsd:whiteSpace value="replace"/>
        </xsd:restriction>
    </xsd:simpleType>
    
    <xsd:simpleType name="airDestinationCode">
        <xsd:restriction base="xsd:normalizedString">
            <xsd:maxLength value="3"/>
            <xsd:whiteSpace value="replace"/>
        </xsd:restriction>
    </xsd:simpleType>  
    
    <xsd:simpleType name="airServiceFeeNumber">
        <xsd:restriction base="xsd:normalizedString">
            <xsd:maxLength value="20"/>
            <xsd:whiteSpace value="replace"/>
        </xsd:restriction>
    </xsd:simpleType>
    
    <xsd:simpleType name="transactionUniqueId">
        <xsd:restriction base="xsd:normalizedString">
            <xsd:maxLength value="40"/>
            <xsd:whiteSpace value="replace"/>
        </xsd:restriction>
    </xsd:simpleType>
    
    <xsd:simpleType name="lodgingItineraryNumber">
        <xsd:restriction base="xsd:normalizedString">
            <xsd:maxLength value="20"/>
            <xsd:whiteSpace value="replace"/>
        </xsd:restriction>
    </xsd:simpleType>
    
    <xsd:simpleType name="lodgingPrepayDaysNumber">
        <xsd:restriction base="xsd:normalizedString">
            <xsd:maxLength value="3"/>
            <xsd:whiteSpace value="replace"/>
        </xsd:restriction>
    </xsd:simpleType>
    
    <xsd:simpleType name="lodgingPropName">
        <xsd:restriction base="xsd:normalizedString">
            <xsd:maxLength value="30"/>
            <xsd:whiteSpace value="replace"/>
        </xsd:restriction>
    </xsd:simpleType>
    
    <xsd:simpleType name="lodgingPropertyCityName">
        <xsd:restriction base="xsd:normalizedString">
            <xsd:maxLength value="40"/>
            <xsd:whiteSpace value="replace"/>
        </xsd:restriction>
    </xsd:simpleType>
    
    <xsd:simpleType name="lodgingPropertyStateCode">
        <xsd:restriction base="xsd:normalizedString">
            <xsd:maxLength value="2"/>
            <xsd:whiteSpace value="replace"/>
        </xsd:restriction>
    </xsd:simpleType>
    
    <xsd:simpleType name="lodgingCountryName">
        <xsd:restriction base="xsd:normalizedString">
            <xsd:maxLength value="40"/>
            <xsd:whiteSpace value="replace"/>
        </xsd:restriction>
    </xsd:simpleType>
    
    <xsd:simpleType name="rentalCarItineraryNumber">
        <xsd:restriction base="xsd:normalizedString">
            <xsd:maxLength value="20"/>
            <xsd:whiteSpace value="replace"/>
        </xsd:restriction>
    </xsd:simpleType>
    
    <xsd:simpleType name="rentalCarNumberOfDays">
        <xsd:restriction base="xsd:normalizedString">
            <xsd:maxLength value="3"/>
            <xsd:whiteSpace value="replace"/>
        </xsd:restriction>
    </xsd:simpleType>
    
    <xsd:simpleType name="rentalCarCompanyName">
        <xsd:restriction base="xsd:normalizedString">
            <xsd:maxLength value="30"/>
            <xsd:whiteSpace value="replace"/>
        </xsd:restriction>
    </xsd:simpleType>
    
    <xsd:simpleType name="rentalCarDetailText">
        <xsd:restriction base="xsd:normalizedString">
            <xsd:maxLength value="100"/>
            <xsd:whiteSpace value="replace"/>
        </xsd:restriction>
    </xsd:simpleType>
    
    <xsd:simpleType name="registrationCompanyName">
        <xsd:restriction base="xsd:normalizedString">
            <xsd:maxLength value="50"/>
            <xsd:whiteSpace value="replace"/>
        </xsd:restriction>
    </xsd:simpleType>
    
    <xsd:simpleType name="moveToHistoryIndicator">
        <xsd:restriction base="xsd:normalizedString">
            <xsd:maxLength value="1"/>
            <xsd:whiteSpace value="replace"/>
        </xsd:restriction>
    </xsd:simpleType>

	<xsd:complexType name="agencyDataType">
		<xsd:sequence>
			<xsd:element minOccurs="1" maxOccurs="1" ref="importBy" />
			<xsd:element maxOccurs="unbounded" ref="record" />
		</xsd:sequence>
	</xsd:complexType>

	<xsd:complexType name="agencyDataRecordType">
		<xsd:sequence>
    
		    <!-- Agency Information -->
		    <xsd:element ref="agencyDataId" minOccurs="0" maxOccurs="1" />
		    <xsd:element ref="creditCardOrAgencyCode" minOccurs="1" maxOccurs="1" />
		    <xsd:element ref="agency" minOccurs="0" maxOccurs="1" />    
		    <xsd:element ref="otherCompanyName" minOccurs="0" maxOccurs="1" />
		    <xsd:element ref="agencyFileName" minOccurs="0" maxOccurs="1" />
		    <xsd:element ref="merchantName" minOccurs="0" maxOccurs="1" />
		    <xsd:element ref="billingCycleDate" minOccurs="0" maxOccurs="1" />
		    
		    <!-- Trip Information -->
		    
		    <xsd:element ref="tripId" minOccurs="0" maxOccurs="1" />
		    <xsd:element ref="tripInvoiceNumber" minOccurs="0" maxOccurs="1" />
		    <xsd:element ref="tripTravelerTypeId" minOccurs="0" maxOccurs="1" />
		    <xsd:element ref="otherAmount" minOccurs="0" maxOccurs="1" />
		    
		    <!-- Traveler Information -->
		    
		    <xsd:element ref="travelerName" minOccurs="1" maxOccurs="1" />
		    <xsd:element ref="travelerId" minOccurs="0" maxOccurs="1" />
		    <xsd:element ref="travelerNetworkId" minOccurs="0" maxOccurs="1" />
		    <xsd:element ref="tripExpenseAmount" minOccurs="0" maxOccurs="1" />
		    <xsd:element ref="alternateTripId" minOccurs="0" maxOccurs="1" />
		    <xsd:element ref="tripArrangerName" minOccurs="0" maxOccurs="1" />
		    
		    <!-- Accounting Information -->
		    <xsd:element ref="accountingInfo" minOccurs="0" maxOccurs="3" />
		    <xsd:element ref="groupObjectCode" minOccurs="0" maxOccurs="1" />
		    <xsd:element ref="distributionCode" minOccurs="0" maxOccurs="1" />
		    
		    <!-- Airline Information -->
		    
		    <xsd:element ref="tripDepartureDate" minOccurs="0" maxOccurs="1" />
		    <xsd:element ref="tripReturnDate" minOccurs="0" maxOccurs="1" />
		    <xsd:element ref="fareSaverCode" minOccurs="0" maxOccurs="1" />
		    <xsd:element ref="airBookDate" minOccurs="0" maxOccurs="1" />
		    <xsd:element ref="airCarrierCode" minOccurs="0" maxOccurs="1" />
		    <xsd:element ref="airTicketNumber" minOccurs="0" maxOccurs="1" />
		    <xsd:element ref="pnrNumber" minOccurs="0" maxOccurs="1" />
		    <xsd:element ref="airTicketClass" minOccurs="0" maxOccurs="1" />
		    <xsd:element ref="airTransactionAmount" minOccurs="0" maxOccurs="1" />
		    <xsd:element ref="airBaseFareAmount" minOccurs="0" maxOccurs="1" />
		    <xsd:element ref="airTaxAmount" minOccurs="0" maxOccurs="1" />
		    <xsd:element ref="airLowFareAmount" minOccurs="0" maxOccurs="1" />
		    <xsd:element ref="airReasonCode" minOccurs="0" maxOccurs="1" />
		    <xsd:element ref="airSegmentId" minOccurs="0" maxOccurs="1" />
		    <xsd:element ref="airDestinationCode" minOccurs="0" maxOccurs="1" />
		    <xsd:element ref="airServiceFeeNumber" minOccurs="0" maxOccurs="1" />
		    <xsd:element ref="airServiceFeeAmount" minOccurs="0" maxOccurs="1" />
		    <xsd:element ref="transactionUniqueId" minOccurs="0" maxOccurs="1" />
		    
		    <!-- Lodging information -->
		    
		    <xsd:element ref="lodgingItineraryNumber" minOccurs="0" maxOccurs="1" />
		    <xsd:element ref="lodgingPrepayDate" minOccurs="0" maxOccurs="1" />
		    <xsd:element ref="lodgingAmount" minOccurs="0" maxOccurs="1" />
		    <xsd:element ref="lodgingPrepayDaysNumber" minOccurs="0" maxOccurs="1" />
		    <xsd:element ref="lodgingPropName" minOccurs="0" maxOccurs="1" />
		    <xsd:element ref="tripLodgingArrivalDate" minOccurs="0" maxOccurs="1" />
		    <xsd:element ref="lodgingDepartureDate" minOccurs="0" maxOccurs="1" />
		    <xsd:element ref="lodgingBookingDate" minOccurs="0" maxOccurs="1" />
		    <xsd:element ref="lodgingPropertyCityName" minOccurs="0" maxOccurs="1" />  
		    <xsd:element ref="lodgingPropertyStateCode" minOccurs="0" maxOccurs="1" />
		    <xsd:element ref="lodgingCountryName" minOccurs="0" maxOccurs="1" />
		    
		    <!-- Rental Car Information -->
		    
		    <xsd:element ref="rentalCarItineraryNumber" minOccurs="0" maxOccurs="1" />
		    <xsd:element ref="rentalCarAmount" minOccurs="0" maxOccurs="1" />
		    <xsd:element ref="rentalCarNumberOfDays" minOccurs="0" maxOccurs="1" />
		    <xsd:element ref="rentalCarCompanyName" minOccurs="0" maxOccurs="1" />
		    <xsd:element ref="rentalCarOpenDate" minOccurs="0" maxOccurs="1" />
		    <xsd:element ref="rentalCarCloseDate" minOccurs="0" maxOccurs="1" />
		    <xsd:element ref="rentalCarFuelAmount" minOccurs="0" maxOccurs="1" />
		    <xsd:element ref="rentalCarAdditionalAmount" minOccurs="0" maxOccurs="1" />
		    <xsd:element ref="rentalCarTaxAmount" minOccurs="0" maxOccurs="1" />
		    <xsd:element ref="rentalCarSurchargeAmount" minOccurs="0" maxOccurs="1" />
		    <xsd:element ref="rentalCarGovernmentSurchargeAmount" minOccurs="0" maxOccurs="1" />
		    <xsd:element ref="rentalCarBillingAmount" minOccurs="0" maxOccurs="1" />
		    <xsd:element ref="rentalCarDetailText" minOccurs="0" maxOccurs="1" />
		    
		    <!-- Conference Registration -->
		    <xsd:element ref="registrationCompanyName" minOccurs="0" maxOccurs="1"/>
		    <xsd:element ref="registrationAmount" minOccurs="0" maxOccurs="1" />
		    
		    <!-- Processing Information -->
		    <xsd:element ref="transactionPostingDate" minOccurs="1" maxOccurs="1" />
		    <xsd:element ref="objectVersionNumber" minOccurs="0" maxOccurs="1" />
		    <xsd:element ref="creationTimestamp" minOccurs="0" maxOccurs="1" /> 
		    <xsd:element ref="processingTimestamp" minOccurs="0" maxOccurs="1" /> 
		    <xsd:element ref="moveToHistoryIndicator" minOccurs="0" maxOccurs="1" />

		</xsd:sequence>
	</xsd:complexType>
	
	<xsd:element name="record" type="agencyDataRecordType" />	 
	<xsd:element name="agencyData" type="agencyDataType" />	
	
	
</xsd:schema>
