+package edu.sample.kuali.module.chart.bo;
+
+import java.util.LinkedHashMap;
+
+import org.kuali.core.bo.PersistableBusinessObjectBase;
+
+/**
+ * 
+ */
+public class Program extends PersistableBusinessObjectBase {
+
+    private String programCode;
+    private String programName;
+    private boolean programActiveIndicator;
+
+    /**
+     * Default constructor.
+     */
+    public Program() {
+
+    }
+
+    /**
+     * Gets the programCode attribute.
+     * 
+     * @return Returns the programCode
+     * 
+     */
+    public String getProgramCode() {
+        return programCode;
+    }
+
+    /**
+     * Sets the programCode attribute.
+     * 
+     * @param programCode The programCode to set.
+     * 
+     */
+    public void setProgramCode(String programCode) {
+        this.programCode = programCode;
+    }
+
+
+    /**
+     * Gets the programName attribute.
+     * 
+     * @return Returns the programName
+     * 
+     */
+    public String getProgramName() {
+        return programName;
+    }
+
+    /**
+     * Sets the programName attribute.
+     * 
+     * @param programName The programName to set.
+     * 
+     */
+    public void setProgramName(String programName) {
+        this.programName = programName;
+    }
+
+
+    /**
+     * @return Returns the code and description in format: xx - xxxxxxxxxxxxxxxx
+     */
+    public String getCodeAndDescription() {
+        String theString = getProgramCode() + " - " + getProgramName();
+        return theString;
+    }
+
+
+    /**
+     * Gets the programActiveIndicator attribute.
+     * 
+     * @return Returns the programActiveIndicator
+     * 
+     */
+    public boolean isProgramActiveIndicator() {
+        return programActiveIndicator;
+    }
+
+    /**
+     * Sets the programActiveIndicator attribute.
+     * 
+     * @param programActiveIndicator The programActiveIndicator to set.
+     * 
+     */
+    public void setProgramActiveIndicator(boolean programActiveIndicator) {
+        this.programActiveIndicator = programActiveIndicator;
+    }
+
+
+    /**
+     * @see org.kuali.core.bo.BusinessObjectBase#toStringMapper()
+     */
+    protected LinkedHashMap toStringMapper() {
+        LinkedHashMap m = new LinkedHashMap();
+        m.put("programCode", this.programCode);
+        return m;
+    }
+}
